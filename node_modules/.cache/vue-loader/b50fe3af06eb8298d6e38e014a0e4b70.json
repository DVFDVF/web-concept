AC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACzD,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACV,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EACzD,CAAC;;EAED,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;IACL,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;MACtB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IAC3B,CAAC;EACH,CAAC;EACD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC;EACd,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;IACL,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACd,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACT,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACV,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACf,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACR,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EACd,CAAC;AACH,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/concept/web3-concept/src/views/game/dice/left/GameForm.vue","sourceRoot":"","sourcesContent":["<template>\n  <form class=\"form\">\n    <div class=\"amount\">\n      <div class=\"amount-left\">\n        <button\n          class=\"amount-btn amount-min\"\n          type=\"button\"\n          @click=\"numberAmount('min')\"\n          :disabled=\"isStart\"\n        >\n          {{ lang.v.dice_min }}\n        </button>\n        <button\n          class=\"amount-btn amount-max\"\n          type=\"button\"\n          @click=\"numberAmount('max')\"\n          :disabled=\"isStart\"\n        >\n          {{ lang.v.dice_max }}\n        </button>\n      </div>\n      <div class=\"amount-center\">\n        <div>\n          <Image :src=\"selectIcon\" loading=\"lazy\" />\n          <input\n            type=\"text\"\n            inputmode=\"decimal\"\n            v-model=\"inputText\"\n            @input=\"handleInput($event)\"\n            :disabled=\"isStart\"\n          />\n        </div>\n      </div>\n      <div class=\"amount-right\">\n        <button\n          class=\"amount-btn\"\n          type=\"button\"\n          @click=\"BettingAmount(0.5)\"\n          :disabled=\"isStart\"\n        >\n          1/2\n        </button>\n        <button\n          class=\"amount-btn\"\n          type=\"button\"\n          @click=\"BettingAmount(2)\"\n          :disabled=\"isStart\"\n        >\n          2x\n        </button>\n      </div>\n    </div>\n    <button\n      type=\"button\"\n      class=\"btn btn-blue btn-large\"\n      @click=\"startClick\"\n      :disabled=\"!isEnable\"\n    >\n      <span class=\"start\">\n        <span> {{ !isStart ? lang.v.dice_start : lang.v.limbo_stop }}</span>\n        <div class=\"lds-dual-ring\" v-show=\"isStart\">\n          <div\n            class=\"lds-dual-ring-after\"\n            style=\"border-color: rgb(255, 255, 255) transparent\"\n          ></div>\n        </div>\n      </span>\n      <span>{{ btnText }}</span>\n    </button>\n  </form>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport {\n  AmontKeyDown,\n  amountMax,\n  amountMin,\n  gameBettingAmount,\n  gameHandleInput,\n} from '@/games/methods';\nimport { lang } from '@/lang';\nimport { formatBalance } from '@/utils';\n\nimport { defineComponent } from 'vue';\n\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      inputText: '',\n      oldInput: '100.000000',\n      smallMode: false,\n    };\n  },\n  computed: {\n    selectIcon() {\n      return (\n        dataCenter.webConfig.currencyMap[dataCenter.wallet.selectId]?.icon ??\n        dataCenter.webConfig.cryptoCurrencies[0].icon //处理未登录\n      );\n    },\n    btnText() {\n      const inputNumber = parseInt(this.inputText) || 0;\n      let profit = (inputNumber * this.returnRate).toFixed(6);\n      let currency =\n        dataCenter.webConfig.currencyMap[dataCenter.wallet.selectId];\n      if (currency) {\n        profit = formatBalance(currency, inputNumber * this.returnRate);\n      }\n      const profitOnWin = `${lang.v.dice_profitOnWin}${profit}${\n        dataCenter.webConfig.currencyMap[dataCenter.wallet.selectId]?.name\n      }`;\n      return this.isAuto\n        ? this.childAutobet.count !== ''\n          ? this.childAutobet.count\n          : '∞'\n        : this.smallMode\n        ? profit\n        : profitOnWin;\n    },\n  },\n  methods: {\n    numberAmount(type: string) {\n      if (type == 'min') {\n        this.inputText = amountMin();\n      } else if (type == 'max') {\n        this.inputText = amountMax();\n      }\n      this.$emit('updateValue', this.inputText);\n    },\n    BettingAmount(magnification: number) {\n      this.inputText = gameBettingAmount(magnification, this.inputText);\n      this.$emit('updateValue', this.inputText);\n    },\n    formatInput(text: string) {\n      this.inputText = gameHandleInput(String(text));\n      this.$emit('updateValue', this.inputText);\n    },\n    handleInput(event: Event) {\n      this.inputText = gameHandleInput(this.inputText, event);\n      this.$emit('updateValue', this.inputText);\n    },\n    handleKeyDown(event: KeyboardEvent) {\n      if (dataCenter.gameConfiguration.HotkeyEnabled) {\n        if (event.key == ' ') {\n          event.preventDefault();\n          this.$emit('startGame', this.bettingAmount);\n          return;\n        }\n      }\n      this.inputText = AmontKeyDown(event, this.inputText);\n    },\n    async startClick() {\n      this.$emit('startGame', this.bettingAmount);\n    },\n    handleResize() {\n      // 获取窗口宽度\n      const windowWidth = window.innerWidth;\n\n      // 根据窗口宽度设置不同的文字\n      if (windowWidth < 767) {\n        this.smallMode = true;\n      } else {\n        this.smallMode = false;\n      }\n    },\n  },\n  mounted() {\n    window.addEventListener('keydown', this.handleKeyDown);\n    // 监听窗口大小变化\n    window.addEventListener('resize', this.handleResize);\n    // 初始设置文字\n    this.handleResize();\n    this.inputText = '10.00';\n  },\n  beforeUnmount() {\n    window.removeEventListener('keydown', this.handleKeyDown);\n    // 监听窗口大小变化\n    window.removeEventListener('resize', this.handleResize);\n  },\n\n  watch: {\n    bettingAmount(newValue) {\n      this.inputText = newValue;\n    },\n  },\n  components: {},\n  props: [\n    'childAutobet',\n    'isStart',\n    'isEnable',\n    'bettingAmount',\n    'isAuto',\n    'returnRate',\n  ],\n});\n</script>\n\n<style lang=\"less\" scoped>\n.form {\n  display: flex;\n  justify-content: space-between;\n  margin-top: 30px;\n}\n\n.form .lds-dual-ring {\n  width: 18px;\n}\n\n.form .amount {\n  margin-right: 10px;\n  width: calc(50% - 10px);\n}\n\n.form .btn > span {\n  align-items: center;\n  display: flex;\n  line-height: 1;\n  text-align: center;\n}\n\n.form .btn > span:first-child {\n  overflow: hidden;\n  position: relative;\n  text-overflow: ellipsis;\n  text-transform: uppercase;\n  white-space: nowrap;\n}\n\n.form .amount {\n  margin-right: 10px;\n  width: calc(50% - 10px);\n}\n\n.form .btn {\n  align-items: center;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  margin-left: 10px;\n  width: calc(50% - 10px);\n}\n\n.amount {\n  background: #161f2c;\n  border: 1px solid #4b5b74;\n  border-radius: 10px;\n  padding: 10px;\n  position: relative;\n}\n\n.amount,\n.amount-center {\n  align-items: center;\n  display: flex;\n}\n\n.amount-center {\n  flex: 1 1;\n  flex-direction: column;\n}\n\n.amount-center > div {\n  display: flex;\n  justify-content: center;\n  margin-bottom: 5px;\n}\n\n.amount-center img {\n  height: 24px;\n  margin-right: 5px;\n  width: 24px;\n}\n\n.amount-center input {\n  background-color: transparent;\n  border: none;\n  color: #fff;\n  font-size: 18px;\n  font-weight: 700;\n  outline: none;\n  padding: 0;\n  width: 120px;\n}\n\n.amount-center > span {\n  color: #55657e;\n  font-size: 14px;\n  font-weight: 600;\n  text-align: center;\n  text-transform: uppercase;\n}\n\n.amount-left {\n  display: flex;\n  flex-direction: column;\n}\n\n.amount-right {\n  display: flex;\n  flex-direction: column;\n}\n\n.amount-btn {\n  background-color: #283343;\n  border: 1px solid transparent;\n  border-radius: 7px;\n  color: #55657e;\n  font-size: 12px;\n  font-weight: 600;\n  height: 25px;\n  line-height: 23px;\n  padding: 0;\n  touch-action: manipulation;\n  transition: all 0.3s;\n  width: 40px;\n}\n\n.amount-btn:first-child {\n  margin-bottom: 5px;\n}\n\n.amount-btn:hover {\n  background: #122e4f;\n  border: 1px solid #297fe5;\n  color: #fff;\n}\n\n.amount-btn:active,\n.amount-btn:focus {\n  background: #297fe5;\n  color: #fff;\n}\n\n.amount-btn:disabled {\n  pointer-events: none;\n}\n\n.btn-large {\n  font-size: 18px;\n  padding: 15px 30px;\n}\n\n.btn-blue {\n  background-color: #2283f6;\n}\n\n.btn {\n  border: 1px solid transparent;\n  border-radius: 7px;\n  color: #fff;\n  display: inline-block;\n  font-style: normal;\n  font-weight: 600;\n  line-height: 1.4;\n  position: relative;\n  text-align: center;\n  text-decoration: none;\n  transition: all 0.3s;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  user-select: none;\n  white-space: nowrap;\n}\n\n.btn-blue:hover {\n  box-shadow: 0 14px 20px #2283f64d;\n}\n\n.btn-blue:hover {\n  background-color: #0a75f5;\n}\n\nbody button {\n  background-color: transparent;\n  border: none;\n  cursor: pointer;\n}\n\n.lds-dual-ring-after {\n  animation: rotateAnimation 1.2s linear infinite;\n  border: 3px solid #fff;\n  border-radius: 50%;\n  content: ' ';\n  display: block;\n  height: 13px;\n  margin: 1px;\n  width: 13px;\n}\n\n@keyframes rotateAnimation {\n  0% {\n    transform: rotate(0);\n  }\n\n  100% {\n    transform: rotate(1turn);\n  }\n}\n\n.start {\n  display: flex;\n  flex-direction: row;\n}\n\n.form .btn > span:nth-child(2) {\n  color: #ffffffbf;\n  font-size: 12px;\n  font-weight: 500;\n  margin-top: 10px;\n  text-transform: uppercase;\n}\n\n.btn-blue:disabled {\n  background-color: #096adc;\n}\n@media only screen and (max-width: 767px) {\n  .form .amount {\n    margin-right: 0;\n    width: 70%;\n  }\n  .form .btn {\n    align-items: center;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    margin-left: 10px;\n    width: calc(30% - 10px);\n  }\n  .amount-min,\n  .amount-max {\n    display: none;\n  }\n}\n</style>\n"]}]}