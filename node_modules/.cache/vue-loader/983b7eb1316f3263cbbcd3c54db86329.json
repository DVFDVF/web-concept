ate>\n  <div class=\"recommendation-main\">\n    <div class=\"recommendation-statistics\">\n      <div\n        v-for=\"item in title\"\n        :key=\"item.key\"\n        class=\"statistics-title col-center\"\n      >\n        <img :src=\"item.icon\" class=\"icon\" />\n        <div>{{ item.text }}</div>\n      </div>\n      <div\n        class=\"recommendation-content row-center\"\n        v-for=\"item in content\"\n        :key=\"item.key\"\n      >\n        <div class=\"title col-middle\" v-show=\"item.title\" :style=\"item.color\">\n          {{ item.title }}\n        </div>\n        <div\n          class=\"number\"\n          v-if=\"\n            item.key === 3 || item.key === 2 || item.key === 6 || item.key === 7\n          \"\n        >\n          <div>\n            <Image\n              width=\"24\"\n              height=\"24\"\n              :src=\"dataCenter.webConfig.currencyIcon\"\n              loading=\"lazy\"\n            />\n            <div>\n              <amount-prompt :amountValue=\"item.number\">\n                <span style=\"color: rgb(27, 184, 61); white-space: nowrap\">\n                  $\n                  {{ item.number }}\n                </span>\n              </amount-prompt>\n            </div>\n          </div>\n        </div>\n        <div class=\"number\" v-else>{{ item.number }}</div>\n      </div>\n      <div class=\"lines col-center\">\n        <div class=\"row-lines\" v-for=\"item in 3\" :key=\"item\"></div>\n      </div>\n      <div class=\"lines row-center\">\n        <div class=\"col-lines\" v-for=\"item in 4\" :key=\"item\"></div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\" setup>\nimport { lang } from '@/lang';\nimport { UserRefereeInfoResp } from '@/views/home/api/protocol';\nimport { computed, defineProps } from 'vue';\nimport AmountPrompt from '@/views/chats/prompt/amountPrompt.vue';\nimport dataCenter from '@/dataCenter';\nconst props = defineProps({\n  userRefereeInfo: Object as () => UserRefereeInfoResp,\n});\nconst title = [\n  {\n    key: 0,\n    icon: require('@/assets/newRecommendation/8818.png'),\n    get text() {\n      return lang.v.fk_inscrt;\n    },\n  },\n  {\n    key: 1,\n    icon: require('@/assets/newRecommendation/8819.png'),\n    get text() {\n      return lang.v.fk_novos;\n    },\n  },\n  {\n    key: 2,\n    icon: require('@/assets/newRecommendation/8820.png'),\n    get text() {\n      return lang.v.fk_aposta;\n    },\n  },\n  {\n    key: 3,\n    icon: require('@/assets/newRecommendation/8821.png'),\n    get text() {\n      return lang.v.fk_comissao;\n    },\n  },\n];\nconst content = [\n  {\n    key: 0,\n    get title() {\n      return lang.v.fk_today;\n    },\n    number: computed(() => props.userRefereeInfo?.todayRegister || 0),\n    color: {\n      background:\n        'linear-gradient(180deg,hsla(218, 26%, 19%, 0.3) 0%,hsla(347, 89%, 57%, 0.3) 100%)',\n    },\n  },\n  {\n    key: 1,\n    number: computed(() => props.userRefereeInfo?.todayRecharge || 0),\n  },\n  {\n    key: 2,\n    number: computed(() => Number(props.userRefereeInfo?.todayExp) || 0),\n  },\n  {\n    key: 3,\n    number: computed(() => Number(props.userRefereeInfo?.todayIn) || 0),\n  },\n  {\n    key: 4,\n    get title() {\n      return lang.v.fk_todo;\n    },\n    number: computed(() => props.userRefereeInfo?.totalRegister || 0),\n    color: {\n      background: `linear-gradient(\n            180deg,\n            hsla(218, 26%, 19%, 0.3) 0%,\n            hsla(219, 96%, 59%, 0.3) 100%\n          )`,\n    },\n  },\n  {\n    key: 5,\n    number: computed(() => props.userRefereeInfo?.totalRecharge || 0),\n  },\n  {\n    key: 6,\n    number: computed(() => Number(props.userRefereeInfo?.totalExp) || 0),\n  },\n  {\n    key: 7,\n    number: computed(() => Number(props.userRefereeInfo?.totalIn) || 0),\n  },\n];\n</script>\n\n<style lang=\"less\" scoped>\n.recommendation-main {\n  background: #1c222f;\n  padding: 30px 20px;\n  border-radius: 11px;\n  .recommendation-statistics {\n    display: grid;\n    grid-template-rows: repeat(3, 1fr);\n    grid-template-columns: repeat(4, 1fr);\n    justify-content: center;\n    align-items: center;\n    text-align: center;\n    position: relative;\n    .statistics-title {\n      gap: 10px;\n      color: #acacac;\n      font-weight: bold;\n      height: 100px;\n      font-size: 13px;\n      .icon {\n        width: 20%;\n      }\n    }\n    .recommendation-content {\n      width: 100%;\n      height: 100%;\n      border-radius: 5px;\n      background: #242d3d;\n      overflow: hidden;\n      .title {\n        height: 100%;\n        width: 40%;\n        font-size: 13px;\n        font-weight: 900;\n      }\n      .number {\n        flex: 1;\n        font-family: Montserrat Alternates, Montserrat Alternates;\n        color: #f4e392;\n        font-weight: bold;\n        font-size: 19px;\n      }\n    }\n    .lines {\n      position: absolute;\n      top: 0;\n      left: 0;\n      width: 100%;\n      height: 100%;\n      .row-lines {\n        width: 100%;\n        flex: 1;\n        border-bottom: 2px solid;\n        border-image: linear-gradient(\n          117deg,\n          #1c222f 0%,\n          #ffffff 52%,\n          #1c222f 100%\n        );\n        border-image-slice: 1;\n        opacity: 0.35;\n      }\n      .col-lines {\n        height: 100%;\n        flex: 1;\n        opacity: 0.35;\n      }\n      .col-lines:not(:last-child) {\n        border-right: 2px solid;\n        border-image: linear-gradient(\n          to bottom,\n          #1c222f 0%,\n          #ffffff 52%,\n          #1c222f 100%\n        );\n        border-image-slice: 1;\n      }\n    }\n  }\n}\n\n@media only screen and (max-width: 767px) {\n  .recommendation-main {\n    padding: 20px 10px;\n    .recommendation-statistics {\n      .statistics-title {\n        font-size: 9px;\n        .icon {\n          width: 40%;\n        }\n      }\n      .recommendation-content {\n        .title {\n          font-size: 9px;\n        }\n        .number {\n          font-size: 10px;\n        }\n      }\n    }\n  }\n}\n</style>\n"]}]}