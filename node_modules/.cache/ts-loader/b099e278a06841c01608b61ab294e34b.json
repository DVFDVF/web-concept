 },\n        isActive: false,\n        iconSrc: require('@/assets/header/icon-transactions.webp'),\n      },\n      {\n        id: 3,\n        get title() {\n          return lang.v.da_gameHistory;\n        },\n        isActive: false,\n        iconSrc: require('@/assets/header/icon-game-history.webp'),\n      },\n\n      // ... add other tabs as needed\n    ] as Tab[]);\n    const selectItem = ref(tabs.value[0]);\n    return {\n      tabs,\n      setActives,\n      selectItem,\n    };\n  },\n  methods: {\n    setActiveTab(selectedId: number) {\n      const routesMap: { [key: number]: string } = {\n        1: '/Userprofe/Account',\n        2: '/Userprofe/Transactions',\n        3: '/Userprofe/GameHistory',\n      };\n\n      this.selectItem = this.tabs[selectedId - 1];\n      this.tabs.forEach((tab) => {\n        tab.isActive = tab.id === selectedId;\n      });\n\n      const selectedRoute = routesMap[selectedId] || '/Userprofe/Verification';\n      this.$router.push(selectedRoute);\n    },\n  },\n});\n"],"mappings":";AACA,OAAOA,YAAY,MAAM,uCAAuC;AAChE,SAASC,eAAe,EAAEC,KAAK,EAAEC,GAAG,EAAiBC,SAAS,QAAQ,KAAK;AAC3E,SAASC,IAAI,QAAQ,QAAQ;AAC7B,SAASC,QAAQ,QAAQ,YAAY;AASrC,eAAeL,eAAe,CAAC;EAC7BM,UAAU,EAAE;IAAEP;EAAY,CAAE;EAC5BQ,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA;IACF,OAAO;MACLJ;KACD;EACH,CAAC;EACDK,KAAKA,CAAA;IACH,SAASC,wBAAwBA,CAACC,KAAU;MAC1C,QAAQA,KAAK,CAACJ,IAAI;QAChB,KAAK,SAAS;UACZK,UAAU,CAAC,CAAC,CAAC;UACbC,UAAU,CAACC,KAAK,GAAGC,IAAI,CAACD,KAAK,CAAC,CAAC,CAAC;UAChC;QACF,KAAK,cAAc;UACjBF,UAAU,CAAC,CAAC,CAAC;UACbC,UAAU,CAACC,KAAK,GAAGC,IAAI,CAACD,KAAK,CAAC,CAAC,CAAC;UAChC;QACF,KAAK,aAAa;UAChBF,UAAU,CAAC,CAAC,CAAC;UACbC,UAAU,CAACC,KAAK,GAAGC,IAAI,CAACD,KAAK,CAAC,CAAC,CAAC;UAChC;QACF,KAAK,UAAU;UACbF,UAAU,CAAC,CAAC,CAAC;UACbC,UAAU,CAACC,KAAK,GAAGC,IAAI,CAACD,KAAK,CAAC,CAAC,CAAC;UAChC;QACF;UACEF,UAAU,CAAC,CAAC,CAAC;UACbC,UAAU,CAACC,KAAK,GAAGC,IAAI,CAACD,KAAK,CAAC,CAAC,CAAC;UAChC;;IAEN;IACAX,SAAS,CAAC,MAAK;MACb,MAAMQ,KAAK,GAAGN,QAAQ,EAAE;MAExB;MACAK,wBAAwB,CAACC,KAAK,CAAC;MAE