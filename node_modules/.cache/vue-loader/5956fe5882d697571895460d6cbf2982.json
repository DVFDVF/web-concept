meço\"\n        />\n        ~\n        <a-date-picker\n          v-model:value=\"endDate\"\n          format=\"YYYY-MM-DD\"\n          @change=\"dateSelect\"\n          placeholder=\"Hora de fim\"\n        />\n      </div>\n      <div v-show=\"(refereeData?.length ?? 0) > 0\" style=\"width: 100%\">\n        <div class=\"payment-content\">\n          <div\n            class=\"payment-item\"\n            v-for=\"titleItem in titleData\"\n            :key=\"titleItem.key\"\n          >\n            <div class=\"name\">{{ titleItem.title }}</div>\n            <div\n              class=\"text-data col-center\"\n              v-for=\"recordItem in refereeData\"\n              :key=\"recordItem.userId\"\n            >\n              <span> {{ (recordItem as any)[titleItem.pop] }} </span>\n            </div>\n          </div>\n        </div>\n        <a-pagination\n          style=\"margin-top: 20px\"\n          v-model:pageNumber=\"pagination.pageNumber\"\n          :total=\"pagination.total\"\n          :pageSize=\"pagination.pageSize\"\n          :showSizeChanger=\"false\"\n          show-less-items\n          @change=\"handlePageChange\"\n        />\n      </div>\n      <div class=\"rewards-table-empty\" v-show=\"refereeData?.length === 0\">\n        <div class=\"no-data\">\n          <img\n            class=\"no-data-images\"\n            src=\"@/assets/home/casino/no-data-image.webp\"\n          />\n          <span>{{ lang.v.re_da_oops }}</span>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\" setup>\nimport { onMounted, reactive, ref } from 'vue';\nimport { Pagination as APagination } from 'ant-design-vue';\nimport { getNetworkReferee } from '@/views/home/api';\nimport { lang } from '@/lang';\nimport { NetworkRefereeData } from '@/views/home/api/protocol';\nimport dayjs, { Dayjs } from 'dayjs';\nimport { DatePicker as aDatePicker } from 'ant-design-vue';\nconst startDate = ref<Dayjs>(dayjs().startOf('day'));\nconst endDate = ref<Dayjs>(dayjs().endOf('day'));\nimport { tip } from '@/components/warn/warn';\nimport { affilliate } from '.';\nconst pagination = reactive({\n  pageNumber: 1,\n  pageSize: 10,\n  total: 0,\n});\nconst titleData = [\n  {\n    key: 0,\n    get title() {\n      return lang.v.rc_userid;\n    },\n    pop: 'userId',\n  },\n  {\n    key: 1,\n    get title() {\n      return lang.v.rc_createtime;\n    },\n    pop: 'createTime',\n  },\n  {\n    key: 2,\n    get title() {\n      return lang.v.rc_totalRechargeAmount;\n    },\n    pop: 'totalRechargeAmount',\n  },\n];\nconst refereeData = ref<NetworkRefereeData[]>();\nfunction networkReferee() {\n  const start = startDate.value\n    .startOf('day')\n    .add(3, 'hour')\n    .format('YYYY-MM-DD hh:mm:ss');\n  const end = endDate.value\n    .endOf('day')\n    .add(3, 'hour')\n    .format('YYYY-MM-DD hh:mm:ss');\n  getNetworkReferee(\n    pagination.pageSize,\n    pagination.pageNumber,\n    start,\n    end,\n  ).then((resp) => {\n    if (!resp) return;\n    refereeData.value = resp.netWorkRefereeInfoMyPage.data;\n    pagination.total = resp.netWorkRefereeInfoMyPage.total;\n  });\n}\nfunction handlePageChange(curren: number) {\n  pagination.pageNumber = curren;\n  networkReferee();\n}\nfunction dateSelect() {\n  if (!startDate.value || !endDate.value) return;\n  if (startDate.value.isBefore(endDate.value)) {\n    refereeData.value! = [];\n    networkReferee();\n  } else {\n    tip.error(lang.v.rc_horario);\n  }\n}\nonMounted(() => {\n  networkReferee();\n});\n</script>\n\n<style lang=\"less\" scoped>\n.recommendation-payment {\n  background: #1c222f;\n  border-radius: 11px;\n  padding: 20px;\n  margin-right: 20px;\n  flex: 1;\n  .title {\n    color: white;\n    font-weight: bold;\n    font-size: 20px;\n  }\n  .payment-content {\n    margin-top: 10px;\n    width: 100%;\n    display: flex;\n    flex-direction: row;\n    .payment-item {\n      padding: 10px 0;\n      border-radius: 10px;\n      background: linear-gradient(180deg, #242d3d 0%, #1c222f 100%);\n      margin: 0 5px;\n      flex: 1;\n      .name {\n        color: white;\n        font-weight: bold;\n        font-size: 20px;\n        height: 40px;\n        border-bottom: 1px solid #ffffff30;\n        margin-bottom: 20px;\n      }\n      .text-data {\n        font-weight: 500;\n        font-size: 12px;\n        color: #f0f0f0;\n        height: 30px;\n        white-space: none;\n      }\n    }\n\n    .payment-item:nth-child(3) {\n      .text-data {\n        color: #2783f6;\n        font-weight: bold;\n      }\n    }\n    .payment-item:nth-child(2) {\n      flex: 1.5;\n    }\n  }\n}\n.rewards-table-empty {\n  display: flex;\n  height: 100%;\n  justify-content: center;\n  background: #1c222f;\n  border-radius: 11px;\n  margin-right: 20px;\n  padding: 20px;\n}\n\n.no-data {\n  align-items: center;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  margin-bottom: 44px;\n  min-height: 200px;\n}\n.data-picker {\n  width: 100%;\n  padding: 10px 0;\n}\n@media only screen and (max-width: 767px) {\n  .recommendation-payment {\n    border-radius: 11px;\n    padding: 10px;\n    margin-right: 0;\n    flex: 1;\n    .title {\n      font-size: 16px;\n    }\n    .payment-content {\n      grid-gap: 5px;\n      .payment-item {\n        .name {\n          font-size: 15px;\n          height: 30px;\n        }\n        .text-data {\n          font-size: 9px;\n          height: 30px;\n        }\n      }\n    }\n  }\n  .rewards-table-empty {\n    width: 100%;\n    margin-right: 0;\n  }\n}\n</style>\n"]}]}