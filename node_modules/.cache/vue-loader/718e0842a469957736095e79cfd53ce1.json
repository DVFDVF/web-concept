s.getBoundingClientRect();\n        // 计算移动的偏移量\n        const offsetX = rect.left - rulesRect.left + rect.width * 0.5;\n        const offsetY = rect.top - rulesRect.top + rect.height;\n        const duration = this.calculateDistance(\n          rect.left,\n          rect.top,\n          rulesRect.left,\n          rulesRect.top,\n        );\n        rules.style.transition = `all ${Math.pow(duration, 0.5) / 50}s ease`;\n        rules.style.transform = `translate(${offsetX}px, ${offsetY}px) scale(0)`;\n        rules.style.transformOrigin = 'top left';\n        this.closeFunction(undefined, true, Math.pow(duration, 0.5) * 20);\n      }\n    },\n    calculateDistance(x1: number, y1: number, x2: number, y2: number): number {\n      const distance: number = Math.sqrt(\n        Math.pow(x2 - x1, 2) + Math.pow(y2 - y1, 2),\n      );\n      return distance;\n    },\n  },\n  props: ['hideClose', 'reward'],\n});\n</script>\n\n<style lang=\"less\" scoped>\n.content {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  z-index: 10;\n  font-family: Arial-BoldMT, Arial;\n  user-select: none;\n}\n.rules {\n  position: relative;\n  justify-content: flex-start;\n  flex-direction: column;\n  width: 300px;\n  padding: 10px;\n  height: auto;\n  height: 350px;\n  background: url(~@/assets/redEnvelope/open.jpg) no-repeat;\n  background-size: 100% 100%;\n  z-index: 120;\n  animation: scale 0.4s ease-in-out;\n}\n.rules-small {\n  animation: scaleSmall 0.4s ease-in-out;\n}\n@keyframes scale {\n  0% {\n    transform: scale(0);\n  }\n\n  100% {\n    transform: scale(1);\n  }\n}\n@keyframes scaleSmall {\n  0% {\n    transform: scale(0);\n  }\n\n  100% {\n    transform: scale(1);\n  }\n}\n.rules-close {\n  position: absolute;\n  right: 25px;\n  top: 15px;\n  width: 23px;\n  height: 23px;\n  background: url(~@/assets/redEnvelope/close2.jpg) no-repeat;\n  background-size: 23px 23px;\n  cursor: pointer;\n}\n.title {\n  margin-top: 20px;\n  height: 20px;\n  font-size: 20px;\n  font-weight: 400;\n  color: #a86d07;\n  text-align: center;\n}\n.number {\n  font-size: 80px;\n  margin-top: 10%;\n  color: rgb(190, 49, 49);\n}\n.text {\n  margin-top: 20%;\n  padding: 0 20px;\n  font-size: 11px;\n  text-align: center;\n  color: rgb(230, 176, 69);\n}\n</style>\n"]}]}