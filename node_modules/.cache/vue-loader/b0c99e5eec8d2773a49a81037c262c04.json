itionPage.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"competits\" :class=\"{ fod: !layout.isCompetitionOpen() }\">\n    <div class=\"competits-header\">\n      <p class=\"competits-title\">{{ lang.v.ba_battles }}</p>\n      <div class=\"competits-selet\">\n        <div class=\"competits-rilet\">\n          <span class=\"competits-span\">{{ lang.v.ba_active }}</span>\n        </div>\n        <button class=\"competits-out center\" @click=\"contestsClose\">\n          <svg class=\"svg-use\" focusable=\"false\" aria-hidden=\"true\">\n            <use xlink:href=\"/svg/sprite.svg#icon-close\"></use>\n          </svg>\n        </button>\n      </div>\n    </div>\n    <div v-show=\"isUnread\" class=\"competits-md\">\n      <button\n        class=\"competits-menu-link\"\n        type=\"button\"\n        @click=\"updateSelectedOption('Active')\"\n      >\n        {{ lang.v.ba_active }}\n      </button>\n    </div>\n    <div class=\"competits-main\" v-show=\"competitionData.length > 0\">\n      <CarouselWidget\n        class=\"competits-main-carousel\"\n        @changeSlide=\"handleSlideChange\"\n        :CarouselData=\"competitionData\"\n      />\n      <BattleShrink :contents=\"competitionData[slideChangeIndex]\" />\n      <PlaceRanking :PlaceData=\"competitionData[slideChangeIndex]\" />\n      <div class=\"competition-game\">\n        <div class=\"competition-game-tl\">\n          <img\n            style=\"width: 24px; height: 24px\"\n            src=\"@/assets/competition/icon-lobby.png\"\n          />\n          <span class=\"competition-game-title\">\n            {{ lang.v.ba_participatingBattles }}\n          </span>\n          <span class=\"games-title-count\">({{ GameData.length }})</span>\n        </div>\n        <div class=\"games-list\">\n          <CompetitionGame\n            v-for=\"item in GameData\"\n            :key=\"item.id\"\n            :-competition-item=\"item\"\n          />\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<!-- 竞赛页面 右边栏-->\n<script lang=\"ts\">\nimport { defineComponent, reactive, ref, watch } from 'vue';\nimport CarouselWidget from '@/views/home/competition/widget/CarouselWidget.vue';\nimport BattleShrink from '@/views/home/competition/widget/BattleShrink.vue';\nimport PlaceRanking from '@/views/home/competition/widget/PlaceRanking.vue';\n//游戏组件\nimport IcoLoginSearch from '@/assets/icon/ico-login-search.vue';\nimport dataCenter from '@/dataCenter';\nimport CompetitionGame from '@/views/home/competition/widget/SmallWidget/CompetitionGame.vue';\nimport {\n  gameByData,\n  getCompetition,\n} from '@/views/home/competition/api/competitionIndex';\nimport {\n  CompetitionResp,\n  CompetitionTyp,\n  GameItem,\n} from '@/views/home/competition/api/competitionProtocol';\nimport { lang } from '@/lang';\n\nexport default defineComponent({\n  components: {\n    CarouselWidget, //轮播组件\n    BattleShrink, //Battle 展开组件\n    PlaceRanking, //竞赛排行表\n    CompetitionGame,\n  },\n  setup() {\n    const isUnread = ref(false);\n    const competitionData = ref<CompetitionResp[]>([]);\n    /**游戏数据 */\n    const GameData = ref<GameItem[]>([]);\n    /**数据下标 */\n    const slideChangeIndex = ref(0);\n    /**获取竞赛数据 */\n    function getCompetitionData() {\n      getCompetition(CompetitionTyp.INPROGRESS).then((resp) => {\n        if (!resp) return;\n        competitionData.value = resp;\n        updateGameData();\n      });\n    }\n    /**获取游戏数据 */\n    const updateGameData = () => {\n      const currentComp = competitionData.value[slideChangeIndex.value];\n      if (currentComp && currentComp.battle && currentComp.battle.gameIds) {\n        gameByData(JSON.parse(currentComp.battle.gameIds)).then((resp) => {\n          if (!resp) return;\n          GameData.value = resp.data;\n        });\n      }\n    };\n    /**查和广告相等的ID，并获取下标 */\n    const updateIndexData = () => {\n      if (dataCenter.battle) {\n        const foundIndex = competitionData.value.findIndex(\n          (comp) => comp.battleId === dataCenter.battle,\n        );\n        // 如果找到匹配项，则更新 slideChangeIndex\n        if (foundIndex !== -1) {\n          slideChangeIndex.value = foundIndex;\n        } else {\n          slideChangeIndex.value = 0;\n        }\n      }\n    };\n    watch(slideChangeIndex, () => {\n      updateGameData();\n    });\n    // 监听 dataCenter.battle 的变化\n    watch(() => dataCenter.battle, updateIndexData);\n    /**\n     * 打开和关闭竞赛都会去请求一次竞赛数据\n     */\n    watch(\n      () => dataCenter.layout.isCompetitionOpen(),\n      (value) => {\n        if (value) {\n          getCompetitionData();\n        }\n      },\n    );\n    return {\n      isUnread,\n      competitionData,\n      GameData,\n      slideChangeIndex,\n      updateGameData,\n    };\n  },\n  data() {\n    return {\n      lang,\n      //竞赛右边是否显示\n      layout: dataCenter.layout,\n      selectedOption: 'Active',\n      list: [\n        {\n          header: {\n            icon: IcoLoginSearch,\n            name: 'Games you should try',\n          },\n          type: '1',\n          content: [\n            {\n              vip: false,\n              btc: false,\n              wins: false,\n            },\n            {\n              vip: false,\n              btc: false,\n              wins: false,\n            },\n          ],\n        },\n      ],\n    };\n  },\n  methods: {\n    //下拉选择\n    updateSelectedOption(option: string) {\n      this.selectedOption = option;\n      this.isUnread = false; // 可选，隐藏下拉菜单\n      if (option === 'Active') {\n        /**获取竞赛数据 */\n        getCompetition(CompetitionTyp.INPROGRESS).then((resp) => {\n          if (!resp) return;\n          this.competitionData = resp;\n          this.updateGameData();\n        });\n      } else if (option === 'Finished') {\n        /**获取竞赛数据 */\n        getCompetition(CompetitionTyp.END).then((resp) => {\n          if (!resp) return;\n          this.competitionData = resp;\n          this.updateGameData();\n        });\n      }\n    },\n    //点击轮播图切换数据\n    handleSlideChange(slideChangeIndex: number) {\n      this.slideChangeIndex = slideChangeIndex;\n    },\n    contestsClose() {\n      dataCenter.layout.toggleCompetition();\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.competits {\n  background-color: #111923;\n  bottom: 0;\n  position: relative;\n  height: 100%;\n  right: 0;\n  top: 0;\n  width: 325px;\n  overflow: hidden;\n  transition: width 0.2s ease-in-out;\n\n  &.fod {\n    width: 0px;\n  }\n}\n// 头部\n.competits-header {\n  align-items: center;\n  background-color: #202a39;\n  display: flex;\n  justify-content: space-between;\n  padding: 8px;\n}\n.competits-selet {\n  display: flex;\n  justify-content: flex-end;\n}\n.competits-title {\n  color: #fff;\n  margin: 0px 0px 0px 4px;\n}\n\n.competits-rilet {\n  align-items: center;\n  cursor: pointer;\n  display: flex;\n  position: relative;\n  width: 100%;\n  border-radius: 8px;\n  font-size: 12px;\n  padding: 7px 13px;\n  background-color: #4458743d;\n  border: 1px solid transparent;\n  box-shadow: 0 2px 4px #0000002e;\n  transition: background-color 0.1s ease-in-out, border-color 0.1s ease-in-out;\n}\n.competits-span {\n  color: #fff;\n  font-weight: 600;\n  line-height: 18px;\n  font-family: Montserrat Variable, sans-serif;\n  font-size: 12px;\n}\n.competits-img {\n  transition: transform 0.3s ease, filter 0.3s ease;\n  width: 10px;\n  height: 10px;\n  margin-left: 4px;\n  margin-right: -6px;\n}\n.competits-out {\n  background-color: transparent;\n  border: none;\n  cursor: pointer;\n  width: 25.6px;\n  height: 36px;\n  padding: 3.5px 5.258px;\n\n  .svg-use {\n    width: 16px;\n    height: 16px;\n    fill: #fff;\n  }\n}\n.competits-md {\n  position: absolute;\n  align-items: center;\n  background: #1b2636;\n  box-shadow: 0 4px 26px #0d131c78;\n  border: 2px;\n  display: column;\n  gap: 8px;\n  margin-top: -2px;\n  right: 28px;\n  border-radius: 8px;\n  font-size: 12px;\n  padding: 6px 10px;\n  z-index: 12;\n\n  .competits-menu-link {\n    align-items: center;\n    color: #55657e;\n    display: flex;\n    outline: none;\n    padding: 0;\n    text-align: left;\n    transition: color 0.1s ease-in-out;\n    width: 100%;\n    background-color: transparent;\n    border: none;\n    padding: 6px 10px;\n    font-weight: 600;\n    line-height: 18px;\n    font-family: Montserrat Variable, sans-serif;\n    font-size: 12px;\n    cursor: pointer;\n  }\n}\n\n//main\n.competits-main {\n  display: flex;\n  flex-direction: column;\n  margin-left: 12px;\n  margin-right: 12px;\n  margin-top: 12px;\n  overflow: scroll;\n  height: calc(100% - 64px);\n  padding-bottom: 40px;\n}\n.competits-main-carousel {\n  margin-bottom: 36px;\n}\n\n//游戏标题\n.competition-game {\n  margin-top: 20px;\n}\n.competition-game-tl {\n  font-size: 16px;\n  font-weight: 600;\n  letter-spacing: -0.02em;\n  line-height: 20px;\n  text-align: center;\n}\n.competition-game-title {\n  color: #fff;\n}\n.games-title-count {\n  color: #55657e;\n}\n\n//游戏\n.games-list {\n  display: grid;\n  gap: 10px;\n  grid-template-columns: repeat(3, 1fr);\n  margin-top: 18px;\n}\n\n@media only screen and (max-width: 767px) {\n  .competits {\n    width: 100%;\n    z-index: 100;\n    position: absolute;\n    height: auto;\n  }\n}\n</style>\n"]}]}