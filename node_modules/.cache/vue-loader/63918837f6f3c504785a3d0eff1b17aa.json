   v-for=\"item in tabbarList\"\n        :key=\"item.key\"\n        :class=\"[\n          'mobile-menu-item',\n          {\n            'mobile-menu-item-menu':\n              selectTabbarItem == item.key &&\n              (item.key == 'share' ? true : !layout.isAllClosed()),\n            'mobile-menu-share': item.key == 'share',\n          },\n        ]\"\n        @click=\"clickItem(item.key)\"\n      >\n        <span class=\"icon mobile-menu-icon\">\n          <component :is=\"item.icon\" />\n        </span>\n        <div class=\"mobile-menu-name\">{{ item.name }}</div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport IcoRecharge from '@/assets/icon/tabbar/ico-recharge.vue';\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { getRouter, openModal } from '@/manager/view';\nimport { defineComponent, markRaw, ref, watch } from 'vue';\nimport IcoTabbarChat from '../../assets/icon/tabbar/ico-tabbar-chat.vue';\nimport IcoTabbarContests from '../../assets/icon/tabbar/ico-tabbar-contests.vue';\nimport IcoTabbarMenu from '../../assets/icon/tabbar/ico-tabbar-menu.vue';\nimport IcoTabbarShare from '../../assets/icon/tabbar/ico-tabbar-share.vue';\nimport WalletButtonPopVue from '../home/head/popup/WalletButtonPop.vue';\nimport Login from '../home/login/Login.vue';\nimport { get } from '@/api';\n\nenum tabbarItems {\n  menu = 'menu',\n  recharge = 'recharge',\n  share = 'share',\n  contests = 'contests',\n  chats = 'chats',\n}\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n    };\n  },\n  methods: {\n    clickItem(key: string) {\n      this.selectTabbarItem = key;\n      if (key == tabbarItems.menu) {\n        dataCenter.layout.toggleLeft();\n        dataCenter.layout.closeRight();\n      } else if (key == tabbarItems.chats) {\n        if (!dataCenter.user.data) {\n          openModal(Login);\n          return;\n        }\n        dataCenter.layout.toggleChat();\n        dataCenter.layout.closeLeft();\n      } else if (key == tabbarItems.share) {\n        getRouter().push('/referral/dashboard');\n        dataCenter.layout.closeLeft();\n        dataCenter.layout.closeRight();\n      } else if (key == tabbarItems.contests) {\n        dataCenter.layout.toggleCompetition();\n        dataCenter.layout.closeLeft();\n      } else if (key == tabbarItems.recharge) {\n        dataCenter.layout.toggleSearch();\n        dataCenter.layout.closeLeft();\n      }\n    },\n  },\n  setup() {\n    const layout = ref(dataCenter.layout);\n\n    const selectTabbarItem = ref('');\n\n    const tabbarList = [\n      {\n        key: tabbarItems.menu,\n        get name() {\n          return lang.v.tab_menu;\n        },\n        icon: markRaw(IcoTabbarMenu),\n      },\n      {\n        key: tabbarItems.recharge,\n        get name() {\n          return lang.v.pix_deposito;\n        },\n        icon: markRaw(IcoRecharge),\n      },\n      {\n        key: tabbarItems.share,\n        get name() {\n          return lang.v.tb_Affiliate;\n        },\n        icon: markRaw(IcoTabbarShare),\n      },\n      {\n        key: tabbarItems.contests,\n        get name() {\n          return lang.v.tab_contests;\n        },\n        icon: markRaw(IcoTabbarContests),\n      },\n      {\n        key: tabbarItems.chats,\n        get name() {\n          return lang.v.tab_chats;\n        },\n        icon: markRaw(IcoTabbarChat),\n      },\n    ];\n    watch(\n      () => dataCenter.layout.currentState,\n      () => {\n        switch (dataCenter.layout.currentState) {\n          case 2:\n            selectTabbarItem.value = tabbarItems.contests;\n            break;\n          case 3:\n            selectTabbarItem.value = tabbarItems.chats;\n            break;\n          case 4:\n            selectTabbarItem.value = tabbarItems.recharge;\n            if (dataCenter.user.data) {\n              openModal(WalletButtonPopVue);\n            } else {\n              openModal(Login);\n            }\n            break;\n          default:\n            break;\n        }\n      },\n    );\n    return {\n      layout,\n      selectTabbarItem,\n      tabbarList,\n    };\n  },\n  components: {},\n});\n</script>\n\n<style lang=\"less\" scoped>\n.mobile-menu {\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  user-select: none;\n  width: 100%;\n  z-index: 104;\n  position: relative;\n}\n.mobile-menu-inner {\n  align-items: center;\n  background: #111923;\n  border: solid #2a3546;\n  border-radius: 12px 12px 0 0;\n  border-width: 1px 1px 0;\n  display: flex;\n  height: 72px;\n  justify-content: space-between;\n  padding: 12px 20px 16px;\n  position: relative;\n  transition: border-radius 0.1s ease-in-out;\n  z-index: 222;\n}\n.extrude {\n  position: absolute;\n  width: 60px;\n  height: 60px;\n  top: -23px;\n  border-radius: 50%;\n  left: calc(50% - 30px);\n  background-color: #111923;\n  border: solid #2a3546;\n}\n.remove-line {\n  position: absolute;\n  width: 60px;\n  height: 10px;\n  top: -1px;\n  left: calc(50% - 30px);\n  background-color: #111923;\n}\n.mobile-menu-item {\n  align-items: center;\n  color: #55657e;\n  display: flex;\n  flex-direction: column;\n  height: 44px;\n  min-width: 64px;\n  position: relative;\n  transition: color 0.1s ease-in-out;\n}\n\n.mobile-menu-item-menu:before {\n  background: #ed1d49;\n  bottom: -25px;\n  content: '';\n  display: block;\n  filter: blur(9.5px);\n  height: 36px;\n  left: 50%;\n  opacity: 0.6;\n  position: absolute;\n  transform: translate(-50%) matrix(0, -1, -1, 0, 0, 0);\n  width: 10px;\n}\n.mobile-menu-icon {\n  font-size: 26px;\n}\n.mobile-menu-name {\n  font-size: 11px;\n}\n.mobile-menu-item-menu:after {\n  background: linear-gradient(\n    101deg,\n    rgba(237, 29, 73, 0),\n    #ed1d49 48.44%,\n    rgba(237, 29, 73, 0)\n  );\n  border-radius: 50px 50px 0 0;\n  bottom: -16px;\n  content: '';\n  height: 3px;\n  left: 50%;\n  position: absolute;\n  transform: translate(-50%);\n  width: 36px;\n}\n\n.mobile-menu-item-menu:after,\n.mobile-menu-item-menu:before {\n  opacity: 1;\n}\n.icon {\n  width: 24px;\n  height: 24px;\n  margin-bottom: 3px;\n}\n.icon svg {\n  width: 24px;\n  height: 24px;\n}\n\n.mobile-menu-item-menu {\n  color: #fff !important;\n  position: relative;\n}\n.mobile-menu-share {\n  .mobile-menu-icon {\n    width: 50px;\n    height: 50px;\n    margin-top: -25px;\n    img {\n      width: 100%;\n      height: 100%;\n    }\n  }\n}\n</style>\n"]}]}