- <ChatsDrop v-show=\"downDrop\"></ChatsDrop> -->\n        </div>\n\n        <div class=\"wager-trigger\" style=\"display: none\">\n          <img\n            src=\"@/assets/chats/dollar-flying.png\"\n            width=\"20\"\n            height=\"20\"\n            class=\"wager-icon\"\n          />\n          <div class=\"wager-icon-state\">\n            <svg\n              class=\"svg-use-state\"\n              width=\"12\"\n              height=\"12\"\n              focusable=\"false\"\n              aria-hidden=\"true\"\n            >\n              <use xlink:href=\"/svg/sprite.svg#icon-close\"></use>\n            </svg>\n          </div>\n        </div>\n      </div>\n      <div class=\"settings\">\n        <!-- 提示和设置,暂不需要 -->\n        <button\n          class=\"header-btn\"\n          v-for=\"(item, index) in topBtns\"\n          :key=\"item\"\n          @click=\"clickHeaderBtn(index)\"\n        >\n          <svg class=\"header-icon\" width=\"18\" height=\"18\">\n            <use :xlink:href=\"item\"></use>\n          </svg>\n        </button>\n        <button class=\"header-btn\" @click=\"closeMessage\">\n          <svg class=\"header-icon-cancle\" width=\"24\" height=\"24\">\n            <use xlink:href=\"/svg/sprite.svg#icon-close\"></use>\n          </svg>\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { defineComponent } from 'vue';\n// import ChatsDrop from './ChatsDrop.vue';\nimport { lang } from '@/lang';\nimport { openModal } from '@/manager/view';\nimport ChatRules from '../chatsArea/ChatRules.vue';\nexport default defineComponent({\n  name: 'chats',\n  data() {\n    return {\n      lang,\n      downDrop: false,\n      topBtns: [\n        // 设置暂不需要\n        // '/svg/sprite.svg#icon-settings-new',\n        '/svg/sprite.svg#icon-info-circle',\n      ],\n    };\n  },\n  methods: {\n    closeMessage() {\n      dataCenter.layout.toggleChat();\n    },\n    selectDownDrop() {\n      this.downDrop = !this.downDrop;\n    },\n    closeDownDrop() {\n      if (this.downDrop) {\n        this.downDrop = false;\n      }\n    },\n    clickHeaderBtn(index: number) {\n      if (index == 0) {\n        openModal(ChatRules, { class: 'test' });\n      }\n    },\n  },\n  components: {\n    // ChatsDrop,\n  },\n  mounted() {\n    document.addEventListener('click', this.closeDownDrop);\n  },\n  beforeUnmount() {\n    document.removeEventListener('click', this.closeDownDrop);\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.container {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  align-items: center;\n  align-items: center;\n  background-color: rgb(38, 48, 65);\n  display: flex;\n  padding: 8px;\n  z-index: 10;\n\n  .country {\n    display: flex;\n    align-items: center;\n    width: 100px;\n\n    .countryBtn {\n      display: flex;\n      flex-direction: row;\n      background-color: rgb(42, 53, 70);\n      border: none;\n      box-shadow: none;\n      border-radius: 8px;\n      font-size: 12px;\n      height: 36px;\n      padding: 7px 13px;\n      width: 140px;\n      justify-content: left;\n      align-items: center;\n\n      .icon {\n        display: flex;\n        flex-direction: row;\n        gap: 5px;\n\n        .country-name {\n          flex: 1;\n          gap: 4px;\n          width: 65px;\n          text-align: left;\n        }\n\n        .svg-use {\n          height: 20px;\n          margin-left: 4px;\n          margin-right: -6px;\n          width: 20px;\n          color: #93acd3;\n        }\n      }\n    }\n\n    .wager-trigger {\n      width: 20px;\n      height: 20px;\n      margin-left: 10px;\n      position: relative;\n\n      .wager-icon {\n        width: 20px;\n        height: 20px;\n      }\n\n      .wager-icon-state {\n        background-color: rgb(237, 29, 73);\n        border-radius: 50%;\n        bottom: -3px;\n        height: 10px;\n        position: absolute;\n        right: -3px;\n        width: 10px;\n        display: flex;\n        justify-content: center;\n      }\n\n      .svg-use-state {\n        height: 10px;\n        width: 10px;\n        color: white;\n      }\n    }\n\n    .countryBtn:hover {\n      background-color: #44587452;\n    }\n  }\n\n  .settings {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: center;\n    gap: 10px;\n\n    .header-btn {\n      display: flex;\n      flex-direction: row;\n      align-items: center;\n      justify-content: center;\n      padding: 3.5px, 5.25px;\n\n      .header-icon {\n        color: rgb(85, 101, 126);\n        transition: color 0.1s ease-in-out 0s;\n        width: 18px;\n        height: 21px;\n      }\n      .header-icon-cancle {\n        color: rgb(85, 101, 126);\n        transition: color 0.1s ease-in-out 0s;\n        width: 24px;\n      }\n    }\n\n    .header-btn:hover svg {\n      color: white;\n    }\n  }\n}\n\n.down-drop {\n  display: flex;\n  flex-direction: column;\n  position: relative;\n}\n\n.rotate-180 {\n  transform: rotate(180deg);\n}\n</style>\n"]}]}