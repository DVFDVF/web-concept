{"remainingRequest":"/Users/nuomi/Desktop/项目/concept/web3-concept/node_modules/babel-loader/lib/index.js!/Users/nuomi/Desktop/项目/concept/web3-concept/node_modules/ts-loader/index.js??ref--15-2!/Users/nuomi/Desktop/项目/concept/web3-concept/node_modules/eslint-loader/index.js??ref--14-0!/Users/nuomi/Desktop/项目/concept/web3-concept/src/games/wheel/index.ts","dependencies":[{"path":"/Users/nuomi/Desktop/项目/concept/web3-concept/src/games/wheel/index.ts","mtime":1716171553012},{"path":"/Users/nuomi/Desktop/项目/concept/web3-concept/babel.config.js","mtime":1716171552917},{"path":"/Users/nuomi/Desktop/项目/concept/web3-concept/node_modules/cache-loader/dist/cjs.js","mtime":1716171608994},{"path":"/Users/nuomi/Desktop/项目/concept/web3-concept/node_modules/babel-loader/lib/index.js","mtime":1716171609486},{"path":"/Users/nuomi/Desktop/项目/concept/web3-concept/node_modules/ts-loader/index.js","mtime":1716171609531},{"path":"/Users/nuomi/Desktop/项目/concept/web3-concept/node_modules/eslint-loader/index.js","mtime":1716171609105}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["ref","getAward","playWheelGame","tunTableTime","dayjs","countDownTime","dataCenter","soundManager","overSound","backgroudSound","spinSound","MAX_INT","tip","lang","WheelRunner","WheelStage","getUserBalance","Wheel","constructor","active","time","timeAfter","timer","isStart","isSoundPlaying","showOverflow","isFunWin","isWheeling","isDisplayRanking","allData","displayedData","currentIndex","intervalId","maxDisplayCount","isStartWheel","closeStopSound","soundIconSvg","isRunWheel","hours","minutes","seconds","countdownInterval","isTiming","WheelRespData","icon","value","name","runner","awardData","tempAwardData","updateInterval","bgPlayer","closePopUp","closeFunction","stopAll","checkIsTiming","newTime","format","then","resp","timeAfter12Hours","add","isBefore","startCountdown","clearInterval","setInterval","isAfter","info","v","me_timeup","diff","subtract","startWheel","watwheel","wheelCent","layout","isMobile","style","top","left","setTimeout","_this$runner$value","play","player","resume","pause","runTo","index","step","SLOW_DOWN","COMPLETE","currency","webConfig","currencyMap","currencyId","wallet","change","selectId","finally","wheelClass","getWheelSrc","require","toggleSound","pauseAll","updateData","slice","length","displayRanking","distance","getCuurrencyIcon","currencyid","mounted","wheel","gameConfiguration","gameId","push","soundVolume","update","degree","transform","toFixed","areaCount","duration","acceler","speed","beginBack","beginBackSpeed","endBack","unmounted","_this$runner$value2","unregister","init","wheelTime","user","data","destroy"],"sources":["/Users/nuomi/Desktop/项目/concept/web3-concept/node_modules/eslint-loader/index.js??ref--14-0!/Users/nuomi/Desktop/项目/concept/web3-concept/src/games/wheel/index.ts"],"sourcesContent":["import { inject, reactive, ref } from 'vue';\nimport { getAward, playWheelGame, tunTableTime } from './api';\nimport dayjs from 'dayjs';\nimport { countDownTime } from '@/utils';\nimport dataCenter from '@/dataCenter';\nimport { SoundPlayer, soundManager } from '@/manager/SoundManager';\nimport overSound from '@/assets/game/wheel/FWCircle.mp3';\nimport backgroudSound from '@/assets/game/wheel/FWInfinity.mp3';\nimport spinSound from '@/assets/game/wheel/FWStart.mp3';\nimport { MAX_INT } from '@/utils/define';\nimport { tip } from '@/components/warn/warn';\nimport { lang } from '@/lang';\nimport { AwardResp } from './api/protocol';\nimport WheelRunner, { WheelStage } from './game/WheelRunner';\nimport { ModalCloseFun } from '@/manager/view';\nimport { getUserBalance } from '@/views/home/api/user-api/headIndex';\ninterface WheelWinItem {\n  //中奖数据\n  icon: string;\n  value: number;\n  name: string;\n}\nclass Wheel {\n  public active = ref(false);\n  public time = ref();\n  public timeAfter = ref();\n  private timer = 0;\n  public isStart = ref(false);\n  public isSoundPlaying = ref(true); // 是否播放\n  public showOverflow = ref(false); // 是否全部显示\n  public isFunWin = ref(false); // 是否显示最后的页面\n  public isWheeling = ref(false); // 开始转，右边是否显示\n  public isDisplayRanking = ref('0px'); // 是否显示排行\n  public allData = ref([]); // 存储从 API 获取的全部数据\n  public displayedData = ref([]); // 当前显示的数据\n  public currentIndex = ref(0); // 当前显示数据的起始索引\n  public intervalId = ref(0); // 用于存储定时器ID\n  public maxDisplayCount = ref(6); // 下边中奖记录\n  public isStartWheel = ref(false); // 是否开始游戏\n  public closeStopSound = ref(false);\n  public soundIconSvg = ref('/svg/sprite.svg#icon-sound-on'); // 声音图片\n  public isRunWheel = ref(false); // 转盘是否在执行中\n  public hours = ref(0);\n  public minutes = ref(0);\n  public seconds = ref(0);\n  public countdownInterval = ref(0);\n  public isTiming = ref(false);\n  // 定义 runner 变量\n  public WheelRespData = ref<WheelWinItem>({\n    icon: '',\n    value: 0,\n    name: '',\n  }); //中奖数据\n  public runner = ref<WheelRunner>();\n  //获取转盘记录\n  public awardData = ref<AwardResp[]>([]);\n  public tempAwardData = ref<AwardResp[]>([]); //临时接受\n  updateInterval = 0;\n  private bgPlayer = ref<Promise<SoundPlayer>>();\n  // 初始化转盘时间\n  init() {\n    this.wheelTime();\n    this.timer = setInterval(() => {\n      if (!this.active.value && dataCenter.user.data) {\n        if (!this.timeAfter.value) {\n          this.timeAfter.value = dayjs()\n            .add(24, 'hour')\n            .format('YYYY-MM-DD HH:mm:ss');\n        }\n        this.time.value = countDownTime(this.timeAfter.value);\n      }\n    }, 1000);\n  }\n  wheelTime() {\n    tunTableTime().then((resp) => {\n      if (!resp) {\n        this.active.value = true;\n        return;\n      }\n      this.timeAfter.value = dayjs(`${resp}Z`)\n        .add(24, 'hour')\n        .format('YYYY-MM-DD HH:mm:ss');\n      this.time.value = countDownTime(this.timeAfter.value);\n      this.active.value = dayjs(this.timeAfter.value).isBefore(dayjs());\n    });\n  }\n\n  destroy() {\n    clearInterval(this.timer);\n  }\n  closePopUp = (closeFunction: ModalCloseFun) => {\n    this.isFunWin.value = false;\n    this.isStartWheel.value = false;\n    this.isWheeling.value = false;\n    this.closeStopSound.value = true;\n    this.isStart.value = false;\n    soundManager.stopAll();\n    closeFunction?.();\n  };\n  /**判断是否还在倒计时 */\n  checkIsTiming = () => {\n    const newTime = dayjs().format('YYYY-MM-DD HH:mm:ss');\n    tunTableTime().then((resp) => {\n      if (!resp) {\n        this.isTiming.value = false;\n\n        return;\n      }\n      const timeAfter12Hours = dayjs(`${resp}Z`)\n        .add(24, 'hour')\n        .format('YYYY-MM-DD HH:mm:ss');\n      const isBefore = dayjs(timeAfter12Hours).isBefore(newTime);\n      if (isBefore) {\n        this.isTiming.value = false;\n      } else {\n        this.startCountdown(`${resp}Z`);\n      }\n    });\n  };\n\n  startCountdown = (resp: string) => {\n    this.isTiming.value = true;\n    if (this.countdownInterval.value) {\n      clearInterval(this.countdownInterval.value);\n    }\n\n    this.countdownInterval.value = setInterval(() => {\n      const newTime = dayjs();\n      const timeAfter12Hours = dayjs(resp).add(24, 'hour');\n\n      if (dayjs(newTime).isAfter(timeAfter12Hours)) {\n        clearInterval(this.countdownInterval.value);\n        this.countdownInterval.value = 0;\n        tip.info(lang.v.me_timeup);\n\n        return;\n      }\n\n      this.hours.value = dayjs(timeAfter12Hours).diff(newTime, 'hour');\n      this.minutes.value = dayjs(timeAfter12Hours)\n        .subtract(this.hours.value, 'hour')\n        .diff(newTime, 'minute');\n      this.seconds.value = dayjs(timeAfter12Hours)\n        .subtract(this.hours.value, 'hour')\n        .subtract(this.minutes.value, 'minute')\n        .diff(newTime, 'second');\n    }, 1000);\n  };\n\n  startWheel = (\n    watwheel: HTMLElement | undefined,\n    wheelCent: HTMLElement | undefined,\n  ) => {\n    if (this.isStart.value || this.isTiming.value) return;\n    this.isStart.value = true;\n    if (this.isRunWheel.value === false) {\n      playWheelGame()\n        .then((resp) => {\n          // 因为报错都在底层处理了，所以上层直接过滤即可\n          if (!resp) {\n            tip.info('error');\n            return;\n          }\n\n          this.isRunWheel.value = true;\n          this.isWheeling.value = true;\n          this.isStartWheel.value = true;\n\n          if (watwheel && wheelCent) {\n            if (dataCenter.layout.isMobile) {\n              watwheel!.style.top = '160px';\n            } else {\n              watwheel!.style.left = '120px';\n              wheelCent!.style.left = '260px';\n            }\n          }\n          // watwheel移动到中间的动画要1s，所以延迟了1秒\n          setTimeout(() => {\n            this.showOverflow.value = true;\n            // 播放音效\n            if (this.isSoundPlaying.value) {\n              if (this.closeStopSound.value) return;\n              soundManager.play(spinSound, 1).then((player) => player.resume());\n            } else {\n              if (this.closeStopSound.value) return;\n              soundManager.play(spinSound, 1).then((player) => player.pause());\n            }\n\n            this.runner.value?.runTo(resp.index, (step) => {\n              if (step === WheelStage.SLOW_DOWN) {\n                if (this.isSoundPlaying.value) {\n                  if (this.closeStopSound.value) return;\n                  soundManager\n                    .play(overSound, 1)\n                    .then((player) => player.resume());\n                } else {\n                  if (this.closeStopSound.value) return;\n                  soundManager\n                    .play(overSound, 1)\n                    .then((player) => player.pause());\n                }\n              } else if (step === WheelStage.COMPLETE) {\n                this.isRunWheel.value = false;\n                // 等待用户看看界面的效果\n                this.WheelRespData.value.value = resp.value;\n                const currency =\n                  dataCenter.webConfig.currencyMap[resp.currencyId];\n                if (currency) {\n                  this.WheelRespData.value.icon = currency.icon;\n                  this.WheelRespData.value.name = currency.name;\n                }\n                setTimeout(() => {\n                  dataCenter.wallet.change(\n                    dataCenter.wallet.selectId,\n                    resp.value,\n                  );\n                  this.isFunWin.value = true;\n                  //更新数据\n                  this.active.value = false;\n                }, 1000);\n              }\n            });\n          }, 1000);\n        })\n        .finally(() => {\n          this.isStart.value = false;\n        });\n    }\n  };\n\n  wheelClass = (index: number) => {\n    return index % 2 ? 'wheel-wapl-s' : 'wheel-wapl-help';\n  };\n  getWheelSrc = (index: number) => {\n    // 使用 require 和模板字符串来动态构建 src 路径\n    return require(`@/assets/game/wheel/${index}.png`);\n  };\n  //播放背景音乐\n  toggleSound = () => {\n    this.isSoundPlaying.value = !this.isSoundPlaying.value;\n    if (this.isSoundPlaying.value) {\n      if (this.bgPlayer.value) {\n        this.bgPlayer.value.then((player) => player.resume());\n      }\n      this.soundIconSvg.value = '/svg/sprite.svg#icon-sound-on';\n    } else {\n      soundManager.pauseAll();\n      this.soundIconSvg.value = '/svg/sprite.svg#icon-sound-off';\n    }\n  };\n  /**\n   * 展示排行\n   * 更新当前显示的数据\n   */\n  updateData = () => {\n    this.displayedData.value = this.allData.value.slice(\n      this.currentIndex.value,\n      this.currentIndex.value + 8,\n    );\n    /**\n     * 更新下一次显示数据的起始索引\n     * 如果到达数据末尾，则重新开始\n     */\n    this.currentIndex.value++;\n    if (this.currentIndex.value > this.allData.value.length - 1) {\n      this.currentIndex.value = 0;\n    }\n  };\n  displayRanking = (distance: string) => {\n    this.isDisplayRanking.value = distance;\n    if (distance == '530px') {\n      if (dataCenter.layout.isMobile) {\n        this.isDisplayRanking.value = '100%';\n      }\n    }\n    // this.$nextTick(() => {\n    //   (this.$refs.ranking as any).reload();\n    // });\n  };\n\n  /**查询货币icon */\n  getCuurrencyIcon = (currencyid: number) => {\n    const currency = dataCenter.webConfig.currencyMap[currencyid];\n    return currency?.icon;\n  };\n\n  mounted = (wheel: HTMLElement) => {\n    this.bgPlayer.value = soundManager.play(backgroudSound, MAX_INT);\n    getAward(dataCenter.gameConfiguration.gameId, '10').then((resp) => {\n      if (!resp) return;\n      this.awardData.value = resp;\n\n      if (this.awardData.value.length > 6) {\n        this.tempAwardData.value = this.awardData.value.slice(0, 6);\n        this.updateInterval = setInterval(() => {\n          if (this.tempAwardData.value.length < this.awardData.value.length) {\n            this.tempAwardData.value.push(\n              this.awardData.value[this.tempAwardData.value.length],\n            );\n          } else {\n            //暂停加载\n            clearInterval(this.updateInterval);\n            return this.tempAwardData.value;\n          }\n        }, 2000); // Update every 2 seconds\n      } else {\n        this.tempAwardData.value = this.awardData.value.slice(\n          0,\n          this.awardData.value.length,\n        );\n      }\n    });\n    // 当组件挂载时，根据音量设置相应的图标\n    this.soundIconSvg.value =\n      dataCenter.gameConfiguration.soundVolume === 0\n        ? '/svg/sprite.svg#icon-sound-off'\n        : '/svg/sprite.svg#icon-sound-on';\n    this.runner.value = new WheelRunner({\n      update: (degree) => {\n        degree %= 360;\n        if (wheel) {\n          wheel.style.transform = `translate(-50%, -50%) rotate(${degree.toFixed(\n            2,\n          )}deg)`;\n        }\n      },\n      areaCount: 12,\n      duration: 7.5,\n      acceler: 144,\n      speed: 720,\n      beginBack: 60,\n      beginBackSpeed: 120,\n      endBack: 5,\n    });\n    this.checkIsTiming();\n  };\n  unmounted = () => {\n    this.runner.value?.unregister();\n\n    clearInterval(this.updateInterval);\