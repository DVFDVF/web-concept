ACxB,CAAC,CAAC;IACJ,CAAC;EACH,CAAC;AACH,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/concept/fury-client/src/games/wheel/wheelRanking.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"wheel-history\">\n    <div class=\"wheel-history-content flex-col\" :style=\"{ right: rightNumber }\">\n      <div class=\"wheel-history-head\">\n        <button class=\"wheel-history-btn-close\" @click=\"$emit('button-out')\">\n          ×\n        </button>\n        <button\n          class=\"wheel-history-btn-back center\"\n          @click=\"$emit('button-out')\"\n        >\n          <arrowLeft style=\"width: 16px; height: 16px\" />\n        </button>\n        <h3 class=\"wheel-history-title\">{{ lang.v.zp_recentBigWin }}</h3>\n      </div>\n      <div class=\"flex-col flex1\">\n        <div class=\"wheel-history-tabs\">\n          <div\n            class=\"wheel-history-tab\"\n            :class=\"{\n              'wheel-history-tab-active': isSelect === 'Recent wins',\n            }\"\n            @click=\"isSelect = 'Recent wins'\"\n          >\n            {{ lang.v.zp_wins }}\n          </div>\n          <div\n            class=\"wheel-history-tab\"\n            :class=\"{\n              'wheel-history-tab-active': isSelect === 'Big wins (all time)',\n            }\"\n            @click=\"isSelect = 'Big wins (all time)'\"\n          >\n            {{ lang.v.zp_big }}\n          </div>\n        </div>\n        <div class=\"flex-col flex1\">\n          <div class=\"wheel-history-row\">\n            <div style=\"width: 28%\">{{ lang.v.zp_time }}</div>\n            <div style=\"width: 40%\">{{ lang.v.zp_user }}</div>\n            <div style=\"width: 32%\">{{ lang.v.zp_reward }}</div>\n          </div>\n          <div class=\"flex-scrolly\">\n            <WheelRow\n              v-for=\"(item, index) in recentData\"\n              v-bind:key=\"index\"\n              :record=\"item\"\n            />\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<!-- 转盘排行榜 -->\n<script lang=\"ts\">\nimport arrowLeft from '@/assets/game/wheel/arrow-left.vue';\nimport WheelRow from '@/games/wheel/WheelRow.vue';\nimport { lang } from '@/lang';\nimport { defineComponent, ref } from 'vue';\nimport { getAward } from './api';\nimport { AwardResp } from './api/protocol';\n\nexport default defineComponent({\n  components: {\n    WheelRow,\n    arrowLeft,\n  },\n  props: {\n    rightNumber: {\n      type: String,\n      default: '0px', // 如果没有传递 'nums'，则默认值为 1\n    },\n  },\n  data() {\n    return {\n      lang,\n      isSelect: 'Recent wins',\n    };\n  },\n  setup() {\n    /** 获取转盘记录 */\n    const recentData = ref<AwardResp[]>([]);\n\n    return { recentData };\n  },\n  methods: {\n    reload() {\n      getAward(0, '30').then((resp) => {\n        if (!resp) return;\n        this.recentData = resp;\n      });\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.wheel-history-content {\n  background: linear-gradient(180deg, #001768, #0f1420 15.43%, #0f1420);\n  border-radius: 20px;\n  height: 100%;\n  max-width: 100%;\n  padding: 32px 32px 0;\n  position: absolute;\n  z-index: 12;\n  top: 0;\n  right: 0;\n  transform: translate(100%);\n  transition: all 1s;\n  width: 530px;\n  pointer-events: auto;\n}\n.wheel-history-head {\n  align-items: center;\n  display: flex;\n  margin-bottom: 28px;\n}\n.wheel-history-btn-back {\n  height: 32px;\n  margin-right: 8px;\n  padding: 5px;\n  width: 32px;\n  border-radius: 8px;\n  background-color: #2a3546;\n  border-color: transparent;\n  color: #93acd3;\n\n  &:hover {\n    background-color: #202a39;\n    color: #93acd3;\n  }\n}\n.wheel-history-title {\n  font-size: 18px;\n  font-weight: 600;\n  line-height: 24px;\n  margin: 0;\n}\n.wheel-history-tabs {\n  align-items: center;\n  display: flex;\n  margin-bottom: 16px;\n}\n\n.wheel-history-tab-active {\n  color: #fff !important;\n  background-color: #2283f6;\n  border-radius: 32px;\n}\n.wheel-history-tab {\n  color: #93acd3;\n  cursor: pointer;\n  font-size: 14px;\n  font-weight: 600;\n  line-height: 18px;\n  margin-right: 14px;\n  padding: 7px 10px;\n  text-align: center;\n}\n.wheel-history-row {\n  align-items: center;\n  border-bottom: 1px solid #1c2532;\n  display: flex;\n  justify-content: space-between;\n}\n\n.wheel-history-btn-close {\n  background-color: transparent;\n  border: none;\n  color: #93acd3;\n  font-size: 38px;\n  font-weight: 500;\n  height: 38px;\n  line-height: 38px;\n  outline: none;\n  padding: 0;\n  position: absolute;\n  right: 0;\n  top: 0;\n  transition: color 0.1s ease-in-out;\n  width: 38px;\n\n  &:hover {\n    color: #fff;\n  }\n}\n@media only screen and (max-width: 767px) {\n  .wheel-history {\n    width: 100%;\n  }\n  .wheel-history {\n    left: 0 !important;\n    position: fixed !important;\n  }\n}\n</style>\n"]}]}