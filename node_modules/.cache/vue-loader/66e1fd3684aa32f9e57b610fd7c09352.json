on>\n      <address>\n        {{ lang.v.fk_or }}\n        <a href=\"mailto:feedback@betfury.io\">{{ email }}</a>\n      </address>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { openModal } from '@/manager/view';\nimport LoginWidget from '@/views/home/login/Login.vue';\nimport { defineComponent } from 'vue';\nimport FeedbackButton from './FeedbackButton.vue';\nimport FeedbackSelection from './FeedbackSelection.vue';\nimport FeedbackSend from './FeedbackSend.vue';\nexport default defineComponent({\n  data() {\n    return {\n      email: dataCenter.webConfig.feedBackmail,\n      lang,\n      inputValue: '',\n      isValidationFailed: false,\n      FeedbackType: '',\n      FeedbackData: [\n        'Casino games',\n        'Sport',\n        'Bonuses',\n        'Staking',\n        'Battles and tournaments',\n        'Events',\n        'Promotions',\n        'VIP Rank System',\n        'Other',\n      ],\n      aboutList: {},\n      maxCount: 100,\n      nowCount: 0,\n    };\n  },\n  computed: {},\n  components: { FeedbackSelection, FeedbackButton },\n  methods: {\n    updatecountry(newValue: string) {\n      this.FeedbackType = newValue;\n      // this.isValidationFailed = false;\n    },\n    clickBtn() {\n      if (!dataCenter.user.data) {\n        openModal(LoginWidget);\n        return;\n      }\n      if (this.FeedbackType != '' && this.inputValue != '') {\n        this.forgotClick();\n      } else {\n        this.isValidationFailed = true;\n      }\n    },\n    handleInput() {\n      if (this.inputValue.length > this.maxCount) {\n        this.inputValue = this.inputValue.substring(0, this.maxCount);\n      }\n      this.nowCount = this.inputValue.length;\n    },\n    forgotClick() {\n      openModal(FeedbackSend);\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.main {\n  float: right;\n  margin: 0 0 16px 64px;\n  max-width: 488px;\n  text-align: left;\n  display: flex;\n  flex-direction: column;\n  align-items: flex-start;\n}\n.header {\n  display: flex;\n  flex-direction: row;\n  font-size: 18px;\n  font-weight: 600;\n  gap: 8px;\n  letter-spacing: -0.02em;\n  line-height: 135%;\n  margin-top: 20px;\n}\n.header img {\n  width: 24px;\n  height: 24px;\n}\nh3 {\n  font-size: 18px;\n  font-weight: 600;\n  letter-spacing: -0.02em;\n  margin-bottom: 0;\n  color: white;\n}\n.description {\n  color: #93acd3;\n  font-size: 14px;\n  font-weight: 500;\n  line-height: 150%;\n  margin: 12px 0 16px;\n  text-align: left;\n}\n\n.Feedback-selection {\n  width: 100%;\n  font-size: 14px;\n  height: 58px;\n  // padding: 10px 17px;\n}\n\n.form-textarea {\n  background: rgba(9, 12, 17, 0.6);\n  border: 1px solid rgba(147, 172, 211, 0.2);\n  border-radius: 10px;\n  color: white;\n  font-weight: 600;\n  height: 125px;\n  line-height: 1.5;\n  margin-top: 8px;\n  outline: 0;\n  padding: 12px 16px;\n  resize: none;\n  width: 100%;\n}\n\n.form-textarea-error {\n  border: 1px solid #ed1d49;\n}\n\n.validate-info {\n  margin: 4px 0 24px;\n  color: #55657e;\n  font-size: 12px;\n  font-weight: 500;\n  line-height: 150%;\n  margin: 4px 0;\n  padding-left: 18px;\n}\n\naddress {\n  color: #93acd3;\n  font-size: 12px;\n  font-style: inherit;\n  margin-left: 10px;\n  margin-bottom: 5px;\n}\n\n.down-btn {\n  display: flex;\n  flex-direction: row;\n  align-items: flex-end;\n  margin-top: 15px;\n}\n\n.dropdown-error-text {\n  color: #ed1d49;\n  font-size: 12px;\n  font-weight: 500;\n  line-height: 18px;\n  margin: 0 12px;\n}\n@media only screen and (max-width: 767px) {\n  .main {\n    margin: 0;\n  }\n  .header h3 {\n    font-weight: 700;\n    font-size: 20px;\n  }\n  .description {\n    font-size: 13px;\n    font-weight: 500;\n  }\n  .down-btn {\n    font-weight: 600;\n  }\n  .down-btn a {\n    font-weight: 500;\n  }\n}\n</style>\n"]}]}