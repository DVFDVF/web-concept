fury-client/src/views/home/head/right/MyUserAccount.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"account-alt\">\n    <div class=\"account-container\">\n      <h2 class=\"container-title\">{{ lang.v.da_account }}</h2>\n      <div class=\"category-tabs\">\n        <div\n          v-for=\"tab in tabs\"\n          :key=\"tab.id\"\n          :class=\"{ active: tab.isActive }\"\n          @click.prevent=\"setActiveTab(tab.id)\"\n          :aria-current=\"tab.isActive ? 'page' : undefined\"\n          class=\"tab\"\n        >\n          <img class=\"tab-icon\" :src=\"tab.iconSrc\" />\n          <span class=\"tab-title\">{{ tab.title }}</span>\n        </div>\n      </div>\n      <!-- 手机模式下菜单 -->\n      <drop-down-menu class=\"drop-down-menu\">\n        <template v-slot:header>\n          <div>\n            <img class=\"tab-icon\" :src=\"selectItem.iconSrc\" />\n            <span class=\"tab-title\">{{ selectItem.title }}</span>\n          </div>\n        </template>\n        <template v-slot:drop-down>\n          <div\n            v-for=\"tab in tabs\"\n            :key=\"tab.id\"\n            :class=\"['drop-down-item', { active: tab.isActive }]\"\n            @click.prevent=\"setActiveTab(tab.id)\"\n            :aria-current=\"tab.isActive ? 'page' : undefined\"\n            class=\"tab\"\n          >\n            <img class=\"tab-icon\" :src=\"tab.iconSrc\" />\n            <span class=\"tab-title\">{{ tab.title }}</span>\n          </div>\n        </template>\n      </drop-down-menu>\n\n      <router-view />\n    </div>\n  </div>\n</template>\n<!-- 这是我的信息,5个导航页 -->\n<script lang=\"ts\">\nimport dropDownMenu from '@/views/dropDownMenu/dropDownMenu.vue';\nimport { defineComponent, watch, ref, Ref, reactive, onMounted } from 'vue';\nimport { lang } from '@/lang';\nimport { useRoute } from 'vue-router';\ninterface Tab {\n  id: number;\n  title: string;\n  isActive: boolean;\n  iconSrc: string;\n  altText: string;\n}\n\nexport default defineComponent({\n  components: { dropDownMenu },\n  name: 'AccountComponent',\n  data() {\n    return {\n      lang,\n    };\n  },\n  setup() {\n    function setActiveTabBasedOnRoute(route: any) {\n      switch (route.name) {\n        case 'Account':\n          setActives(1);\n          selectItem.value = tabs.value[0];\n          break;\n        case 'Transactions':\n          setActives(2);\n          selectItem.value = tabs.value[1];\n          break;\n        case 'GameHistory':\n          setActives(3);\n          selectItem.value = tabs.value[2];\n          break;\n        case 'Sessions':\n          setActives(4);\n          selectItem.value = tabs.value[3];\n          break;\n        default:\n          setActives(5);\n          selectItem.value = tabs.value[4];\n          break;\n      }\n    }\n    onMounted(() => {\n      const route = useRoute();\n\n      // 在组件挂载时立即根据当前路由设置活动状态\n      setActiveTabBasedOnRoute(route);\n\n      // 继续侦听路由变化\n      watch(route, () => {\n        setActiveTabBasedOnRoute(route);\n      });\n    });\n\n    function setActives(selectedId: number) {\n      tabs.value.forEach((tab) => {\n        tab.isActive = tab.id === selectedId;\n      });\n    }\n    const tabs: Ref<Tab[]> = ref([\n      {\n        id: 1,\n        get title() {\n          return lang.v.da_myAccout;\n        },\n        isActive: true,\n        iconSrc: require('@/assets/header/icon-my-account.webp'),\n      },\n      {\n        id: 2,\n        get title() {\n          return lang.v.da_transactions;\n        },\n        isActive: false,\n        iconSrc: require('@/assets/header/icon-transactions.webp'),\n      },\n      {\n        id: 3,\n        get title() {\n          return lang.v.da_gameHistory;\n        },\n        isActive: false,\n        iconSrc: require('@/assets/header/icon-game-history.webp'),\n      },\n\n      // ... add other tabs as needed\n    ] as Tab[]);\n    const selectItem = ref(tabs.value[0]);\n    return {\n      tabs,\n      setActives,\n      selectItem,\n    };\n  },\n  methods: {\n    setActiveTab(selectedId: number) {\n      const routesMap: { [key: number]: string } = {\n        1: '/Userprofe/Account',\n        2: '/Userprofe/Transactions',\n        3: '/Userprofe/GameHistory',\n      };\n\n      this.selectItem = this.tabs[selectedId - 1];\n      this.tabs.forEach((tab) => {\n        tab.isActive = tab.id === selectedId;\n      });\n\n      const selectedRoute = routesMap[selectedId] || '/Userprofe/Verification';\n      this.$router.push(selectedRoute);\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n@primary-color: #1c2532;\n@secondary-color: #2267f6;\n@font-family: 'Montserrat Variable', sans-serif, Arial, sans-serif;\n\n.account-alt {\n  padding: 34px 0;\n  flex: 1;\n  min-height: 400px;\n\n  .account-container {\n    margin: 0 auto;\n    max-width: 1088px;\n    padding-left: 24px;\n    padding-right: 24px;\n    position: relative;\n    width: 100%;\n    font-feature-settings: 'tnum' on, 'lnum' on;\n    font-family: @font-family;\n    color: #fff;\n    text-align: left;\n\n    .container-title {\n      font-size: 24px;\n      line-height: 135%;\n      font-weight: 700;\n      letter-spacing: -0.02em;\n      margin-bottom: 1rem;\n      margin-top: 0;\n    }\n\n    .category-tabs {\n      border-radius: 14px;\n      display: flex;\n      background: linear-gradient(180deg, rgba(28, 37, 50, 0), @primary-color);\n      cursor: pointer;\n\n      .tab {\n        flex: 1;\n        padding: 15px 25px;\n        display: flex;\n        flex-direction: column;\n        align-items: center;\n        border-radius: 14px;\n        position: relative;\n\n        &-icon {\n          width: 40px;\n          height: 40px;\n          margin-bottom: 8px;\n        }\n\n        &-title {\n          font-size: 16px;\n          font-weight: 600;\n        }\n\n        &:hover {\n          background: linear-gradient(\n            180deg,\n            rgba(@secondary-color, 0),\n            rgba(@secondary-color, 0.1)\n          );\n        }\n\n        &:before {\n          content: '';\n          position: absolute;\n          top: 0;\n          bottom: 0;\n          left: 0;\n          right: 0;\n          z-index: 0;\n          transition: opacity 0.1s ease-in-out;\n          opacity: 0;\n          border-radius: 14px;\n          background: linear-gradient(\n            180deg,\n            rgba(@secondary-color, 0),\n            rgba(@secondary-color, 0.4)\n          );\n        }\n\n        &.active:before {\n          opacity: 1;\n        }\n      }\n    }\n  }\n}\n.drop-down-menu {\n  display: none;\n}\n@media only screen and (max-width: 767px) {\n  .drop-down-menu {\n    display: inline;\n  }\n  .category-tabs {\n    display: none !important;\n  }\n  .tab-icon {\n    height: 20px;\n    margin-left: 0;\n    margin-right: 6px;\n    width: 20px;\n  }\n  .tab-title {\n    line-height: 20px;\n    font-weight: 700;\n  }\n  .drop-down-item {\n    padding: 10px 0;\n  }\n  .active {\n    background-color: #2a3546;\n    padding: 10px 10px;\n    border-radius: 6px;\n    color: #fff;\n    pointer-events: none;\n  }\n  .account-alt .account-container {\n    padding-left: 0px;\n    padding-right: 0px;\n  }\n}\n</style>\n"]}]}