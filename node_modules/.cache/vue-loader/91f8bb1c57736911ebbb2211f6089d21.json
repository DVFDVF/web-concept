            <Image\n              :src=\"game.currencyIcon\"\n              style=\"width: 24px; height: 24px; margin-right: 6px\"\n            />\n            <p style=\"margin: 0\">\n              <Money :value=\"game.win\" :currency=\"game.currencyId\"></Money>\n            </p>\n          </div>\n        </span>\n      </div>\n    </div>\n    <div class=\"no-data\" v-show=\"total <= 0\">\n      <img class=\"no-data-image\" src=\"@/assets/other/noData.png\" />\n      <div class=\"no-data-text\">{{ lang.v.da_information }}</div>\n    </div>\n\n    <a-pagination\n      v-show=\"total > 0\"\n      v-model:current=\"current\"\n      :total=\"total\"\n      style=\"margin-top: 20px\"\n      @change=\"handlePageChange\"\n      :pageSize=\"perPage\"\n      :showSizeChanger=\"false\"\n    />\n  </div>\n</template>\n<!-- GameHistory 游戏记录 -->\n<script lang=\"ts\" setup>\nimport { onMounted, ref } from 'vue';\nimport { userAllGameRecords } from '../../api/user-api/headIndex';\nimport { UserGameItem } from '../../api/user-api/headProtocol';\nimport dayjs from 'dayjs';\nimport { lang } from '@/lang';\nimport { Pagination as APagination } from 'ant-design-vue';\n\n// 响应式引用，用于跟踪当前页码\nconst current = ref(1);\nconst perPage = ref(10); // 每页显示的项目数\nconst total = ref(0); // 总项目数\nconst games = ref<UserGameItem[]>([]);\n\nfunction allGame() {\n  userAllGameRecords(perPage.value, current.value).then((resp) => {\n    if (!resp) return;\n    games.value = resp.data;\n    total.value = resp.total;\n  });\n}\nfunction handlePageChange(curren: number) {\n  current.value = curren;\n  allGame();\n}\n\nonMounted(() => {\n  allGame();\n});\n</script>\n\n<style lang=\"less\" scoped>\n.game-auens {\n  font-family: Montserrat Variable, sans-serif;\n  margin: 36px 0px;\n  text-align: center;\n}\n\n.game-list-header {\n  align-items: center;\n  color: #55657e;\n  display: flex;\n  font-size: 11px;\n  font-weight: 700;\n  height: 28px;\n  letter-spacing: 0.66px;\n  text-transform: uppercase;\n}\n\n.game-item {\n  align-items: center;\n  background: rgba(22, 31, 44, 0.8);\n  border-radius: 8px;\n  display: flex;\n  height: 50px;\n  margin-bottom: 6px;\n}\n\n.game-list {\n  margin-bottom: 40px;\n}\n\n.game-list-header span,\n.game-item span {\n  flex: 1 1;\n  padding: 0px 0px 0px 16px;\n  text-align: left;\n  display: flex;\n  flex-direction: row;\n\n  color: #93acd3;\n  font-size: 14px;\n  font-weight: 600;\n  letter-spacing: -0.28px;\n  line-height: 125%;\n\n  display: flex;\n  align-items: center;\n}\n\n.gameun-liens {\n  max-width: 60px;\n  margin-top: 8px;\n  background: #2a3546;\n  border-radius: 56px;\n  color: #55657e;\n  display: inline-flex;\n  align-items: center;\n  /* Centers content vertically */\n  justify-content: center;\n  /* Centers content horizontally */\n  font-size: 14px;\n  font-weight: 600;\n  letter-spacing: -0.28px;\n  line-height: 20px;\n  padding: 0 7px;\n}\n\n.payout-share {\n  background: #2a3546;\n  border-radius: 8px;\n  cursor: pointer;\n  height: 24px;\n  width: 24px;\n  margin-right: 16px;\n}\n\n.payout-share-icon {\n  color: #93acd3;\n  width: 16px;\n  height: 16px;\n}\n.no-data {\n  align-items: center;\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n}\n\n.no-data-image {\n  height: 165px;\n  width: 165px;\n}\n\n.no-data-text {\n  color: #55657e;\n  font-size: 14px;\n  font-weight: 500;\n}\n@media only screen and (max-width: 767px) {\n  .game-list-header > :nth-child(2),\n  .game-list-header > :nth-child(4) {\n    display: none;\n  }\n  .game-list-header span {\n    font-size: 11px;\n    margin-bottom: 3px;\n  }\n  .game-item span {\n    font-size: 12px;\n  }\n  .game-item > :nth-child(2),\n  .game-item > :nth-child(4) {\n    display: none;\n  }\n}\n</style>\n"]}]}