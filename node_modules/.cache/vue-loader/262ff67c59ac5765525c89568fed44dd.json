n\">\n            <div class=\"main-content\">\n              <CompetitionGameButton></CompetitionGameButton>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent } from 'vue';\nimport CompetitionGameButton from '@/views/home/competition/widget/SmallWidget/CompetitionGameButton.vue';\nimport { removeArrayToLocal, addArrayToLocal, splitArrData } from '@/utils';\nexport default defineComponent({\n  components: {\n    CompetitionGameButton,\n  },\n  props: {\n    imageSrc: {\n      type: String,\n      required: true,\n    },\n    gameId: {\n      type: Number,\n      required: true,\n    },\n  },\n  data() {\n    return {\n      isLike: false,\n    };\n  },\n  computed: {\n    isLikeNow() {\n      if (this.isLike) {\n        return 'red';\n      } else {\n        return '#ed1d4933';\n      }\n    },\n  },\n  methods: {\n    isLikeClick(event: Event) {\n      event.stopPropagation();\n      this.isLike = !this.isLike;\n\n      if (this.isLike) {\n        addArrayToLocal('Favorites', this.gameId);\n      } else {\n        removeArrayToLocal('Favorites', this.gameId);\n      }\n    },\n  },\n  mounted() {\n    //判断是否存在\n    this.isLike = splitArrData('Favorites').includes(this.gameId);\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.card-wrapper {\n  border-radius: 14px;\n  overflow: hidden;\n  position: relative;\n  cursor: pointer;\n}\n\n.card {\n  display: flex;\n  aspect-ratio: 182/240;\n  border-radius: 14px;\n  box-shadow: 0 0 0 transparent, 0 0 0 transparent;\n  cursor: pointer;\n  font-feature-settings: 'tnum' on, 'lnum' on;\n  height: 100%;\n  letter-spacing: -0.02em;\n  overflow: hidden;\n  text-decoration: none;\n  width: 100%;\n  z-index: 0;\n  transition: box-shadow 0.1s ease-in-out, transform 0.1s ease-in-out;\n}\n\n.wrapper {\n  aspect-ratio: 182 / 240;\n  background: linear-gradient(\n      150.15deg,\n      rgba(34, 131, 246, 0.147) 27.17%,\n      rgba(34, 131, 246, 0.084) 46.76%,\n      rgba(34, 131, 246, 0) 61.36%\n    ),\n    #263041;\n  width: 100%;\n}\n\n.card-image,\n.card.locked:after {\n  border-radius: 14px;\n  left: 0;\n  position: absolute;\n  top: 0;\n  display: block;\n  height: 100%;\n  pointer-events: none;\n  transition: filter 0.1s ease-in-out, transform 0.1s ease-in-out;\n  width: 100%;\n}\n\n.card-badges {\n  display: flex;\n  flex-direction: column;\n  height: 100%;\n  justify-content: space-between;\n  padding: 11px;\n  width: 100%;\n  font-size: 11px;\n  font-weight: 700;\n  letter-spacing: 0.06em;\n  line-height: 150%;\n  text-transform: uppercase;\n}\n\n.badges-block {\n  display: flex;\n  gap: 10px;\n  justify-content: space-between;\n  width: 100%;\n}\n\n.card-badge {\n  align-items: center;\n  border-radius: 300px;\n  color: #fff;\n  display: flex;\n  font-size: 11px;\n  font-weight: 700;\n  height: 25px;\n  justify-content: center;\n  letter-spacing: 0.06em;\n  line-height: 150%;\n  padding: 4px 8px;\n  white-space: nowrap;\n  width: min-content;\n}\n\n.card-badge-top {\n  background-color: #ed1d49;\n  box-shadow: 0 3px 18px #97240080;\n}\n\n.card-badge-jackpot {\n  background: #ed8e00;\n  box-shadow: 0 3px 18px #d8820080;\n}\n\n.background,\n.img {\n  height: 100%;\n  width: 100%;\n  border-radius: 14px;\n  object-fit: cover;\n}\n\n//\n.favorite-games-game-wrap {\n  position: absolute;\n  left: 0;\n  top: 0;\n  width: 100%;\n  height: 100%;\n  background: Linear-gradient(\n    0deg,\n    rgba(3, 1, 73, 0.9) 33.53%,\n    rgba(3, 53, 128, 0.531) 66.43%,\n    rgba(35, 3, 128, 0.873) 89.26%\n  );\n\n  box-shadow: 0 4px 4px #00000040, 0 4px 50px #000;\n  z-index: 2;\n  padding: 11px;\n  // 默认隐藏\n  opacity: 0;\n  visibility: hidden;\n  transition: opacity 0.3s ease, visibility 0.3s ease;\n}\n\n.card-wrapper:hover .favorite-games-game-wrap {\n  // 鼠标悬停时显示\n  opacity: 1;\n  visibility: visible;\n}\n.card-wrapper:hover .card-badges {\n  opacity: 0;\n}\n\n.hover-box {\n  display: flex;\n  flex-direction: column;\n  height: 100%;\n\n  .hover-topbar {\n    display: flex;\n    flex-direction: row;\n    position: relative;\n\n    .btn-like {\n      display: flex;\n      flex-direction: row;\n      align-items: center;\n      position: absolute;\n      right: 3px;\n      background-color: rgba(0, 0, 0, 0);\n\n      .btn-icon {\n        width: 24px;\n        height: 24px;\n        color: red;\n      }\n    }\n  }\n}\n.hover-main {\n  display: flex;\n  flex-grow: 1;\n  height: 100%;\n  justify-content: center;\n  align-items: center;\n\n  .main-content {\n    width: 40px;\n    height: 40px;\n    position: relative;\n    transform-style: preserve-3d; /* 允许元素在3D空间中存在 */\n    transform: perspective(-200px) rotateX(0deg) rotateY(0deg) translateZ(0px);\n  }\n}\n\nbutton {\n  border: none;\n  outline: none;\n  padding: 0;\n  margin: 0;\n  text-decoration: none;\n}\n\n@media only screen and (max-width: 767px) {\n  .card {\n    display: flex;\n    border-radius: 14px;\n    box-shadow: 0 0 0 transparent, 0 0 0 transparent;\n    cursor: pointer;\n    font-feature-settings: 'tnum' on, 'lnum' on;\n    height: 190px;\n    letter-spacing: -0.02em;\n    overflow: hidden;\n    text-decoration: none;\n    width: 100%;\n    z-index: 0;\n    transition: box-shadow 0.1s ease-in-out, transform 0.1s ease-in-out;\n  }\n}\n</style>\n"]}]}