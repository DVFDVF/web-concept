   </svg>\n        Play\n      </div>\n    </div>\n  </Drawer>\n</template>\n<script lang=\"ts\" setup>\nimport { ref, defineProps, defineEmits } from 'vue';\nimport type { DrawerProps } from 'ant-design-vue';\nimport { Drawer } from 'ant-design-vue';\nimport { GamesDatas } from '../home/api/protocol';\nimport { addArrayToLocal, removeArrayToLocal, splitArrData } from '@/utils';\nimport { enterGame } from '@/manager/view';\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nconst placement = ref<DrawerProps['placement']>('bottom');\nconst props = defineProps({\n  detials: {\n    type: Object as () => {\n      open: boolean;\n      gameData: GamesDatas | undefined;\n    },\n    required: true,\n  },\n});\nconst isLike = ref(false);\nif (props.detials.gameData) {\n  splitArrData('Favorites').includes(props.detials.gameData.id);\n}\nconst rules = [\n  {\n    key: 0,\n    icon: require('@/assets/popUp/icon1.png'),\n    text: lang.v.gm_adaptable,\n  },\n  {\n    key: 1,\n    icon: require('@/assets/popUp/icon2.png'),\n    text: lang.v.gm_equitable,\n  },\n  {\n    key: 2,\n    icon: require('@/assets/popUp/icon3.png'),\n    text: lang.v.gm_prompt,\n  },\n];\nconst emits = defineEmits(['close']);\nfunction onClose() {\n  emits('close');\n}\nfunction isLikeNow() {\n  if (isLike.value) {\n    return 'red';\n  } else {\n    return '#ed1d4933';\n  }\n}\nfunction isLikeClick(event: Event) {\n  if (props.detials.gameData) {\n    event.stopPropagation();\n    isLike.value = !isLike.value;\n\n    if (isLike.value) {\n      addArrayToLocal('Favorites', props.detials.gameData.id);\n    } else {\n      removeArrayToLocal('Favorites', props.detials.gameData.id);\n    }\n  }\n}\nfunction submit() {\n  if (props.detials.gameData) {\n    emits('close');\n    enterGame(props.detials.gameData);\n  }\n}\n</script>\n<style lang=\"less\" scoped>\n.bottom-pop-up {\n  background: white;\n  height: 100%;\n  font-family: Montserrat Alternates, Montserrat Alternates;\n  color: black;\n}\n.game-details {\n  padding: 20px 14px;\n  width: 100%;\n  .details-content {\n    width: 100%;\n    gap: 20px;\n    .left {\n      width: 40%;\n      .game-icon {\n        width: 100%;\n      }\n    }\n    .right {\n      flex: 1;\n      .header {\n        align-items: flex-start;\n        justify-content: space-between;\n        margin-bottom: 10px;\n        .header-left {\n          .title {\n            color: white;\n            font-size: 15px;\n            line-height: 15px;\n          }\n          .name {\n            color: #97a8c7;\n            font-size: 13px;\n            line-height: 17px;\n          }\n          .rpt {\n            color: #286aff;\n            font-size: 13px;\n          }\n        }\n        .header-right {\n          .like {\n            width: 20px;\n            height: 20px;\n            color: red;\n          }\n        }\n      }\n      .right-body {\n        gap: 10px;\n        background: #15161c;\n        padding: 20px 10px;\n        .body-item {\n          gap: 3px;\n          color: #97a8c7;\n          .item-icon {\n            width: 20px;\n          }\n          .item-text {\n            font-size: 11px;\n          }\n        }\n      }\n    }\n  }\n  .submit {\n    margin-top: 20px;\n    width: 100%;\n    background: #fa305c;\n    padding: 12px;\n    color: white;\n    font-weight: 800;\n    gap: 3px;\n    .svg-use {\n      width: 15px;\n      height: 15px;\n      padding: 3.5px;\n      border-radius: 15px;\n      border: 1.2px solid white;\n    }\n  }\n}\n</style>\n"]}]}