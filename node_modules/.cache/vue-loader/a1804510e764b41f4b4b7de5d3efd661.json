>\n            <span class=\"cashback-progress\">{{ oldCashBack() }}</span>\n            <Image\n              class=\"cashb-psune\"\n              src=\"/fury/icon/cashback-arrows.30e38849.png\"\n            />\n            <span class=\"cashback-progress-item\">{{ formatCashBack() }} </span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport Avatar from '@/components/Avatar.vue';\nimport { lang } from '@/lang';\nimport { filteredLevelConfigs } from '@/utils';\nimport { UserProfileResp } from '../../api/user-api/headProtocol';\nimport { onMounted, ref } from 'vue';\nimport { getUserInformation } from '../../api/user-api/headIndex';\n\nconst userInformations = ref<UserProfileResp>();\nfunction userInformationData() {\n  getUserInformation().then((resp) => {\n    if (!resp) return;\n    userInformations.value = resp;\n  });\n}\nonMounted(() => {\n  userInformationData();\n});\nlet levelConfigs = filteredLevelConfigs();\nfunction formatCashBack() {\n  if (\n    userInformations.value?.user &&\n    userInformations.value.user.cashBack !== undefined\n  ) {\n    let levelConfig = levelConfigs[userInformations.value.user.level];\n    if (!levelConfig) {\n      // 如果没有，说明到了最高等级，那就显示当前等级的数据\n      levelConfig = levelConfigs[userInformations.value.user.level - 1];\n    }\n    return `${(levelConfig?.cashBack * 100).toString()}%`;\n  }\n  return '0%';\n}\nfunction oldCashBack() {\n  if (\n    userInformations.value?.user &&\n    userInformations.value.user.cashBack !== undefined\n  ) {\n    return `${(\n      levelConfigs[userInformations.value.user.level - 1].cashBack * 100\n    ).toString()}%`;\n  }\n  return '0%';\n}\n</script>\n\n<style lang=\"less\" scoped>\n.maunt {\n  margin-top: 32px;\n\n  font-feature-settings: 'tnum' on, 'lnum' on;\n  font-family: Montserrat Variable, sans-serif;\n  color: #fff;\n\n  .dashboard {\n    grid-gap: 24px;\n    display: grid;\n    grid-template-columns: repeat(auto-fit, minmax(280px, 1fr));\n    margin-bottom: 32px;\n\n    .dashboard-tem {\n      background-color: #161f2c;\n      border-radius: 14px;\n      padding: 20px;\n      display: flex;\n      flex-direction: center;\n      overflow: hidden;\n\n      .userpick {\n        height: 60px;\n        width: 60px;\n      }\n\n      .dasho-title {\n        text-align: center;\n        font-weight: 600;\n        font-size: 18px;\n        margin: 0;\n      }\n\n      .dasho-al {\n        margin-left: 20px;\n      }\n    }\n\n    // 第二个\n    .dashboard-tems {\n      background-color: #161f2c;\n      border-radius: 14px;\n      padding: 20px;\n      gap: 20px;\n\n      .iatne {\n        align-items: center;\n        display: flex;\n        gap: 6px;\n        margin-bottom: 6px;\n      }\n\n      .preunsg {\n        align-items: center;\n        display: flex;\n\n        position: relative;\n        width: 100%;\n        gap: 6px;\n      }\n\n      .itena {\n        margin-bottom: 4px;\n      }\n\n      .progress-wrapper {\n        max-width: calc(100% - 44px);\n        width: 100%;\n      }\n\n      .dashboard-description {\n        color: #93acd3;\n        font-size: 11px;\n        font-style: normal;\n        font-weight: 600;\n        letter-spacing: -0.22px;\n        line-height: 135%;\n        margin-bottom: 0;\n        text-align: center;\n      }\n\n      .progress-value {\n        top: 5.5px;\n        color: #fff;\n        line-height: 10px;\n        font-size: 0.85em;\n        left: 30%;\n        position: absolute;\n        font-size: 10px;\n      }\n\n      .progress {\n        border-radius: 5px;\n        height: 10px;\n        background-color: #202a39;\n        border: none;\n        display: block;\n        overflow: hidden;\n        padding: 0;\n        width: 100%;\n      }\n\n      .dante-stile {\n        font-size: 12px;\n        font-weight: 600;\n        margin-bottom: 8px;\n      }\n\n      .dante-iatne {\n        margin-left: 6px;\n        border: none;\n        color: #2283f6;\n        font-size: 11px;\n        padding-top: 1px;\n        text-decoration: underline;\n        white-space: nowrap;\n      }\n    }\n\n    // 第三个\n    .dashboard-ihback {\n      padding-left: 96px;\n      position: relative;\n\n      .dashboard-unet {\n        align-items: center;\n        display: flex;\n      }\n\n      .mask-img {\n        margin: 0;\n        top: 0;\n        left: 0;\n        position: absolute;\n      }\n\n      .mask-twimg {\n        bottom: 0;\n        left: 0;\n        margin: auto;\n        position: absolute;\n        width: 96px;\n      }\n\n      .dashboard-ihback-title {\n        font-size: 12px;\n        font-weight: 600;\n        margin-bottom: 8px;\n      }\n\n      .cashb-psune {\n        margin: 0 8px;\n        width: 48px;\n        height: 20px;\n      }\n\n      .cashback-progress {\n        color: #1bb83d;\n        font-size: 32px;\n        font-weight: 700;\n        line-height: 1;\n      }\n\n      .cashback-progress-item {\n        color: #202a39;\n        font-size: 32px;\n        font-weight: 700;\n        line-height: 1;\n        white-space: nowrap;\n      }\n    }\n  }\n}\n</style>\n"]}]}