s/home/login/Login.vue';\nimport { defineComponent } from 'vue';\nexport default defineComponent({\n  name: 'Recharge',\n  data() {\n    return {\n      toggleLeft: dataCenter.layout,\n      lang,\n    };\n  },\n  methods: {\n    //跳转钱包\n    goToWallet() {\n      if (dataCenter.user.data) {\n        openModal(WalletButtonPop);\n      } else {\n        openModal(LoginWidget);\n      }\n    },\n    /**跳转客服 */\n    support() {\n      window.open(dataCenter.webConfig.customerServiceLink);\n    },\n  },\n  components: { IcoBuyCry, IcoSupport },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.item {\n  font-size: 14px;\n  font-weight: 600;\n  letter-spacing: -0.02em;\n  line-height: 24px;\n  min-height: 24px;\n  padding: 9px;\n  border-radius: 10px;\n\n  .content {\n    width: 100%;\n  }\n}\n\n.icon {\n  color: #55657e;\n  font-size: 19px;\n  width: 24px;\n  height: 24px;\n}\n\n.item:hover {\n  color: #2283f6;\n  background: #1d2736;\n  border-radius: 5px;\n}\n\n.item:active {\n  color: #2283f6;\n  background: #1d2736;\n  border-radius: 5px;\n}\n\n.support {\n  flex-direction: column;\n  border-radius: 10px;\n  height: 44px;\n  margin-top: 24px;\n  margin-left: 10px;\n  margin-right: 10px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n\n  .down-btn {\n    background-color: #2a3546;\n    border-color: transparent;\n    color: #93acd3;\n    border-radius: 10px;\n    width: 100%;\n    height: 100%;\n\n    .down-text {\n      font-size: 14px;\n      letter-spacing: -0.26px;\n      font-weight: 700;\n    }\n  }\n\n  .down-btn:hover {\n    background-color: #202a39;\n  }\n\n  .down-btn:active {\n    background-color: #1c2532;\n  }\n}\n\n.buy {\n  background: #1c2532;\n  border: 1px solid var(--dark-shades-gray-80, #202a39);\n  border-radius: 10px;\n  flex-direction: column;\n  height: 82px;\n  margin-top: 24px;\n  margin-left: 10px;\n  margin-right: 10px;\n  padding: 10px;\n  display: flex;\n  justify-content: space-between;\n  align-items: cen