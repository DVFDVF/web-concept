n  </div>\n</template>\n\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { Tooltip as ATooltip } from 'ant-design-vue';\nimport { defineComponent } from 'vue';\nimport SetupButton from '../head/SetupButton.vue';\nimport GameMenu from './GameMenu.vue';\nimport InstallBtn from './installBtn.vue';\nimport OtherLink from './OtherLink.vue';\nimport Recharge from './Recharge.vue';\nimport Welfare from './Welfare.vue';\n\nexport default defineComponent({\n  name: 'home-left',\n  data() {\n    return {\n      lang: lang,\n      layout: dataCenter.layout,\n      customColors: '#1b2636',\n      user: dataCenter.user,\n      dataCenter,\n    };\n  },\n  components: {\n    Welfare,\n    GameMenu,\n    OtherLink,\n    Recharge,\n    SetupButton,\n    ATooltip,\n    InstallBtn,\n  },\n  methods: {\n    closePopUp() {\n      dataCenter.layout.closeAll();\n    },\n  },\n  mounted() {\n    if (window.innerWidth < 768) {\n      dataCenter.layout.closeAll();\n    } else {\n      dataCenter.layout.leftMenuFold = false;\n    }\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.home-left {\n  width: 232px;\n  padding: 28px 0 32px;\n  box-sizing: border-box;\n  overflow-y: scroll;\n  overflow-x: auto;\n  color: #93acd3;\n  font-size: 14px;\n  font-weight: 600;\n  background-color: #161f2c;\n  box-shadow: 6px 4px 10px #0d131c66;\n  font-family: Montserrat Variable, sans-serif;\n  transition: width 0.2s ease-in-out;\n  &::-webkit-scrollbar {\n    background-color: #2a3546;\n    border-radius: 2px;\n    height: 4px;\n    width: 4px;\n  }\n\n  .container {\n    display: flex;\n    flex-direction: column;\n    padding: 0 11px;\n  }\n  &.fold {\n    width: 52px;\n    padding: 24px 6px 32px 6px;\n    .container {\n      padding: 0;\n    }\n  }\n}\n.setting {\n  display: none;\n}\n@media only screen and (max-width: 767px) {\n  .back-shadow {\n    position: fixed;\n    top: 0;\n    left: 0;\n    bottom: 0;\n    right: 0;\n    width: 100%;\n    height: 100%;\n    background-color: rgba(0, 0, 0, 0.5);\n    overflow: hidden;\n    z-index: 100; /* 确保遮罩在弹窗之上 */\n  }\n  .home-left {\n    box-sizing: border-box;\n    width: 65%;\n    position: absolute;\n    z-index: 100;\n    animation: fadeIn 1s ease;\n    height: 100%;\n  }\n\n  &.fold {\n    width: 0 !important;\n    padding: 0 !important;\n  }\n  .fold .container {\n    display: none;\n  }\n  .setting {\n    display: flex;\n    width: 100%;\n    justify-content: space-between;\n    margin-top: -10px;\n    margin-bottom: 20px;\n  }\n  .language {\n    width: 100px;\n  }\n  .container {\n    padding-bottom: 200px;\n  }\n}\n</style>\n"]}]}