random() * 2.3 + 1.3}s`;\n\n  private keyframesName = `slideIn_${Math.random().toString(36).substring(7)}`;\n  anim() {\n    const keyframes = `\n    @keyframes ${this.keyframesName} {\n        from {\n            transform:translateX(0)  translateY(${\n              Math.random() * -1000 - 200\n            }px) rotateZ(0deg);\n        }\n        \n        to {\n            transform:translateX(${\n              Math.random() * 360 - 180\n            }px)  translateY(100vh) rotateZ(${Math.random() * 720 - 360}deg);\n        }\n    }\n`;\n    const style = document.createElement('style');\n    style.appendChild(document.createTextNode(keyframes));\n    document.head.appendChild(style);\n\n    this.img.style.animation = `${this.keyframesName} ${this.direction} linear infinite`;\n    this.img.style.direction = this.direction;\n    this.img.style.animationDirection = this.direction;\n  }\n  public init() {\n    this.img.width = this.width;\n    this.img.style.position = 'absolute';\n    this.img.style.top = this.top;\n    this.img.style.left = this.left;\n    this.img.draggable = false;\n    this.img.style.userSelect = 'none';\n    this.img.src = require('@/assets/redEnvelope/redEnvelope.png');\n    this.anim();\n    return this.img;\n  }\n}\n"],"mappings":"AAAA,eAAc,MAAOA,WAAW;EAAhCC,YAAA;IACU,KAAAC,GAAG,GAAGC,QAAQ,CAACC,aAAa,CAAC,KAAK,CAAC;IACnC,KAAAC,KAAK,GAAG,CAACC,IAAI,CAACC,MAAM,EAAE,IAAI,CAAC,GAAG,GAAG,CAAC,GAAG,GAAG,IAAI,GAAG;IACvD;IACQ,KAAAC,GAAG,GAAG,GAAG;IACT,KAAAC,IAAI,GAAG,GAAGH,IAAI,CAACC,MAAM,EAAE,GAAG,GAAG,IAAI;IACzC;IACQ,KAAAG,SAAS,GAAG,GAAGJ,IAAI,CAACC,MAAM,EAAE,GAAG,GAAG,GAAG,GAAG,GAAG;IAE3C,KAAAI,aAAa,GAAG,WAAWL,IAAI,CAACC,MAAM,EAAE,CAACK,QAAQ,CAAC,EAAE,CAAC,CAACC,SAAS,CAAC,CAAC,CAAC,EAAE;EAoC9E;EAnCEC,IAAIA,CAAA;IACF,MAAMC,SAAS,GAAG;iBACL,IAAI,CAACJ,aAAa;;kDAGrBL,IAAI,CAACC,MAAM,EAAE,GAAG,CAAC,IAAI,GAAG,GAC1B;;;;mCAKED,IAAI,CAACC,MAAM,EAAE,GAAG,GAAG,GAAG,GACxB,kCAAkCD,IAAI,CAACC,MAAM,EAAE,GAAG,GAAG,GAAG,GAAG;;;CAGtE;IACG,MAAMS,KAAK,GAAGb,QAAQ,CAACC,