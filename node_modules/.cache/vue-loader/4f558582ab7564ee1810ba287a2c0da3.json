CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACtD,CAAC,CAAC,CAAC,CAAC,CAAC;MACL,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACtB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;MAC1D,CAAC,CAAC,CAAC,CAAC,CAAC;MACL,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACf,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UAC9C,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QAC1D,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACV,CAAC,CAAC,CAAC,CAAC,CAAC;IACP,CAAC,CAAC,CAAC,CAAC,CAAC;EACP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/concept/web3-concept/src/views/home/head/user/UserFormationPop.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"fopop\">\n    <div class=\"fopop-maytn\">\n      <div class=\"col-center\">\n        <Avatar :size=\"64\" :user=\"userInfo\" />\n        <span class=\"fopop-fontp\">{{ userInfo?.name }}</span>\n      </div>\n      <div class=\"fopop-lisvp\">\n        <HeadMenuList :list=\"list\" v-on:selected=\"onClickMenu\" />\n      </div>\n      <div class=\"iten\">\n        <button class=\"menu-logout\" @click=\"logOutClick\">\n          <span class=\"fopop-lonte\"> {{ lang.v.da_logout }} </span>\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n<!-- 我的信息和退出登录 -->\n<script lang=\"ts\">\nimport { defineComponent, markRaw } from 'vue';\nimport { openModal } from '@/manager/view';\nimport { MenuData } from '@/views/home/left/interface';\nimport HeadMenuList from '../menu/HeadMenuList.vue';\nimport UserProfilePop from './UserProfilePop.vue';\nimport Vault from '@/views/home/head/popup/VaultPop.vue';\n//图标\nimport IconUser from '@/assets/icon/head/ico-user.vue';\nimport IconAccour from '@/assets/icon/head/ico-accour.vue';\nimport IconTransaction from '@/assets/icon/head/icon-transaction.vue';\nimport IconHistory from '@/assets/icon/head/icon-history.vue';\nimport IconSessions from '@/assets/icon/head/icon-sessions.vue';\nimport IconVault from '@/assets/icon/head/icon-vault.vue';\nimport { tokenController } from '@/manager/token';\nimport { user } from '@/dataCenter/user';\nimport { lang } from '@/lang';\nimport dataCenter from '@/dataCenter';\nimport Avatar from '@/components/Avatar.vue';\nimport icoNotificatVue from '@/assets/icon/head/ico-notificat.vue';\nimport icoGift from '@/assets/icon/head/ico-gift.vue';\nimport Receba from '../../other/Receba.vue';\nimport Login from '../../login/Login.vue';\nimport dayjs from 'dayjs';\nimport { jiugongge } from '../../other/game/jiugongge';\nimport { LoginResp } from '../../api/protocol';\nconst icoNotificatRef = markRaw(icoNotificatVue);\nconst IcoUserRef = markRaw(IconUser);\nconst IcoAccourRef = markRaw(IconAccour);\nconst IconTransactionRef = markRaw(IconTransaction);\nconst IconHistoryRef = markRaw(IconHistory);\nconst icoGiftRef = markRaw(icoGift);\nexport default defineComponent({\n  name: 'home-common-menus',\n  components: {\n    HeadMenuList,\n    Avatar,\n  },\n  props: {\n    // 例如，接收一个名为 'parentData' 的 prop\n    userInfo: {\n      type: Object as () => LoginResp,\n      default: () => null,\n    },\n    // 可以定义更多的 props\n  },\n  data() {\n    return {\n      lang,\n      fold: false,\n      layout: dataCenter.layout,\n      list: [\n        {\n          icon: icoNotificatRef,\n          key: 'notification',\n          get name() {\n            return lang.v.sy_d_notifications;\n          },\n          get hidden() {\n            return !dataCenter.layout.isMobile;\n          },\n          cornerMark: true,\n        },\n        {\n          icon: icoGiftRef,\n          key: 'jiugongge',\n          get name() {\n            return lang.v.sy_z_receba + ' ' + dataCenter.conform + ' 100';\n          },\n          get hidden() {\n            return jiugongge.isOverTime;\n          },\n          cornerMark: false,\n        },\n        {\n          icon: IcoUserRef,\n          key: 'My Profile',\n          get name() {\n            return lang.v.da_myProfile;\n          },\n        },\n        {\n          icon: IcoAccourRef,\n          key: 'Account',\n          get name() {\n            return lang.v.da_account;\n          },\n        },\n        {\n          icon: IconTransactionRef,\n          key: 'Transaction History',\n          get name() {\n            return lang.v.da_tranSactionHistory;\n          },\n        },\n        {\n          icon: IconHistoryRef,\n          key: 'Game History',\n          get name() {\n            return lang.v.da_gameHistory;\n          },\n        },\n      ],\n    };\n  },\n  methods: {\n    onClickMenu(item: MenuData) {\n      this.$emit('selectItem');\n      dataCenter.layout.closeAll();\n      switch (item.key) {\n        case 'notification':\n          dataCenter.layout.toggleNotify();\n          break;\n        case 'My Profile':\n          openModal(UserProfilePop, {\n            isMyProfile: true as any, // 移除 as any，确保传递正确的类型\n            UserId: 0 as any, // 移除 as any，确保传递正确的类型\n          });\n          break;\n        case 'Vault':\n          openModal(Vault);\n          break;\n        case 'Account':\n          this.$router.push('/Userprofe/Account');\n          break;\n        case 'Transaction History':\n          this.$router.push('/Userprofe/Transactions');\n          break;\n        case 'Game History':\n          this.$router.push('/Userprofe/GameHistory');\n          break;\n        case 'Sessions':\n          this.$router.push('/Userprofe/Sessions');\n          break;\n        case 'jiugongge':\n          if (dataCenter.user.data) {\n            openModal(Receba);\n          } else {\n            openModal(Login);\n          }\n          break;\n      }\n    },\n    logOutClick() {\n      tokenController.update('', '');\n      user.update();\n      location.href = '/';\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.fopop {\n  background: #1c2532;\n  box-shadow: 0 4px 26px #0d131c78;\n  min-width: 240px;\n  margin-top: 8px;\n  border: 2px;\n  border-radius: 10px;\n  position: absolute;\n  right: 210px;\n  &-maytn {\n    padding: 12px;\n  }\n\n  &-icon {\n    margin-top: 8px;\n    width: 64px;\n    height: 64px;\n    margin-bottom: 4px;\n  }\n\n  &-fontp {\n    font-size: 16px;\n    font-weight: 600;\n    font-family: Montserrat Variable, sans-serif;\n    color: #fff;\n    font-feature-settings: 'tnum' on, 'lnum' on;\n    margin-bottom: 8px;\n  }\n\n  &-lisvp {\n    background: #2a3546;\n    border-radius: 12px;\n    width: 212px;\n    margin-bottom: 8px;\n  }\n\n  .iten {\n    display: grid;\n    justify-items: start;\n    cursor: pointer;\n\n    .menu-logout {\n      color: #55657e;\n      padding: 6.5px 24px;\n      border-color: transparent;\n      background-color: #1c2532;\n      border-radius: 8px;\n\n      .fopop-lonte {\n        font-size: 12px;\n        letter-spacing: -0.24px;\n        line-height: 20px;\n        font-weight: 600;\n        font-family: Montserrat Variable, sans-serif;\n      }\n    }\n\n    :hover {\n      background-color: #202a39;\n      color: #93acd3;\n    }\n  }\n}\n.col-img {\n  margin-top: 8px;\n  margin-bottom: 4px;\n}\n@media only screen and (max-width: 767px) {\n  .fopop {\n    right: 0px;\n  }\n}\n</style>\n"]}]}