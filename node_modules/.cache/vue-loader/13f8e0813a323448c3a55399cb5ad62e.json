ba(28, 37, 50, 0.65);\n  display: flex;\n  height: calc(100% - 16px);\n  justify-content: center;\n  position: absolute;\n  padding-bottom: 32px;\n  width: 100%;\n  z-index: 4;\n  border-radius: 8px;\n}\n.rewards-leaderboard-wrap .status {\n  -webkit-backdrop-filter: blur(12px);\n  backdrop-filter: blur(12px);\n  background: radial-gradient(\n      47.06% 47.06% at 50% 50%,\n      hsla(0, 0%, 100%, 0.06) 0,\n      hsla(0, 0%, 100%, 0) 100%\n    ),\n    rgba(38, 48, 65, 0.6);\n  border: 1px solid #3c485c;\n  border-radius: 6px;\n  padding: 8px 12px;\n  font-size: 13px;\n}\n\n@media only screen and (max-width: 767px) {\n  .balance-card {\n    display: flex;\n    flex-direction: column;\n    width: 100%;\n  }\n}\n</style>\n"]}]}