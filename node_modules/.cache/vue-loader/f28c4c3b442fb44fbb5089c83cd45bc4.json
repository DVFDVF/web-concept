 this.$emit('selected', item);\n    },\n  },\n  watch: {\n    $route() {\n      if (this.$route.path == '/vip-club') {\n        this.selected = ['VIP Club'];\n      }\n      if (this.$route.path == '/install-app') {\n        this.selected = ['install'];\n      }\n      if (this.$route.path == '/referral/dashboard') {\n        this.selected = ['Refer & Earn'];\n      }\n      if (this.$route.path == '/CompetitionDetails') {\n        this.selected = ['Battle'];\n      }\n      if (this.$route.path == '/LatestNews') {\n        this.selected = ['News'];\n      }\n      if (this.$route.path == '/TeamIntroduction') {\n        this.selected = ['TeamIntroduction'];\n      }\n      if (this.$route.path == '/Fair') {\n        this.selected = ['Fair'];\n      }\n      if (this.$route.path == '/cashback') {\n        this.selected = ['cashback'];\n      }\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.sub-menu-list {\n  margin-top: 6px;\n  color: #93acd3;\n  font-weight: 600;\n}\n.shadow {\n  background: url(~@/assets/icon/8882.png);\n  background-size: 100%;\n  margin-left: -11px;\n  margin-top: -11px;\n  padding-left: 11px;\n  padding-top: 11px;\n  .menu-item:hover,\n  .selected {\n    background: rgba(255, 255, 255, 0.04);\n  }\n}\n</style>\n"]}]}