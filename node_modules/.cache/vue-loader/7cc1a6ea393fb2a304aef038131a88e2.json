pan class=\"currency currency-slice\">\n                <Image\n                  class=\"currency-icon\"\n                  width=\"20\"\n                  height=\"20\"\n                  :src=\"usd\"\n                  loading=\"lazy\"\n                />\n                <div class=\"currency-balances-wrap\">\n                  <div class=\"currency-balances\">\n                    <span class=\"currency-balances-balance\"\n                      ><span style=\"color: #1bb83d\" class=\"font-digits\">\n                        <Money :value=\"item.amount\"></Money> </span\n                    ></span>\n                  </div>\n                </div>\n                <!-- <svg\n                  class=\"svg-use\"\n                  width=\"22\"\n                  height=\"22\"\n                  focusable=\"false\"\n                  aria-hidden=\"true\"\n                >\n                  <use xlink:href=\"/svg/sprite.svg#icon-question\"></use>\n                </svg> -->\n              </span>\n            </amount-prompt>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nconst RecommendationEnum = {\n  LEVEL1: 41,\n  LEVEL2: 42,\n  LEVEL3: 43,\n};\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport AmountPrompt from '@/views/chats/prompt/amountPrompt.vue';\nimport { RewardHistoryData, UserReportItem } from '@/views/home/api/protocol';\nimport { defineComponent } from 'vue';\nexport default defineComponent({\n  name: 'contact-roller-list',\n  components: { AmountPrompt },\n  data() {\n    return {\n      lang,\n      styles: ['wrap', 'wrap animation--start'],\n      currentStyleIndex: 0,\n      intervalId: 0,\n      usd: dataCenter.webConfig.currencyIcon,\n    };\n  },\n  computed: {},\n  beforeUnmount() {\n    clearInterval(this.intervalId);\n  },\n  methods: {\n    currentStyle() {\n      return this.styles[this.currentStyleIndex];\n    },\n    convertDateFormat(inputDate: string): string {\n      const dateObj = new Date(inputDate);\n      const year = (dateObj.getUTCFullYear() % 100).toString().padStart(2, '0');\n      const month = (dateObj.getMonth() + 1).toString().padStart(2, '0');\n      const day = dateObj.getUTCDate().toString().padStart(2, '0');\n      return `${year}/${month}/${day}`;\n    },\n    toggleStyle() {\n      this.currentStyleIndex =\n        (this.currentStyleIndex + 1) % this.styles.length;\n    },\n    rewardType(type: number) {\n      if (type == RecommendationEnum.LEVEL1) {\n        return '1级推荐';\n      } else if (type == RecommendationEnum.LEVEL2) {\n        return '2级推荐';\n      } else if (type == RecommendationEnum.LEVEL3) {\n        return '3级推荐';\n      }\n      return '';\n    },\n  },\n  props: {\n    rewardHistory: {\n      type: Object as () => UserReportItem[],\n    },\n  },\n});\n</script>\n<style lang=\"less\" scoped>\n.statistic-content {\n  display: flex;\n  flex-direction: column;\n}\n.statistic-line {\n  align-items: center;\n  background: rgba(22, 31, 44, 0.8);\n  border-radius: 8px;\n  height: 50px;\n  margin-top: 6px;\n  will-change: transform;\n  display: flex;\n  flex-direction: row;\n\n  .name {\n    align-items: center;\n    color: #93acd3;\n    display: flex;\n    cursor: pointer;\n\n    .name-icon {\n      margin: 0 8px;\n      width: 19px;\n    }\n\n    .name-wrap {\n      overflow-x: hidden;\n      text-overflow: ellipsis;\n      white-space: nowrap;\n    }\n  }\n\n  .time {\n    font-feature-settings: 'tnum' on, 'lnum' on;\n    color: #93acd3;\n    font-weight: 600;\n    letter-spacing: -0.28px;\n    line-height: 125%;\n\n    .datetime {\n      text-align: left;\n      white-space: nowrap;\n    }\n  }\n\n  .user {\n    align-items: center;\n    display: flex;\n    cursor: pointer;\n\n    .user-avatar {\n      flex-shrink: 0;\n      position: relative;\n      cursor: pointer;\n\n      .user-inner {\n        border-radius: 50%;\n        overflow: hidden;\n        right: 0;\n        bottom: 0;\n        left: 0;\n        position: absolute;\n        top: 0;\n      }\n\n      .user-level {\n        background-color: #55657e;\n        border-radius: 50%;\n        bottom: -4px;\n        color: white;\n        height: 15px;\n        line-height: 15px;\n        pointer-events: none;\n        position: absolute;\n        right: -4px;\n        text-align: center;\n        width: 15px;\n      }\n\n      .user-level-centered {\n        background: #ed1d49;\n        border-radius: 38px;\n        font-weight: 700;\n        height: 12px;\n        left: 50%;\n        letter-spacing: -0.2px;\n        line-height: 135%;\n        transform: translate(-50%);\n        width: 19px;\n      }\n    }\n\n    .user-wrap {\n      display: flex;\n      flex-direction: column;\n\n      .user-name {\n        overflow-x: hidden;\n        text-overflow: ellipsis;\n        white-space: nowrap;\n        margin-left: 8px;\n        color: white;\n        font-feature-settings: 'tnum' on, 'lnum' on;\n        font-family: Montserrat Variable, sans-serif;\n        font-weight: 500;\n        line-height: 1.5;\n      }\n    }\n  }\n\n  .amount {\n    align-items: center;\n    display: flex;\n\n    .currency {\n      align-items: center;\n      display: inline-flex;\n      flex-shrink: 0;\n\n      .currency-icon {\n        flex-shrink: 0;\n        margin-right: 0.5em;\n      }\n    }\n\n    .currency-slice,\n    .currency-slice * {\n      max-width: 100%;\n    }\n\n    .currency-slice .currency-balances-wrap {\n      overflow: hidden;\n    }\n\n    .currency-balances-wrap {\n      align-items: center;\n      display: flex;\n\n      .currency-balances {\n        display: flex;\n        flex-direction: column;\n        justify-content: space-between;\n\n        .currency-balances-balance {\n          align-items: center;\n          display: flex;\n          line-height: 18px;\n\n          .font-digits {\n            font-weight: 600;\n            letter-spacing: -0.5px;\n            white-space: nowrap;\n          }\n        }\n      }\n    }\n  }\n\n  .multiplier {\n    text-align: left;\n  }\n\n  .multiplier-item {\n    align-items: center;\n    background: #2a3546;\n    border-radius: 56px;\n    color: #55657e;\n    display: inline-flex;\n\n    font-weight: 600;\n    height: 20px;\n    letter-spacing: -0.28px;\n    line-height: 20px;\n    padding: 0 7px;\n  }\n\n  .multiplier-item-win {\n    color: #1bb83d;\n  }\n\n  .payout {\n    align-items: center;\n    display: flex;\n\n    .currency-slice,\n    .currency-slice * {\n      max-width: 100%;\n    }\n\n    .currency {\n      align-items: center;\n      display: inline-flex;\n      flex-shrink: 0;\n    }\n\n    .currency-icon {\n      flex-shrink: 0;\n      margin-right: 0.5em;\n    }\n\n    .currency-balances-wrap {\n      align-items: center;\n      display: flex;\n\n      .currency-balances {\n        display: flex;\n        flex-direction: column;\n        justify-content: space-between;\n\n        .currency-balances-balance {\n          align-items: center;\n          display: flex;\n          line-height: 18px;\n\n          .font-digits {\n            font-weight: 600;\n            letter-spacing: -0.5px;\n            white-space: nowrap;\n          }\n        }\n      }\n    }\n  }\n}\n\n.element {\n  width: 100%;\n}\n\n.svg-use {\n  width: 22px;\n  height: 22px;\n  margin-left: 4px;\n  color: #55657e;\n}\n\nimg {\n  object-fit: fill;\n}\n\n.animation--start .line {\n  animation: shuffle 0.5s ease-out;\n}\n\n@keyframes shuffle {\n  0% {\n    transform: translateY(-50px);\n  }\n\n  to {\n    transform: translateY(0);\n  }\n}\n.main {\n  height: 100%;\n  overflow-y: auto;\n  font-size: 14px;\n}\n.statistic-header-item-name-0 {\n  max-width: calc(21%);\n  min-width: calc(21%);\n  text-align: left;\n}\n.statistic-header-item-name-1 {\n  max-width: calc(19%);\n  min-width: calc(19%);\n  text-align: center;\n}\n.statistic-header-item-name-2 {\n  max-width: calc(18%);\n  min-width: calc(18%);\n  text-align: center;\n}\n.statistic-header-item-name-3 {\n  max-width: calc(19%);\n  min-width: calc(19%);\n  text-align: center;\n}\n.statistic-header-item-name-4 {\n  max-width: calc(24%);\n  min-width: calc(24%);\n  text-align: right;\n  display: flex;\n  justify-content: center;\n}\n@media only screen and (max-width: 767px) {\n  .main {\n    font-size: 8px;\n  }\n  .currency-icon {\n    display: none;\n  }\n}\n</style>\n"]}]}