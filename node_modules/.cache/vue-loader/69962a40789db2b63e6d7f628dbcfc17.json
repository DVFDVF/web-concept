,"sourcesContent":["<template>\n  <div class=\"modal\" style=\"width: 360px; height: 618px\">\n    <div class=\"modal-container bottom-shadow has-height\">\n      <button\n        title=\"Close modal\"\n        class=\"modal-btn-close\"\n        @click=\"closeFunction()\"\n      >\n        <svg\n          class=\"svg-use\"\n          width=\"24\"\n          height=\"24\"\n          focusable=\"false\"\n          aria-hidden=\"true\"\n        >\n          <use xlink:href=\"/svg/sprite.svg#icon-close\"></use>\n        </svg>\n      </button>\n      <div class=\"default-tab\">\n        <div class=\"tab-inner share-win\">\n          <div class=\"header\">\n            <h1>Share</h1>\n          </div>\n          <div class=\"body\">\n            <div class=\"body-inner\">\n              <div id=\"share-win\" class=\"share-win-info-wrap\">\n                <p class=\"share-win-desc\">\n                  {{\n                    lang.v.sg_god.replace(\n                      '{siteName}',\n                      dataCenter.websiteName_Medium,\n                    )\n                  }}\n                </p>\n                <div class=\"share-win-about\">\n                  <img\n                    class=\"share-win-about-img\"\n                    src=\"@/assets/home/share/info.png\"\n                  />\n                  <div class=\"share-win-about-block\">\n                    <p class=\"share-win-about-desc\">\n                      {{ lang.v.sg_will }} &amp; {{ lang.v.sg_win_comp }}\n                    </p>\n                    <button class=\"link link-sm\">\n                      <span class=\"link-text\"\n                        >{{ lang.v.sg_about }} &amp; {{ lang.v.sg_win }}</span\n                      ><svg\n                        class=\"svg-use link-icon link-icon\"\n                        focusable=\"false\"\n                        aria-hidden=\"true\"\n                      >\n                        <use\n                          xlink:href=\"/svg/sprite.svg#icon-arrow-right-small\"\n                        ></use>\n                      </svg>\n                    </button>\n                  </div>\n                </div>\n              </div>\n              <div class=\"share-win-details-wrap\">\n                <div class=\"share-win-details-label\">\n                  {{ lang.v.sg_message }}\n                  <div\n                    class=\"v-popper v-popper--theme-menu v-popper--theme-dropdown tooltip tooltip-empty share-win-details-tooltip\"\n                    iconsize=\"22\"\n                    showicon=\"true\"\n                    iconcolor=\"#55657e\"\n                  >\n                    <div class=\"icon\">\n                      <svg\n                        class=\"svg-use\"\n                        width=\"22\"\n                        height=\"22\"\n                        focusable=\"false\"\n                        aria-hidden=\"true\"\n                      >\n                        <use xlink:href=\"/svg/sprite.svg#icon-question\"></use>\n                      </svg>\n                    </div>\n                  </div>\n                </div>\n                <div class=\"share-win-details-info\">\n                  {{ lang.v.sg_grys }}\n                </div>\n              </div>\n              <div class=\"share-win-share-card\">\n                <div>\n                  <div class=\"share-card\">\n                    <img\n                      class=\"share-card-bg\"\n                      src=\"@/assets/home/share/bg-dice.jpg\"\n                    />\n                    <div class=\"share-card-wrap\">\n                      <img\n                        class=\"share-card-logo\"\n                        src=\"@/assets/home/share/logo.png\"\n                      />\n                      <h2 class=\"share-card-title share-card-title--xl\">\n                        {{ lang.v.sg_mywin }} 🎉\n                      </h2>\n                      <Image\n                        class=\"share-card-img-type\"\n                        :src=\"gameRecord.gameIcon\"\n                      />\n                      <div class=\"share-card-win\">\n                        <span class=\"currency\"\n                          ><Image\n                            class=\"currency-icon\"\n                            width=\"25\"\n                            height=\"25\"\n                            :src=\"gameRecord.currencyIcon\"\n                            alt=\"FUNFURY\"\n                            loading=\"lazy\" /><span style=\"font-size: 26px\"\n                            ><span style=\"color: #fff\" class=\"font-digits\"\n                              >{{ gameRecord.win.toFixed(2)\n                              }}<span\n                                style=\"color: #ffffff\"\n                              ></span></span></span\n                        ></span>\n                        <p class=\"share-card-currency share-card-currency--3\">\n                          {{ lang.v.sg_fun }}\n                        </p>\n                      </div>\n                      <div class=\"share-card-multi\">\n                        x {{ gameRecord.rate.toFixed(2) }}\n                      </div>\n\n                      <div class=\"share-card-details\">\n                        <div class=\"user share-card-user\">\n                          <div\n                            class=\"user-avatar\"\n                            style=\"width: 18px; height: 18px\"\n                          >\n                            <Avatar :size=\"18\" :user=\"userData\" />\n                          </div>\n\n                          <div class=\"user-wrap\">\n                            <span class=\"user-name\">{{ userData?.name }}</span>\n                          </div>\n                        </div>\n                        <p class=\"share-card-date\">\n                          <span class=\"datetime\">{{ setTime() }}</span>\n                        </p>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n          <div class=\"footer\">\n            <div class=\"share-win-socials\">\n              <div class=\"share-win-social\">\n                <div\n                  class=\"v-popper v-popper--theme-menu v-popper--theme-dropdown tooltip\"\n                  customclass=\"\"\n                  iconsize=\"22\"\n                  showicon=\"true\"\n                  iconcolor=\"#55657e\"\n                >\n                  <button\n                    class=\"button-flat button-flat-md button-flat-grey2 button-flat-center button-flat-fullwidth button-flat-only-icon share-win-btn-disabled\"\n                    type=\"button\"\n                    disabled\n                  >\n                    <span class=\"button-flat-inner\"\n                      ><span class=\"button-flat-icon\"\n                        ><svg\n                          class=\"svg-use\"\n                          focusable=\"false\"\n                          aria-hidden=\"true\"\n                        >\n                          <use\n                            xlink:href=\"/svg/sprite.svg#icon-chat\"\n                          ></use></svg></span\n                    ></span>\n                  </button>\n                </div>\n                <p class=\"share-win-social-label\">\n                  {{ dataCenter.websiteName_Medium }} {{ lang.v.sg_chat }}\n                </p>\n              </div>\n              <div class=\"share-win-social\">\n                <button\n                  class=\"button-3d button-3d-md button-3d-blue button-3d-center button-3d-fullwidth button-3d-only-icon\"\n                  type=\"button\"\n                  @click=\"shareToTwitter()\"\n                >\n                  <span class=\"button-3d-outer\"\n                    ><span class=\"button-3d-inner\"\n                      ><span class=\"button-3d-icon\"\n                        ><svg\n                          class=\"svg-use\"\n                          focusable=\"false\"\n                          aria-hidden=\"true\"\n                        >\n                          <use\n                            xlink:href=\"/svg/sprite.svg#icon-twitter\"\n                          ></use></svg></span></span\n                  ></span>\n                </button>\n                <p class=\"share-win-social-label\">{{ lang.v.sg_twitter }}</p>\n              </div>\n              <div class=\"share-win-social\">\n                <button\n                  class=\"button-3d button-3d-md button-3d-blue button-3d-center button-3d-fullwidth button-3d-only-icon\"\n                  type=\"button\"\n                  @click=\"shareToTelegram()\"\n                >\n                  <span class=\"button-3d-outer\"\n                    ><span class=\"button-3d-inner\"\n                      ><span class=\"button-3d-icon\"\n                        ><svg\n                          class=\"svg-use\"\n                          focusable=\"false\"\n                          aria-hidden=\"true\"\n                        >\n                          <use\n                            xlink:href=\"/svg/sprite.svg#icon-telegram\"\n                          ></use></svg></span></span\n                  ></span>\n                </button>\n                <p class=\"share-win-social-label\">{{ lang.v.sg_telegram }}</p>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport Avatar from '@/components/Avatar.vue';\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { ModalCloseFun } from '@/manager/view';\nimport dayjs from 'dayjs';\nimport { defineComponent, inject } from 'vue';\n\nexport default defineComponent({\n  components: {\n    Avatar,\n  },\n  data() {\n    return {\n      lang,\n      dataCenter,\n      userData: dataCenter.user.data,\n      closeFunction: inject('close') as ModalCloseFun,\n      shareText: '',\n    };\n  },\n  methods: {\n    shareToTelegram() {\n      // 获取当前页面的URL\n      const gameURL = `${window.location.protocol}//${window.location.hostname}/#${this.fullPath}`;\n\n      // 要分享的内容\n      const shareContent = `🎉Haha! Lady luck is on my #${this.gameRecord.gameName}! It was a great win on Between. #bw_share_win`;\n\n      // 拼接分享链接\n      const telegramShareLink = `https://t.me/share/url?url=${encodeURIComponent(\n        gameURL,\n      )}&text=${encodeURIComponent(shareContent)}`;\n      // 打开Telegram分享链接\n      window.open(telegramShareLink, '_blank');\n    },\n    shareToTwitter() {\n      // 获取当前页面的URL\n      const gameURL = `${window.location.protocol}//${window.location.hostname}/#${this.fullPath}`;\n      // 获取要分享的内容\n      const shareContent =\n        gameURL +\n        `🎉Haha! Lady luck is on my #${this.gameRecord.gameName}! It was a great win on Between. #bw_share_win`;\n\n      // 构建Twitter分享链接\n      const twitterShareLink = `https://twitter.com/intent/tweet?text=${encodeURIComponent(\n        shareContent,\n      )}`;\n\n      // 打开Twitter分享链接\n      window.open(twitterShareLink, '_blank');\n    },\n    setTime() {\n      return dayjs(this.gameRecord.createTime).format('YYYY-MM-DD');\n    },\n  },\n  props: ['gameRecord', 'fullPath'],\n});\n</script>\n\n<style lang=\"less\" scoped>\n.share-win-info-wrap {\n  border-bottom: 1px solid rgb(38, 48, 65);\n  margin-bottom: 12px;\n  padding-bottom: 16px;\n}\n.share-win-desc {\n  color: rgb(147, 172, 211);\n  font-size: 12px;\n  font-weight: 500;\n  line-height: 18px;\n  margin-bottom: 12px;\n}\n.share-win-about {\n  background: rgb(28, 37, 50);\n  border-radius: 8px;\n  display: flex;\n  padding: 7px 12px 8px 10px;\n}\n.share-win-about-img {\n  height: 20px;\n  width: 20px;\n}\n.share-win-about-block {\n  margin-left: 6px;\n}\n.share-win-about-desc {\n  font-