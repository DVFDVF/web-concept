ass=\"invite-main\">\n    <div class=\"title\">\n      {{ lang.v.re_da_myReferralCampaign }}\n      <span class=\"label\">{{ lang.v.re_da_default }}</span>\n    </div>\n    <InviteBtn v-for=\"item in list\" :key=\"item.title\" :list=\"item\"></InviteBtn>\n    <!-- 佣金 -->\n    <div class=\"text\">\n      <div class=\"text-left\">\n        <span class=\"text-title\">{{ lang.v.re_da_yourCommissions }}</span>\n        <span class=\"text-value\">16%</span>\n      </div>\n      <div class=\"text-right\">\n        <span class=\"text-title\">{{ lang.v.re_da_sharesForReferrals }}</span>\n        <span class=\"text-value\">0%</span>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { getUserInformation } from '@/views/home/api/user-api/headIndex';\nimport { defineComponent } from 'vue';\nimport InviteBtn from './input/InviteBtn.vue';\nimport { shareUserId } from '@/utils';\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      userId: shareUserId(true),\n      list: [\n        {\n          key: 0,\n          icon: '/svg/sprite.svg#icon-link-gray',\n          get title() {\n            return lang.v.re_da_yourReferralLink;\n          },\n          text:\n            window.location.protocol +\n            '//' +\n            window.location.hostname +\n            '/#/?r=' +\n            this.userId,\n        },\n        {\n          key: 1,\n          icon: '/svg/sprite.svg#icon-promocode',\n          get title() {\n            return lang.v.re_da_yourReferralCode;\n          },\n          text: '',\n        },\n      ],\n    };\n  },\n  methods: {},\n  mounted() {\n    getUserInformation().then((response) => {\n      if (!response) return;\n      this.userId = shareUserId(true);\n      this.list[1].text = this.userId;\n      this.list[0].text =\n        window.location.protocol +\n        '//' +\n        window.location.hostname +\n        '/#/?r=' +\n        this.userId;\n    });\n  },\n  components: { InviteBtn },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.invite-main {\n  padding: 0 16px;\n}\n\n.invite-main .title {\n  align-items: center;\n  color: white;\n  display: flex;\n  font-size: 16px;\n  font-variant-numeric: lining-nums tabular-nums;\n  font-weight: 600;\n  letter-spacing: -0.32px;\n  line-height: 125%;\n  margin: 16px 0 10px;\n}\n\n.invite-main .label {\n  background: #2283f6;\n  border-radius: 8px;\n  color: white;\n  font-size: 9px;\n  font-weight: 800;\n  letter-spacing: 0.36px;\n  line-height: 120%;\n  margin-left: 6px;\n  padding: 2px 6px;\n  text-transform: uppercase;\n}\n\n.invite-main .text {\n  color: #93acd3;\n  display: flex;\n  font-size: 12px;\n  font-style: normal;\n  font-variant-numeric: lining-nums tabular-nums;\n  font-weight: 600;\n  justify-content: space-around;\n  letter-spacing: -0.22px;\n  line-height: 135%;\n  margin: 16px 0 24px;\n  position: relative;\n}\n\n.invite-main .text:before {\n  background: #3c485c;\n  content: '';\n  height: 20px;\n  left: 50%;\n  position: absolute;\n  top: 50%;\n  transform: translate(-50%, -50%);\n  width: 2px;\n  z-index: 2;\n}\n\n.text-title {\n  font-size: 11px;\n  color: #93acd3;\n  margin: 0 6px 0 6px;\n}\n\n.invite-main .text-value {\n  color: #fff;\n  font-size: 14px;\n  font-style: normal;\n  font-variant-numeric: lining-nums tabular-nums;\n  font-weight: 600;\n  letter-spacing: -0.24px;\n  line-height: 130%;\n}\n.text-left,\n.text-right {\n  display: flex;\n  flex-direction: row;\n  align-items: center;\n}\n</style>\n"]}]}