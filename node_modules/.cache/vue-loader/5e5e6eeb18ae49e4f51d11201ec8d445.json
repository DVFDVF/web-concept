lor: #0a75f5;\n}\n\n.lds-dual-ring {\n  display: inline-block;\n}\n\n.lds-dual-ring-after {\n  animation: rotateAnimation 1.2s linear infinite;\n  border: 3px solid #fff;\n  border-radius: 50%;\n  content: ' ';\n  display: block;\n  height: 13px;\n  margin: 1px;\n  width: 13px;\n}\n\n@keyframes rotateAnimation {\n  0% {\n    transform: rotate(0);\n  }\n\n  100% {\n    transform: rotate(1turn);\n  }\n}\n\n:disabled {\n  opacity: 0.8;\n}\n@media only screen and (max-width: 767px) {\n  .btn-bet {\n    width: 103px;\n    height: 77px;\n    padding: 0;\n  }\n}\n</style>\n"]}]}