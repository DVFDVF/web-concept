nt-size: 11px;\n}\n\n.link:focus .link-icon,\n.link:hover .link-icon {\n  fill: #fff;\n  transform: translate(10%);\n}\n\n.link-sm .link-icon {\n  height: 18px;\n  width: 18px;\n}\n\n.balance .card-main {\n  align-items: center;\n  align-self: stretch;\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n  justify-content: center;\n}\n\n.balance .card-main-top {\n  align-items: center;\n  display: flex;\n  flex-direction: column;\n  padding-bottom: 12px;\n  position: relative;\n  width: 100%;\n}\n\n.balance .card-main-top .text {\n  color: #55657e;\n  font-size: 11px;\n  font-variant-numeric: lining-nums tabular-nums;\n  font-weight: 600;\n  letter-spacing: -0.22px;\n  line-height: 135%;\n  margin-bottom: 2px;\n  text-align: center;\n}\n\n.balance .card-main-top .value {\n  color: var(--dark-shades-gray-0, #fff);\n  display: flex;\n  font-size: 26px;\n  font-style: normal;\n  font-variant-numeric: lining-nums tabular-nums;\n  font-weight: 700;\n  gap: 6px;\n  letter-spacing: -0.52px;\n  line-height: 135%;\n  overflow: hidden;\n  text-align: center;\n  text-overflow: ellipsis;\n}\n\n.balance .card-main-top .value {\n  color: var(--dark-shades-gray-0, #fff);\n  display: flex;\n  font-size: 26px;\n  font-style: normal;\n  font-variant-numeric: lining-nums tabular-nums;\n  font-weight: 700;\n  gap: 6px;\n  letter-spacing: -0.52px;\n  line-height: 135%;\n  overflow: hidden;\n  text-align: center;\n  text-overflow: ellipsis;\n}\n\n.main-wrap {\n  grid-gap: 6px;\n  align-items: center;\n  display: grid;\n  grid-template-columns: auto 1fr;\n}\n\n.main-wrap-currency {\n  align-items: center;\n  display: flex;\n}\n\n.currency {\n  align-items: center;\n  display: inline-flex;\n  flex-shrink: 0;\n}\n\n.tooltip {\n  cursor: pointer;\n  display: inline-block;\n  vertical-align: middle;\n}\n\n.currency-body,\n.currency-tooltip-body {\n  align-items: center;\n  display: flex;\n}\n\n.currency span span {\n  font-size: inherit;\n  line-height: inherit;\n}\n\n.balance {\n  fon