VipClub from '@/assets/icon/ico-vip-club.vue';\n\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { defineComponent, markRaw } from 'vue';\nimport { getGameCategory, login } from '../api';\nimport { getGameGroupResp } from '../api/protocol';\nimport { MenuData } from './interface';\nimport MenuList from './widget/menu/MenuList.vue';\nimport NavLine from './widget/NavLine.vue';\nimport { enterSports, openModal } from '@/manager/view';\nimport Login from '../login/Login.vue';\nimport icoGift from '@/assets/icon/ico-receba.vue';\nimport Receba from '../other/Receba.vue';\nimport { jiugongge } from '../other/game/jiugongge';\nimport ReceiveAward from '@/views/redEnvelopeRain/receiveAward.vue';\nimport { reentryReward } from '@/views/reentryReward';\nimport reentryRewardVue from '@/views/reentryReward/index.vue';\nimport IcoOldUser from '@/assets/icon/head/ico-old-user.vue';\nimport IcoCashback from '@/assets/icon/ico-cashback1.vue';\nexport default defineComponent({\n  name: 'home-game-menu',\n  components: {\n    NavLine,\n    MenuList,\n  },\n  data() {\n    return {\n      lang,\n      fold: false,\n      gameCategory: [] as getGameGroupResp[],\n      list: [\n        {\n          key: 'CASINO',\n          get name() {\n            return lang.v.sy_z_casino;\n          },\n          icon: markRaw(IcoCasino),\n          children: [\n            {\n              key: 'Favorites',\n              get name() {\n                return lang.v.sy_z_favorites;\n              },\n              type: 'Favorites',\n              icon: '/svg/sprite.svg#favorites',\n            },\n            {\n              key: 'Recent',\n              get name() {\n                return lang.v.sy_z_recent;\n              },\n              type: 'Recent',\n              icon: '/svg/sprite.svg#recent',\n            },\n          ],\n        },\n        {\n          key: 'SPORT',\n          get name() {\n            return lang.v.sy_z_sport;\n          },\n          icon: markRaw(IcoSports),\n        },\n        {\n          key: 'COIN STAKING',\n          get name() {\n            return lang.v.sy_z_coinStaking;\n          },\n          icon: markRaw(IcoBfoStaking),\n          disenable: true,\n        },\n        {\n          icon: markRaw(IcoPromotion),\n          key: 'Battle',\n          get name() {\n            return lang.v.sy_z_promotions;\n          },\n        },\n      ],\n      list2: [\n        {\n          icon: markRaw(IcoRefeEarn),\n          key: 'Refer & Earn',\n          get name() {\n            return lang.v.sy_z_refer;\n          },\n          extra: dataCenter.conform,\n        },\n        {\n          icon: markRaw(IcoCashback),\n          key: 'cashback',\n          get name() {\n            return lang.v.vi_cashback;\n          },\n        },\n\n        // 每日任务，暂时先隐藏\n        // {\n        //   icon: markRaw(IcoDailyTasks),\n        //   key: 'Daily Tasks',\n        //   get name() {\n        //     return lang.v.sy_z_dailyTask;\n        //   },\n        // },\n        {\n          icon: markRaw(icoGift),\n          key: 'jiugongge',\n          get name() {\n            return lang.v.sy_z_receba;\n          },\n          amount: ` ${dataCenter.conform} 100`,\n          cornerMark: false,\n          get hidden() {\n            return jiugongge.isOverTime;\n          },\n        },\n      ],\n      list3: [\n        {\n          icon: markRaw(IcoOldUser),\n          key: 'oldUser',\n          name: 'reentry rewards',\n          cornerMark: false,\n          get hidden() {\n            return !reentryReward.active;\n          },\n        },\n        {\n          icon: markRaw(IcoVipClub),\n          span: 'VIP',\n          key: 'VIP Club',\n          get name() {\n            return 'VIP';\n          },\n        },\n        {\n          icon: markRaw(IcoRankSystem),\n          key: 'Ranking',\n          get name() {\n            return lang.v.sy_z_rank;\n          },\n        },\n        {\n          icon: markRaw(IcoNews),\n          key: 'News',\n          get name() {\n            return lang.v.sy_z_news;\n          },\n        },\n      ],\n    };\n  },\n  methods: {\n    onClickMenu(item: MenuData) {\n      if (item.key != 'CASINO') {\n        dataCenter.layout.closeLeft();\n      }\n      //进入游戏首页\n      if (item.key === 'CASINO') {\n        this.$router.push('/casino');\n        return;\n      }\n      if (item.key === 'SPORT') {\n        enterSports();\n        return;\n      }\n      //竞赛是否显示\n      if (item.key === 'Battle') {\n        this.$router.push('/CompetitionDetails');\n        dataCenter.layout.closeLeft();\n        dataCenter.layout.toggleCompetition();\n        return;\n      }\n      if (item.key === 'Refer & Earn') {\n        this.$router.push('/referral/dashboard');\n        return;\n      }\n      if (item.key === 'Daily Tasks') {\n        this.$router.push('/daily-tasks');\n        return;\n      }\n      if (item.key === 'Favorites') {\n        if (!dataCenter.user.data) {\n          openModal(Login);\n          return;\n        }\n        this.$router.push('/casino/favorites');\n      }\n      if (item.key === 'Recent') {\n        if (!dataCenter.user.data) {\n          openModal(Login);\n          return;\n        }\n        this.$router.push('/casino/recent');\n        return;\n      }\n      if (item.type === 'game') {\n        this.$router.push(\n          `/casino/originals?groupId=${\n            this.gameCategory[parseInt(item.key)].id\n          }`,\n        );\n        return;\n      }\n      if (item.key === 'jiugongge') {\n        if (dataCenter.user.data) {\n          openModal(Receba);\n        } else {\n          openModal(Login);\n        }\n        return;\n      }\n\n      if (item.key === 'News') {\n        this.$router.push('/LatestNews');\n        return;\n      }\n      if (item.key === 'VIP Club') {\n        this.$router.push('/vip-club');\n        return;\n      }\n      if (item.key === 'cashback') {\n        this.$router.push('/cashback');\n        return;\n      }\n      if (item.key === 'Ranking') {\n        this.$router.push('/ranking');\n        return;\n      }\n      if (item.key === 'oldUser') {\n        openModal(reentryRewardVue, { hideClose: true });\n        return;\n      }\n    },\n  },\n  mounted() {\n    getGameCategory().then((data) => {\n      if (data) {\n        this.gameCategory = data;\n        for (let index = 0; index < data.length; index++) {\n          let item = {\n            key: index.toString(),\n            name: data[index].name.replace(/-/g, ' '),\n            type: 'game',\n            icon: data[index].icon,\n          };\n          this.list[0].children?.push(item);\n        }\n      }\n    });\n  },\n});\n</script>\n\n<style></style>\n"]}]}