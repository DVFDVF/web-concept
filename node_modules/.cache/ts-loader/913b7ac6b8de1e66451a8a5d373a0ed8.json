t.addEventListener('mousemove', this.onDrag);\n      document.addEventListener('mouseup', this.stopDrag);\n      const container = this.$refs.emblaContent as HTMLElement;\n      this.lastX = e.clientX;\n      if (container) {\n        this.emblaContentRect = container.getBoundingClientRect();\n      }\n      this.moveDistance = 0;\n      this.draggings = true;\n      this.startX = this.transX;\n    },\n\n    onDrag(e: MouseEvent) {\n      if (this.draggings) {\n        this.isRoll = true;\n        const percentage = e.clientX - this.lastX + this.startX;\n        // 累积移动距离，超过阈值就不触发点击事件\n        this.moveDistance += Math.abs(percentage);\n\n        if (percentage > this.emblaContentRect.width * 0.4) {\n          this.transX = this.emblaContentRect.width * 0.4;\n        } else if (\n          percentage <\n          -((this.emblaContentRect.width - 32) / 3) *\n            this.advertiseData.length +\n            (this.advertiseData.length - 1) * 16 +\n            this.emblaContentRect.width * 0.6\n        ) {\n          this.transX = -(\n            ((this.emblaContentRect.width - 32) / 3) *\n              this.advertiseData.length +\n            (this.advertiseData.length - 1) * 16 -\n            this.emblaContentRect.width * 0.6\n          );\n        } else {\n          this.transX = percentage;\n        }\n      }\n    },\n\n    stopDrag(e: MouseEvent) {\n      document.removeEventListener('mousemove', this.onDrag);\n      document.removeEventListener('mouseup', this.stopDrag);\n      this.isRoll = false;\n      const percentage = e.clientX - this.lastX + this.startX;\n      if (percentage > 0) {\n        this.transX = 0;\n        //赋值给序号\n        this.ProgressTag = 1;\n        this.ProgressBar = 10;\n      } else if (\n        percentage <\n        -((this.emblaContentRect.width - 32) / 3) * this.advertiseData.length +\n          (this.advertiseData.length - 2) +\n          this.emblaContentRect.width\n      ) {\n        this.transX = -(\n          ((this.emblaContentRect.width - 32) / 3) * this.advertiseData.length +\n          (this.advertiseData.length - 1) * 16 -\n          this.emblaContentRect.width\n        );\n        //赋值给序号\n        this.ProgressTag = this.advertiseData.length - 2;\n        this.ProgressBar = 10;\n      } else {\n        //计算当前序列\n        const result = Math.floor(\n          -(percentage / ((this.emblaContentRect.width - 32) / 3 + 16) - 0.4) +\n            1,\n        );\n\n        //赋值给序号\n        this.ProgressTag = result;\n        this.ProgressBar = 10;\n        //滑动到目标位置\n        this.transX = -(\n          ((this.emblaContentRect.width - 32) / 3) * (this.ProgressTag - 1) +\n          16 * (this.ProgressTag - 1)\n        );\n      }\n\n      this.draggings = false;\n    },\n    //左滑\n    leftSwitch() {\n      if (this.isSmallScreen) {\n        this.leftPhoneModeSwitch();\n        return;\n      }\n      const container = this.$refs.emblaContent as HTMLElement;\n      if (container) {\n        this.emblaContentRect = container.getBoundingClientRect();\n      }\n      if (this.ProgressTag == 1) {\n        this.ProgressTag = this.advertiseData.length - 2;\n      } else {\n        this.ProgressTag -= 1;\n      }\n      this.ProgressBar = 10;\n      if (this.transX >= 0) {\n        this.transX = -(\n          ((this.emblaContentRect.width - 32) / 3) * this.advertiseData.length +\n          (this.advertiseData.length - 1) * 16 -\n          this.emblaContentRect.width\n        );\n      } else {\n        this.transX = -(\n          ((this.emblaContentRect.width - 32) / 3) * (this.ProgressTag - 1) +\n          16 * (this.ProgressTag - 1)\n        );\n      }\n    },\n    //右滑\n    rightSwitch() {\n      if (this.isSmallScreen) {\n        this.rightPhoneModeSwitch();\n        return;\n      }\n      const container = this.$refs.emblaContent as HTMLElement;\n      if (container) {\n        this.emblaContentRect = container.getBoundingClientRect();\n      }\n      if (this.ProgressTag == this.advertiseData.length - 2) {\n        this.ProgressTag = 0;\n      }\n      {\n        this.ProgressTag += 1;\n      }\n      this.ProgressBar = 10;\n      if (\n        this.transX <=\n        -(\n          ((this.emblaContentRect.width - 32) / 3) * this.advertiseData.length +\n          (this.advertiseData.length - 1) * 16 -\n          this.emblaContentRect.width\n        )\n      ) {\n        this.transX = 0;\n      } else {\n        this.transX = -(\n          ((this.emblaContentRect.width - 32) / 3) * (this.ProgressTag - 1) +\n          16 * (this.ProgressTag - 1)\n        );\n      }\n    },\n    //手机模式左滑\n    leftPhoneModeSwitch() {\n      const container = this.$refs.emblaContent as HTMLElement;\n      if (container) {\n        this.emblaContentRect = container.getBoundingClientRect();\n      }\n      if (this.ProgressTag == 1) {\n        this.ProgressTag = this.advertiseData.length;\n      } else {\n        this.ProgressTag -= 1;\n      }\n      this.ProgressBar = 10;\n      if (this.transX >= 0) {\n        this.transX =\n          -this.emblaContentRect.width * (this.advertiseData.length - 1) -\n          this.ProgressTag * 16;\n      } else {\n        this.transX =\n          -this.emblaContentRect.width * (this.ProgressTag - 1) -\n          (this.ProgressTag - 1) * 16;\n      }\n    },\n    //手机模式右滑\n    rightPhoneModeSwitch() {\n      const container = this.$refs.emblaContent as HTMLElement;\n      if (container) {\n        this.emblaContentRect = container.getBoundingClientRect();\n      }\n      if (this.ProgressTag == this.advertiseData.length) {\n        this.ProgressTag = 0;\n      }\n      {\n        this.ProgressTag += 1;\n      }\n      this.ProgressBar = 10;\n      if (\n        this.transX <=\n        -this.emblaContentRect.width * this.advertiseData.length -\n          this.ProgressTag * 16\n      ) {\n        this.transX = 0;\n      } else {\n        this.transX =\n          -this.emblaContentRect.width * (this.ProgressTag - 1) -\n          (this.ProgressTag - 1) * 16;\n      }\n    },\n    //手机触摸事件\n    touchStartDrag(e: TouchEvent) {\n      const container = this.$refs.emblaContent as HTMLElement;\n      this.lastX = e.touches[0].clientX;\n      if (container) {\n        this.emblaContentRect = container.getBoundingClientRect();\n      }\n      this.moveDistance = 0;\n      this.draggings = true;\n      this.startX = this.transX;\n    },\n    touchOnDrag(e: TouchEvent) {\n      if (this.draggings) {\n        const percentage = e.touches[0].clientX - this.lastX + this.startX;\n        // 累积移动距离，超过阈值就不触发点击事件\n        this.moveDistance += Math.abs(percentage);\n\n        if (percentage > this.emblaContentRect.width * 0.4) {\n          this.transX = this.emblaContentRect.width * 0.4;\n        } else if (\n          percentage <\n          -this.emblaContentRect.width * (this.advertiseData.length - 1) -\n            this.ProgressTag * 16 -\n            this.emblaContentRect.width * 0.6\n        ) {\n          this.transX =\n            -this.emblaContentRect.width * (this.advertiseData.length - 1) -\n            this.ProgressTag * 16 -\n            this.emblaContentRect.width * 0.6;\n        } else {\n          this.transX = percentage;\n        }\n      }\n    },\n    touchStopDrag(e: TouchEvent) {\n      const percentage = e.changedTouches[0].clientX - this.lastX + this.startX;\n      if (percentage >= 0) {\n        this.transX = 0;\n        //赋值给序号\n        this.ProgressTag = 1;\n        this.ProgressBar = 10;\n      } else if (\n        percentage <\n        -this.emblaContentRect.width * (this.advertiseData.length - 1) -\n          this.ProgressTag * 16\n      ) {\n        this.transX =\n          -this.emblaContentRect.width * (this.advertiseData.length - 1) -\n          this.ProgressTag * 16;\n        //赋值给序号\n        this.ProgressTag = this.advertiseData.length;\n        this.ProgressBar = 10;\n      } else {\n        //计算移动距离\n        if (percentage - this.startX > this.emblaContentRect.width * 0.3) {\n          this.ProgressTag -= 1;\n        } else if (\n          percentage - this.startX <\n          -this.emblaContentRect.width * 0.3\n        ) {\n          this.ProgressTag += 1;\n        }\n\n        this.ProgressBar = 10;\n        //滑动到目标位置\n        this.transX =\n          -this.emblaContentRect.width * (this.ProgressTag - 1) -\n          (this.ProgressTag - 1) * 16;\n      }\n      this.draggings = false;\n    },\n    dotStyle(item: number) {\n      if (item == this.ProgressTag) {\n        return 'dot active';\n      } else if (item < this.ProgressTag) {\n        return 'dot filled';\n      } else {\n        return 'dot';\n      }\n    },\n    checkScreenSize() {\n      // 根据窗口宽度判断屏幕大小\n      this.isSmallScreen = window.innerWidth < 768; // 你可以根据需要调整阈值\n      if (this.isSmallScreen) {\n        this.dotCount = this.advertiseData.length - 1;\n      } else {\n        this.dotCount = this.advertiseData.length - 2;\n      }\n    },\n  },\n  mounted() {\n    this.timer = setInterval(() => {\n      if (this.ProgressBar == 100) {\n        this.rightSwitch();\n      } else {\n        this.ProgressBar += 1;\n      }\n    }, 100);\n\n    getAdvertiseGames().then((data) => {\n      if (data) {\n        this.advertiseData = data;\n\n        let dotCount = 0;\n        if (this.isSmallScreen) {\n          dotCount = this.advertiseData.length;\n        } else {\n          dotCount = this.advertiseData.length - 2;\n        }\n        if (dotCount < 0) {\n          dotCount = 0;\n        }\n\n        this.dotCount = dotCount;\n      }\n    });\n    // 初始化时判断屏幕大小\n    this.checkScreenSize();\n    // 监听窗口大小变化\n    window.addEventListener('resize', this.checkScreenSize);\n  },\n  beforeUnmount() {\n    // 在组件销毁前移除窗口大小变化的监听器\n    window.removeEventListener('resize', this.checkScreenSize);\n    clearInterval(this.timer);\n  },\n});\n"],"mappings":"AACA,OAAOA,UAAU,MAAM,cAAc;AACrC,SAASC,OAAO,QAAQ,eAAe;AACvC,SAASC,SAAS,QAAQ,gBAAgB;AAC1C,SAASC,iBAAiB,QAAQ,gBAAgB;AAClD,SAASC,eAAe,QAAQ,KAA