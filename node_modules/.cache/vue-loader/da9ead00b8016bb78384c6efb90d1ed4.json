put-inner\"\n    :class=\"{ 'input-inner-money': isInputmoney }\"\n    @click=\"inputFocused\"\n  >\n    <Image\n      v-if=\"showInputIcon\"\n      class=\"curren-icon\"\n      :src=\"showImage\"\n      loading=\"lazy\"\n    />\n    <div class=\"input-tag-wrapper\">\n      <label\n        class=\"input-placeholder\"\n        :class=\"{ 'placeholder-focus': isInputFocused }\"\n        for=\"addressInput\"\n      >\n        <b>{{ EnterText }}</b>\n      </label>\n      <input\n        class=\"input-tag\"\n        autocomplete=\"off\"\n        ref=\"inputRef\"\n        type=\"text\"\n        v-model=\"inputText\"\n        @input=\"$emit('update:value', inputText)\"\n        @blur=\"inputBlurred\"\n        id=\"addressInput\"\n        :style=\"{ top: inputTop }\"\n        :disabled=\"isDisabled\"\n      />\n    </div>\n    <button\n      @click=\"clearInput\"\n      v-if=\"showInput\"\n      type=\"button\"\n      class=\"input-btn-clear\"\n    >\n      <img src=\"@/assets/header/Delect.svg\" />\n    </button>\n    <button\n      class=\"button-flat\"\n      type=\"button\"\n      v-if=\"showButtonFlat\"\n      @click=\"$emit('button-flat-click')\"\n    >\n      <span class=\"button-flat-inner\">\n        <span class=\"button-flat-text\"\n          ><b>{{ showButtonFlatStr }}</b></span\n        >\n      </span>\n    </button>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { notification } from 'ant-design-vue';\nimport { h, watch, ref, defineComponent } from 'vue';\nexport default defineComponent({\n  setup(props) {\n    const inputText = ref(props.inputMoney);\n    watch(\n      () => props.inputMoney,\n      (newValue) => {\n        inputText.value = newValue;\n      },\n    );\n    const openNotification = () => {\n      notification.open({\n        class: 'notifications',\n        duration: 2,\n        placement: 'top',\n        message: h(\n          'div',\n          { class: 'custom-notification-title' },\n          'Notification Title',\n        ),\n        description: h(\n          'div',\n          { class: 'custom-notification-content' },\n          'This is the content of the notification.',\n        ),\n      });\n    };\n    return {\n      openNotification,\n      inputText,\n    };\n  },\n  data() {\n    return {\n      isInputFocused: this.isInputFocuseds,\n      inputTop: this.inputTops,\n    };\n  },\n  computed: {\n    showInput() {\n      if (this.isInputmoney) {\n        return false;\n      } else {\n        return this.inputText.trim().length > 0;\n      }\n    },\n  },\n  props: {\n    showInputIcon: {\n      type: Boolean,\n      default: false, // 默认隐藏--最左边图标\n    },\n    showImage: {\n      type: String,\n      default: '',\n    },\n    showButtonFlat: {\n      type: Boolean,\n      default: false, // 默认隐藏--最右边\"ALL\"按钮\n    },\n    showButtonFlatStr: {\n      type: String,\n      default: 'All', // 默认--\n    },\n    EnterText: {\n      type: String,\n      default: 'Enter Address', // 默认显示--默认文本\n    },\n    inputMoney: {\n      type: String,\n      default: '', // 默认输入文本----默认为空\n    },\n    isInputmoney: {\n      type: Boolean,\n      default: false, // 默认不禁止点击，只展示数据\n    },\n    inputTops: {\n      type: String,\n      default: '0px', // 偏移值\n    },\n    isInputFocuseds: {\n      type: Boolean,\n      default: false, // 选中\n    },\n    isDisabled: {\n      type: Boolean,\n      default: false, // 是否可输入\n    },\n  },\n  methods: {\n    clearInput() {\n      this.inputText = '';\n    },\n    inputFocused() {\n      if (!this.isInputmoney) {\n        (this.$refs.inputRef as HTMLInputElement).focus();\n        this.isInputFocused = true;\n        this.inputTop = '12px';\n      }\n    },\n    inputBlurred() {\n      if (this.inputText.trim().length == 0) {\n        this.isInputFocused = false;\n        this.inputTop = '0px';\n      }\n    },\n    getInputValue() {\n      return this.inputText;\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.input-inner:hover {\n  border: 1px solid #2283f6;\n}\n\n.input-inner * {\n  font-family: Montserrat Variable, sans-serif;\n}\n\n.input-inner-money {\n  font-size: 12px;\n  color: #333;\n  height: 100%;\n  border-radius: 8px;\n  padding: 3px 1px;\n  align-items: center;\n  background-color: #090c1199;\n  border: 1px solid #2a3546;\n  cursor: text;\n  display: flex;\n  position: relative;\n  transition: color 0.1s ease-in-out, border-color 0.1s ease-in-out;\n}\n\n.input-inner {\n  height: 100%;\n  border-radius: 8px;\n  padding: 3px 1px;\n  align-items: center;\n  background-color: #090c1199;\n  border: 1px solid #2a3546;\n  cursor: text;\n  display: flex;\n  position: relative;\n  transition: color 0.1s ease-in-out, border-color 0.1s ease-in-out;\n}\n\n.input-icon {\n  height: 20px;\n  margin-left: -4px;\n  margin-right: 6px;\n  width: 20px;\n  align-items: center;\n  display: flex;\n  justify-content: center;\n  flex-shrink: 0;\n  pointer-events: none;\n}\n\n.input-tag-wrapper {\n  width: 100%;\n  position: relative;\n  margin-bottom: 20px;\n  margin-left: 10px;\n}\n\n.input-placeholder {\n  position: absolute;\n  top: 0;\n  font-size: 14px;\n  left: 0px;\n  transition: all 0.3s;\n  pointer-events: none;\n  color: #555;\n}\n\n.input-tag {\n  font-size: 12px;\n  outline: none;\n  border: none;\n  width: 100%;\n  height: 20px;\n  color: white;\n  margin-left: -1px;\n  background-color: transparent;\n  transition: all 0.3s;\n  position: absolute;\n  top: 0px;\n  font-weight: bold;\n  /* 通过调整此值来移动input */\n}\n\n.placeholder-focus {\n  top: -7px;\n  font-size: 12px;\n  color: #555;\n}\n\n.input-btn-clear {\n  margin-right: 10px;\n  height: 18px;\n  width: 18px;\n  margin-left: auto;\n  padding: 0;\n  background-color: transparent;\n  border: none;\n  cursor: pointer;\n}\n\n.button-flat {\n  width: 45.72px;\n  height: 42px;\n  border-radius: 5px;\n  font-size: 14px;\n  margin: 8px;\n  background-color: #2a3546;\n  border-color: transparent;\n  color: #93acd3;\n}\n\n.curren-icon {\n  width: 17px;\n  height: 17px;\n  margin-left: 10px;\n}\n@media only screen and (max-width: 767px) {\n  .button-flat {\n    width: 80px;\n  }\n  .button-flat-text {\n    font-weight: 500;\n  }\n}\n</style>\n"]}]}