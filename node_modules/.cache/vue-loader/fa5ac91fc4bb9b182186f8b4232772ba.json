s/home/head/notify/Notifications.vue","sourceRoot":"","sourcesContent":["<template>\n  <div :class=\"['content', { notif: true, fold: !layout.isNotifyOpen() }]\">\n    <div class=\"notif-header\">\n      <p class=\"header-title\">{{ lang.v.tz_notifications }}</p>\n      <div class=\"notifications-right\">\n        <div class=\"dropdown-trigger\" @click=\"isUnread = !isUnread\">\n          <span class=\"dropdown-span\">{{\n            selectedOption ? lang.v.tz_viewAll : lang.v.tz_unread\n          }}</span>\n          <img\n            class=\"dropdown-img\"\n            src=\"@/assets/header/MoneyBottom.svg\"\n            :style=\"{ transform: isUnread ? 'rotate(180deg)' : 'none' }\"\n          />\n        </div>\n\n        <button class=\"dropdown-out center\" @click=\"turnOffNotifications\">\n          <svg class=\"svg-use\" focusable=\"false\" aria-hidden=\"true\">\n            <use xlink:href=\"/svg/sprite.svg#icon-close\"></use>\n          </svg>\n        </button>\n      </div>\n    </div>\n    <div v-show=\"isUnread\" class=\"dropdown-md\">\n      <button\n        class=\"dropdown-menu-link\"\n        type=\"button\"\n        @click=\"updateSelectedOption(true)\"\n      >\n        {{ lang.v.tz_viewAll }}\n      </button>\n      <button\n        class=\"dropdown-menu-link\"\n        type=\"button\"\n        @click=\"updateSelectedOption(false)\"\n      >\n        {{ lang.v.tz_unread }}\n      </button>\n    </div>\n\n    <div class=\"notifications-PlatformList\">\n      <div class=\"towns\">\n        <button\n          @click=\"isPlatform = true\"\n          class=\"switch-option center\"\n          :class=\"{ 'open-counter': isPlatform }\"\n        >\n          {{ lang.v.tz_platform }}\n          <div\n            class=\"switch-option-counter center\"\n            :class=\"{ 'seich-expter': isPlatform }\"\n          >\n            {{ readCount() }}\n          </div>\n        </button>\n        <button\n          @click=\"isPlatform = false\"\n          class=\"switch-option center\"\n          :class=\"{ 'open-counter': !isPlatform }\"\n        >\n          {{ lang.v.tz_personal }}\n          <div\n            class=\"switch-option-counter center\"\n            :class=\"{ 'seich-expter': !isPlatform }\"\n          >\n            {{ personalReadCount() }}\n          </div>\n        </button>\n      </div>\n      <div class=\"slide\">\n        <div class=\"dielt\">\n          <Platform\n            v-show=\"isPlatform\"\n            v-for=\"(value, index) in filteredPlatformList\"\n            :PlatformMessage=\"value\"\n            v-bind:key=\"index\"\n            @Platform-click=\"readMessage(value)\"\n          />\n          <Personal\n            v-show=\"!isPlatform\"\n            v-for=\"(value, index) in filteredPersonalList\"\n            v-bind:key=\"index\"\n            :PersonalMessage=\"value\"\n            @Personal-click=\"readMessage(value)\"\n          />\n        </div>\n      </div>\n    </div>\n\n    <div class=\"notifies-footer\">\n      <button class=\"button-grey1\" type=\"button\" @click=\"markAllAsRead()\">\n        <span class=\"button-inner\">\n          <span class=\"button-text\">{{ lang.v.tz_markRead }}</span\n          ><span class=\"button-icon\"></span\n        ></span>\n      </button>\n    </div>\n  </div>\n</template>\n<!-- 通知页面 -->\n<script lang=\"ts\">\nimport { Message, MessageType } from '@/api/protocol';\nimport dataCenter from '@/dataCenter';\nimport { RightSideBarState } from '@/dataCenter/layout';\nimport { lang } from '@/lang';\nimport Personal from '@/views/home/head/notify/widget/Personal.vue';\nimport Platform from '@/views/home/head/notify/widget/Platform.vue';\nimport { defineComponent, ref } from 'vue';\n\nexport default defineComponent({\n  components: {\n    Platform,\n    Personal,\n  },\n  setup() {\n    //切换 Platform\n    const isPlatform = ref(false);\n    //切换 Unread\n    const isUnread = ref(false);\n\n    return {\n      isPlatform,\n      isUnread,\n    };\n  },\n  data() {\n    return {\n      lang,\n      selectedOption: true, // 初始选项\n      layout: dataCenter.layout, //关闭通知页面\n    };\n  },\n  methods: {\n    //Platform 计算未阅读的数\n    readCount() {\n      return dataCenter.siteMessage\n        .getMessageByType(MessageType.PLATFORM)\n        .filter((item) => item.status === 0).length;\n    },\n\n    //全部阅读\n    markAllAsRead() {\n      dataCenter.siteMessage.readAll(\n        this.isPlatform ? MessageType.PLATFORM : MessageType.PERSONAL,\n      );\n    },\n    readMessage(message: Message) {\n      message.status = 1;\n      dataCenter.siteMessage.haveReadMessage(message.id);\n    },\n    //Personal 计算未阅读的数\n    personalReadCount() {\n      return dataCenter.siteMessage\n        .getMessageByType(MessageType.PERSONAL)\n        .filter((item) => item.status === 0).length;\n    },\n    //下拉选择\n    updateSelectedOption(option: boolean) {\n      this.selectedOption = option;\n      this.isUnread = false; // 可选，隐藏下拉菜单\n    },\n\n    //关闭通知页面\n    turnOffNotifications() {\n      dataCenter.layout.closeRight(RightSideBarState.NOTICE);\n    },\n  },\n  computed: {\n    // 获取平台未读信息只包含 status 为 1 的项的计算属性\n    filteredPlatformList() {\n      const messages = dataCenter.siteMessage.getMessageByType(\n        MessageType.PLATFORM,\n      );\n      if (this.selectedOption === false) {\n        return messages.filter((item) => item.status === 0);\n      }\n      return messages;\n    },\n    // 获取个人未读信息 status 为 1 的项的计算属性\n    filteredPersonalList() {\n      const messages = dataCenter.siteMessage.getMessageByType(\n        MessageType.PERSONAL,\n      );\n      if (this.selectedOption === false) {\n        return messages.filter((item) => item.status === 0);\n      }\n      return messages;\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.content {\n  z-index: 20;\n}\n.notif {\n  box-sizing: border-box;\n  overflow: hidden;\n  background-color: #111923;\n  bottom: 0px;\n  position: fixed;\n  right: 0px;\n  top: 0px;\n 