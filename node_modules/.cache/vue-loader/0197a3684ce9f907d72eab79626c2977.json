     <div class=\"icon-btns\">\n          <!-- 暂无功能 -->\n          <!-- <svg\n            class=\"svg-use icon-coin\"\n            width=\"16\"\n            height=\"16\"\n            focusable=\"false\"\n            aria-hidden=\"true\"\n          >\n            <use xlink:href=\"/svg/sprite.svg#icon-coin-drop\"></use>\n          </svg>\n          <svg\n            class=\"svg-use icon-smile\"\n            width=\"16\"\n            height=\"16\"\n            focusable=\"false\"\n            aria-hidden=\"true\"\n          >\n            <use xlink:href=\"/svg/sprite.svg#icon-smile\"></use>\n          </svg> -->\n          <div class=\"send-btn\" @click=\"sendClick()\">\n            <svg\n              class=\"svg-use\"\n              width=\"16\"\n              height=\"16\"\n              focusable=\"false\"\n              aria-hidden=\"true\"\n            >\n              <use xlink:href=\"/svg/sprite.svg#icon-chat-send\"></use>\n            </svg>\n          </div>\n        </div>\n      </div>\n      <div\n        class=\"textarea-wrapper\"\n        v-show=\"(userData?.exp ?? 0) < dataCenter.webConfig.chatAmount\"\n      >\n        <div class=\"banner\">\n          <div class=\"alert\">\n            <svg\n              class=\"alert-icon alert-icon\"\n              width=\"16\"\n              height=\"16\"\n              focusable=\"false\"\n              aria-hidden=\"true\"\n            >\n              <use xlink:href=\"/svg/sprite.svg#icon-chat-mute\"></use>\n            </svg>\n            <div>\n              {{\n                lang.v.chats_acc.replace(\n                  /\\d{6}/,\n                  dataCenter.webConfig.chatAmount.toString(),\n                )\n              }}\n            </div>\n          </div>\n          <div class=\"progress\">\n            <div class=\"progress-data\">\n              <p class=\"progress-value\">\n                <Money :value=\"userData?.exp ?? 0\"></Money>\n                <span class=\"progress-slash\">/</span>\n                {{ dataCenter.webConfig.chatAmount }}\n                <span class=\"progress-currency\">BRL</span>\n              </p>\n              <p class=\"progress-value\">\n                <Money\n                  :value=\"\n                    ((userData?.exp ?? 0) / dataCenter.webConfig.chatAmount) *\n                    100\n                  \"\n                ></Money>\n                %\n              </p>\n            </div>\n            <div class=\"progress-line\">\n              <div\n                class=\"line-fill\"\n                :style=\"{\n                  width:\n                    ((userData?.exp ?? 0) / dataCenter.webConfig.chatAmount) *\n                      100 +\n                    '%',\n                }\"\n              ></div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { ChatData } from '@/dataCenter/chatRoom';\nimport { lang } from '@/lang';\nimport userManager from '@/manager/userManager';\nimport { defineComponent, PropType } from 'vue';\nimport ChatsReply from '../chatsArea/reply/ChatsReply.vue';\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      myMessage: '',\n      input: '',\n      search: '',\n      userData: dataCenter.user.data,\n      dataCenter,\n    };\n  },\n  methods: {\n    sendClick() {\n      this.$emit('sendMessage', this.myMessage);\n      this.myMessage = '';\n    },\n    closeReply() {\n      this.$emit('cancelReply');\n    },\n    handleKeyDown(event: KeyboardEvent) {\n      const isEnterPressed = event.key === 'Enter';\n      // 处理热键触发后的逻辑\n      if (isEnterPressed) {\n        this.sendClick();\n      }\n    },\n  },\n  components: { ChatsReply },\n  props: {\n    msgItem: Object as PropType<ChatData>,\n  },\n  mounted() {\n    window.addEventListener('keydown', this.handleKeyDown);\n  },\n  beforeUnmount() {\n    window.removeEventListener('keydown', this.handleKeyDown);\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.with-shadow {\n  box-shadow: rgb(17, 25, 35) 0px -8px 32px;\n}\n\n.new-chat-footer {\n  background-color: rgb(28, 37, 50);\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  position: relative;\n  transition: all 0.4s ease-in-out 0s;\n  z-index: 4;\n}\n.reply {\n  margin: 10px 15px;\n}\n\n.user-input {\n  background-color: rgb(28, 37, 50);\n  box-shadow: rgba(0, 0, 0, 0.3) 0px 0px 20px;\n  padding: 16px;\n  padding-bottom: 30px;\n}\n\n.textarea-wrapper {\n  align-items: center;\n  display: flex;\n  position: relative;\n  width: 100%;\n}\n\n.banner {\n  font-size: 12px;\n  width: 100%;\n}\n\n.alert {\n  background-color: rgb(42, 53, 70);\n  border-radius: 8px;\n  display: flex;\n  gap: 8px;\n  padding: 8px 10px;\n}\n\n.alert-icon {\n  color: rgb(255, 182, 54);\n}\n\n.progress {\n  margin-top: 10px;\n}\n\n.progress-data {\n  display: flex;\n  justify-content: space-between;\n}\n\n.progress-value {\n  margin: 0px;\n}\n\n.progress-currency,\n.progress-slash {\n  color: rgb(85, 101, 126);\n}\n\n.progress-line {\n  background-color: rgba(0, 0, 0, 0.2);\n  border-radius: 100px;\n  height: 6px;\n  margin-top: 4px;\n  overflow: hidden;\n  position: relative;\n  width: 100%;\n}\n.chatContent {\n  background: #161f2c;\n  width: 100%;\n  display: flex;\n  flex-direction: row;\n  font-size: 12px;\n  padding: 10px;\n  border-radius: 11px;\n}\n.chatContent input {\n  font-weight: 600;\n  flex: 1;\n}\n::placeholder {\n  color: #3c485c;\n  font-weight: 600;\n}\n.icon-btns {\n  display: flex;\n  align-items: center;\n  gap: 7px;\n}\n.send-btn {\n  width: 32px;\n  height: 32px;\n  background: #2283f6;\n  border-radius: 8px;\n  color: white;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n}\n.svg-use {\n  width: 16px;\n  cursor: pointer;\n}\n.icon-coin {\n  color: #6e81a0;\n}\n.icon-smile {\n  color: #55657e;\n}\n.icon-smile:hover {\n  color: white;\n}\n.header-icon-cancle {\n  color: rgb(85, 101, 126);\n  transition: color 0.1s ease-in-out 0s;\n  width: 24px;\n}\n.line-fill {\n  background: rgb(27, 184, 61);\n  height: 100%;\n}\n</style>\n"]}]}