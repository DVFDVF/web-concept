nimport SetupIcon from '@/assets/icon/head/ico-setup.vue';\nimport Switchlanguage from './popup/Switchlanguage.vue';\nimport { lang } from '@/lang';\nimport { IconMapping, getLangeIcon } from '@/lang/localizationLang';\nexport default defineComponent({\n  components: {\n    SetupIcon,\n    Switchlanguage,\n  },\n  data() {\n    return {\n      isKlsible: false,\n      selectedIcon: IconMapping[lang.m],\n    };\n  },\n\n  methods: {\n    togglePopover() {\n      this.isKlsible = false;\n    },\n    updateSelectedIcon(iconName: any) {\n      this.selectedIcon = iconName;\n    },\n    updateSelectedLanguageComponent(languageKey: string) {\n      this.selectedIcon = IconMapping[languageKey];\n    },\n  },\n  mounted() {\n    document.addEventListener('click', this.togglePopover);\n    getLangeIcon();\n  },\n  unmounted() {\n    document.removeEventListener('click', this.togglePopover);\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.settings {\n  background: #253245;\n  border-radius: 9px;\n  box-shadow: 0 2px 3px #0000001f;\n  margin-left: 16px;\n  cursor: pointer;\n  min-width: 74px;\n  padding: 6.5px 7px;\n\n  &-line {\n    background-color: #3c485cb3;\n    height: 23px;\n    margin-left: 9px;\n    margin-right: 7px;\n    width: 1px;\n  }\n\n  .over-icon {\n    height: 23px;\n    width: 23px;\n  }\n\n  .icon {\n    color: #93acd3;\n  }\n\n  .iconColor {\n    color: #2283f6;\n  }\n}\n\n.settings:hover .icon {\n  color: #fff;\n}\n</style>\n"]}]}