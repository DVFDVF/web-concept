  width=\"79\"\n          height=\"80\"\n          loading=\"lazy\"\n        />\n        <div\n          class=\"mid-layer\"\n          style=\"transform: translate3d(-17.5746px, 19.0747px, 0px)\"\n        >\n          <img\n            aria-hidden=\"true\"\n            src=\"@/assets/casino/bomb.webp\"\n            class=\"decoration decoration-center decoration-left\"\n            width=\"63\"\n            height=\"82\"\n            loading=\"lazy\"\n            v-if=\"!dataCenter.layout.isMobile\"\n          />\n          <img\n            aria-hidden=\"true\"\n            src=\"@/assets/casino/cards.webp\"\n            class=\"decoration decoration-center decoration-right\"\n            width=\"94\"\n            height=\"94\"\n            loading=\"lazy\"\n          />\n          <img\n            aria-hidden=\"true\"\n            src=\"@/assets/casino/ball.png\"\n            class=\"decoration\"\n            width=\"73\"\n            height=\"71\"\n            loading=\"lazy\"\n            v-if=\"!dataCenter.layout.isMobile\"\n          />\n        </div>\n        <img\n          aria-hidden=\"true\"\n          src=\"@/assets/casino/monster.webp\"\n          class=\"decoration decoration-bottom decoration-left\"\n          width=\"121\"\n          height=\"123\"\n          loading=\"lazy\"\n        />\n        <img\n          aria-hidden=\"true\"\n          src=\"@/assets/casino/cube.webp\"\n          class=\"decoration decoration-top decoration-right dice\"\n          width=\"80\"\n          height=\"79\"\n          loading=\"lazy\"\n          v-if=\"!dataCenter.layout.isMobile\"\n        />\n        <img\n          aria-hidden=\"true\"\n          src=\"@/assets/casino/cherries.webp\"\n          class=\"decoration decoration-bottom decoration-right\"\n          width=\"108\"\n          height=\"105\"\n          loading=\"lazy\"\n        />\n      </div>\n      <svg class=\"close\" @click=\"closeFunction()\">\n        <use xlink:href=\"/svg/sprite.svg#icon-close\"></use>\n      </svg>\n      <div class=\"header col-middle\">\n        <div class=\"title\">{{ lang.v.dl_jogo }}</div>\n        <div class=\"tips\">{{ lang.v.dl_hoje }}</div>\n        <div class=\"rpt\">{{ lang.v.dl_tem }}<span>99.8RPT</span></div>\n      </div>\n      <div class=\"cols-container\" ref=\"colsContainer\">\n        <div class=\"cols-wrapper\" ref=\"colsWrapper\">\n          <div class=\"col\" v-for=\"(item, index) in chunkedArray\" :key=\"index\">\n            <ul class=\"list\">\n              <li\n                class=\"list-item\"\n                id=\"list-item\"\n                v-for=\"ChildItem in item\"\n                :key=\"ChildItem.id\"\n              >\n                <Image :src=\"ChildItem.icon\" />\n              </li>\n              <li\n                class=\"list-item\"\n                id=\"list-item\"\n                v-for=\"ChildItem in item\"\n                :key=\"ChildItem.id\"\n              >\n                <Image :src=\"ChildItem.icon\" />\n              </li>\n              <li\n                class=\"list-item\"\n                id=\"list-item\"\n                v-for=\"ChildItem in item\"\n                :key=\"ChildItem.id\"\n              >\n                <Image :src=\"ChildItem.icon\" />\n              </li>\n            </ul>\n          </div>\n        </div>\n        <div class=\"win-row\" v-show=\"showWin\" ref=\"winRow\">\n          <li\n            class=\"win-li\"\n            style=\"z-index: 10\"\n            v-for=\"(item, index) in winGame\"\n            :key=\"index\"\n          >\n            <CardWrapper\n              :game-data=\"chunkedArray[index][item]\"\n              class=\"card-wrapper\"\n              @click=\"cardClick(chunkedArray[index][item])\"\n            >\n            </CardWrapper>\n          </li>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { defineComponent, inject } from 'vue';\nimport CardWrapper from '../LobbySections/card/CardWrapper.vue';\nimport { GamesDatas } from '../../api/protocol';\nimport dataCenter from '@/dataCenter';\nimport { ModalCloseFun, enterGame } from '@/manager/view';\nimport { gameLuck } from '.';\nimport { lang } from '@/lang';\n\nexport default defineComponent({\n  data() {\n    return {\n      DecorationsTop: 0,\n      DecorationsDown: 0,\n      chunkedArray: [] as GamesDatas[][],\n      showWin: true,\n      winGame: [] as number[],\n      directionTop: false,\n      gameNumber: 0,\n      dataCenter,\n      rowNumber: 3,\n      gameHeight: 0,\n      gameLuck,\n      closeFunction: inject('close') as ModalCloseFun,\n      lang,\n    };\n  },\n  components: { CardWrapper },\n  mounted() {\n    if (dataCenter.layout.isMobile) {\n      this.rowNumber = 2;\n    }\n    this.hotGame();\n    this.$nextTick(() => {\n      const colsContainer = this.$refs.colsContainer as HTMLElement;\n      const listItem = colsContainer.querySelector('#list-item') as HTMLElement;\n      if (listItem) {\n        this.gameHeight = listItem.getBoundingClientRect().height;\n\n        if (colsContainer) {\n          colsContainer.style.height = `${this.gameHeight * 2 + 8}px`;\n        }\n        const colsWrapper = this.$refs.colsWrapper as HTMLElement;\n        if (colsWrapper) {\n          const col = colsWrapper.querySelectorAll('.col');\n          const colItem = col[1] as HTMLElement;\n          colItem.style.transform = `translateY(${\n            -this.gameHeight * this.gameNumber * 3 + this.gameHeight\n          }px)`;\n          setTimeout(() => {\n            this.start();\n          }, 300);\n        }\n      }\n    });\n  },\n\n  methods: {\n    moveDecorations(event: MouseEvent) {\n      const der = this.$refs.decorations as HTMLElement;\n      this.DecorationsTop = (event.clientX - der.clientWidth / 2) * 0.05;\n      this.DecorationsDown = (event.clientY - der.clientHeight / 2) * 0.05;\n    },\n    hotGame() {\n      if (gameLuck.gameData) {\n        const data = gameLuck.gameData;\n        this.chunkedArray = [];\n        this.gameNumber = Math.floor(data.length / this.rowNumber);\n        for (let i = 0; i < this.rowNumber; i++) {\n          this.chunkedArray.push(\n            data.slice(this.gameNumber * i, this.gameNumber * (i + 1)),\n          );\n        }\n        if (!dataCenter.layout.isMobile) {\n          this.winGame = [0, 0, 0];\n        } else {\n          this.winGame = [0, 0];\n        }\n      }\n    },\n    cardClick(item: GamesDatas) {\n      this.closeFunction(undefined, undefined, undefined, true);\n      if (!dataCenter.layout.isMobile) {\n        enterGame(item);\n      }\n    },\n    start() {\n      // if (this.showWin) return;\n      this.showWin = false;\n      const colsWrapper = this.$refs.colsWrapper as HTMLElement;\n      if (colsWrapper) {\n        const col = colsWrapper.querySelectorAll('.col');\n        let height = this.gameHeight;\n        for (let index = 0; index < this.rowNumber; index++) {\n          const colItem = col[index] as HTMLElement;\n          if (colItem) {\n            //滚动增加的游戏数\n            let addNumber = 0;\n            const random = Math.floor(Math.random() * this.gameNumber);\n            this.directionTop = !this.directionTop;\n            if (\n              (!this.directionTop && random == 0) ||\n              (this.directionTop && random == this.gameNumber - 1)\n            ) {\n              addNumber = this.gameNumber + random - 0.5;\n            } else if (this.directionTop) {\n              addNumber = this.gameNumber * 2 + random - 0.5;\n            } else {\n              addNumber = random - 0.5;\n            }\n            height = -(this.gameHeight + 8) * addNumber;\n            colItem.style.transform = `translateY(${height}px)`;\n            colItem.style.transition =\n              'transform 1.2s cubic-bezier(0, 0, 0, 1)';\n            this.winGame[index] = random;\n          }\n        }\n        setTimeout(() => {\n          this.showWin = true;\n          gameLuck.playGame();\n        }, 1200);\n      }\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.page,\n.page-section {\n  position: relative;\n}\n\n.background {\n  left: 51%;\n  object-fit: cover;\n  opacity: 0.1;\n  pointer-events: none;\n  position: absolute;\n  top: 45%;\n  transform: translate(-50%, -50%);\n  z-index: -1;\n}\n\n.chooser {\n  height: 650px;\n  background: linear-gradient(\n    90deg,\n    rgba(13, 37, 77, 0.67) 6.35%,\n    #223b65 56.66%,\n    rgba(32, 51, 83, 0.32)\n  );\n  border: 1px solid #161f2c;\n  border-radius: 23px;\n  margin-block: 28px;\n  margin-inline: auto;\n  max-width: 648px;\n  padding-inline: 44px;\n  position: relative;\n  width: 100%;\n}\n\nelement.style {\n  transform: translate3d(-17.6289px, 9.03577px, 0px);\n}\n\n.decoration.-chip {\n  left: 15px;\n  top: -35px;\n}\n\ndecoration-left {\n  left: 0;\n}\n\n.decoration-top {\n  top: 0;\n}\n\n.decorations {\n  height: 100%;\n  left: -44px;\n  position: absolute;\n  top: -10px;\n  transform: translateZ(0);\n  transition: transform 1.5s ease-out;\n  width: calc(100% + 88px);\n  z-index: 10;\n}\n\n@keyframes rotateAnimation {\n  0% {\n    transform: rotate(-20deg);\n  }\n\n  100% {\n    transform: rotate(10deg);\n  }\n}\n\n.decoration {\n  animation: rotateAnimation 8s ease-in-out infinite alternate;\n  filter: drop-shadow(5.64846px 6.00149px 5.29543px rgba(7, 6, 31, 0.1))\n    drop-shadow(16.7377px 13.151px 13.151px rgba(0, 0, 0, 0.25));\n  position: absolute;\n  transform: rotate(-20deg);\n  transform-origin: top center;\n}\n\n:where(iframe, img, input, video, select, textarea) {\n  height: auto;\n  max-width: 100%;\n}\n\n:where(audio, canvas, iframe, img, svg, video) {\n  vertical-align: middle;\n}\n\n.mid-layer {\n  height: 94px;\n  position: absolute;\n  top: 45%;\n  transition: transform 0.5s ease-out;\n  width: 100%;\n}\n\n.mid-layer .decoration.-bomb {\n  bottom: 0;\n  left: 0;\n  top: auto;\n}\n\n.mid-layer .decoration.-cards {\n  right: -60px;\n  