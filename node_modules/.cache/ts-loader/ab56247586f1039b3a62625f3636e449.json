rom 'vue';\nimport duration from 'dayjs/plugin/duration';\ndayjs.extend(duration);\nclass Jiugongge {\n  public remainTime = ref('');\n  get isOverTime() {\n    return dayjs().isAfter(\n      dayjs(`${dataCenter?.gridNine?.startTime}Z`).add(3, 'day'),\n    );\n  }\n  private timer = 0;\n  constructor() {\n    this.timer = setInterval(() => {\n      this.updateCountdown();\n    }, 1000);\n  }\n  destroy() {\n    clearInterval(this.timer);\n  }\n  updateCountdown() {\n    const duration = dayjs.duration(\n      dayjs(`${dataCenter?.gridNine?.startTime}Z`).add(3, 'day').diff(dayjs()),\n    );\n\n    this.remainTime.value = `${duration.days()}d ${duration\n      .hours()\n      .toString()\n      .padStart(2, '0')}:${duration\n      .minutes()\n      .toString()\n      .padStart(2, '0')}:${duration.seconds().toString().padStart(2, '0')}`;\n  }\n}\n\nexport function numberAddMotion(\n  callback: (result: string) => void,\n  addAmount?: number,\n): void {\n  const curtAmoun = dataCenter.gridNine?.curtAmount ?? 0;\n  let startNumber = curtAmoun > 40 ? curtAmoun - 40 : 0;\n  let curtAmount = curtAmoun.toFixed(2);\n  if (addAmount != undefined) {\n    startNumber = 0;\n    curtAmount = addAmount.toFixed(2);\n    if (addAmount < 1.5) {\n      const timer = setInterval(function () {\n        if (startNumber < parseFloat(curtAmount)) {\n          startNumber = parseFloat((startNumber + 0.01).toFixed(2));\n          callback((startNumber + 0.01).toFixed(2));\n        } else {\n          callback(startNumber.toFixed(2));\n          clearInterval(timer);\n        }\n      }, 50);\n      return;\n    }\n  }\n  const firstTimer = setInterval(function () {\n    if (\n      Math.floor(startNumber / 10) <\n      Math.floor(parseFloat(curtAmount) / 10) - 1\n    ) {\n      startNumber += 10;\n      callback(startNumber.toFixed(2));\n    } else {\n      clearInterval(firstTimer);\n    }\n  }, 200);\n  const secTimer = setInterval(function () {\n    if (startNumber < parseFloat(curtAmount) - 1) {\n      startNumber += 1;\n      callback(startNumber.toFixed(2));\n    } else {\n      clearInterval(secTimer);\n    }\n  }, 100);\n  const thirdTimer = setInterval(function () {\n    if (startNumber < parseFloat(curtAmount) - 0.2) {\n      startNumber += 0.1;\n      callback(startNumber.toFixed(2));\n    } else {\n      clearInterval(thirdTimer);\n    }\n  }, 50);\n  const timer = setInterval(function () {\n    if (startNumber < parseFloat(curtAmount)) {\n      startNumber = parseFloat((startNumber + 0.01).toFixed(2));\n      callback((startNumber + 0.01).toFixed(2));\n    } else {\n      callback(startNumber.toFixed(2));\n      clearInterval(timer);\n    }\n  }, 20);\n}\n\nexport const jiugongge = reactive(new Jiugongge());\n"],"mappings":"AAAA,OAAOA,UAAU,MAAM,cAAc;AACrC,OAAOC,KAAK,MAAM,OAAO;AACzB,SAASC,QAAQ,EAAEC,GAAG,QAAQ,KAAK;AACnC,OAAOC,QAAQ,MAAM,uBAAuB;AAC5CH,KAAK,CAACI,MAAM,CAACD,QAAQ,CAAC;AACtB,MAAME,SAAS;EAQbC,YAAA;IAPO,KAAAC,UAAU,GAAGL,GAAG,CAAC,EAAE,CAAC;IAMnB,KAAAM,KAAK,GAAG,CAAC;IAEf,IAAI,CAACA,KAAK,GAAGC,WAAW,CAAC,MAAK;MAC5B,IAAI,CAACC,eAAe,EAAE;IACxB,CAAC,EAAE,IAAI,CAAC;EACV;EAVA,IAAIC,UAAUA,CAAA;IAAA,IAAAC,oBAAA;IACZ,OAAOZ,KAAK,EAAE,CAACa,OAAO,CACpBb,KAAK,CAAC,GAAGD,UAAU,aAAVA,UAAU,gBAAAa,oBAAA,GAAVb,UAAU,CAAEe,QAAQ,cAAAF,oBAAA,uBAApBA,oBAAA,CAAsBG,SAAS,GAAG,CAAC,CAACC,GAAG,CAAC,CAAC,EAAE,KAAK,CAAC,CAC3D;EACH;EAOAC,OAAOA,CAAA;IACLC,aAAa,CAAC,IAAI,CAACV,KAAK,CAAC;EAC3B;EACAE,eAAeA,CAAA;IAAA,IAAAS,qBAAA;IACb,MAAMhB,QAAQ,GAAGH,KAAK,CAACG,QAAQ,CAC7BH,KAAK,CAAC,GAAGD,UAAU,aAAVA,UAAU,gBAAAoB,qBAAA,GAAVpB,UAAU,CAAEe,QAAQ,cAAAK,qBAAA,uBAApBA,qBAAA,CAAsBJ,SAAS,GAAG,CAAC,CAACC,GAAG,CAAC,CAAC,EAAE,KAAK,CAAC,CAACI,IAAI,CAACpB,KAAK,EAAE,CAAC,CACzE;IAED,IAAI,CAACO,UAAU,CAACc,KAAK,GAAG,GAAGlB,QAAQ,CAACmB,IAAI,EAAE,KAAKnB,QAAQ,CACpDoB,KAAK,EAAE,CACPC,QAAQ,EAAE,CACVC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC,IAAItB,QAAQ,CAC5BuB,OAAO,EAAE,CACTF,QAAQ,EAAE,CACVC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC,IAAItB,QAAQ,CAACwB,OAAO,EAAE,CAACH,QAAQ,EAAE,CAACC,QAAQ,CAAC,CAAC,EAAE,GAAG,CAAC,E