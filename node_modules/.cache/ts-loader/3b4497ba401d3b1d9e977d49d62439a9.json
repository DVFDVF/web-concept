ript&lang=ts"],"sourcesContent":["\nimport { defineComponent } from 'vue';\nimport Login from '../../login/Login.vue';\nimport { contactRoller } from '../contacRoller';\nexport default defineComponent({\n  props: ['numberList', 'itemList'],\n  computed: {},\n  methods: {\n    selectNumber(text: string) {\n      return this.selectNumberText != text\n        ? 'dropdown-menu-link'\n        : 'dropdown-menu-link dropdown-menu-link-click';\n    },\n    selectBtn(text: string) {\n      this.selectNumberText = text;\n      this.isSpend = false;\n      this.spendIcon = '/svg/sprite.svg#icon-arrow-down';\n      this.$emit('selectNumber', text);\n    },\n    clickBtn(name: string) {\n      if (contactRoller.selectItem == name) return;\n      contactRoller.selectItem = name;\n      let selectType = '';\n      if (contactRoller.selectItem == 'My Bets') {\n        contactRoller.selectType = '4';\n      } else {\n        if (contactRoller.selectItem == 'High Rollers') {\n          contactRoller.selectType = '3';\n        }\n        if (contactRoller.selectItem == 'Rare Wins') {\n          contactRoller.selectType = '1';\n        }\n        if (contactRoller.selectItem == 'All Bets') {\n          contactRoller.selectType = '1';\n        }\n      }\n      if (contactRoller.selectItem == 'High Rollers') {\n        contactRoller.selectType = '3';\n      } else if (contactRoller.selectItem == 'Sports Bets') {\n        contactRoller.selectType = '3';\n      } else if (contactRoller.selectItem == 'Casino Bets') {\n        contactRoller.selectType = '1';\n      } else if (contactRoller.selectItem == 'Staking Payouts') {\n        selectType = '3';\n        contactRoller.selectType = '3';\n      }\n      // this.$emit('childEvent', this.selectItem, selectType);\n    },\n    showColor(name: string) {\n      return contactRoller.selectItem == name\n        ? 'switcher-label select-lable'\n        : 'switcher-label';\n    },\n    dropdownExpand() {\n      this.isSpend = !this.isSpend;\n      if (this.isSpend) {\n        this.spendIcon = '/svg/sprite.svg#icon-arrow-up';\n      } else {\n        this.spendIcon = '/svg/sprite.svg#icon-arrow-down';\n      }\n    },\n  },\n  mounted() {\n    this.clickBtn(contactRoller.selectItem);\n  },\n  data() {\n    return {\n      isSpend: false,\n      spendIcon: '/svg/sprite.svg#icon-arrow-down',\n      selectItem: contactRoller.selectItem,\n      selectNumberText: '10',\n    };\n  },\n  components: { Login },\n});\n"],"mappings":"AACA,SAASA,eAAe,QAAQ,KAAK;AACrC,OAAOC,KAAK,MAAM,uBAAuB;AACzC,SAASC,aAAa,QAAQ,iBAAiB;AAC/C,eAAeF,eAAe,CAAC;EAC7BG,KAAK,EAAE,CAAC,YAAY,EAAE,UAAU,CAAC;EACjCC,QAAQ,EAAE,EAAE;EACZC,OAAO,EAAE;IACPC,YAAYA,CAACC,IAAY;MACvB,OAAO,IAAI,CAACC,gBAAgB,IAAID,IAAI,GAChC,oBAAoB,GACpB,6CAA6C;IACnD,CAAC;IACDE,SAASA,CAACF,IAAY;MACpB,IAAI,CAACC,gBAAgB,GAAGD,IAAI;MAC5B,IAAI,CAACG,OAAO,GAAG,KAAK;MACpB,IAAI,CAACC,SAAS,GAAG,iCAAiC;MAClD,IAAI,CAACC,KAAK,CAAC,cAAc,EAAEL,IAAI,CAAC;IAClC,CAAC;IACDM,QAAQA,CAACC,IAAY;MACnB,IAAIZ,aAAa,CAACa,UAAU,IAAID,IAAI,EAAE;MACtCZ,aAAa,CAACa,UAAU,GAAGD,IAAI;MAC/B,IAAIE,UAAU,GAAG,EAAE;MACnB,IAAId,aAAa,CAACa,UAAU,IAAI,SAAS,EAAE;QACzCb,aAAa,CAACc,UAAU,GAAG,GAAG;OAC/B,MAAM;QACL,IAAId,aAAa,CAACa,UAAU,IAAI,cAAc,EAAE;UAC9Cb,aAAa,CAACc,UAAU,GAAG,GAAG;;QAEhC,IAAId,aAAa,CAACa,UAAU,IAAI,WAAW,EAAE;UAC3Cb,aAAa,CAACc,UAAU,GAAG,GAAG;;QAEhC,IAAId,aAAa,CAACa,UAAU,IAAI,UAAU,EAAE;UAC1Cb,aAAa,CAACc,UAAU,GAAG,GAAG;;;MAGlC,IAAId,aAAa,CAACa,UAAU,IAAI,cAAc,EAAE;QAC9Cb,aAAa,CAACc,UAAU,GAAG,GAAG;OAC/B,MAAM,IAAId,aAAa,CAACa,UAAU,IAAI,aAAa,EAAE;QACpDb,aAAa,CAACc,UAAU,GAAG,GAAG;OAC/B,MAAM,IAAId,aAAa,CAACa,UAAU,IAAI,aAAa,EAAE;QACpDb,aAAa,CAACc,UAAU,GAAG,GAAG;OAC/B,MAAM,IAAId,aAAa,CAACa,UAAU,IAAI,iBAAiB,EAAE;QACxDC,UAAU,GAAG,GAAG;QAChBd,aAAa,CAACc,UAAU,GAAG,GAAG;;MAEhC;IACF,CAAC;;IACDC,SAASA,CAACH,IAAY;MACpB,OAAOZ,aAAa,CAACa,UAAU,IAAID,IAAI,GACnC,6BAA6B,GAC7B,gBAAgB;IACtB,CAAC;IACDI,cAAcA,CAAA;MACZ,IAAI,CAACR,OAAO,GAAG,CAAC,IAAI,CAACA,OAAO;MAC5B,IAAI,IAAI,CAACA,OAAO,EAAE;QAChB,IAAI,CAACC,SAAS,GAAG,+BAA+B;OACjD,MAA