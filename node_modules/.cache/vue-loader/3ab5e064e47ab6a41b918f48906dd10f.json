     <div class=\"tab\">\n                <div class=\"carousel\">\n                  <div class=\"carousel-outer\">\n                    <div\n                      class=\"carousel-inner\"\n                      :style=\"{\n                        transform: 'translateX(' + transX + '%)',\n                        width: '200%',\n                      }\"\n                    >\n                      <img\n                        loading=\"lazy\"\n                        v-for=\"item in dataRules\"\n                        :key=\"item.text\"\n                        :src=\"item.icon\"\n                        class=\"carousel-img\"\n                        style=\"width: 50%\"\n                      />\n                      <img\n                        loading=\"lazy\"\n                        src=\"@/assets/dice/step-2.webp\"\n                        class=\"carousel-img\"\n                        style=\"width: 50%\"\n                      />\n                    </div>\n                  </div>\n                  <div class=\"carousel-dots\">\n                    <div\n                      :class=\"['carousel-dot', { active: transX == 0 }]\"\n                      @click=\"selectDotLeft()\"\n                    ></div>\n                    <div\n                      :class=\"['carousel-dot', { active: transX == -50 }]\"\n                      @click=\"selectDotRight()\"\n                    ></div>\n                  </div>\n                </div>\n                <p class=\"modal-ruleslimits-desc\">\n                  {{ transX == 0 ? dataRules[0].text : dataRules[1].text }}\n                </p>\n                <IntroductionBtn\n                  :btn-text=\"lang.v[btnText]\"\n                  @click=\"closeBtn\"\n                ></IntroductionBtn>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { defineComponent, inject } from 'vue';\nimport IntroductionBtn from './IntroductionBtn.vue';\nimport { ModalCloseFun } from '@/manager/view';\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      closeFunction: inject('close') as ModalCloseFun,\n      transX: 0,\n      btnText: 'dice_ru_next',\n      dataRules: dataCenter.gameConfiguration.ruleData,\n    };\n  },\n  methods: {\n    topClick() {\n      this.closeFunction?.();\n    },\n    closeBtn() {\n      if (this.transX == 0) {\n        this.transX = -50;\n        this.btnText = 'dice_ru_play';\n      } else {\n        this.closeFunction?.();\n      }\n    },\n    selectDotLeft() {\n      this.transX = 0;\n      this.btnText = 'dice_ru_next';\n    },\n    selectDotRight() {\n      this.transX = -50;\n      this.btnText = 'dice_ru_play';\n    },\n  },\n  components: { IntroductionBtn },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.header {\n  border-bottom: 1px solid transparent;\n  padding: 20px 24px 0px;\n  position: relative;\n  transition: background-color 0.3s, border-bottom-color 0.3s;\n\n  .header-title {\n    align-items: center;\n    display: flex;\n  }\n}\n\n.body {\n  flex: 1 1;\n  min-height: 200px;\n  position: relative;\n\n  .body-inner {\n    padding: 0 24px 20px;\n    display: flex;\n    flex-direction: column;\n\n    .tabs-lg.tabs {\n      padding: 6px;\n    }\n\n    .tabs-fullwidth {\n      width: 100%;\n    }\n\n    .tabs {\n      align-items: stretch;\n      background: rgba(55, 70, 92, 0.3);\n      border-radius: 10px;\n      display: inline-flex;\n      color: #55657e;\n    }\n\n    .tab-lg.tab {\n      height: 40px;\n    }\n\n    .tab-lg .tab-label {\n      font-size: 14px;\n      line-height: 18px;\n    }\n\n    .tab-label {\n      font-weight: 600;\n      letter-spacing: -0.28px;\n      margin: 0;\n      text-align: center;\n      transition: color 0.1s ease-in-out;\n      white-space: nowrap;\n    }\n\n    .tab-active {\n      background-color: #2283f6;\n      color: white;\n    }\n  }\n}\n\n.tab {\n  align-items: center;\n  border-radius: 8px;\n  justify-content: center;\n  margin-right: 4px;\n  transition: background-color 0.1s ease-in-out;\n  flex: 1 1;\n\n  .carousel {\n    display: block;\n    margin-bottom: 35px;\n    position: relative;\n    width: 100%;\n    margin-top: 20px;\n\n    .carousel-outer {\n      background-color: #1c2532;\n      border-radius: 14px;\n      overflow: hidden;\n      position: relative;\n\n      .carousel-inner {\n        box-sizing: border-box;\n        display: flex;\n        transition: all 0.3s;\n\n        .carousel img {\n          height: 100%;\n        }\n      }\n    }\n\n    .carousel-dots {\n      bottom: -24px;\n      display: flex;\n      justify-content: center;\n      position: absolute;\n      width: 100%;\n\n      .carousel-dot {\n        background-color: #5b6773;\n        border-radius: 50%;\n        cursor: pointer;\n        margin: 0 4px;\n        padding: 6px;\n        transition: all 0.3s;\n      }\n\n      .carousel-dot.active {\n        background-color: #297fe5;\n      }\n    }\n  }\n}\n\n.default-tab .header:before {\n  background: linear-gradient(180deg, #003370, #08264a 40%, #111923);\n  bottom: 0;\n  content: '';\n  display: block;\n  left: 0;\n  position: absolute;\n  right: 0;\n  top: 0;\n  transition: bottom 0.3s;\n  z-index: -1;\n}\n.modal {\n  max-width: 530px;\n  height: 600px;\n}\n@media only screen and (max-width: 767px) {\n}\n</style>\n"]}]}