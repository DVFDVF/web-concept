AAC;QAC1B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACR,CAAC,CAAC,CAAC,CAAC,CAAC;IACP,CAAC,CAAC,CAAC,CAAC,CAAC;EACP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/concept/web3-concept/src/views/home/head/popup/MyCurrencyPop.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"mcpv\" @click.stop>\n    <div\n      v-show=\"inputHidden\"\n      class=\"input-alst input-inner\"\n      :class=\"{ 'input-focused': isFocused }\"\n    >\n      <span class=\"input-icon center\">\n        <HSearchIcon />\n      </span>\n      <div class=\"input-tag-wrapper\">\n        <input\n          class=\"input-tag\"\n          placeholder=\"Search\"\n          autocomplete=\"off\"\n          tabindex=\"1\"\n          v-model=\"searchText\"\n          @input=\"toggleClearButton\"\n          @focus=\"isFocused = true\"\n          @blur=\"isFocused = false\"\n        />\n      </div>\n      <button\n        type=\"button\"\n        class=\"input-btn-clear\"\n        @click=\"clearInput\"\n        v-show=\"showClearButton\"\n      >\n        <svg\n          class=\"svg-use\"\n          width=\"20\"\n          height=\"20\"\n          focusable=\"false\"\n          aria-hidden=\"true\"\n        >\n          <use xlink:href=\"/svg/sprite.svg#icon-close\"></use>\n        </svg>\n      </button>\n    </div>\n    <div class=\"mcpv-lives\">\n      <!-- 后台不好修改,目前只要一种货币（BRL） -->\n      <div\n        class=\"mcpv-list\"\n        v-for=\"(item, index) in filteredList.slice(0, 1)\"\n        :key=\"index\"\n        :class=\"{\n          'first-item': index === 0,\n          'selected-item': 0 === index,\n        }\"\n        @click=\"selectItem(index)\"\n      >\n        <Image :src=\"item.icon\" alt=\"Icon\" />\n        <span class=\"name\">{{ item.name }}</span>\n        <span class=\"extra\">{{ display(item) }}</span>\n      </div>\n    </div>\n    <div class=\"mcpv-metn\">\n      <div class=\"toggle\" v-for=\"(item, index) in myToggleButtons\" :key=\"index\">\n        <div class=\"toggle-label\">\n          <span class=\"toggle-title\">{{ lang.v[dynamicTexts[index]] }}</span>\n          <button class=\"fiat center\" v-show=\"index == 1\">\n            <HSearchIcon class=\"fiat-icon\" />\n            <p class=\"fiat-currency-name\">usd</p>\n          </button>\n        </div>\n        <div\n          class=\"toggle-btn\"\n          @click=\"toggleToggleButton(index)\"\n          :class=\"{ active: item }\"\n        ></div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { Currency } from '@/api/protocol';\nimport dataCenter from '@/dataCenter';\nimport { displayBalance } from '@/utils';\nimport HSearchIcon from '@/assets/icon/head/ico-search.vue';\nimport { computed, defineComponent, nextTick, PropType, ref } from 'vue';\nimport { lang } from '@/lang';\nimport { extenalOfficial } from '@/utils/currency';\n\nexport default defineComponent({\n  components: {\n    HSearchIcon,\n  },\n  data() {\n    return {\n      lang,\n    };\n  },\n  props: {\n    toggleButtons: {\n      type: Array as PropType<boolean[]>,\n      required: true,\n    },\n    dynamicTexts: {\n      type: Array as PropType<string[]>,\n      default: () => [],\n    },\n    inputHidden: {\n      type: Boolean as PropType<boolean>,\n      required: true,\n    },\n    excludeBRL: {\n      type: Boolean as PropType<boolean>,\n      default: false, // 设置默认值为false\n    },\n  },\n  setup(props, { emit }) {\n    const searchText = ref('');\n    const showClearButton = ref(false);\n    const isFocused = ref(false);\n    const list = ref<Currency[]>([]);\n    const balance = dataCenter.wallet.map;\n    const showUsdt = ref(false);\n    list.value = dataCenter.webConfig.cryptoCurrencies.concat();\n\n    nextTick(() => {\n      const currency = extenalOfficial(list.value, props.excludeBRL);\n      if (currency[0]) {\n        emit('change', currency[0]);\n      }\n    });\n\n    /**自动显示清除按钮 */\n    const toggleClearButton = () => {\n      showClearButton.value = searchText.value.length > 0;\n    };\n    const clearInput = () => {\n      searchText.value = '';\n    };\n\n    // 搜索\n    const filteredList = computed(() => {\n      // 检查 searchText 是否为空或只包含空白字符\n      if (!searchText.value || searchText.value.trim() === '') {\n        return extenalOfficial(list.value, props.excludeBRL);\n      }\n\n      // 根据 searchText 过滤列表,slice 主要去除usdt\n      let filtered = list.value.slice(0, 1).filter((item) => {\n        return item.name\n          .toLowerCase()\n          .includes(searchText.value.toLowerCase().trim());\n      });\n      // 如果 excludeBRL 为 true，则过滤掉含有 BRL 的数据\n      filtered = extenalOfficial(filtered, props.excludeBRL);\n      return filtered;\n    });\n\n    const buttonStates = ref(props.toggleButtons); // Renamed to avoid conflicts\n    /**底部的3个切换按钮 */\n    const toggleToggleButton = (index: number) => {\n      buttonStates.value[index] = !buttonStates.value[index];\n      if (index === 0) {\n        // 隐藏金额为0的\n        const currencies = dataCenter.webConfig.cryptoCurrencies;\n        if (currencies) {\n          if (buttonStates.value[index]) {\n            list.value = currencies.filter((item) => {\n              return balance[item.id]?.value;\n            });\n            if (list.value.length < 1) {\n              list.value = [currencies[0]];\n            }\n          } else {\n            list.value = currencies;\n          }\n        } else {\n          console.log('货币列表为空');\n        }\n      }\n      // todo index===1 Show vault balance\n      if (index === 1) {\n        // 显示汇率\n        showUsdt.value = !showUsdt.value;\n        emit('useUsdt', showUsdt.value);\n      }\n    };\n\n    const selectItem = (index: number) => {\n      // 检查索引是否有效\n      if (index < 0 || index >= filteredList.value.length) return;\n\n      // 获取被选中的项\n      const selectedItem = filteredList.value[index];\n      // 使用 emit 发送选中的数据项\n      emit('change', selectedItem);\n    };\n    function display(currency: Currency) {\n      return displayBalance(currency, balance, showUsdt.value);\n    }\n    // 如果 excludeBRL 为 true，则过滤掉含有 BRL 的数据\n\n    return {\n      searchText,\n      showClearButton,\n      isFocused,\n      myToggleButtons: buttonStates,\n      list,\n      balance,\n      filteredList,\n      selectItem,\n      toggleClearButton,\n      clearInput,\n      toggleToggleButton,\n      display,\n    };\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.mcpv {\n  background: #1b2636;\n  box-shadow: 0 4px 26px #0d131c78;\n  padding: 10px 12px;\n  border-radius: 10px;\n  position: absolute;\n  top: 50px;\n  width: 100%;\n  flex-direction: column;\n\n  /* 垂直方向的列布局 */\n\n  &-lives {\n    max-height: 300px;\n    margin-right: -12px;\n    overflow: auto;\n    overflow-x: hidden;\n\n    &::-webkit-scrollbar {\n      width: 6px;\n      height: 6px;\n    }\n\n    &::-webkit-scrollbar-track {\n      background-color: #1b2636;\n      border-radius: 2px;\n    }\n\n    &::-webkit-scrollbar-thumb {\n      background-color: #2a3546;\n      border-radius: 2px;\n    }\n  }\n\n  &-limt {\n    margin-right: 6px;\n  }\n\n  &-metn {\n    background-color: #202a39;\n    margin: 0 -10px;\n    padding: 14px 20px 14px 28px;\n  }\n}\n\n.input-alst {\n  height: 44px;\n  border-radius: 8px;\n  padding: 3px 12px;\n  align-items: center;\n  background-color: #090c1199;\n  cursor: text;\n  display: flex;\n}\n\n.input-inner {\n  border: 1px solid #2a3546;\n\n  &.input-focused {\n    border: 1px solid #2283f6;\n    /* 输入框获取焦点后的边框颜色 */\n  }\n}\n\n.input-inner:hover {\n  border: 1px solid #2283f6;\n}\n\n.input-icon {\n  height: 20px;\n  margin-left: -2px;\n  margin-right: 6px;\n  width: 20px;\n}\n\n.input-tag-wrapper {\n  flex: 1 1;\n}\n\n.input-tag {\n  padding: 0;\n  background-color: transparent;\n  outline: none;\n  width: 100%;\n  color: #fff;\n  border: none;\n  font-size: 14px;\n  letter-spacing: -0.24px;\n  font-family: Montserrat Variable, sans-serif;\n  font-weight: 600;\n}\n\n.input-btn-clear {\n  margin-right: -5px;\n  height: 18px;\n  width: 18px;\n  margin-left: 4px;\n  padding: 0;\n  background-color: transparent;\n  border: none;\n  cursor: pointer;\n}\n\n.toggle {\n  align-items: center;\n  display: flex;\n  border: none;\n  color: #93acd3;\n  cursor: pointer;\n\n  padding: 0;\n  transition: color 0.1s ease-in-out;\n  width: 100%;\n\n  margin-bottom: 10px;\n\n  &-label {\n    flex-grow: 1;\n    align-items: center;\n    display: flex;\n    font-size: 12px;\n    font-weight: 600;\n    gap: 8px;\n    letter-spacing: -0.02em;\n    line-height: 20px;\n  }\n}\n\n.toggle-btn {\n  background: #3c485c;\n  border-radius: 10px;\n  height: 20px;\n  margin-left: 0.5em;\n  padding: 3px;\n  position: relative;\n  width: 34px;\n\n  &::before {\n    background-color: #93acd3;\n    border-radius: 50%;\n    content: '';\n    display: block;\n    height: 14px;\n    position: absolute;\n    top: 50%;\n    transform: translate(0%, -50%);\n    transition: background-color 0.1s ease-in-out, transform 0.1s ease-in-out;\n    width: 14px;\n  }\n\n  &.active {\n    background: #1bb83d;\n\n    &::before {\n      background-color: #fff;\n      transform: translate(100%, -50%);\n    }\n  }\n}\n\n.fiat {\n  background: #2a3546;\n  border-radius: 6px;\n  gap: 2px;\n  height: 28px;\n  margin-left: auto;\n  padding: 0 14px 0 10px;\n  border: 1px solid transparent;\n  cursor: pointer;\n}\n\n.fiat-icon {\n  width: 16px;\n  height: 16px;\n}\n\n.fiat-currency-name {\n  color: #93acd3;\n  margin: 0;\n  text-transform: uppercase;\n}\n\n//\n.first-item {\n  background-color: #293546;\n}\n.mcpv-lives {\n  max-height: 200px;\n}\n.mcpv-list {\n  border-radius: 8px;\n  border-left: none;\n  color: #93acd3;\n  height: 50px;\n  margin-right: 6px;\n  padding: 0 8px;\n  display: flex;\n  align-items: center;\n  cursor: pointer;\n  margin-top: 8px;\n  margin-bottom: 8px;\n}\n\n.mcpv-list :hover {\n  color: white;\n}\n\n.mcpv-list img {\n  width: 24px;\n  height: 24px;\n  margin-right: 7px;\n}\n\nspan {\n  font-family: Montserrat Variable, sans-serif;\n  font-size: 14px;\n  font-weight: 700;\n  line-height: 1.5;\n}\n\n.name {\n  font-size: 16px;\n  margin-right: auto;\n  font-weight: bold;\n}\n\n.svg-use:hover {\n  fill: #93acd3;\n}\n\n.extra {\n  color: white;\n}\n@media only screen and (max-width: 767px) {\n  // .mcpv-list {\n  //   width: 100%;\n  // }\n  .mcpv {\n    left: 10px;\n    right: 10px;\n    width: calc(100% - 20px);\n  }\n}\n</style>\n"]}]}