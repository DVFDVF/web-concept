lientY","clientHeight","gameData","Math","floor","length","i","push","slice","cardClick","item","undefined","index","addNumber","random","transition","playGame"],"sources":["/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/cache-loader/dist/cjs.js??ref--1-0!/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/vue-loader-v16/dist/index.js??ref--1-1!/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/casino/drawLottery/DrawLottery.vue?vue&type=script&lang=ts"],"sourcesContent":["\nimport { defineComponent, inject } from 'vue';\nimport CardWrapper from '../LobbySections/card/CardWrapper.vue';\nimport { GamesDatas } from '../../api/protocol';\nimport dataCenter from '@/dataCenter';\nimport { ModalCloseFun, enterGame } from '@/manager/view';\nimport { gameLuck } from '.';\nimport { lang } from '@/lang';\n\nexport default defineComponent({\n  data() {\n    return {\n      DecorationsTop: 0,\n      DecorationsDown: 0,\n      chunkedArray: [] as GamesDatas[][],\n      showWin: true,\n      winGame: [] as number[],\n      directionTop: false,\n      gameNumber: 0,\n      dataCenter,\n      rowNumber: 3,\n      gameHeight: 0,\n      gameLuck,\n      closeFunction: inject('close') as ModalCloseFun,\n      lang,\n    };\n  },\n  components: { CardWrapper },\n  mounted() {\n    if (dataCenter.layout.isMobile) {\n      this.rowNumber = 2;\n    }\n    this.hotGame();\n    this.$nextTick(() => {\n      const colsContainer = this.$refs.colsContainer as HTMLElement;\n      const listItem = colsContainer.querySelector('#list-item') as HTMLElement;\n      if (listItem) {\n        this.gameHeight = listItem.getBoundingClientRect().height;\n\n        if (colsContainer) {\n          colsContainer.style.height = `${this.gameHeight * 2 + 8}px`;\n        }\n        const colsWrapper = this.$refs.colsWrapper as HTMLElement;\n        if (colsWrapper) {\n          const col = colsWrapper.querySelectorAll('.col');\n          const colItem = col[1] as HTMLElement;\n          colItem.style.transform = `translateY(${\n            -this.gameHeight * this.gameNumber * 3 + this.gameHeight\n          }px)`;\n          setTimeout(() => {\n            this.start();\n          }, 300);\n        }\n      }\n    });\n  },\n\n  methods: {\n    moveDecorations(event: MouseEvent) {\n      const der = this.$refs.decorations as HTMLElement;\n      this.DecorationsTop = (event.clientX - der.clientWidth / 2) * 0.05;\n      this.DecorationsDown = (event.clientY - der.clientHeight / 2) * 0.05;\n    },\n    hotGame() {\n      if (gameLuck.gameData) {\n        const data = gameLuck.gameData;\n        this.chunkedArray = [];\n        this.gameNumber = Math.floor(data.length / this.rowNumber);\n        for (let i = 0; i < this.rowNumber; i++) {\n          this.chunkedArray.push(\n            data.slice(this.gameNumber * i, this.gameNumber * (i + 1)),\n          );\n        }\n        if (!dataCenter.layout.isMobile) {\n          this.winGame = [0, 0, 0];\n        } else {\n          this.winGame = [0, 0];\n        }\n      }\n    },\n    cardClick(item: GamesDatas) {\n      this.closeFunction(undefined, undefined, undefined, true);\n      if (!dataCenter.layout.isMobile) {\n        enterGame(item);\n      }\n    },\n    start() {\n      // if (this.showWin) return;\n      this.showWin = false;\n      const colsWrapper = this.$refs.colsWrapper as HTMLElement;\n      if (colsWrapper) {\n        const col = colsWrapper.querySelectorAll('.col');\n        let height = this.gameHeight;\n        for (let index = 0; index < this.rowNumber; index++) {\n          const colItem = col[index] as HTMLElement;\n          if (colItem) {\n            //滚动增加的游戏数\n            let addNumber = 0;\n            const random = Math.floor(Math.random() * this.gameNumber);\n            this.directionTop = !this.directionTop;\n            if (\n              (!this.directionTop && random == 0) ||\n              (this.directionTop && random == this.gameNumber - 1)\n            ) {\n              addNumber = this.gameNumber + random - 0.5;\n            } else if (this.directionTop) {\n              addNumber = this.gameNumber * 2 + random - 0.5;\n            } else {\n              addNumber = random - 0.5;\n            }\n            height = -(this.gameHeight + 8) * addNumber;\n            colItem.style.transform = `translateY(${height}px)`;\n            colItem.style.transition =\n              'transform 1.2s cubic-bezier(0, 0, 0, 1)';\n            this.winGame[index] = random;\n          }\n        }\n        setTimeout(() => {\n          this.showWin = true;\n          gameLuck.playGame();\n        }, 1200);\n      }\n    },\n  },\n});\n"],"mappings":";AACA,SAASA,eAAe,EAAEC,MAAM,QAAQ,KAAK;AAC7C,OAAOC,WAAW,MAAM,uCAAuC;AAE/D,OAAOC,UAAU,MAAM,cAAc;AACrC,SAAwBC,SAAS,QAAQ,gBAAgB;AACzD,SAASC,QAAQ,QAAQ,GAAG;AAC5B,SAASC,IAAI,QAAQ,QAAQ;AAE7B,eAAeN,eAAe,CAAC;EAC7BO,IAAIA,CAAA;IACF,OAAO;MACLC,cAAc,EAAE,CAAC;MACjBC,eAAe,EAAE,CAAC;MAClBC,YAAY,EAAE,EAAoB;MAClCC,OAAO,EAAE,IAAI;MACbC,OAAO,EAAE,EAAc;MACvBC,YAAY,EAAE,KAAK;MACnBC,UAAU,EAAE,CAAC;MACbX,UAAU;MACVY,SAAS,EAAE,CAAC;MACZC,UAAU,EAAE,CAAC;MACbX,QAAQ;MACRY,aAAa,EAAEhB,MAAM,CAAC,OAAO,CAAkB;MAC/CK;KACD;EACH,CAAC;EACDY,UAAU,EAAE;IAAEhB;EAAW,CAAE;EAC3BiB,OAAOA,CAAA;IACL,IAAIhB,UAAU,CAACiB,MAAM,CAACC,QAAQ,EAAE;MAC9B,IAAI,CAACN,SAAS,GAAG,CAAC;;IAEpB,IAAI,CAACO,OAAO,EAAE;IACd,IAAI,CAACC,SAAS,CAAC,MAAK;MAClB,MAAMC,aAAa,GAAG,IAAI,CAACC,KAAK,CAACD,aAA4B;MAC7D,MAAME,QAAQ,GAAGF,aAAa,CAACG,aAAa,CAAC,YAAY,CAAgB;MACzE,IAAID,QAAQ,EAAE;QACZ,IAAI,CAACV,UAAU,GAAGU,QAAQ,CAACE,qBAAqB,EAAE,CAACC,MAAM;QAEzD,IAAIL,aAAa,EAAE;UACjBA,aAAa,CAACM,KAAK,CAACD,MAAM,GAAG,GAAG,IAAI,CAACb,UAAU,GAAG,CAAC,GAAG,CAAC,IAAI;;QAE7D,MAAMe,WAAW,GAAG,IAAI,CAACN,KAAK,CAACM,WAA0B;QACzD,IAAIA,WAAW,EAAE;UACf,MAAMC,GAAG,GAAGD,WAAW,CAACE,gBAAgB,CAAC,MAAM,CAAC;UAChD,MAAMC,OAAO,GAAGF,GAAG,CAAC,CAAC,CAAgB;UACrCE,OAAO,CAACJ,KAAK,CAACK,SAAS,GAAG,cACxB,CAAC,IAAI,CAACnB,UAAU,GAAG,IAAI,CAACF,UAAU,GAAG,CAAC,GAAG,IAAI,CAACE,UAChD,KAAK;UACLoB,UAAU,CAAC,MAAK