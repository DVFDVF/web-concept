iv>\n    </div>\n  </Drawer>\n</template>\n<script lang=\"ts\" setup>\nimport { ref } from 'vue';\nimport type { DrawerProps } from 'ant-design-vue';\nimport { Drawer } from 'ant-design-vue';\nimport { installApp } from '@/utils';\nconst placement = ref<DrawerProps['placement']>('bottom');\nconst open = ref<boolean>(true);\nconst onSubmit = () => {\n  open.value = false;\n  installApp();\n};\n\nconst onClose = () => {\n  const installTipsNumber = localStorage.getItem('installTipsNumber');\n  if (installTipsNumber) {\n    const number = parseInt(installTipsNumber) + 1;\n    localStorage.setItem('installTipsNumber', `${number}`);\n  } else {\n    localStorage.setItem('installTipsNumber', '1');\n  }\n  open.value = false;\n};\nconst isShow = () => {\n  const client = localStorage.getItem('client');\n  if (client != 'H5') {\n    open.value = false;\n    return;\n  }\n  const installTipsNumber = localStorage.getItem('installTipsNumber');\n  if (installTipsNumber) {\n    const number = parseInt(installTipsNumber);\n    if (number > 3) {\n      open.value = false;\n    }\n  }\n};\nisShow();\n</script>\n<style lang=\"less\" scoped>\n.bottom-pop-up {\n  background: white;\n  height: 100%;\n  font-family: Montserrat Alternates, Montserrat Alternates;\n  color: black;\n\n  .left {\n    height: 100%;\n  }\n  .right {\n    padding: 10px 20px 10px 10px;\n    gap: 20px;\n    .text {\n      font-size: 23px;\n      line-height: 23px;\n      font-weight: 600;\n    }\n    .bottons {\n      width: 100%;\n      text-align: center;\n      font-size: 20px;\n      line-height: 50px;\n      font-weight: 900;\n      gap: 20px;\n      .cancel {\n        flex: 1;\n        background: #1d2635;\n        border-radius: 5px;\n        color: #a0a0a0;\n      }\n      .submit {\n        flex: 1;\n        background: linear-gradient(180deg, #ba183b 0%, #a61236 100%);\n        box-shadow: inset 7px 10px 20px 1px rgba(255, 255, 255, 0.23);\n        border-radius: 5px;\n        color: white;\n      }\n    }\n  }\n}\n@media only screen and (max-width: 767px) {\n  .bottom-pop-up {\n    .left {\n      height: 70%;\n    }\n    .right {\n      gap: 10px;\n      .text {\n        font-size: 12px;\n        line-height: 12px;\n      }\n      .bottons {\n        font-size: 12px;\n        line-height: 30px;\n      }\n    }\n  }\n}\n</style>\n"]}]}