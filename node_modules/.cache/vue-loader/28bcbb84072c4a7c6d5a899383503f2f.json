:key=\"item.text\" class=\"top-icon\">\n    <GameSound v-if=\"index == 3\" @volumeSelect=\"volumeSelect\"></GameSound>\n    <div class=\"dapps-top-button\" v-else @click=\"selectItem(item.key)\">\n      <svg\n        :class=\"IconColor(item.key)\"\n        width=\"24\"\n        height=\"24\"\n        focusable=\"false\"\n        aria-hidden=\"true\"\n      >\n        <use :xlink:href=\"item.icon\"></use>\n      </svg>\n      <label for=\"anim-button\">{{ item.text }}</label>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { openModal } from '@/manager/view';\nimport GameRules from '@/views/game/dice/left/GameRules.vue';\nimport GameSound from '@/views/game/dice/left/GameSound.vue';\nimport { defineComponent } from 'vue';\nimport HotkeysPop from './HotkeysPop.vue';\n\nexport default defineComponent({\n  data() {\n    return {\n      isFast: dataCenter.gameConfiguration.isFast,\n    };\n  },\n  methods: {\n    volumeSelect(volume: number) {\n      this.$emit('volumeSelect', volume);\n    },\n    selectItem(text: string) {\n      if (text == 'Fast') {\n        this.isFast = !this.isFast;\n        dataCenter.gameConfiguration.isFast = this.isFast;\n      }\n      if (text == 'Rules') {\n        openModal(GameRules);\n      }\n      if (text == 'Hotkeys') {\n        openModal(HotkeysPop);\n      }\n    },\n    IconColor(text: string) {\n      if (text == 'Fast') {\n        if (this.isFast) {\n          return 'item-select';\n        } else {\n          return 'svg-use';\n        }\n      } else {\n        return 'svg-use';\n      }\n    },\n  },\n\n  components: { GameSound },\n  props: ['topIcons', 'RulesData'],\n});\n</script>\n\n<style lang=\"less\" scoped>\n.dapps-top-button {\n  align-items: center;\n  background-color: #161f2c;\n  border: none;\n  border-radius: 8px;\n  display: flex;\n  height: 30px;\n  justify-content: center;\n  min-width: 30px;\n  overflow: visible;\n  padding: 8px;\n  position: relative;\n  transition: all 0.3s;\n\n  .svg-use {\n    fill: #55657e;\n    height: 14px;\n    pointer-events: none;\n    transition: all 0.3s;\n    width: 14px;\n  }\n}\n\n.dapps-top-button label {\n  background-color: #283343;\n  border-radius: 5px;\n  box-shadow: 0 0 15px #00000075;\n  color: #55657e;\n  display: block;\n  font-size: 9px;\n  opacity: 0;\n  padding: 8px;\n  pointer-events: none;\n  position: absolute;\n  text-align: center;\n  top: 0;\n  transform: translateY(5px);\n  transition: all 0.3s;\n  visibility: hidden;\n  white-space: nowrap;\n}\n\n.dapps-top-button:hover label {\n  opacity: 1;\n  transform: translateY(calc(-100% - 5px));\n  visibility: visible;\n  z-index: 10;\n}\n\n.item-select {\n  fill: #2283f6;\n  height: 14px;\n  pointer-events: none;\n  transition: all 0.3s;\n  width: 14px;\n}\n@media only screen and (max-width: 767px) {\n  .top-icon {\n    display: flex;\n    width: 100%;\n  }\n  .dapps-top-button {\n    flex: 1;\n  }\n}\n</style>\n"]}]}