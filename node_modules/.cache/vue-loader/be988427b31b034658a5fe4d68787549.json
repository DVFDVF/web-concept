n      required: true,\n    },\n    hidden: {\n      type: Boolean,\n      required: true,\n    },\n  },\n  data() {\n    return {\n      isInputFocused: false,\n      inputText: '',\n      inputTop: '0px',\n    };\n  },\n  methods: {\n    inputFocused() {\n      this.isInputFocused = true;\n      this.inputTop = '12px';\n    },\n    inputBlurred() {\n      this.isInputFocused = false;\n      this.inputTop = '0px';\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.input-inner {\n  height: 58px;\n  border-radius: 8px;\n  padding: 3px 1px;\n  align-items: center;\n  background-color: #090c1199;\n  border: 1px solid #2a3546;\n  cursor: text;\n  display: flex;\n  position: relative;\n  transition: color 0.1s ease-in-out, border-color 0.1s ease-in-out;\n}\n\n.input-icon {\n  height: 20px;\n  margin-left: -4px;\n  margin-right: 6px;\n  width: 20px;\n  align-items: center;\n  display: flex;\n  justify-content: center;\n  flex-shrink: 0;\n  pointer-events: none;\n}\n\n.input-tag-wrapper {\n  width: 100%;\n  position: relative;\n  margin-bottom: 20px;\n  font-family: Montserrat Variable, sans-serif;\n}\n\n.input__placeholder {\n  position: absolute;\n  top: 4px;\n  font-size: 13px;\n  left: 12px;\n  transition: all 0.3s;\n  pointer-events: none;\n  color: #555;\n}\n\n.input-tag {\n  outline: none;\n  border: none;\n  width: 100%;\n  height: 15px;\n  color: white;\n  margin-left: -1px;\n  background-color: transparent;\n  transition: all 0.3s;\n  position: absolute;\n  top: 0px;\n  /* 通过调整此值来移动input */\n}\n\n.placeholder-focus {\n  top: -7px;\n  font-size: 12px;\n  color: #555;\n}\n\n.input-btn-clear {\n  margin-right: 15px;\n  height: 18px;\n  width: 18px;\n  margin-left: 4px;\n  padding: 0;\n  background-color: transparent;\n  border: none;\n  cursor: pointer;\n}\n\n.input-btn {\n  height: 14px;\n  width: 14px;\n}\n\n.button-flat {\n  border-radius: 5px;\n  font-size: 14px;\n  margin-left: 8px;\n  margin-right: 12px;\n  padding: 9px 12px;\n  background-color: #2a3546;\n  border-color: transparent;\n  color: #93acd3;\n}\n\n.curren-icon {\n  width: 17px;\n  height: 17px;\n  margin-left: -6px;\n  margin-right: 10px;\n}\n</style>\n"]}]}