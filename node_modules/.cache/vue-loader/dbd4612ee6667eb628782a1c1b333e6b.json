 </h2>\n    <p class=\"depov-plain-text\">\n      {{ lang.v.qb_este }} <span>{{ lang.v.qb_bep }}</span\n      >. {{ lang.v.qb_transferir }} <span>{{ currency?.name }}</span>\n      {{ lang.v.qb_aqui }}:\n    </p>\n    <div class=\"input\">\n      <div class=\"input-tag-wrapper\">\n        <input\n          id=\"copy_deposit_code\"\n          class=\"input-tag\"\n          :placeholder=\"walletAddress\"\n          autocomplete=\"off\"\n          disabled=\"true\"\n        />\n      </div>\n      <img\n        src=\"@/assets/icon/head/icon-copy.svg\"\n        class=\"button-flat\"\n        @click=\"copyToClipboard(walletAddress)\"\n      />\n    </div>\n    <div class=\"qr-block\">\n      <div class=\"qr-texts\">\n        <p class=\"min_dep\">\n          ⚠️ {{ lang.v.qb_valor }}\n          <span>{{ TransitionRespItemValue?.minCharge }}</span>\n          <span style=\"margin-left: 2px\">{{ currency?.name }}</span>\n          {{ lang.v.qb_menores }}\n        </p>\n        <p class=\"tab_depositsd\">\n          {{\n            lang.v.qb_betfur.replace(\n              '{siteName}',\n              dataCenter.websiteName_Medium,\n            )\n          }}\n          <span>{{ currency?.name }}</span\n          >{{ lang.v.qb_debit }}\n        </p>\n      </div>\n      <div\n        class=\"qr\"\n        style=\"width: 120px; height: 120px\"\n        v-show=\"walletAddress\"\n      >\n        <QRCode\n          :size=\"110\"\n          type=\"canvas\"\n          :value=\"walletAddress\"\n          errorCorrectionLevel=\"M\"\n        />\n      </div>\n    </div>\n    <div class=\"select-chain-overlay\" v-show=\"isTipsTitle\">\n      <span>{{ lang.v.qb_network }}</span>\n    </div>\n  </div>\n</template>\n<!-- 充值页面 -->\n<script lang=\"ts\">\nimport { defineComponent, ref, onMounted, onUnmounted } from 'vue';\nimport { Currency, NetWork } from '@/api/protocol';\nimport {\n  getCryptoTransitionAll,\n  getWalletAddress,\n} from '@/views/home/api/wallet-api/walletIndex';\nimport { TransitionRespItem } from '@/views/home/api/wallet-api/walletProtocol';\nimport CurrencySelector from './widget/CurrencySelector/index.vue';\nimport NetWorkSelector from './widget/NetWorkSelector/index.vue';\nimport dataCenter from '@/dataCenter';\nimport { QRCode } from 'ant-design-vue';\nimport { lang } from '@/lang';\nimport { extenalOfficial } from '@/utils/currency';\nimport { tip } from '@/components/warn/warn';\n\nexport default defineComponent({\n  name: 'DepositoPop',\n  components: {\n    CurrencySelector,\n    NetWorkSelector,\n    QRCode,\n  },\n  data() {\n    return {\n      dataCenter,\n    };\n  },\n  setup() {\n    const currency = ref<Currency>();\n    const transitionList = ref<TransitionRespItem[]>([]);\n    const isNetworkTitle = ref(false);\n    const TransitionRespItemValue = ref<TransitionRespItem>();\n    const walletAddress = ref('');\n    const netWorkOptions = ref<NetWork[]>([]);\n    const isTipsTitle = ref(false);\n    let timer = 0;\n    const isStart = ref(true);\n    function transitionPlet() {\n      getCryptoTransitionAll().then((resp) => {\n        if (!resp) return;\n        transitionList.value = resp;\n        isStart.value = false;\n        const currency = extenalOfficial(\n          dataCenter.webConfig.cryptoCurrencies,\n          true,\n        )[0];\n        onCurrencyChange(currency);\n      });\n    }\n\n    const onCurrencyChange = (item: Currency) => {\n      if (isStart.value) return;\n      currency.value = item;\n      // 过滤 list\n      const list = transitionList.value.filter(\n        (transitionItem) => transitionItem.currencyId === item.id,\n      );\n      // 接下来的逻辑将在 list 过滤完成后执行\n      if (list.length === 0) {\n        // 没有网络选项的情况\n        reouestRechargeAddr(item.id.toString());\n        isNetworkTitle.value = false;\n        isTipsTitle.value = false;\n      } else if (list.length === 1) {\n        // 只有一个网络选项的情况\n        reouestRechargeAddr(item.id.toString(), list[0].networkId.toString());\n        isNetworkTitle.value = false;\n        isTipsTitle.value = false;\n      } else {\n        // 多个网络选项的情况\n        isNetworkTitle.value = true;\n        isTipsTitle.value = true;\n\n        netWorkOptions.value = list.map(\n          (item) => dataCenter.webConfig.networkMap[item.networkId]!,\n        );\n      }\n    };\n\n    const onNetWorkChange = (item: NetWork) => {\n      if (item) {\n        isTipsTitle.value = false;\n        if (currency.value) {\n          reouestRechargeAddr(currency.value.id.toString(), item.id.toString());\n        }\n      }\n    };\n\n    function reouestRechargeAddr(currencyId: string, networkId?: string) {\n      clearTimeout(timer);\n      getWalletAddress(currencyId, networkId).then((resp) => {\n        if (resp) {\n          walletAddress.value = resp;\n        }\n      });\n      timer = setTimeout(\n        () => reouestRechargeAddr(currencyId, networkId),\n        30 * 60 * 1000,\n      );\n    }\n\n    /**复制 */\n    function copyToClipboard(text: string) {\n      var textarea = document.createElement('textarea');\n      textarea.value = text;\n      document.body.appendChild(textarea);\n      textarea.select();\n      document.execCommand('copy');\n      document.body.removeChild(textarea);\n      tip.success('copy success');\n    }\n\n    onMounted(() => {\n      transitionPlet();\n    });\n\n    onUnmounted(() => {\n      clearTimeout(timer);\n    });\n\n    return {\n      currency,\n      transitionList,\n      isNetworkTitle,\n      TransitionRespItemValue,\n      walletAddress,\n      onCurrencyChange,\n      onNetWorkChange,\n      lang,\n      netWorkOptions,\n      isTipsTitle,\n      copyToClipboard,\n    };\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n@icon-color: #93acd3;\n\n.depov {\n  height: 100%;\n  width: 100%;\n\n  &-title {\n    font-size: 24px;\n    line-height: 135%;\n    letter-spacing: -0.02em;\n    margin-bottom: 1rem;\n    margin-top: 0;\n    font-weight: bold;\n    font-family: Montserrat Variable, sans-serif;\n    color: #fff;\n  }\n\n  .curencies {\n    column-gap: 16px;\n    display: grid;\n    grid-template-columns: 2fr 1fr;\n    margin-bottom: 16px;\n    min-height: 58px;\n    position: relative;\n  }\n\n  &-btctle {\n    font-size: 24px;\n    line-height: 135%;\n    letter-spacing: -0.02em;\n    margin-bottom: 16px;\n    font-weight: bold;\n    font-family: Montserrat Variable, sans-serif;\n    color: #fff;\n  }\n\n  &-plain-text {\n    margin-bottom: 16px;\n    line-height: 135%;\n    letter-spacing: -0.02em;\n    font-family: Montserrat Variable, sans-serif;\n    color: #fff;\n    font-size: 14px;\n  }\n\n  .input {\n    width: 100%;\n    align-items: center;\n    background-color: #090c1199;\n    border: 1px solid var(--color_grey-60);\n    cursor: text;\n    display: flex;\n    position: relative;\n    transition: color 0.1s ease-in-out, border-color 0.1s ease-in-out;\n    border-radius: 8px;\n    height: 36px;\n    padding: 3px 12px;\n\n    .input-tag-wrapper {\n      display: block;\n      flex: 1 1;\n      position: relative;\n      width: 0;\n      z-index: 0;\n    }\n\n    .input-tag {\n      background-color: transparent;\n      border: none;\n      font-size: 12px;\n      letter-spacing: -0.24px;\n      padding: 0;\n      cursor: default;\n      color: #fff;\n      margin-top: 0;\n      outline: none;\n      transition: margin-top 0.1s ease-in-out;\n      width: 100%;\n      display: block;\n      font-weight: 600;\n      line-height: 18px;\n    }\n\n    .button-flat {\n      border-radius: 6px;\n      margin-left: 6px;\n      margin-right: -9px;\n      padding: 6px;\n      width: 28px;\n      height: 28px;\n      background-color: #2a3546;\n      border-color: transparent;\n      color: #93acd3;\n    }\n  }\n  .input:hover {\n    border: 1px solid #2283f6;\n  }\n  .qr-block {\n    align-items: flex-start;\n    display: flex;\n    margin-top: 20px;\n  }\n\n  .qr-texts {\n    color: #55657e;\n    display: flex;\n    flex-direction: column;\n    font-size: 11px;\n  }\n\n  .min_dep {\n    background: #1c2532;\n    border-radius: 8px;\n    color: #fff;\n    font-weight: 600;\n    margin-bottom: 8px !important;\n    padding: 8px 10px;\n  }\n\n  .tab_depositsd {\n    color: #55657e;\n    font-size: 11px;\n  }\n\n  .qr {\n    background-color: #fff;\n    border-radius: 10px;\n    margin-left: 15px;\n    padding: 3px;\n  }\n\n  .depcy {\n    position: absolute;\n    margin-top: 74px;\n    width: 260px;\n    z-index: 11;\n  }\n}\n\n.blue-text {\n  color: #2283f6;\n}\n\n.rwt-text {\n  color: #55657e;\n}\n\n//\n.first-item {\n  background-color: #293546;\n}\n\nspan {\n  font-family: Montserrat Variable, sans-serif;\n  font-size: 14px;\n  font-weight: 700;\n  line-height: 1.5;\n}\n\n.name {\n  font-size: 16px;\n  margin-right: auto;\n  font-weight: bold;\n}\n\n.extra {\n  color: white;\n}\n\n.select-chain-overlay {\n  align-items: center;\n  background: rgba(17, 25, 35, 0.9);\n  bottom: 0;\n  display: flex;\n  filter: none !important;\n  height: calc(100% - 120px);\n  justify-content: center;\n  left: 0;\n  position: absolute;\n  width: 100%;\n  z-index: 9;\n}\n@media only screen and (max-width: 767px) {\n  .curencies {\n    gap: 16px;\n    grid-template-columns: repeat(1, 1fr) !important;\n  }\n}\n</style>\n"]}]}