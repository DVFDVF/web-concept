   const resp = await post<PlayGameRocketResp, PlayGameRocketReq>(\n      USERPLAY_PLAY_PLAYGAME_ROCKET,\n      {\n        targetRate,\n        betAmount,\n        currencyId,\n      },\n    );\n    return resp;\n  } catch (error) {\n    return undefined;\n  }\n}\n\n//当前游戏记录\nexport async function getGameRecord(limit?: string, gameId?: number) {\n  try {\n    const resp = await get<GetGameRecordResp[], GetGameRecordReq>(\n      GET_GAME_RECORD,\n      {\n        limit,\n        gameId,\n      },\n    );\n    resp.forEach((item) => {\n      item.createTime = greenToBrazil(item.createTime);\n    });\n\n    return resp;\n  } catch (error) {\n    return undefined;\n  }\n}\n\n/**登录第三方游戏 */\nexport async function loginGame(gameId: string) {\n  try {\n    const resp = await get<string, LoginGameReq>(LOGINGAME, {\n      gameId,\n    });\n    return resp;\n  } catch (error) {\n    return undefined;\n  }\n}\n\n/** 登录假PG */\nexport async function falsePG(gameId: string, language: string) {\n  try {\n    const resp = await get<string, FalseGameReq>(FALSEPG, {\n      gameId,\n      language,\n    });\n    return resp;\n  } catch (error) {\n    return undefined;\n  }\n}\n"],"mappings":"AAAA,SAASA,GAAG,EAAEC,IAAI,QAAQ,OAAO;AACjC,SACEC,OAAO,EAEPC,eAAe,EAGfC,SAAS,EAITC,6BAA6B,QACxB,YAAY;AACnB,SAASC,aAAa,QAAQ,SAAS;AAEvC,OAAO,eAAeC,cAAcA,CAClCC,UAAkB,EAClBC,SAAiB,EACjBC,UAAkB;EAElB,IAAI;IACF,MAAMC,IAAI,GAAG,MAAMV,IAAI,CACrBI,6BAA6B,EAC7B;MACEG,UAAU;MACVC,SAAS;MACTC;KACD,CACF;IACD,OAAOC,IAAI;GACZ,CAAC,OAAOC,KAAK,EAAE;IACd,OAAOC,SAAS;;AAEpB;AAEA;AACA,OAAO,eAAeC,aAAaA,CAACC,KAAc,EAAEC,MAAe;EACjE,IAAI;IACF,MAAML,IAAI,GAAG,MAAMX,GAAG,CACpBG,eAAe,EACf;MACEY,KAAK;MACLC;KACD,CACF;IACDL,IAAI,CAACM,OAAO,CAAEC,IAAI,IAAI;MACpBA,IAAI,CAACC,UAAU,GAAGb,aAAa,CAACY,IAAI,CAACC,UAAU,CAAC;IAClD,CAAC,CAAC;IAEF,OAAOR,IAAI;GACZ,CAAC,OAAOC,KAAK,EAAE;IACd,OAAOC,SAAS;;AAEpB;AAEA;AACA,OAAO,eAAeO,SAASA,CAACJ,MAAc;EAC5C,IAAI;IACF,MAAML,IAAI,GAAG,MAAMX,GAAG,CAAuBI,SAAS,EAAE;MACtDY;KACD,CAAC;IACF,OAAOL,IAAI;GACZ,CA