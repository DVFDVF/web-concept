te>\n  <div :class=\"['slider', { disable: isStart }]\">\n    <div\n      :class=\"[\n        'noUi-horizontal',\n        { 'noUi-target-red': isRollUnder, 'noUi-target-blue': !isRollUnder },\n      ]\"\n    >\n      <div class=\"noUi-base\">\n        <div class=\"noUi-connects\" ref=\"noUiConnect\">\n          <div\n            :class=\"[\n              'noUi-connect',\n              {\n                'noUi-target-red': !isRollUnder,\n                'noUi-target-blue': isRollUnder,\n              },\n            ]\"\n            :style=\"{ transform: 'translate(0%, 0px) scale(' + posX + ', 1)' }\"\n          ></div>\n        </div>\n        <div\n          class=\"noUi-origin\"\n          :style=\"{ transform: 'translate(' + transX + '%, 0px)', zIndex: 4 }\"\n          v-on:mousedown=\"startDrag\"\n          v-on:mousemove=\"onDrag\"\n          v-on:mouseup=\"stopDrag\"\n          v-on:touchstart=\"touchStartDrag\"\n          v-on:touchmove=\"touchOnDrag\"\n          v-on:touchend=\"touchStopDrag\"\n        >\n          <div\n            :class=\"['noUi-handle', { 'noUi-active': dragging }]\"\n            data-handle=\"0\"\n            role=\"slider\"\n            aria-orientation=\"horizontal\"\n            aria-valuemin=\"1.0\"\n            aria-valuemax=\"95.0\"\n            aria-valuenow=\"50.0\"\n            aria-valuetext=\"50\"\n          >\n            <div class=\"noUi-touch-area\"></div>\n            <div class=\"noUi-tooltip\">{{ getNumber }}</div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { defineComponent } from 'vue';\n\nexport default defineComponent({\n  data() {\n    return {\n      dragging: false,\n      posX: 0.5,\n      transX: -500,\n    };\n  },\n  computed: {\n    getNumber() {\n      return Math.round((this as any).posX * 100);\n    },\n  },\n  methods: {\n    startDrag(e: MouseEvent) {\n      this.dragging = true;\n    },\n\n    onDrag(e: MouseEvent) {\n      if (this.dragging) {\n        const container = this.$refs.noUiConnect as HTMLElement;\n        if (container) {\n          const rect = container.getBoundingClientRect();\n          const percentage = ((e.clientX - rect.x) / rect.width) * 0.99 + 0.01;\n          if (percentage <= this.minNumber * 0.01) {\n            this.posX = this.minNumber * 0.01;\n            this.transX = -((100 - this.minNumber) * 10);\n          } else if (percentage >= this.maxNumber * 0.01) {\n            this.posX = this.maxNumber * 0.01;\n            this.transX = -((100 - this.maxNumber) * 10);\n          } else {\n            this.posX = percentage;\n            this.transX = -(1 - percentage) * 1000;\n          }\n          this.$emit('SliderNumber', Math.round((this as any).posX * 100));\n        }\n      }\n    },\n\n    stopDrag() {\n      this.dragging = false;\n    },\n    //手机触摸事件\n    touchStartDrag(e: TouchEvent) {\n      this.dragging = true;\n    },\n\n    touchOnDrag(e: TouchEvent) {\n      if (this.dragging) {\n        const container = this.$refs.noUiConnect as HTMLElement;\n        if (container) {\n          const rect = container.getBoundingClientRect();\n          const percentage =\n            ((e.touches[0].clientX - rect.x) / rect.width) * 0.99 + 0.01;\n          if (percentage <= this.minNumber * 0.01) {\n            this.posX = this.minNumber * 0.01;\n            this.transX = -((100 - this.minNumber) * 10);\n          } else if (percentage >= this.maxNumber * 0.01) {\n            this.posX = this.maxNumber * 0.01;\n            this.transX = -((100 - this.maxNumber) * 10);\n          } else {\n            this.posX = percentage;\n            this.transX = -(1 - percentage) * 1000;\n          }\n          this.$emit('SliderNumber', Math.round((this as any).posX * 100));\n        }\n      }\n    },\n\n    touchStopDrag(e: TouchEvent) {\n      this.dragging = false;\n    },\n    handleKeyDown(event: KeyboardEvent) {\n      if (dataCenter.gameConfiguration.HotkeyEnabled) {\n        const isWPressed = event.key === 'w';\n        const isEPressed = event.key === 'e';\n        // 处理热键触发后的逻辑\n        if (isWPressed) {\n          if (this.posX >= this.minNumber * 0.01) {\n            this.posX -= 0.01;\n            this.transX = -(1 - this.posX) * 1000;\n          }\n        }\n        if (isEPressed) {\n          if (this.posX <= this.maxNumber * 0.01) {\n            this.posX += 0.01;\n            this.transX = -(1 - this.posX) * 1000;\n          }\n        }\n      }\n    },\n  },\n  watch: {\n    minNumber(newVal) {\n      if (this.posX < newVal * 0.01) {\n        this.posX = newVal * 0.01;\n        this.transX = -((100 - newVal) * 10);\n        this.getNumber;\n        this.$emit('SliderNumber', Math.round((this as any).posX * 100));\n      }\n    },\n    maxNumber(newVal) {\n      if (this.posX > newVal * 0.01) {\n        this.posX = newVal * 0.01;\n        this.transX = -((100 - newVal) * 10);\n        this.getNumber;\n        this.$emit('SliderNumber', Math.round((this as any).posX * 100));\n      }\n    },\n  },\n  mounted() {\n    document.addEventListener('mousemove', this.onDrag);\n    document.addEventListener('mouseup', this.stopDrag);\n    window.addEventListener('keydown', this.handleKeyDown);\n  },\n  beforeUnmount() {\n    document.removeEventListener('mousemove', this.onDrag);\n    document.removeEventListener('mouseup', this.stopDrag);\n    window.removeEventListener('keydown', this.handleKeyDown);\n  },\n  components: {},\n  props: ['minNumber', 'maxNumber', 'isStart', 'isRollUnder'],\n});\n</script>\n\n<style lang=\"less\" scoped>\n.noUi-target,\n.noUi-target * {\n  -webkit-touch-callout: none;\n  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);\n  box-sizing: border-box;\n  touch-action: none;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  user-select: none;\n}\n\n.noUi-target {\n  direction: ltr;\n  position: relative;\n}\n\n.noUi-base,\n.noUi-connects {\n  height: 100%;\n  position: relative;\n  width: 100%;\n  z-index: 1;\n}\n\n.noUi-connects {\n  overflow: hidden;\n  z-index: 0;\n}\n\n.noUi-connect,\n.noUi-origin {\n  left: 0;\n  position: absolute;\n  top: 0;\n  -ms-transform-origin: 0 0;\n  -webkit-transform-origin: 0 0;\n  transform-origin: 0 0;\n  -webkit-transform-style: preserve-3d;\n  transform-style: flat;\n  will-change: transform;\n  z-index: 1;\n}\n\n.noUi-connect {\n  height: 100%;\n  width: 100%;\n}\n\n.noUi-origin {\n  height: 10%;\n  width: 10%;\n}\n\nhtml:not([dir='rtl']) .noUi-horizontal .noUi-origin {\n  left: auto;\n  right: 0;\n}\n\n.noUi-vertical .noUi-origin {\n  width: 0;\n}\n\n.noUi-horizontal .noUi-origin {\n  height: 0;\n}\n\n.noUi-handle {\n  backface-visibility: hidden;\n  position: absolute;\n}\n\n.noUi-touch-area {\n  height: 100%;\n  width: 100%;\n}\n\n.noUi-origin {\n  transition: transform 0.1s;\n}\n\n.noUi-horizontal {\n  height: 10px;\n  transition: opacity 0.2s;\n  border-radius: 11px;\n}\n\n[disabled].noUi-horizontal {\n  opacity: 0.85;\n}\n\n.noUi-horizontal .noUi-handle {\n  height: 30px;\n  left: -15px;\n  top: -10px;\n  width: 30px;\n}\n\n.noUi-vertical {\n  width: 18px;\n}\n\n.noUi-vertical .noUi-handle {\n  height: 34px;\n  left: -6px;\n  top: -17px;\n  width: 28px;\n}\n\nhtml:not([dir='rtl']) .noUi-horizontal .noUi-handle {\n  left: auto;\n  right: -17px;\n}\n\n.noUi-target-red {\n  background: #fa305c;\n}\n\n.noUi-connects,\n.noUi-target {\n  border-radius: 10px;\n}\n\n.noUi-connect {\n  transition: transform 0.1s;\n}\n.noUi-target-blue {\n  background: #297fe5;\n}\n\n.noUi-handle {\n  background-color: #fff;\n  border-radius: 7px;\n  box-shadow: 0 0 0 4px #fff3;\n  cursor: pointer;\n  transition: box-shadow 0.3s;\n}\n\n.noUi-handle:after,\n.noUi-handle:before {\n  height: 1px;\n  left: 6px;\n  top: 14px;\n  width: 14px;\n}\n\n.noUi-handle:after {\n  top: 17px;\n}\n\n.noUi-tooltip {\n  bottom: 0;\n  color: #191b20;\n  display: block;\n  font-size: 12px;\n  font-weight: 700;\n  height: 30px;\n  left: 0;\n  line-height: 30px;\n  pointer-events: none;\n  position: absolute;\n  right: 0;\n  text-align: center;\n  top: 0;\n  white-space: nowrap;\n  width: 30px;\n}\n\n.noUi-horizontal .noUi-tooltip {\n  bottom: 120%;\n  left: 50%;\n  transform: translate(-50%);\n}\n\n.noUi-vertical .noUi-tooltip {\n  right: 120%;\n  top: 50%;\n  transform: translateY(-50%);\n}\n\n.noUi-active {\n  box-shadow: 0 0 0 5px #fff6;\n}\n\n.slider {\n  position: relative;\n}\n\n.slider:after {\n  background-image: url(../../../../assets/dice/diceLine.png);\n  background-repeat: repeat-x;\n  content: '';\n  display: block;\n  height: 7px;\n  margin-top: 20px;\n  width: 100%;\n}\n\n.disable {\n  opacity: 0.8;\n  pointer-events: none;\n}\n@media only screen and (max-width: 767px) {\n  .slider {\n    margin: 0 15px;\n  }\n}\n</style>\n"]}]}