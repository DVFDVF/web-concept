omeHeader },\n  watch: {\n    selectItem(newValue) {\n      if (newValue == 'Home') {\n        this.headerHeight = 65;\n      } else if (newValue == 'Messages') {\n        this.headerHeight = 65;\n      } else if (newValue == 'Help') {\n        this.headerHeight = 125;\n      }\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.header {\n  background: rgb(45, 45, 49);\n  z-index: 10;\n  padding: 10px;\n  height: 65px;\n  position: relative;\n  transition: all 0.3s ease-out 0s;\n}\n.header-title {\n  margin-top: 10px;\n  transition: all 0.3s ease-out 0s;\n  color: rgb(255, 255, 255);\n  display: flex;\n  flex-direction: row;\n  justify-content: center;\n  align-items: center;\n  width: 100%;\n  position: relative;\n}\n.search {\n  padding-top: 10px;\n}\n.intercom-title {\n  font-size: 18px;\n  font-weight: 600px;\n}\n.intercom-10ubxl7 {\n  z-index: 2147483003;\n  width: 48px;\n  height: 48px;\n  border-radius: 10px;\n  -webkit-box-align: center;\n  align-items: center;\n  -webkit-box-pack: center;\n  justify-content: center;\n  cursor: pointer;\n  transition: background-color 150ms ease 0s;\n  display: flex;\n  position: absolute;\n  right: 10px;\n}\n.intercom-10ubxl7:hover {\n  background-color: rgba(0, 0, 0, 0.2);\n}\n.intercom-10ubxl7 i {\n  display: inline;\n}\n.intercom-301xvt {\n  display: flex;\n  -webkit-box-align: center;\n  align-items: center;\n  width: auto;\n  min-width: 12px;\n  height: 12px;\n}\n.intercom-301xvt svg {\n  width: auto;\n  min-width: 12px;\n  height: 12px;\n}\n\n.intercom-301xvt svg {\n  display: block;\n}\n</style>\n"]}]}