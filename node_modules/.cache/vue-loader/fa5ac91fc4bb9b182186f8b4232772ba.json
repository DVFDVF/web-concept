{\n  background-color: transparent;\n  border: none;\n  cursor: pointer;\n  width: 25.6px;\n  height: 36px;\n  padding: 3.5px 5.258px;\n\n  .svg-use {\n    width: 16px;\n    height: 16px;\n    fill: #fff;\n  }\n}\n\n//main\n.notifications-PlatformList {\n  height: 100%;\n  display: flex;\n  flex-direction: column;\n  margin-left: 12px;\n  margin-right: 12px;\n  margin-top: 12px;\n}\n.towns {\n  display: flex;\n  justify-content: space-between;\n  background-color: #1c2532;\n  border-radius: 50px;\n  margin-bottom: 16px;\n  margin-top: 12px;\n  position: relative;\n}\n\n.switch-option {\n  flex-grow: 1; /* 每个按钮将占据可用空间的等份 */\n  padding-bottom: 7px;\n  padding-top: 7px;\n  white-space: nowrap;\n  background-color: transparent;\n  border: none;\n  cursor: pointer;\n}\n.switch-option-counter {\n  font-family: Montserrat Variable, sans-serif;\n  background-color: #2283f6;\n  border-radius: 50%;\n  color: #fff;\n  font-size: 10px;\n  font-weight: 700;\n  height: 18px;\n  margin-left: 8px;\n  min-width: 18px;\n  padding-left: 6px;\n  padding-right: 6px;\n  transition: color 0.1s ease-in-out, background-color 0.1s ease-in-out;\n}\n.seich-expter {\n  background-color: #fff;\n  color: #2283f6;\n}\n.open-counter {\n  background-color: #2283f6;\n  border-radius: 50px;\n}\n\n//滑动\n.slide {\n  height: 100%;\n  overflow: hidden;\n}\n.dielt {\n  height: 86%;\n  overflow: scroll;\n}\n\n.notifies-footer {\n  bottom: 0;\n  left: 0;\n  position: absolute;\n  right: 0;\n}\n.button-grey1 {\n  align-items: center;\n  width: 100%;\n  padding: 10px 14px;\n  border-radius: 10px;\n  border-top: 2px solid rgba(183, 207, 255, 0.05);\n  background-color: #1c2532;\n  border-color: transparent;\n  color: #55657e;\n  cursor: pointer;\n  display: inline-block;\n  outline: none;\n  position: relative;\n  text-align: center;\n  text-decoration: none;\n  transition: color 0.1s ease-in-out, background-color 0.1s ease-in-out,\n    border-color 0.1s ease-in-out;\n  white-space: nowrap;\n}\n@media only scre