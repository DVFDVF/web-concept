onent } from 'vue';\n\nexport default defineComponent({\n  name: 'country-selection',\n  computed: {},\n  methods: {\n    dropdownExpand() {\n      this.isSpend = !this.isSpend;\n      if (this.isSpend) {\n        this.spendIcon = '/svg/sprite.svg#icon-arrow-up';\n      } else {\n        this.spendIcon = '/svg/sprite.svg#icon-arrow-down';\n      }\n    },\n    selectCountryClick(country: string) {\n      this.selectCountry = country;\n      this.isSpend = false;\n      this.spendIcon = '/svg/sprite.svg#icon-arrow-down';\n      this.$emit('selectCounty', country);\n    },\n  },\n  data() {\n    return {\n      lang,\n      isSpend: false,\n      selectCountry: '',\n      spendIcon: '/svg/sprite.svg#icon-arrow-down',\n    };\n  },\n  props: ['FeedbackData', 'isValidationFailed'],\n});\n</script>\n<style lang=\"less\" scoped>\n.login-signup-countries-block {\n  position: relative;\n\n  .country-icon-spend {\n    display: flex;\n    align-items: center;\n    color: #93acd3;\n  }\n}\n\n.dropdown {\n  position: relative;\n}\n\n.dropdown-trigger:hover {\n  background-color: #263041;\n}\n\n.dropdown-lg .dropdown-trigger {\n  font-size: 14px;\n  height: 58px;\n  padding: 10px 17px;\n}\n\n.dropdown-trigger {\n  display: flex;\n  flex-direction: row;\n  background-color: #4458743d;\n  border: 1px solid transparent;\n  box-shadow: 0 2px 4px #0000002e;\n  border-radius: 10px;\n  transition: background-color 0.1s ease-in-out, border-color 0.1s ease-in-out;\n}\n\n.dropdown-trigger-error {\n  border: 1px solid #ed1d49;\n}\n\n.dropdown .trigger-content {\n  display: flex;\n  flex: 1 1;\n  flex-direction: column;\n  align-items: flex-start;\n  justify-content: center;\n\n  .trigger-content-placeholder {\n    color: #2283f6;\n    font-weight: 600;\n    line-height: 18px;\n    white-space: nowrap;\n    font-size: 12px;\n  }\n\n  .trigger-content-text {\n    color: white;\n    font-family: Montserrat Variable, sans-serif;\n    font-size: 14px;\n    font-weight: 500;\n    line-height: 1.5;\n  }\n}\n\n.dropdown-inner {\n  overflow: auto !important;\n  background-color: #1c2532;\n  padding: 10px 12px;\n  left: 0;\n  right: 0;\n  height: 234px;\n  border-radius: 10px;\n  position: absolute;\n  font-size: 14px;\n  position: absolute;\n  transform: translateY(0);\n  z-index: 4;\n  top: 100%;\n  margin-top: 10px;\n\n  .input-inner {\n    width: 100%;\n    height: 48px;\n    align-items: center;\n    background-color: #090c1199;\n\n    border: 1px solid #2a3546;\n    cursor: text;\n    display: flex;\n    position: relative;\n    padding: 18px 7px;\n    margin-bottom: 6px;\n    border-radius: 8px;\n    transition: color 0.1s ease-in-out, border-color 0.1s ease-in-out;\n\n    .input-icon {\n      margin: 8px;\n    }\n\n    .input-tag {\n      border: none;\n      font-size: 14px;\n      letter-spacing: -0.28px;\n      padding: 0;\n      display: block;\n      font-weight: 600;\n      line-height: 18px;\n      color: white;\n      background-color: #1c253200;\n      margin-top: 0;\n      outline: none;\n      transition: margin-top 0.1s ease-in-out;\n      width: 100%;\n    }\n  }\n\n  .input-inner:hover {\n    border: 1px solid #2283f6;\n  }\n\n  .dropdown-menu-inner {\n    display: flex;\n    position: absolute;\n    flex-direction: column;\n  }\n\n  .dropdown-menu-item {\n    height: 48px;\n    border-radius: 10px;\n    font-family: Montserrat Variable, sans-serif;\n    margin-right: 15px;\n    font-size: 14px;\n    font-weight: 600;\n    line-height: 1.5;\n    color: #55657e;\n\n    .dropdown-menu-link {\n      width: 100%;\n      height: 100%;\n\n      text-align: left;\n      padding: 10px 12px;\n      border-radius: 10px;\n    }\n\n    .dropdown-menu-link:hover {\n      background-color: #2a3546;\n      color: white;\n    }\n  }\n}\n\n.dropdown-inner::-webkit-scrollbar {\n  display: none;\n}\n\nul {\n  list-style-type: none;\n\n  padding: 0;\n\n  margin: 0;\n}\n\nli {\n  list-style: none;\n  margin: 0;\n  padding: 0;\n}\n\nbutton {\n  background: none;\n  border: none;\n  padding: 0;\n  font: inherit;\n  cursor: pointer;\n  outline: inherit;\n  /* 添加其他需要的样式 */\n}\n\n.dropdown .trigger-content .text-nomal {\n  color: white;\n  font-weight: 600;\n  line-height: 18px;\n  font-size: 14px;\n}\n\n.dropdown .trigger-content.text-selected {\n  color: #55657e;\n}\n\n@media only screen and (max-width: 767px) {\n  .trigger-content-placeholder {\n    font-size: 13px !important;\n  }\n}\n</style>\n"]}]}