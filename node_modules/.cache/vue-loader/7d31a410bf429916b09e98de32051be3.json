i class=\"ruleitem\">{{ lang.v.hb_valor }} R$7 777</li>\n      </ul>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { defineComponent, inject } from 'vue';\nimport { ModalCloseFun, openModal } from '@/manager/view';\nimport receiveReward from './receiveAward.vue';\nimport { redRainPlay } from './api';\nimport { redEnvelopeRain } from './game';\nimport { tip } from '@/components/warn/warn';\nimport { lang } from '@/lang';\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      isClick: false,\n      redEnvelopeRain,\n      closeFunction: inject('close') as ModalCloseFun,\n    };\n  },\n  mounted() {\n    const content = this.$refs.content as HTMLElement;\n    this.redEnvelopeRain.init(content);\n  },\n  methods: {\n    close() {\n      this.closeFunction();\n    },\n    openAward() {\n      redRainPlay().then((resp) => {\n        if (!resp) return;\n        redEnvelopeRain.canPlay = resp.canPlay;\n        this.isClick = true;\n        this.redEnvelopeRain.hidenRedRain();\n        setTimeout(() => {\n          this.closeFunction();\n          openModal(receiveReward, { hideClose: true, reward: resp.reward });\n        }, 200);\n      });\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.content {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  z-index: 10;\n  user-select: none;\n}\n.rules {\n  position: relative;\n  justify-content: flex-start;\n  flex-direction: column;\n  width: 300px;\n  padding: 10px;\n  height: auto;\n  height: 350px;\n  background: url(~@/assets/redEnvelope/cover.jpg) no-repeat;\n  background-size: 100% 100%;\n  z-index: 120;\n  animation: scale 0.4s forwards;\n}\n.rules-small {\n  animation: scaleSmall 0.2s forwards;\n}\n@keyframes scale {\n  0% {\n    transform: scale(0);\n  }\n\n  100% {\n    transform: scale(1);\n  }\n}\n@keyframes scaleSmall {\n  0% {\n    transform: scale(1);\n  }\n\n  100% {\n    transform: scale(0);\n  }\n}\n.rules-close {\n  position: absolute;\n  right: 18px;\n  top: 10px;\n  width: 23px;\n  height: 23px;\n  background: url(~@/assets/redEnvelope/close2.jpg) no-repeat;\n  background-size: 23px 23px;\n  cursor: pointer;\n}\n.title {\n  margin-top: 20px;\n  height: 20px;\n  font-size: 20px;\n  font-family: Arial-BoldMT, Arial;\n  font-weight: 400;\n  color: #ffd800;\n  text-align: center;\n}\n.rules .rulelist {\n  width: 78%;\n  font-size: 18px;\n  transform: scale(0.96);\n  font-family: Arial-BoldMT, Arial;\n  font-weight: 400;\n  color: #fff;\n  line-height: 20px;\n}\n.rules .rulelist li {\n  list-style: disc;\n  margin-top: 20px;\n}\n.rules .btn {\n  margin-top: 13%;\n  width: 23%;\n}\n.bar-span-ani::after {\n  animation: scaleAnimation 2s forwards;\n}\n@media only screen and (max-width: 767px) {\n}\n</style>\n"]}]}