AAC,CAAC","file":"/Users/nuomi/Desktop/项目/concept/web3-concept/src/views/chats/popUp/BetDetailsPreview.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"main\">\n    <div class=\"game-introduction\">\n      <img\n        class=\"bet-details-image\"\n        src=\"https://static.betfury.io/preview/17@1x.jpeg\"\n      />\n      <div class=\"game-body\">\n        <div class=\"game-title\">\n          <div class=\"left\">\n            <div class=\"name\">{{ lang.v.chats_pharaoh }}</div>\n            <div class=\"manufacturer\">\n              {{ lang.v.chats_by }}\n              <span>{{ dataCenter.websiteName_Medium }}</span>\n            </div>\n          </div>\n          <div class=\"play\">\n            <BtnRed :message=\"lang.v.chats_play\"></BtnRed>\n          </div>\n        </div>\n        <div class=\"player-details\">\n          <div class=\"detail\" v-for=\"item in playerData\" :key=\"item.title\">\n            <div class=\"detail-title\">{{ item.title }}</div>\n            <div class=\"detail-content\" v-show=\"item.player\">\n              <div class=\"player-icon\"></div>\n              <div class=\"player-name\">{{ item.player?.name }}</div>\n            </div>\n            <div v-show=\"item.text\">{{ item.text }}</div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"bet-details-info\">\n      <div class=\"bet-details-info-item\" v-for=\"item in 3\" :key=\"item\">\n        <span class=\"bet-details-info-label\">{{ lang.v.chats_bet }}</span>\n        <div class=\"currency-body\" style=\"font-size: 14px\">\n          <img\n            class=\"currency-icon\"\n            width=\"20\"\n            height=\"20\"\n            src=\"@/assets/home/footer/bfg.jpg\"\n            loading=\"lazy\"\n          />\n          <span class=\"balance\">$0.00</span>\n        </div>\n      </div>\n    </div>\n    <div class=\"bet-details-board\">\n      <img\n        class=\"introduce\"\n        src=\"@/assets/chats/game-introduce.jpg\"\n        loading=\"lazy\"\n      />\n    </div>\n    <BetDetailsFairness></BetDetailsFairness>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport BtnRed from '@/views/home/center/BtnRed.vue';\nimport { defineComponent } from 'vue';\nimport BetDetailsFairness from './BetDetailsFairness.vue';\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      dataCenter,\n      playerData: [\n        {\n          get title() {\n            return lang.v.chats_player;\n          },\n          player: {\n            icon: '',\n            get name() {\n              return lang.v.chats_dierichs;\n            },\n          },\n        },\n        {\n          get title() {\n            return lang.v.chats_data;\n          },\n          text: '11/27/23, 11:38:48 AM',\n        },\n        {\n          get title() {\n            return lang.v.chats_btid;\n          },\n          text: '65640f4825954d1f91ec3497',\n        },\n      ],\n    };\n  },\n  methods: {},\n  components: { BtnRed, BetDetailsFairness },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.main {\n  display: flex;\n  flex-direction: column;\n  margin-top: 5px;\n  width: 100%;\n  .game-introduction {\n    display: flex;\n    flex-direction: row;\n    .bet-details-image {\n      border-radius: 15px;\n      display: block;\n      margin-right: 16px;\n      width: 144px;\n    }\n  }\n  .game-body {\n    display: flex;\n    flex-direction: column;\n    width: 100%;\n    .game-title {\n      display: flex;\n      flex-direction: row;\n      justify-content: space-between;\n      margin-bottom: 20px;\n      align-items: center;\n      .left {\n        display: flex;\n        flex-direction: column;\n        .name {\n          font-size: 18px;\n        }\n        .manufacturer {\n          color: #93acd3;\n          font-size: 11px;\n          font-weight: 600;\n          letter-spacing: -0.02em;\n        }\n        .manufacturer span {\n          text-decoration: underline;\n        }\n      }\n    }\n    .player-details {\n      background: #161f2c;\n      border-radius: 12px;\n      flex: 1;\n      padding: 16px;\n      position: relative;\n      .detail {\n        display: flex;\n        flex-direction: row;\n        justify-content: space-between;\n        color: #55657e;\n        font-size: 14px;\n        font-weight: 500;\n        line-height: 18px;\n        margin-bottom: 12px;\n        .detail-content {\n          display: flex;\n          flex-direction: row;\n          .player-icon {\n            width: 22px;\n            height: 22px;\n            background-image: url('~@/assets/referral/sprite-3.png');\n            background-size: 528px 22px;\n            background-position: -44px 0;\n          }\n          .player-name {\n            color: white;\n            font-size: 14px;\n            font-weight: 600;\n            line-height: 18px;\n            margin-left: 8px;\n          }\n        }\n      }\n      .detail:last-child {\n        margin-bottom: 0;\n      }\n    }\n  }\n  .bet-details-info {\n    column-gap: 12px;\n    display: grid;\n    grid-template-columns: 1fr 1fr 1fr;\n    margin-bottom: 20px;\n    margin-top: 20px;\n    row-gap: 12px;\n    .bet-details-info-item {\n      align-items: center;\n      background-color: #161f2c;\n      border-radius: 8px;\n      display: flex;\n      flex: 1;\n      flex-direction: column;\n      padding: 7px 15px;\n      .bet-details-info-label {\n        color: #55657e;\n        display: inline-block;\n        font-size: 12px;\n        font-weight: 500;\n        margin-bottom: 5px;\n        .currency-body {\n          display: flex;\n          flex-direction: row;\n          align-items: center;\n        }\n      }\n    }\n  }\n  .introduce {\n    width: 100%;\n    border-radius: 10px;\n    margin-bottom: 20px;\n    padding-bottom: 20px;\n  }\n}\n.main :before {\n  background: linear-gradient(0deg, transparent, rgba(17, 25, 35, 0.5));\n  top: 0;\n}\n\n.currency-icon {\n  flex-shrink: 0;\n  margin-right: 0.5em;\n}\n.balance {\n  font-size: inherit;\n  line-height: inherit;\n  font-weight: 800;\n  letter-spacing: -0.5px;\n  margin: auto 0;\n  overflow-x: hidden;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n}\n</style>\n"]}]}