twix row-center\">\n      <div class=\"avatar\">\n        <img\n          class=\"battle-img\"\n          src=\"@/assets/referral/sprite-2.png\"\n          :style=\"`object-position: ${\n            -(parseInt(ranksItem?.avatar ?? '1') - 1) * 24\n          }px 0`\"\n        />\n      </div>\n      <div class=\"user-wrap\">\n        <span class=\"user-name\">{{ ranksItem?.name }}</span>\n        <span class=\"user__additional-info\">{{\n          ranksItem?.score.toFixed(2)\n        }}</span>\n      </div>\n    </div>\n    <div class=\"battle-prize\">\n      <span class=\"balance\"\n        >{{ dataCenter.conform }}{{ ranksItem?.reward }}</span\n      >\n    </div>\n  </div>\n</template>\n<!-- 排行榜组件 -->\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { ranksDataItem } from '../../api/competitionProtocol';\n\nexport default {\n  data() {\n    return {\n      dataCenter,\n    };\n  },\n  props: {\n    ranksItem: {\n      type: Object as () => ranksDataItem,\n    },\n    ranksIndex: {\n      type: Number,\n      required: true,\n    },\n  },\n  methods: {\n    /**前三个排行图 */\n    getPlaceImage(index: number) {\n      if (index === 0) {\n        return require('@/assets/competition/top1.png');\n      } else if (index === 1) {\n        return require('@/assets/competition/top2.png');\n      } else if (index === 2) {\n        return require('@/assets/competition/top3.png');\n      }\n    },\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.battle-table-line {\n  align-items: center;\n  border-top: 1px solid #263041;\n  display: flex;\n  height: 60px;\n  padding: 12px 0;\n}\n.battle-pleist {\n  color: #93acd3;\n  font-size: 14px;\n  font-weight: 600;\n  line-height: 20px;\n  width: 19%;\n  display: grid;\n  justify-items: start;\n}\n.battle-twix {\n  width: 48.5%;\n  display: flex;\n\n  .avatar {\n    height: 24px;\n    width: 24px;\n    overflow: hidden;\n  }\n  .battle-img {\n    height: 24px;\n  }\n}\n\n.user-wrap {\n  display: flex;\n  flex-direction: column;\n  margin-left: 8px;\n  text-align: left;\n}\n.user-name {\n  font-size: 12px;\n  letter-spacing: -0.02em;\n  line-height: 16px;\n  font-weight: 700;\n  color: #fff;\n}\n.user__additional-info {\n  font-size: 12px;\n  letter-spacing: -0.02em;\n  line-height: 16px;\n  font-weight: 600;\n  color: #93acd3;\n}\n.battle-prize {\n  width: 32.5%;\n  text-align: left;\n}\n.balance {\n  font-weight: 600;\n  letter-spacing: -0.5px;\n  margin: auto 0;\n  overflow-x: hidden;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  color: #fff;\n}\n</style>\n"]}]}