tup lang=\"ts\">\nimport icoBottom from '@/assets/icon/head/ico-bottom.vue';\nimport Avatar from '@/components/Avatar.vue';\nimport UserFormationPop from './UserFormationPop.vue';\nimport { ref } from 'vue';\nimport dataCenter from '@/dataCenter';\nimport { isPhone } from '@/utils';\nconst isHovered = ref(false);\nconst user = dataCenter.user;\nconst siteMessage = dataCenter.siteMessage;\nfunction isMouseleave(): void {\n  if (isHovered.value) {\n    const windowWidth = window.innerWidth;\n    // 根据窗口宽度执行相应的逻辑\n    if (windowWidth <= 768) {\n      // 执行移动端逻辑\n      isHovered.value = false;\n    }\n  }\n}\nlet mouseLeaveTimeout: number;\n/**\n *\n * 检测鼠标在父元素中\n */\nconst mouseOver = (event: MouseEvent) => {\n  const windowWidth = window.innerWidth;\n  if (windowWidth <= 768) {\n    return;\n  }\n  window.clearTimeout(mouseLeaveTimeout); // 取消之前的延时关闭\n  isHovered.value = true;\n};\n\nfunction isMobileClick() {\n  const windowWidth = window.innerWidth;\n  if (windowWidth <= 768) {\n    isHovered.value = !isHovered.value;\n  }\n}\n/**\n * 鼠标离开父元素及其子元素\n * 将 event.currentTarget 断言为 HTMLElement\n */\nconst mouseOut = (event: MouseEvent) => {\n  const windowWidth = window.innerWidth;\n\n  if (windowWidth <= 768) {\n    isHovered.value = false;\n    return;\n  }\n\n  const currentElement = event.currentTarget as HTMLElement;\n  if (currentElement && !currentElement.contains(event.relatedTarget as Node)) {\n    mouseLeaveTimeout = window.setTimeout(() => {\n      isHovered.value = false;\n    }, 300); // 延迟 300 毫秒\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n@profile-wrapper-background: linear-gradient(\n  90deg,\n  transparent 0,\n  transparent 35%,\n  rgba(68, 88, 116, 0.24) 0\n);\n@icon-color: #93acd3;\n@hov-color: #2283f6;\n\n.profile-usr {\n  background: @profile-wrapper-background;\n  border-radius: 9px;\n  margin-left: 16px;\n\n  .dropdown-trigger-custom {\n    align-items: center;\n    cursor: pointer;\n    display: flex;\n  }\n\n  .profile {\n    cursor: pointer;\n    display: flex;\n    flex-direction: column;\n    position: relative;\n\n    &-avatar {\n      width: 29px;\n      height: 29px;\n      left: 50%;\n      position: absolute;\n      top: 50%;\n      transform: translate(-50%, -50%);\n    }\n  }\n\n  .icon-arrow {\n    transition: transform 0.3s ease, filter 0.3s ease;\n    margin-right: 11px;\n    margin-left: 11px;\n    fill: @icon-color;\n    width: 10px;\n    height: 10px;\n  }\n}\n\n.profile-usr:hover .icon-arrow {\n  transform: rotate(180deg);\n  fill: @hov-color;\n}\n@media only screen and (max-width: 767px) {\n  .profile-usr {\n    margin-right: 50px;\n  }\n  .profile-usr:hover .icon-mobile {\n    transform: rotate(0);\n    fill: @icon-color;\n  }\n  .notif-count {\n    background: rgb(27, 184, 61);\n    border: 2px solid rgb(27, 38, 54);\n    border-radius: 5px;\n    cursor: pointer;\n    font-family: Montserrat Variable, sans-serif;\n    color: rgb(255, 255, 255);\n    font-size: 10px;\n    font-weight: 800;\n    min-width: 22px;\n    padding: 0px 2.5px;\n    position: absolute;\n    right: -6px;\n    text-align: center;\n    top: -5.5px;\n  }\n}\n</style>\n"]}]}