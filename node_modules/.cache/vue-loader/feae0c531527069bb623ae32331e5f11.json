iv class=\"desc\">\n            <span\n              ><img src=\"@/assets/redEnvelope/star.png\" />{{\n                lang.v.hb_dodia\n              }} </span\n            ><span\n              ><img src=\"@/assets/redEnvelope/star.png\" />{{\n                lang.v.hb_tooda\n              }}</span\n            >\n          </div>\n        </div>\n        <div class=\"timeFooter\">\n          <span\n            v-for=\"(item, index) in redEnvelopeRain.redRainConfiguration[0]\n              .redRainList\"\n            :key=\"index\"\n          >\n            <img src=\"@/assets/redEnvelope/time.png\" />{{ item.startTime }}-{{\n              item.endTime\n            }}\n          </span>\n        </div>\n      </div>\n      <div class=\"time\">\n        <div class=\"timeHeader\">\n          <div class=\"desc1\">3X {{ lang.v.hb_dia }}</div>\n          <div class=\"desc\">\n            <span\n              ><img src=\"@/assets/redEnvelope/star.png\" />{{ lang.v.hb_outras }}\n            </span>\n          </div>\n        </div>\n        <div class=\"timeFooter\">\n          <span\n            v-for=\"(item, index) in redEnvelopeRain.redRainConfiguration[2]\n              .redRainList\"\n            :key=\"index\"\n          >\n            <img src=\"@/assets/redEnvelope/time.png\" />{{ item.startTime }}-{{\n              item.endTime\n            }}\n          </span>\n        </div>\n      </div>\n      <ul class=\"rulelist\">\n        <li class=\"ruleitem\">\n          {{ lang.v.hb_cada }}\n          <span>R$100.000</span>\n        </li>\n        <li class=\"ruleitem\">\n          {{ lang.v.hb_valors }}<span>{{ lang.v.hb_sesao }} </span>\n        </li>\n        <li class=\"ruleitem\">\n          {{ lang.v.hb_recarregados }}\n        </li>\n        <li class=\"ruleitem\">\n          {{ lang.v.hb_dinheiro }}\n        </li>\n        <li class=\"ruleitem\">\n          {{ lang.v.hb_quanto }}\n        </li>\n        <li class=\"ruleitem\">\n          {{ lang.v.hb_end }}\n        </li>\n      </ul>\n      <div class=\"tips\">\n        {{ lang.v.hb_comeca }} {{ redEnvelopeRain.status.time }}\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { defineComponent, inject } from 'vue';\nimport { redEnvelopeRain } from './game';\nimport { ModalCloseFun } from '@/manager/view';\nimport { lang } from '@/lang';\n\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      closeFunction: inject('close') as ModalCloseFun,\n      redEnvelopeRain,\n    };\n  },\n  mounted() {\n    const content = this.$refs.content as HTMLElement;\n    redEnvelopeRain.init(content);\n  },\n  methods: {\n    close() {\n      this.closeFunction();\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.content {\n  position: fixed;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  z-index: 10;\n}\n.rules {\n  position: relative;\n  justify-content: flex-start;\n  flex-direction: column;\n  width: 500px;\n  padding: 10px;\n  height: auto;\n  background: url(~@/assets/redEnvelope/back.png) no-repeat;\n  background-size: 100% 100%;\n  z-index: 120;\n}\n.rules-close {\n  position: absolute;\n  right: 13px;\n  top: 15px;\n  width: 23px;\n  height: 23px;\n  background: url(~@/assets/redEnvelope/close.png) no-repeat;\n  background-size: 23px 23px;\n  cursor: pointer;\n}\n.rules .title {\n  height: 20px;\n  font-size: 20px;\n  font-family: Arial-BoldMT, Arial;\n  font-weight: 400;\n  color: #ffd800;\n  text-align: center;\n}\n.rules .titlebg,\n.rules .titleWrap {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n}\n.rules .titlebg {\n  width: 400px;\n  height: 112px;\n  background: url(~@/assets/redEnvelope/green.png) no-repeat;\n  background-size: 100% 100%;\n  margin-top: 20px;\n  align-items: center;\n  margin-bottom: 0;\n}\n.rules .titleWrap {\n  align-items: flex-start;\n}\n.rules .title2 {\n  text-align: center;\n  font-size: 20px;\n  font-family: Arial-BoldMT, Arial;\n  font-weight: 400;\n  color: #fff;\n  line-height: 24px;\n}\n.rules .title2 img {\n  width: 30px;\n  margin-right: 5px;\n}\n.rules .time {\n  padding: 2px 10px;\n  margin-top: 10px;\n  background: #009d7f;\n  border-radius: 5px;\n  width: 95%;\n  font-size: 13px;\n  font-family: Arial-BoldMT, Arial;\n  font-weight: 400;\n  color: #fff;\n}\n.rules .time .timeHeader {\n  display: flex;\n  width: 100%;\n  justify-content: flex-start;\n  margin-bottom: 2px;\n  align-items: center;\n}\n.rules .time .timeHeader .desc1 {\n  font-size: 23px;\n  font-family: Arial-BoldMT, Arial;\n  font-weight: 400;\n  color: gold;\n  line-height: 23px;\n  margin-right: 4px;\n}\n.rules .time .timeHeader .desc {\n  display: flex;\n  flex-direction: column;\n\n  justify-content: space-between;\n  margin-bottom: 5px;\n\n  transform: scale(0.96);\n}\n\n.rules .time .timeHeader .desc span img {\n  transform: scale(0.96);\n  width: 14px;\n  margin-right: 2px;\n}\n.rules .time .timeFooter {\n  display: flex;\n  flex-wrap: wrap;\n  width: 100%;\n  justify-content: space-between;\n}\n.rules .time .timeFooter span {\n  display: flex;\n  align-items: center;\n  font-size: 14px;\n  width: 30%;\n  line-height: 20px;\n  justify-content: center;\n  white-space: nowrap;\n}\n.rules .time .timeFooter span img {\n  width: 10px;\n  margin-right: 5px;\n}\n.rules .rulelist {\n  margin-top: 10px;\n  list-style-type: disc;\n  width: 93%;\n  font-size: 13px;\n  transform: scale(0.96);\n  font-family: Arial-BoldMT, Arial;\n  font-weight: 400;\n  color: #fff;\n  line-height: 20px;\n}\n.rules .rulelist li {\n  list-style: disc;\n}\n::marker {\n  unicode-bidi: isolate;\n  font-variant-numeric: tabular-nums;\n  text-transform: none;\n  text-indent: 0px !important;\n  text-align: start !important;\n  text-align-last: start !important;\n}\n.rules .rulelist .ruleitem span {\n  font-size: 15px;\n  font-family: Arial-Black, Arial;\n}\n.rules .tips {\n  margin-top: 10px;\n  font-size: 23px;\n  font-family: Arial-BoldMT, Arial;\n  font-weight: 400;\n  color: #ffd800;\n  text-align: center;\n  margin-bottom: 10px;\n}\n@media only screen and (max-width: 767px) {\n  .rules {\n    width: 95%;\n  }\n  .rules .titlebg {\n    width: 300px;\n    height: 84px;\n  }\n  .rules .title2 img {\n    width: 20px;\n  }\n  .rules .title2 {\n    font-size: 12px;\n    line-height: 22px;\n  }\n  .rules .time .timeHeader .desc1 {\n    font-size: 20px;\n  }\n  .rules .time {\n    font-size: 11px;\n    line-height: 14px;\n  }\n  .rules .time .timeHeader .desc span img {\n    width: 10px;\n  }\n  .rules .time .timeFooter span {\n    font-size: 10px;\n    line-height: 18px;\n  }\n  .rules .rulelist {\n    font-size: 10px;\n    line-height: 18px;\n  }\n  .rules .rulelist .ruleitem span {\n    font-size: 12px;\n  }\n  .rules .tips {\n    font-size: 17px;\n  }\n}\n</style>\n"]}]}