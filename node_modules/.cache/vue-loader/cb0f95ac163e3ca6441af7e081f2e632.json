llTimeout.value);\n      scrollTimeout.value = setTimeout(() => {\n        isScroll.value = false;\n      }, 400);\n    }\n    function onChange() {\n      isScroll.value = false;\n    }\n\n    return { ready, handleScroll, isScroll, onChange };\n  },\n  created() {\n    registerRouter(this.$router);\n  },\n  methods: {},\n});\n</script>\n\n<style lang=\"less\">\n#main {\n  display: flex;\n  flex-direction: column;\n  width: 100%;\n  height: 100%;\n  user-select: none;\n  overflow: hidden;\n}\n\n.body {\n  flex: 1;\n  display: flex;\n  height: 100%;\n  overflow: hidden;\n  position: relative;\n}\n\n.body-center {\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  overflow: auto;\n  .center-content {\n    padding: 0 40px;\n  }\n}\n\n::-webkit-scrollbar {\n  width: 0;\n}\n\nbutton {\n  background-color: transparent;\n  border: none;\n  padding: 0;\n  margin: 0;\n  font-family: inherit;\n  cursor: pointer;\n}\n\nul {\n  list-style-type: none;\n  padding: 0;\n  margin: 0;\n}\n\nli {\n  list-style: none;\n  margin: 0;\n  padding: 0;\n}\n\np {\n  margin-top: 0;\n}\n\n.c7,\np {\n  margin-bottom: 1rem;\n}\n\nbody {\n  font-feature-settings: 'tnum' on, 'lnum' on;\n  background-color: #0d131c;\n  color: white;\n  font-family: Montserrat Variable, sans-serif;\n  font-size: 14px;\n  font-weight: 500;\n  line-height: 1.5;\n}\n\ninput {\n  border: none;\n  outline: none;\n  background-color: transparent;\n}\n\n.h3,\n.h4,\nh3,\nh4 {\n  font-weight: 600;\n  letter-spacing: -0.02em;\n  margin-bottom: 1rem;\n  margin-top: 0;\n}\n\n.h3,\nh3 {\n  font-size: 18px;\n  line-height: 135%;\n}\n.tabbar {\n  display: none;\n}\n@media only screen and (max-width: 767px) {\n  body {\n    overflow: hidden;\n  }\n  #app {\n    position: fixed;\n    bottom: 0;\n    top: 0;\n    transition: height 2s ease-in-out;\n  }\n  .tabbar {\n    display: inline;\n  }\n  .body {\n    margin-bottom: 65px;\n  }\n  .center-content {\n    padding: 0 10px !important;\n    background: rgba(0, 0, 0, 0);\n  }\n  //禁用自带的回弹效果\n  body {\n    overscroll-behavior-x: none;\n    overscroll-behavior-y: none;\n    overscroll-behavior: none;\n    overflow: hidden;\n  }\n  // .body-center {\n  //   margin-bottom: 50px;\n  // }\n}\n</style>\n"]}]}