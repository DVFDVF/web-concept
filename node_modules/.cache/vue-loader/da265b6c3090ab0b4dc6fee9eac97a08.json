port { lang } from '@/lang';\nimport { ModalCloseFun } from '@/manager/view';\nimport { defineComponent, inject } from 'vue';\nimport IntroductionBtn from './IntroductionBtn.vue';\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      closeFunction: inject('close') as ModalCloseFun,\n      list: {\n        header: 'dice_autoMode',\n        title: 'dice_saveTimeWithAutoplay',\n        text: 'dice_autoModeAllows',\n        conntent: [\n          {\n            title: 'dice_numberOfBets',\n            text: ' dice_theNumber',\n          },\n          {\n            title: 'dice_nstopOnProfit',\n            text: 'dice_autoModeWill',\n          },\n          {\n            title: 'dice_stopOnLossMode',\n            text: 'dice_stopTheAuto',\n          },\n          {\n            title: 'dice_maxBetAmount',\n            text: 'dice_theMaxBet',\n          },\n          {\n            title: 'dice_onWinModel',\n            text: 'dice_thePercentage',\n          },\n          {\n            title: 'dice_onLossModel',\n            text: 'dice_betIncrease',\n          },\n        ],\n      },\n    };\n  },\n  methods: {\n    closeBtn() {\n      this.closeFunction?.();\n    },\n  },\n  components: { IntroductionBtn },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.autobet-info-desc {\n  font-size: 18px;\n  font-weight: 600;\n  line-height: 24px;\n  margin-bottom: 8px;\n  color: white;\n}\n\n.autobet-info-details-desc {\n  color: rgb(147, 172, 211);\n  font-size: 14px;\n  font-weight: 500;\n  line-height: 20px;\n  margin: 0px;\n}\n\n.autobet-info-details {\n  margin: 12px 0px;\n}\n\n.autobet-info-detail {\n  color: rgb(147, 172, 211);\n  font-size: 14px;\n  font-weight: 500;\n  line-height: 20px;\n  margin-bottom: 8px;\n  padding-left: 20px;\n  position: relative;\n}\n\n.autobet-info-detail::before {\n  background-color: rgb(147, 172, 211);\n  border-radius: 50%;\n  content: '';\n  height: 3px;\n  left: 5px;\n  position: absolute;\n  top: 9px;\n  width: 3px;\n}\n\n.autobet-info-detail-label {\n  color: rgb(255, 255, 255);\n  font-size: 14px;\n  font-weight: 500;\n  line-height: 20px;\n}\n\n.autobet-info-warning {\n  color: rgb(147, 172, 211);\n  font-size: 14px;\n  font-weight: 500;\n  line-height: 20px;\n}\n\n.autobet-info-warning span {\n  color: rgb(255, 255, 255);\n}\n\n.autobet-info-image {\n  width: 100%;\n}\n</style>\n"]}]}