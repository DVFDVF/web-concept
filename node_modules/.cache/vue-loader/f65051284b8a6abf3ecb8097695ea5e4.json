v>\n                  <div class=\"rank-slider-detail\">\n                    <p class=\"rank-slider-label\">{{ lang.v.vi_wager }}</p>\n                    <p class=\"rank-slider-value\">\n                      {{ item.chip }}\n                      <span class=\"rank-slider-currency\">BRL</span>\n                    </p>\n                  </div>\n                  <!-- 暂不需要 -->\n                  <!-- <div class=\"rank-slider-detail\">\n                    <p class=\"rank-slider-label\">{{ lang.v.vi_bonus }}</p>\n                    <p class=\"rank-slider-value\">\n                      {{ item.rankBonus }}\n                      <span class=\"rank-slider-currency\">USDT</span>\n                    </p>\n                  </div> -->\n                </div>\n                <div class=\"rank-slider-possibilities\">\n                  <div\n                    :class=\"[\n                      'rank-slider-possibility',\n                      { 'rank-slider-success': true },\n                    ]\"\n                  >\n                    <svg\n                      class=\"svg-use\"\n                      width=\"24\"\n                      height=\"24\"\n                      focusable=\"false\"\n                      aria-hidden=\"true\"\n                    >\n                      <use xlink:href=\"/svg/sprite.svg#icon-check-small\"></use>\n                    </svg>\n                    {{ lang.v.vi_cashback }}\n                  </div>\n                  <div\n                    :class=\"[\n                      'rank-slider-possibility',\n                      { 'rank-slider-success': true },\n                    ]\"\n                  >\n                    <svg\n                      class=\"svg-use\"\n                      width=\"24\"\n                      height=\"24\"\n                      focusable=\"false\"\n                      aria-hidden=\"true\"\n                    >\n                      <use xlink:href=\"/svg/sprite.svg#icon-check-small\"></use>\n                    </svg>\n                    {{ lang.v.vi_personalization }}\n                  </div>\n                  <div class=\"comming-soon\">{{ lang.v.re_da_comingSoon }}</div>\n                  <div\n                    :class=\"[\n                      'rank-slider-possibility',\n                      { 'rank-slider-success': item.DailyTasks },\n                    ]\"\n                  >\n                    <svg\n                      class=\"svg-use\"\n                      width=\"24\"\n                      height=\"24\"\n                      focusable=\"false\"\n                      aria-hidden=\"true\"\n                    >\n                      <use xlink:href=\"/svg/sprite.svg#icon-check-small\"></use>\n                    </svg>\n                    {{ lang.v.vi_daily }}\n                  </div>\n                  <div class=\"rank-slider-possibility\">\n                    <svg\n                      class=\"svg-use\"\n                      width=\"24\"\n                      height=\"24\"\n                      focusable=\"false\"\n                      aria-hidden=\"true\"\n                    >\n                      <use xlink:href=\"/svg/sprite.svg#icon-check-small\"></use>\n                    </svg>\n                    {{ lang.v.vi_accrual }}\n                  </div>\n                  <div class=\"rank-slider-possibility\">\n                    <svg\n                      class=\"svg-use\"\n                      width=\"24\"\n                      height=\"24\"\n                      focusable=\"false\"\n                      aria-hidden=\"true\"\n                    >\n                      <use xlink:href=\"/svg/sprite.svg#icon-check-small\"></use>\n                    </svg>\n                    {{ lang.v.vi_all }}\n                  </div>\n                  <div class=\"rank-slider-possibility\">\n                    <svg\n                      class=\"svg-use\"\n                      width=\"24\"\n                      height=\"24\"\n                      focusable=\"false\"\n                      aria-hidden=\"true\"\n                    >\n                      <use xlink:href=\"/svg/sprite.svg#icon-check-small\"></use>\n                    </svg>\n                    {{ lang.v.vi_exclusive }}\n                  </div>\n                  <div class=\"rank-slider-possibility\">\n                    <svg\n                      class=\"svg-use\"\n                      width=\"24\"\n                      height=\"24\"\n                      focusable=\"false\"\n                      aria-hidden=\"true\"\n                    >\n                      <use xlink:href=\"/svg/sprite.svg#icon-check-small\"></use>\n                    </svg>\n                    {{ lang.v.vi_vip }}\n                  </div>\n                </div>\n              </div>\n              <div class=\"rank-slider-bg-label\" v-show=\"myRank == item.id\">\n                <p class=\"rank-slider-bg-label-text\">{{ lang.v.vi_myRank }}</p>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"center\">\n      <rank-btn\n        href=\"/svg/sprite.svg#icon-arrow-left-small\"\n        @click=\"scrollLeft()\"\n      ></rank-btn>\n      <rank-btn\n        href=\"/svg/sprite.svg#icon-arrow-right-small\"\n        @click=\"scrollRight()\"\n      ></rank-btn>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport userManager from '@/manager/userManager';\nimport { defineComponent } from 'vue';\nimport RankBtn from './rankBtn.vue';\n\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      dataCenter,\n      myRank: 1,\n      offset: 0,\n      startScroll: false,\n      level: 0,\n      maxLevel: dataCenter.webConfig.levelConfigs.length,\n      name: [\n        'Newcomer',\n        'Gambler I',\n        'Gambler II',\n        'Gambler III',\n        'Bet maker I',\n        'Bet maker II',\n        'Bet maker III',\n        'Game master I',\n        'Game master II',\n        'Game master III',\n        'High Roller I',\n        'High Roller II',\n        'High Roller III',\n        'Between Pro I',\n        'Between Pro II',\n        'Between Pro III',\n        'BW Hero I',\n        'BW Hero II',\n        'BW Hero III',\n        'BW Legend',\n      ],\n    };\n  },\n  methods: {\n    scrollLeft() {\n      if (this.startScroll) return;\n      this.startScroll = true;\n      const container = this.$refs.splideList as HTMLElement;\n      if (container) {\n        const styles = window.getComputedStyle(container);\n        const marginLeft = styles.getPropertyValue('margin-left');\n        const marginLeftValue = parseFloat(marginLeft);\n        let scrollWight = container.scrollWidth;\n        let levelItemWidth =\n          (scrollWight - marginLeftValue * (this.maxLevel - 1)) / this.maxLevel;\n        let offsetLeft = container.scrollLeft;\n        let offset = this.offset;\n        if (Math.abs(offset - offsetLeft) >= 2) {\n          offset = Math.floor(offsetLeft / levelItemWidth) * levelItemWidth;\n        }\n        if (this.offset > 0) {\n          this.offset = offset - levelItemWidth - marginLeftValue;\n        }\n        this.animateScroll(container, this.offset, 500);\n      }\n    },\n    scrollRight() {\n      if (this.startScroll) return;\n      this.startScroll = true;\n      const container = this.$refs.splideList as HTMLElement;\n      if (container) {\n        const styles = window.getComputedStyle(container);\n        const marginLeft = styles.getPropertyValue('margin-left');\n        const marginLeftValue = parseFloat(marginLeft);\n        let scrollWight = container.scrollWidth;\n        let levelItemWidth =\n          (scrollWight - marginLeftValue * (this.maxLevel - 1)) / this.maxLevel;\n        let offsetLeft = container.scrollLeft;\n        let offset = this.offset;\n        if (Math.abs(offset - offsetLeft) >= 2) {\n          offset = Math.floor(offsetLeft / levelItemWidth) * levelItemWidth;\n        }\n        if (offset < scrollWight - container.clientWidth) {\n          this.offset = offset + levelItemWidth + marginLeftValue;\n        }\n        this.animateScroll(container, this.offset, 400);\n      }\n    },\n    // 动画函数\n    animateScroll(element: HTMLElement, target: number, duration: number) {\n      const start = element.scrollLeft;\n      const change = target - start;\n      let currentTime = 0;\n      const increment = 20;\n\n      const animateScroll = () => {\n        currentTime += increment;\n        const val = this.easeInOutQuad(currentTime, start, change, duration);\n        element.scrollLeft = val;\n        if (currentTime < duration) {\n          setTimeout(animateScroll, increment);\n        } else {\n          this.startScroll = false;\n        }\n      };\n      animateScroll();\n    },\n\n    // 缓动函数\n    easeInOutQuad(t: number, b: number, c: number, d: number): number {\n      t /= d / 2;\n      if (t < 1) return (c / 2) * t * t + b;\n      t--;\n      return (-c / 2) * (t * (t - 2) - 1) + b;\n    },\n  },\n  components: { RankBtn },\n  mounted() {\n    if (dataCenter.user.data) {\n      userManager.getUser(dataCenter.user.data?.id).then((userBrief) => {\n        if (userBrief) {\n          this.level = userBrief.level;\n        }\n      });\n    }\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.lp3 {\n  overflow: hidden;\n  padding: 73px 0;\n  position: relative;\n}\n.lp3-signs {\n  left: 0;\n  position: absolute;\n  top: -13px;\n  width: 100%;\n  z-index: -1;\n}\n.lp3-signs img {\n  width: 100%;\n}\n.lp3-heading {\n  margin-bottom: 24px;\n}\n\n.lp-heading {\n  margin: 0 auto 41px;\n}\n.lp3-title {\n  margin-bottom: 8px;\n}\n\n.lp-title {\n  color: #fff;\n  font-size: 40px;\n  line-height: 1.5;\n  margin: 0 auto 21px;\n  text-align: center;\n  text-transform: uppercase;\n}\n\n.lp-title span {\n  color: #fa305c;\n}\n.lp-toptext {\n  color: #9db8e1;\n  font-size: 18px;\n  line-height: 1.3;\n  margin: 0 auto;\n  max-width: 810px;\n  text-align: center;\n}\n.lp-toptext--info {\n  color: #93acd3;\n  padding-top: 16px;\n}\n\n.lp-toptext {\n  color: #9db8e1;\n  font-size: 18px;\n  line-height: 1.3;\n  margin: 0 auto;\n  max-width: 810px;\n  text-align: center;\n}\n.lp-toptext--info span {\n  color: #fff;\n}\n.rank-slider,\n.splide-track {\n  overflow: hidden;\n}\n\n.splide.is-initialized,\n.splide.is-rendered {\n  visibility: visible;\n}\n.rank-slider-slides {\n  display: flex;\n  overflow-x: auto;\n  transition: transform 0.5s ease;\n}\n\n.splide-list {\n  backface-visibility: hidden;\n  display: flex;\n  height: 100%;\n  margin: 0 !important;\n  padding: 0 !important;\n}\n\n.rank-slider-slide {\n  margin-bottom: 16px;\n  padding: 0 12px;\n  position: relative;\n  margin-right: 16px;\n  width: calc(33.3333% - 10.6667px);\n}\n.rank-slider-slide:last-child {\n  margin-right: 0;\n}\n\n.splide-slide {\n  backface-visibility: hidden;\n  box-sizing: border-box;\n  flex-shrink: 0;\n  list-style-type: none !important;\n\n  position: relative;\n  margin-bottom: 20px;\n}\n.splide-slide img {\n  vertical-align: bottom;\n}\n.rank-slider-bg {\n  height: calc(100% - 20px);\n  left: 0;\n  pointer-events: none;\n  position: absolute;\n  top: 20px;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  user-select: none;\n  width: 100%;\n}\n.rank-slider-slider-body {\n  position: relative;\n}\n.rank-slider-image {\n  height: 80px;\n  margin: auto;\n  position: relative;\n  width: 80px;\n}\n.rank-slider-image img {\n  left: 50%;\n  pointer-events: none;\n  position: absolute;\n  width: 80px;\n  top: 0;\n  transform: translate(-50%);\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  user-select: none;\n}\n\n.rank-slider-title {\n  margin-top: 10px;\n}\n.rank-slider-details {\n  border-bottom: 1px solid rgba(147, 172, 211, 0.1);\n  margin-bottom: 16px;\n}\n.rank-slider-label {\n  color: #93acd3;\n  font-size: 10px;\n  font-weight: 500;\n  line-height: 14px;\n  margin: 0;\n  text-align: center;\n}\n.rank-slider-cashback {\n  color: #ffb636;\n}\n.rank-slider-label {\n  color: #93acd3;\n  font-size: 10px;\n  font-weight: 500;\n  line-height: 14px;\n  margin: 0;\n  text-align: center;\n}\n.rank-slider-value {\n  color: #1bb83d;\n  font-size: 14px;\n  font-weight: 600;\n  line-height: 18px;\n  text-align: center;\n}\n.rank-slider-currency {\n  color: #fff;\n}\n.rank-slider-possibilities {\n  text-align: center;\n}\n\n.rank-slider-possibility {\n  color: #55657e;\n  font-size: 10px;\n  font-weight: 500;\n  line-height: 14px;\n  display: flex;\n  flex-direction: row;\n  justify-content: center;\n  align-items: center;\n}\n.comming-soon {\n  color: #ffb5369c;\n  font-size: 13px;\n  margin-top: 8px;\n}\n.rank-slider-possibility .rank-slider-bg-label {\n  background-image: url('~@/assets/rankVip/my-rank-label.png');\n  background-position: 50%;\n  background-repeat: no-repeat;\n  bottom: -16px;\n  left: 50%;\n  position: relative;\n  transform: translate(-50%);\n  width: 85px;\n}\n.rank-slider-bg-label-text {\n  font-size: 10px;\n  font-weight: 600;\n  line-height: 16px;\n  margin: 0;\n  text-align: center;\n}\n.rank-slider,\n.rank-slider-title {\n  font-size: 18px;\n  font-weight: 600;\n  line-height: 24px;\n  text-align: center;\n}\n.rank-slider-cashback {\n  text-align: center;\n}\n.rank-slider-success {\n  color: white;\n}\n.rank-slider-success svg {\n  fill: #1bb83d;\n}\n@media only screen and (max-width: 767px) {\n  .lp3-heading {\n    margin-left: 22px;\n    margin-right: 22px;\n  }\n  .lp3-signs {\n    display: none;\n  }\n  .lp3 {\n    padding: 0;\n  }\n  .lp-title {\n    font-size: 35px;\n    line-height: 45px;\n    margin: 0 auto 19px;\n  }\n\n  .rank-slider-slide {\n    margin: 0 10px 10px;\n    width: calc(100% - 20px);\n  }\n  .rank-slider-slide:last-child {\n    margin-right: 10px;\n  }\n}\n</style>\n"]}]}