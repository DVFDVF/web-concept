HANGEPASSWORD_URL, ChangePassReq } from './headProtocol';\n\n//根据用户ID查询个人档案\nimport { QUERYFILES_URL, UserInfoReq } from './headProtocol';\n//获取用户档案\nexport async function getUserInformation() {\n  try {\n    const resp = await get<UserProfileResp>(USERPROFILE_URL);\n    return resp;\n  } catch (error) {\n    return undefined;\n  }\n}\n\n//获取获取用户各个币种的余额\nexport async function getUserBalance() {\n  try {\n    const resp = await get<BabalceRespItem[]>(USERBALANCE_URL);\n    return resp;\n  } catch (error) {\n    return undefined;\n  }\n}\n\n//获取游戏记录\nexport async function userAllGameRecords(pageSize: number, pageNumber: number) {\n  try {\n    const response = await get<UserGameResp, UserGameReq>(GAMEALL_URL, {\n      pageSize,\n      pageNumber,\n    });\n    return response;\n  } catch (error) {\n    return undefined;\n  }\n}\n\n//修改密码\nexport async function userChangePass(requestData: ChangePassReq) {\n  try {\n    const response = await post<string, ChangePassReq>(\n      CHANGEPASSWORD_URL,\n      requestData,\n    );\n    return response;\n  } catch (error) {\n    return undefined;\n  }\n}\n\n//根据用户ID查询个人档案\nexport async function getQueryPersonalFiles(userId: number) {\n  try {\n    const response = await get<UserProfileResp, UserInfoReq>(QUERYFILES_URL, {\n      userId,\n    });\n    return response;\n  } catch (error) {\n    return undefined;\n  }\n}\n"],"mappings":"AAAA,SAAkBA,GAAG,EAAEC,IAAI,QAAQ,OAAO;AAE1C;AACA,SAASC,eAAe,QAAyB,gBAAgB;AACjE;AACA,SAASC,eAAe,QAAyB,gBAAgB;AAEjE;AACA,SAASC,WAAW,QAAmC,gBAAgB;AAEvE;AACA,SAASC,kBAAkB,QAAuB,gBAAgB;AAElE;AACA,SAASC,cAAc,QAAqB,gBAAgB;AAC5D;AACA,OAAO,eAAeC,kBAAkBA,CAAA;EACtC,IAAI;IACF,MAAMC,IAAI,GAAG,MAAMR,GAAG,CAAkBE,eAAe,CAAC;IACxD,OAAOM,IAAI;GACZ,CAAC,OAAOC,KAAK,EAAE;IACd,OAAOC,SAAS;;AAEpB;AAEA;AACA,OAAO,eAAeC,cAAcA,CAAA;EAClC,IAAI;IACF,MAAMH,IAAI,GAAG,MAAMR,GAAG,CAAoBG,eAAe,CAAC;IAC1D,OAAOK,IAAI;GACZ,CAAC,OAAOC,KAAK,EAAE;IACd,OAAOC,SAAS;;AAEpB;AAEA;AACA,OAAO,eAAeE,kBAAkBA,CAACC,QAAgB,EAAEC,UAAkB;EAC3E,IAAI;IACF,MA