y=\"index\"\n        >\n          <h2>{{ value.title }}</h2>\n          <ol class=\"battles-ol\">\n            <li\n              class=\"battles-li\"\n              v-for=\"(liValue, liIndex) in value.olList\"\n              v-bind:key=\"liIndex\"\n            >\n              {{ liValue.tl }}\n            </li>\n          </ol>\n        </div>\n      </div>\n      <div>\n        <div class=\"battles-controls row-center\">\n          <img\n            style=\"width: 24px; height: 24px\"\n            src=\"@/assets/competition/contests-battle-small.png\"\n          />\n          <span class=\"competition-game-title\">{{ lang.v.ba_battles }}</span>\n        </div>\n        <div class=\"comptitions-casion\">\n          <div v-show=\"competitionData.length > 0\">\n            <CasinoBattlesTournaments :message=\"competitionData\" />\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent, ref } from 'vue';\nimport CasinoBattlesTournaments from '@/views/home/casino/LobbySections/battles/CasinoBattlesTournaments.vue';\nimport { lang } from '@/lang';\nimport { CompetitionResp, CompetitionTyp } from './api/competitionProtocol';\nimport { getCompetition } from './api/competitionIndex';\nimport dataCenter from '@/dataCenter';\nexport default defineComponent({\n  components: { CasinoBattlesTournaments },\n  setup() {\n    const competitionData = ref<CompetitionResp[]>([]);\n    /**数据下标 */\n    const slideChangeIndex = ref(0);\n    /**获取竞赛数据 */\n    getCompetition(CompetitionTyp.INPROGRESS).then((resp) => {\n      if (!resp) return;\n      competitionData.value = resp;\n    });\n    return { competitionData, slideChangeIndex };\n  },\n  data() {\n    return {\n      lang,\n      dataCenter,\n      BatteList: [\n        {\n          get title() {\n            return lang.v.ad_ba_tilte;\n          },\n          olList: [\n            {\n              get tl() {\n                return lang.v.ad_ba_sign.replace(\n                  '{siteName}',\n                  dataCenter.websiteName_Medium,\n                );\n              },\n            },\n            {\n              get tl() {\n                return lang.v.ad_ba_find;\n              },\n            },\n            {\n              get tl() {\n                return lang.v.ad_ba_win;\n              },\n            },\n            {\n              get tl() {\n                return lang.v.ad_ba_enjoy;\n              },\n            },\n          ],\n        },\n        {\n          get title() {\n            return lang.v.ad_ba_yourTitle;\n          },\n          olList: [\n            {\n              get tl() {\n                return lang.v.ad_ba_daily;\n              },\n            },\n            {\n              get tl() {\n                return lang.v.ad_ba_sports;\n              },\n            },\n            {\n              get tl() {\n                return lang.v.ad_ba_special;\n              },\n            },\n            {\n              get tl() {\n                return lang.v.ad_ba_thematic;\n              },\n            },\n            {\n              get tl() {\n                return lang.v.ad_ba_rollers;\n              },\n            },\n          ],\n        },\n      ],\n    };\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.account-alt {\n  padding: 34px 0;\n  flex: 1;\n  min-height: 400px;\n}\n.account-container {\n  margin: 0 auto;\n  max-width: 1088px;\n  padding-left: 24px;\n  padding-right: 24px;\n  position: relative;\n  width: 100%;\n  font-feature-settings: 'tnum' on, 'lnum' on;\n  color: #fff;\n  text-align: left;\n}\n.competis {\n  overflow: hidden;\n  pointer-events: none;\n  border-radius: 20px;\n  width: 100%;\n}\n.battles-landing-h1 {\n  font-size: 45px;\n  font-weight: 900;\n  line-height: 60px;\n  margin-top: 50px;\n  max-width: 90%;\n  text-transform: uppercase;\n\n  & span {\n    color: #ed1d49;\n  }\n}\n.battles-landing-p {\n  font-size: 18px;\n  font-weight: 500;\n  line-height: 1.6;\n  & span {\n    display: block;\n    font-weight: 600;\n  }\n}\n.battles-landing-jon {\n  align-items: flex-start;\n  display: flex;\n  justify-content: space-between;\n  margin-top: 50px;\n}\n\n.battles-lan-container {\n  max-width: 410px;\n  font-family: Montserrat Variable, sans-serif;\n  & h2 {\n    color: #0194ff;\n    font-size: 26px;\n    line-height: 60px;\n    margin-bottom: 0;\n    text-transform: uppercase;\n    font-weight: 700;\n    letter-spacing: -0.02em;\n  }\n}\n.battles-ol {\n  list-style-type: disc;\n  margin-left: -1em;\n  .battles-li {\n    list-style-type: disc;\n    font-size: 17px;\n    font-weight: 600;\n    line-height: 38px;\n  }\n}\n\n.battles-controls {\n  cursor: auto;\n  height: 32px;\n  margin-bottom: 16px;\n  align-items: center;\n}\n.competition-game-title {\n  font-size: 16px;\n  font-weight: 600;\n  letter-spacing: -0.02em;\n  line-height: 20px;\n  color: #55657e;\n  margin-left: 8px;\n  cursor: pointer;\n  &:hover {\n    color: #fff;\n  }\n}\n\n.comptitions-casion {\n  display: flex;\n  overflow-x: auto;\n  white-space: nowrap;\n  gap: 16px;\n  backface-visibility: hidden;\n  cursor: pointer;\n}\n\n@media only screen and (max-width: 767px) {\n  .battles-landing-p {\n    font-weight: 600;\n  }\n  .battles-landing-jon {\n    flex-direction: column;\n  }\n  .battles-ol {\n    margin: 0;\n    padding: 0;\n  }\n  .comptitions-casion {\n    flex-direction: column;\n    width: 100%;\n  }\n  .account-container {\n    padding: 0;\n  }\n}\n</style>\n"]}]}