        <svg class=\"svg-use\" focusable=\"false\" aria-hidden=\"true\">\n              <use :xlink:href=\"spendIcon\"></use>\n            </svg>\n          </div>\n        </div>\n        <div class=\"menu\" v-show=\"isSpend\">\n          <ul class=\"dropdown-menu-inner\">\n            <li\n              class=\"dropdown-menu-item\"\n              v-for=\"item in itemList\"\n              :key=\"item.key\"\n              @click=\"selectBtn(item)\"\n            >\n              <button type=\"button\" class=\"btn\">\n                <label :class=\"selectNumber(item.key)\">\n                  <span class=\"switcher-icon icon\">\n                    <component :is=\"item.icon\" />\n                  </span>\n                  <div class=\"no-wrap\">\n                    {{ item.name }}\n                  </div>\n                </label>\n              </button>\n            </li>\n          </ul>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent } from 'vue';\nimport Login from '../../login/Login.vue';\nimport { contactRoller } from '../contacRoller';\nexport default defineComponent({\n  props: ['numberList', 'itemList'],\n  computed: {},\n  methods: {\n    selectNumber(text: string) {\n      return this.selectItemData.key != text\n        ? 'dropdown-menu-link'\n        : 'dropdown-menu-link dropdown-menu-link-click';\n    },\n    selectBtn(item: any) {\n      this.selectItemData = item;\n      this.isSpend = false;\n      this.spendIcon = '/svg/sprite.svg#icon-arrow-down';\n      this.clickBtn(item.key);\n    },\n    clickBtn(name: string) {\n      if (contactRoller.selectItem == name) return;\n      contactRoller.selectItem = name;\n      contactRoller.selectItem = name;\n      if (contactRoller.selectItem == 'My Bets') {\n        contactRoller.selectType = '4';\n      } else {\n        if (contactRoller.selectItem == 'High Rollers') {\n          contactRoller.selectType = '3';\n        }\n        if (contactRoller.selectItem == 'Rare Wins') {\n          contactRoller.selectType = '1';\n        }\n        if (contactRoller.selectItem == 'All Bets') {\n          contactRoller.selectType = '1';\n        }\n      }\n      if (contactRoller.selectItem == 'High Rollers') {\n        contactRoller.selectType = '3';\n      } else if (contactRoller.selectItem == 'Sports Bets') {\n        contactRoller.selectType = '3';\n      } else if (contactRoller.selectItem == 'Casino Bets') {\n        contactRoller.selectType = '1';\n      }\n      // this.$emit('childEvent', this.selectItem, selectType);\n    },\n    showColor(name: string) {\n      return this.selectItemData.text == name\n        ? 'switcher-label select-lable'\n        : 'switcher-label';\n    },\n    dropdownExpand() {\n      this.isSpend = !this.isSpend;\n      if (this.isSpend) {\n        this.spendIcon = '/svg/sprite.svg#icon-arrow-up';\n      } else {\n        this.spendIcon = '/svg/sprite.svg#icon-arrow-down';\n      }\n    },\n  },\n  mounted() {\n    this.clickBtn(contactRoller.selectItem);\n  },\n  data() {\n    return {\n      isSpend: false,\n      spendIcon: '/svg/sprite.svg#icon-arrow-down',\n      selectItem: contactRoller.selectItem,\n      selectItemData: this.itemList[0],\n      selectNumberText: '10',\n    };\n  },\n  components: { Login },\n});\n</script>\n<style lang=\"less\" scoped>\n.content {\n  position: relative;\n  display: flex;\n  justify-content: space-between;\n  align-items: flex-end;\n  z-index: 99;\n\n  .statistic {\n    display: inline-block;\n    border-radius: 8px;\n    color: #55657e;\n    padding: 6px;\n    background: #37465c50;\n\n    .select-item {\n      .switcher-label {\n        align-items: center;\n        border-radius: 8px;\n        color: #55657e;\n        cursor: pointer;\n        display: flex;\n        font-size: 14px;\n        font-weight: 600;\n        height: 40px;\n        letter-spacing: -0.28px;\n        line-height: 125%;\n        margin-left: 4px;\n        padding: 0 20px 0 16px;\n        transition: background 0.1s ease-in-out, color 0.1s ease-in-out;\n\n        .icon svg {\n          width: 24px;\n        }\n      }\n\n      .select-lable {\n        background-color: #2283f6;\n        color: white;\n      }\n    }\n  }\n}\n.switcher-icon {\n  width: 24px;\n  height: 24px;\n  margin-right: 8px;\n  transition: color 0.1s ease-in-out;\n}\n\nbutton {\n  border: none;\n  padding: 0;\n  background: none;\n  cursor: pointer;\n}\n\n.drop {\n  .dropdown {\n    // position: relative;\n    border-radius: 8px;\n    font-size: 12px;\n    height: 36px;\n    padding: 7px 13px;\n    display: inline-block;\n    background-color: #4458743d;\n    border: 1px solid transparent;\n    box-shadow: 0 2px 4px #0000002e;\n    transition: background-color 0.1s ease-in-out, border-color 0.1s ease-in-out;\n\n    .dropdown-trigger {\n      align-items: center;\n      cursor: pointer;\n      display: flex;\n      height: 100%;\n      position: relative;\n      justify-content: space-between;\n      width: 100%;\n\n      .trigger-content {\n        display: flex;\n        flex: 1 1;\n        flex-direction: column;\n        overflow-x: hidden;\n        text-overflow: ellipsis;\n        white-space: nowrap;\n\n        .trigger-content-text {\n          font-weight: 600;\n          color: white;\n        }\n      }\n\n      .svg-use {\n        height: 20px;\n        margin-left: 4px;\n        margin-right: -6px;\n        width: 20px;\n        fill: #93acd3;\n        color: #93acd3;\n        pointer-events: none;\n        transform: rotate(0);\n        transition: fill 0.1s ease-in-out, transform 0.1s ease-in-out;\n      }\n    }\n  }\n\n  .menu {\n    width: 100%;\n    display: flex;\n    position: relative;\n\n    .dropdown-menu-inner {\n      margin-top: 10px;\n      display: flex;\n      position: relative;\n      border-radius: 8px;\n      flex-direction: column;\n      background-color: #161f2c;\n      border: 1px solid transparent;\n      box-shadow: 0 2px 4px #0000002e;\n      width: 100%;\n\n      .dropdown-menu-item {\n        // padding: 8px;\n        border-radius: 10px;\n        font-family: Montserrat Variable, sans-serif;\n        margin-right: 15px;\n        font-size: 14px;\n        font-weight: 600;\n        line-height: 1.5;\n\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        width: 100%;\n        padding: 6px 8px;\n\n        .dropdown-menu-link {\n          width: 100%;\n          height: 100%;\n          padding: 0px 6px;\n          text-align: left;\n          border-radius: 10px;\n          font-weight: 600;\n          color: #55657e;\n        }\n\n        .dropdown-menu-link :hover {\n          color: white;\n        }\n\n        .dropdown-menu-link-click {\n          padding: 8px 6px;\n          background-color: #2a3546;\n          text-align: center;\n          color: white;\n        }\n      }\n    }\n  }\n}\n@media only screen and (max-width: 767px) {\n  .drop .menu .dropdown-menu-inner {\n    position: absolute;\n    right: 0;\n    width: 100%;\n  }\n  // .drop .dropdown {\n  //   width: 150px;\n  // }\n  .icon svg {\n    width: 24px;\n    height: 24px;\n    margin-right: 5px;\n  }\n  .switcher-label {\n    display: flex;\n    flex-direction: row;\n    margin-right: 5px;\n  }\n  .item-name {\n    color: white;\n    font-size: 13px;\n    font-weight: 600;\n    padding-right: 10px;\n  }\n  .dropdown-menu-link {\n    display: flex;\n    justify-content: flex-start;\n    align-items: center;\n  }\n  .no-wrap {\n    white-space: nowrap;\n    overflow: hidden;\n    text-overflow: ellipsis;\n    font-size: 12px;\n  }\n  .content-title {\n    color: white;\n    font-weight: 700;\n  }\n  .btn {\n    display: flex;\n    justify-content: flex-start;\n    width: 100%;\n  }\n}\n</style>\n"]}]}