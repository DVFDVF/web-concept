    getCode(this.emailValue, 1).then((response) => {\n        if (response) {\n          tip.success('Successfully sent');\n          this.selectItem = 1;\n        }\n      });\n    },\n    //获取input输入\n    inputContent(key: string, value: string) {\n      this.emailValue = value;\n    },\n    //弹出忘记密码界面\n    confirmEmail() {\n      this.isModifyPassword = true;\n    },\n    //获取邮件验证码\n    onChange(codeArr: string[]) {\n      this.emailCodeValue = codeArr.join('');\n    },\n    //提交修改\n    submitClick(password: string) {\n      forgetPassword(this.emailValue, this.emailCodeValue, password).then(\n        (response) => {\n          if (!response) return;\n          this.closeFunction();\n          tip.success(response);\n        },\n      );\n    },\n  },\n});\n"],"mappings":"AACA,SAASA,GAAG,QAAQ,wBAAwB;AAC5C,SAASC,IAAI,QAAQ,QAAQ;AAG7B,SAASC,eAAe,EAAEC,MAAM,QAAQ,KAAK;AAC7C,SAASC,cAAc,EAAEC,OAAO,QAAQ,QAAQ;AAChD,OAAOC,QAAQ,MAAM,gBAAgB;AACrC,OAAOC,iBAAiB,MAAM,2CAA2C;AACzE,OAAOC,eAAe,MAAM,gCAAgC;AAC5D,OAAOC,cAAc,MAAM,sBAAsB;AAEjD,eAAeP,eAAe,CAAC;EAC7BQ,IAAI,EAAE,SAAS;EACfC,UAAU,EAAE;IACVH,eAAe;IACfF,QAAQ;IACRC,iBAAiB;IACjBE;GACD;EACDG,IAAIA,CAAA;IACF,OAAO;MACLX,IAAI;MACJY,aAAa,EAAEV,MAAM,CAAC,OAAO,CAAkB;MAC/CW,IAAI,EAAE;QACJC,IAAI,EAAE,gCAAgC;QACtCC,GAAG,EAAE,OAAO;QACZ,IAAIN,IAAIA,CAAA;UACN,OAAOT,IAAI,CAACgB,CAAC,CAACC,OAAO;QACvB;OACD;MACDC,QAAQ,EAAE,iCAAiC;MAC3CC,aAAa,EAAE;QACbC,IAAI,EAAE,GAAG;QACT,IAAIC,IAAIA,CAAA;UACN,OAAOrB,IAAI,CAACgB,CAAC,CAACM,SAAS;QACzB,CAAC;QACDC,MAAM,EAAE;OACT;MACDC,UAAU,EAAE,CAAC;MACbC,WAAW,EAAE,KAAK;MAClBC,gBAAgB,EAAE,KAAK;MACvBC,UAAU,EAAE,EAAE;MACdC,cAAc,EAAE;KACjB;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA;MACP,IAAI,IAAI,CAACN,UAAU,IAAI,CAAC,EAAE;QACxB,IAAI,CAACZ,aAAa,EAAE;OACrB,MAAM;QACL,IAAI,CAACY,UAAU,GAAG,CAAC;;IAEvB,CAAC;IACDO,UAAUA,CAAA;MACR,IAAI,IAAI,CAACC,YAAY,CAAC,IAAI,CAACL,UAAU,CAAC,EAAE;QACtC,IAAI,CAACM,QAAQ,EAAE;;IAEnB,CAAC;IACDD,YAAYA,CAACE,KAAa;MACxB,MAAMC,UAAU,GAAG,4BAA4B;MAC/C,OAAOA,UAAU,CAACC,IAAI,CAACF,KAAK,CAAC;IAC/B,CAA