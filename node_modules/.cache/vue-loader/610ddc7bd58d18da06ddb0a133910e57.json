an>\n    <div class=\"wheel-history-row-recent\">\n      <Avatar :size=\"24\" :user=\"userData\" />\n      <span class=\"user-name\">{{ userData.name }}</span>\n    </div>\n    <div class=\"wheel-history-row-enry\">\n      <Image\n        style=\"width: 22px; height: 22px\"\n        :src=\"getCuurrencyIcon(record.currencyId)\"\n      />\n      <span class=\"user-name\">{{ record.value }}</span>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent } from 'vue';\nimport { AwardResp } from './api/protocol';\nimport dayjs from 'dayjs';\nimport dataCenter from '@/dataCenter';\nimport userManager from '@/manager/userManager';\nimport Avatar from '@/components/Avatar.vue';\n\nexport default defineComponent({\n  components: {\n    Avatar,\n  },\n  data() {\n    return {\n      /**临时存储用户数据 */\n      userData: { name: '', avatar: '' },\n    };\n  },\n  props: {\n    record: {\n      type: Object as () => AwardResp,\n      required: true,\n    },\n  },\n  watch: {\n    'record.userId': function (value) {\n      this.getUserData(value.userId);\n    },\n  },\n  mounted() {\n    this.getUserData(this.record.userId);\n  },\n  methods: {\n    /**转换时间 */\n    conversionTime(wheelTime: string): string {\n      return dayjs(wheelTime).format('HH:mm:ss');\n    },\n    //查询用户信息\n    getUserData(userId: number) {\n      userManager.getUser(userId)?.then((resp) => {\n        if (!resp) return;\n        this.userData = { name: resp.name, avatar: resp.avatar };\n      });\n    },\n    //查询货币icon\n    getCuurrencyIcon(currencyid: number) {\n      if (dataCenter.webConfig.cryptoCurrencies.concat()) {\n        const foundCurrency = dataCenter.webConfig.cryptoCurrencies\n          .concat()\n          .find((currency) => currency.id === currencyid);\n        return foundCurrency?.icon;\n      }\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.wheel-history-row {\n  align-items: center;\n  border-bottom: 1px solid #1c2532;\n  display: flex;\n  justify-content: space-between;\n  height: 40px;\n}\n.wheel-history-col-time {\n  width: 28%;\n  color: #93acd3;\n  font-size: 12px;\n  font-weight: 600;\n  line-height: 16px;\n}\n.wheel-history-row-recent {\n  width: 40%;\n  cursor: pointer;\n  align-items: center;\n  display: flex;\n}\n\n.user-name {\n  overflow-x: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n  font-size: 12px;\n  margin-left: 8px;\n}\n\n.wheel-history-row-enry {\n  width: 32%;\n  cursor: pointer;\n  align-items: center;\n  display: flex;\n}\n</style>\n"]}]}