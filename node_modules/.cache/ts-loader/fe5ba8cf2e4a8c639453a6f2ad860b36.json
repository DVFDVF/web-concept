","onWinisReset","OnLessisReset","topIcons","icon","text","v","dice_fast","key","dice_fairness","dice_rules","dice_hotkeys","topNumber","created","gameConfiguration","ruleData","require","dice_ru_placeabet","dice_ho_randomNumberGenerator","beforeUnmount","computed","returnRate","caculateRate","methods","toFixed","SliderNumber","RollControlsData","getAutoBetData","value","mechanism","input","WinOrLose","startGame","user","Number","min","startGamePlay","String","selectId","then","response","play","change","isFast","isLess","betResult","delay","set","balance","push","name","currency","getSelectedCurrency","betAmount","downForm","setTimeout","getLuckyNumber","ms","Promise","resolve","generateRandomNumber","max","Math","floor","random","formatInput","updateValue","modeClick","components"],"sources":["/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/cache-loader/dist/cjs.js??ref--1-0!/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/vue-loader-v16/dist/index.js??ref--1-1!/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/game/dice/left/GameLeft.vue?vue&type=script&lang=ts"],"sourcesContent":["\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { soundManager } from '@/manager/SoundManager';\nimport { openModal } from '@/manager/view';\nimport { addArrayToLocal } from '@/utils';\nimport DicePlace from '@/views/game/dice/audio/dice-place.mp3';\nimport DiceWin from '@/views/game/dice/audio/dice-win.mp3';\nimport WalletButtonPop from '@/views/home/head/popup/WalletButtonPop.vue';\nimport LoginWidget from '@/views/home/login/Login.vue';\nimport { defineComponent } from 'vue';\nimport { playGame } from './api';\nimport Autobet from './Autobet.vue';\nimport BoardIn from './BoardIn.vue';\nimport DiceGameMode from './DiceGameMode.vue';\nimport GameForm from './GameForm.vue';\nimport { Configuration, NumberRecordData } from './interface';\nimport LastBets from './LastBets.vue';\nimport RollControls from './RollControls.vue';\nimport Slider from './Slider.vue';\nimport TopIcon from './TopIcon.vue';\nimport { formatMoney } from '@/utils/currency';\nimport { amountAdd } from '@/games/methods';\nexport default defineComponent({\n  setup(): { getBalance: () => number } {\n    return {\n      getBalance: () => dataCenter.wallet.getBalance(),\n    };\n  },\n  data() {\n    return {\n      lang,\n      number: 50,\n      multiplier: dataCenter.webConfig.profitRate * 100 ?? 99.07,\n      amount: 200,\n      minNumber: 1,\n      maxNumber: 95,\n      trans: 50,\n      luckyNumber: 0,\n      currentAmount: 0,\n      bettingAmount: '10.00',\n      isRollUnder: true,\n      isStart: false,\n      isEnable: true,\n      isAuto: false,\n      isWin: false,\n      numberList: [] as NumberRecordData[],\n      childAutobet: {\n        count: '',\n        stopOnProfit: '',\n        StopOnLoss: '',\n        MaxBetAmount: '',\n        OnWin: '',\n        OnLoss: '',\n        onWinisReset: true,\n        OnLessisReset: true,\n      },\n\n      topIcons: [\n        // 统计暂不需要\n        // {\n        //   icon: '/svg/sprite.svg#icon-statistic',\n        //   get text() {\n        //     return lang.v.dice_statictic;\n        //   },\n        //   key: 'Statistic',\n        // },\n        {\n          icon: '/svg/sprite.svg#icon-speed',\n          get text() {\n            return lang.v.dice_fast;\n          },\n          key: 'Fast',\n        },\n        {\n          icon: '/svg/sprite.svg#icon-fairness',\n          get text() {\n            return lang.v.dice_fairness;\n          },\n          key: 'Fairness',\n        },\n        {\n          icon: '/svg/sprite.svg#icon-rules',\n          get text() {\n            return lang.v.dice_rules;\n          },\n          key: 'Rules',\n        },\n        {\n          icon: '/svg/sprite.svg#icon-sound-on',\n          text: 'sound',\n        },\n        {\n          icon: '/svg/sprite.svg#icon-hotkeys',\n          get text() {\n            return lang.v.dice_hotkeys;\n          },\n          key: 'Hotkeys',\n        },\n      ],\n      topNumber: ['5x', '5x', '5x', '5x', '5x', '5x', '5x', '5x'],\n    };\n  },\n  created() {\n    dataCenter.gameConfiguration.ruleData = [\n      {\n        icon: require('@/assets/dice/step-1.webp'),\n        get text() {\n          return lang.v.dice_ru_placeabet;\n        },\n      },\n      {\n        icon: require('@/assets/dice/step-2.webp'),\n        get text() {\n          return lang.v.dice_ho_randomNumberGenerator;\n        },\n      },\n    ];\n  },\n  beforeUnmount() {\n    this.isStart = false;\n  },\n  computed: {\n    returnRate() {\n      return (this as any).caculateRate();\n    },\n  },\n  methods: {\n    caculateRate() {\n      // 回报率\n      return this.isRollUnder\n        ? (this.multiplier / this.number).toFixed(4)\n        : (this.multiplier / (this.maxNumber - this.number + 1)).toFixed(4);\n    },\n    SliderNumber(number: number) {\n      this.number = number;\n    },\n    RollControlsData(\n      minNumber: number,\n      maxNumber: number,\n      isRollUnder: boolean,\n    ) {\n      this.minNumber = minNumber;\n      this.maxNumber = maxNumber;\n      this.isRollUnder = isRollUnder;\n    },\n    getAutoBetData(value: Configuration) {\n      this.childAutobet.count = value.mechanism[0].input;\n      this.childAutobet.stopOnProfit = value.mechanism[1].input;\n      this.childAutobet.StopOnLoss = value.mechanism[2].input;\n      this.childAutobet.MaxBetAmount = value.mechanism[3].input;\n      this.childAutobet.OnWin = value.WinOrLose[0].input;\n      this.childAutobet.OnLoss = value.WinOrLose[1].input;\n    },\n    async startGame() {\n      if (!dataCenter.user.data) {\n        openModal(LoginWidget);\n        return;\n      }\n\n      if (\n        this.getBalance() < Number(this.bettingAmount) ||\n        Number(this.bettingAmount) < dataCenter.webConfig.min\n      ) {\n        openModal(WalletButtonPop);\n        return;\n      }\n      this.isStart = !this.isStart;\n      this.currentAmount = 0;\n      this.startGamePlay();\n    },\n    startGamePlay() {\n      if (this.isStart) {\n        if (this.childAutobet.count != '') {\n          this.childAutobet.count = String(Number(this.childAutobet.count) - 1);\n        }\n\n        playGame(\n          this.number,\n          this.isRollUnder ? 0 : 1,\n          Number(this.bettingAmount),\n          dataCenter.wallet.selectId,\n        ).then(async (response) => {\n          if (!response) {\n            this.isStart = false;\n            return;\n          }\n          addArrayToLocal('Recent', 1);\n\n          this.trans = 2;\n          //播放音效\n          soundManager.play(DicePlace);\n          // 扣钱\n          dataCenter.wallet.change(\n            dataCenter.wallet.selectId,\n            -this.bettingAmount,\n          );\n          this.isWin = response.isWin == 0 ? true : false;\n          //判断是否为快速模式\n          if (!dataCenter.gameConfiguration.isFast) {\n            //判断数字间是否小于30\n            let isLess =\n              response.betResult - this.luckyNumber < 30 &&\n              response.betResult - this.luckyNumber > -30;\n\n            // 开始循环\n            while (this.luckyNumber != response.betResult || isLess) {\n              if (this.luckyNumber == response.betResult) {\n                isLess = false;\n              }\n              if (this.luckyNumber < 99) {\n                this.luckyNumber += 1;\n              } else {\n                this.luckyNumber = 0;\n              }\n              await this.delay(20);\n            }\n            await this.delay(200);\n            this.trans = 50;\n            // 更新新的金额\n            dataCenter.wallet.set(dataCenter.wallet.selectId, response.balance);\n            //当次循环结束，如果不继续循环，让按钮可以使用\n            if (!this.isStart) {\n              this.trans = 50;\n              this.isEnable = true;\n            }\n          } else {\n            // 快速模式\n            await this.delay(400);\n            dataCenter.wallet.set(dataCenter.wallet.selectId, response.balance);\n            this.luckyNumber = response.betResult;\n            this.isEnable = true;\n            this.trans = 50;\n          }\n          //添加游戏记录\n          this.numberList.push({\n            name: this.luckyNumber,\n            isWin: this.isWin,\n          });\n          if (this.isWin) {\n            //如果赢了播放音效\n            soundManager.play(DiceWin);\n          }\n          const currency = dataCenter.wallet.getSelectedCurrency();\n          //判断是否为自动模式\n          if (this.isAuto) {\n            if (this.isWin) {\n              // 计算当前盈利\n              this.currentAmount += Number(response.betAmount);\n              //判断是否开启金额累加\n              if (\n                this.childAutobet.OnWin != '' &&\n                this.childAutobet.onWinisReset\n              ) {\n                this.bettingAmount = amountAdd(\n                  this.bettingAmount,\n                  this.childAutobet.OnWin,\n                );\n              }\n            } else {\n              // 计算当前盈利\n              this.currentAmount -= Number(this.bettingAmount);\n              if (\n                this.childAutobet.OnLoss != '' &&\n                this.childAutobet.OnLessisReset\n              ) {\n                this.bettingAmount = amountAdd(\n                  this.bettingAmount,\n                  this.childAutobet.OnLoss,\n                );\n              }\n            }\n            this.downForm();\n            if (this.isStart) {\n              setTimeout(this.startGamePlay, 0);\n            }\n          } else {\n            this.isStart = false;\n            return;\n          }\n        });\n      } else {\n        this.currentAmount = 0;\n        this.isEnable = false;\n        this.trans = 50;\n      }\n    },\n    getLuckyNumber(luckyNumber: number) {\n      this.luckyNumber = luckyNumber;\n    },\n    delay(ms: number) {\n      return new Promise((resolve) => setTimeout(resolve, ms));\n    },\n    generateRandomNumber(min: number, max: number): number {\n      return Math.floor(Math.random() * (max - min + 1)) + min;\n    },\n    formatInput(text: string) {\n      this.bettingAmount = text;\n    },\n    downForm() {\n      if (\n        this.currentAmount >= Number(this.childAutobet.stopOnProfit) &&\n        this.childAutobet.stopOnProfit != ''\n      ) {\n        this.isStart = false;\n      }\n      if (\n        this.currentAmount <= Number(-this.childAutobet.StopOnLoss) &&\n        this.childAutobet.StopOnLoss != ''\n      ) {\n        this.isStart = false;\n      }\n      if (\n        this.currentAmount >= Number(this.childAutobet.MaxBetAmount) &&\n        this.childAutobet.MaxBetAmount != ''\n      ) {\n        this.isStart = false;\n      }\n      if (!this.isEnable) {\n        this.isStart = false;\n      }\n      if (\n        Number(this.childAutobet.count) < 1 &&\n        this.childAutobet.count != ''\n      ) {\n        this.childAutobet.count = '';\n        this.isStart = false;\n      }\n    },\n    updateValue(text: string) {\n      this.bettingAmount = text;\n    },\n    modeClick(isAuto: boolean) {\n      this.isAuto = isAuto;\n    },\n  },\n\n  components: {\n    TopIcon,\n    BoardIn,\n    RollControls,\n    Slider,\n    GameForm,\n    DiceGameMode,\n    Autobet,\n    LastBets,\n  },\n});\n"],"mappings":";AACA,OAAOA,UAAU,MAAM,cAAc;AACrC,SAASC,IAAI,QAAQ,QAAQ;AAC7B,SAASC,YAAY,QAAQ,wBAAwB;AACrD,SAASC,SAAS,QAAQ,gBAAgB;AAC1C,SAASC,eAAe,QAAQ,SAAS;AACzC,OAAOC,SAAS,MAAM,wCAAwC;AAC9D,OAAOC,OAAO,MAAM,sCAAsC;AAC1D,OAAOC,eAAe,MAAM,6CAA6C;AACzE,OAAOC,WAAW,MAAM,8BAA8B;AACtD,SAASC,eAAe,QAAQ,KAAK;AACrC,SAASC,QAAQ,QAAQ,OAAO;AAChC,OAAOC,OAAO,MAAM,eAAe;AACnC,OAAOC,OAAO,MAAM,eAAe;AACnC,OAAOC,YAAY,MAAM,oBAAoB;AAC7C,OAAOC,QAAQ,MAAM,gBAAgB;AAErC,OAAOC,QAAQ,MAAM,gBAAgB;AACrC,OAAOC,YAAY,MAAM,oBAAoB;AAC7C,OAAOC,MAAM,MAAM,cAAc;AACjC,OAAOC,OAAO,MAAM,eAAe;AAEnC,SAASC,SAAS,QAAQ,iBAAiB;AAC3C,eAAeV,eAAe,CAAC;EAC7BW,KAAKA,CAAA;IACH,OAAO;MACLC,UAAU,EAAEA,CAAA,KAAMrB,UAAU,CAACsB,MAAM,CAACD,UAAU;