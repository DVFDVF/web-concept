{"remainingRequest":"/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/babel-loader/lib/index.js!/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/ts-loader/index.js??ref--15-2!/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/cache-loader/dist/cjs.js??ref--1-0!/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/vue-loader-v16/dist/index.js??ref--1-1!/Users/nuomi/Desktop/项目/concept/fury-client/src/views/home/head/popup/walletSix/Retirada.vue?vue&type=script&lang=ts","dependencies":[{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/src/views/home/head/popup/walletSix/Retirada.vue","mtime":1716171553040},{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/babel.config.js","mtime":1716171552917},{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/cache-loader/dist/cjs.js","mtime":1716171608994},{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/babel-loader/lib/index.js","mtime":1716171609486},{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/ts-loader/index.js","mtime":1716171609531},{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/cache-loader/dist/cjs.js","mtime":1716171608994},{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/vue-loader-v16/dist/index.js","mtime":1716171609468}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["defineComponent","ref","onMounted","reactive","onUnmounted","computed","RetiradaInput","HeadBupop","CurrencySelector","NetWorkSelector","getCryptoTransitionAll","userWithdraw","dataCenter","displayBalance","lang","extenalOfficial","ChatsLodding","tip","name","components","setup","balance","wallet","map","currency","transitionList","isNetworkTitle","TransitionRespItemValue","walletAddress","netWorkOptions","isTipsTitle","isLodding","requestData","currencyId","networkId","payoutAddress","amount","timer","transitionPlet","then","resp","value","webConfig","cryptoCurrencies","onCurrencyChange","item","id","list","filter","transitionItem","length","networkMap","onNetWorkChange","copyToClipboard","text","textarea","document","createElement","body","appendChild","select","execCommand","removeChild","success","onWithdraw","change","selectId","v","me_success","info","me_mess","userMaximum","Number","displayedBalance","clearTimeout"],"sources":["/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/cache-loader/dist/cjs.js??ref--1-0!/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/vue-loader-v16/dist/index.js??ref--1-1!/Users/nuomi/Desktop/项目/concept/fury-client/src/views/home/head/popup/walletSix/Retirada.vue?vue&type=script&lang=ts"],"sourcesContent":["\nimport {\n  defineComponent,\n  ref,\n  onMounted,\n  reactive,\n  onUnmounted,\n  computed,\n} from 'vue';\nimport RetiradaInput from '@/views/home/head/popup/walletSix/widget/RetiradaSelector/RetiradaInput.vue';\nimport HeadBupop from '@/views/home/head/popup/walletSix/headButtonPop.vue';\nimport CurrencySelector from './widget/CurrencySelector/index.vue';\nimport NetWorkSelector from './widget/NetWorkSelector/index.vue';\nimport { Currency, NetWork } from '@/api/protocol';\nimport {\n  TransitionRespItem,\n  WithdrawReq,\n} from '@/views/home/api/wallet-api/walletProtocol';\nimport {\n  getCryptoTransitionAll,\n  userWithdraw,\n} from '@/views/home/api/wallet-api/walletIndex';\nimport dataCenter from '@/dataCenter';\nimport { displayBalance } from '@/utils';\nimport { lang } from '@/lang';\nimport { extenalOfficial } from '@/utils/currency';\nimport ChatsLodding from '@/views/chats/ChatsLodding.vue';\nimport { tip } from '@/components/warn/warn';\nexport default defineComponent({\n  name: 'DepositoPop',\n  components: {\n    RetiradaInput,\n    HeadBupop,\n    CurrencySelector,\n    NetWorkSelector,\n    ChatsLodding,\n  },\n  setup() {\n    const balance = reactive(dataCenter.wallet.map);\n    const currency = ref<Currency>();\n    const transitionList = ref<TransitionRespItem[]>([]);\n    const isNetworkTitle = ref(false);\n    const TransitionRespItemValue = ref<TransitionRespItem>();\n    const walletAddress = ref('');\n    const netWorkOptions = ref<NetWork[]>([]);\n    const isTipsTitle = ref(false);\n    const isLodding = ref(false);\n    // 提现条件\n    const requestData: WithdrawReq = {\n      currencyId: 1,\n      networkId: 1,\n      payoutAddress: '',\n      amount: 0,\n    };\n    let timer = 0;\n\n    function transitionPlet() {\n      getCryptoTransitionAll().then((resp) => {\n        if (!resp) return;\n        transitionList.value = resp;\n        const currency = extenalOfficial(\n          dataCenter.webConfig.cryptoCurrencies,\n          true,\n        )[0];\n        onCurrencyChange(currency);\n      });\n    }\n\n    const onCurrencyChange = (item: Currency) => {\n      requestData.currencyId = item.id;\n      currency.value = item;\n      const list = transitionList.value.filter(\n        (transitionItem) => transitionItem.currencyId === item.id,\n      );\n\n      isNetworkTitle.value = list.length > 1;\n\n      if (list.length <= 1) {\n        isTipsTitle.value = false;\n      } else {\n        isTipsTitle.value = true;\n        netWorkOptions.value = list.map(\n          (item) => dataCenter.webConfig.networkMap[item.networkId]!,\n        );\n      }\n    };\n\n    const onNetWorkChange = (item: NetWork) => {\n      if (item) {\n        requestData.networkId = item.id;\n        isTipsTitle.value = false;\n      }\n    };\n\n    /**复制 */\n    function copyToClipboard(text: string) {\n      var textarea = document.createElement('textarea');\n      textarea.value = text;\n      document.body.appendChild(textarea);\n      textarea.select();\n      document.execCommand('copy');\n      document.body.removeChild(textarea);\n      tip.success('copy success');\n    }\n\n    //用户提现\n    function onWithdraw() {\n      if (requestData.amount != 0 && requestData.payoutAddress != '') {\n        isLodding.value = true;\n        userWithdraw(requestData).then((resp) => {\n          if (!resp) {\n            isLodding.value = false;\n            return;\n          }\n          isLodding.value = false;\n          // 扣钱\n          dataCenter.wallet.change(\n            dataCenter.wallet.selectId,\n            -requestData.amount,\n          );\n          tip.success(lang.v.me_success);\n        });\n      } else {\n        tip.info(lang.v.me_mess);\n      }\n    }\n    //最大余额\n    function userMaximum() {\n      requestData.amount = Number(displayBalance(currency.value, balance));\n    }\n\n    const displayedBalance = computed(() =>\n      displayBalance(currency.value, balance),\n    );\n\n    onMounted(() => {\n      transitionPlet();\n    });\n\n    onUnmounted(() => {\n      clearTimeout(timer);\n    });\n\n    return {\n      currency,\n      transitionList,\n      isNetworkTitle,\n      TransitionRespItemValue,\n      walletAddress,\n      onCurrencyChange,\n      onNetWorkChange,\n      lang,\n      isTipsTitle,\n      copyToClipboard,\n      balance,\n      displayBalance,\n      netWorkOptions,\n      requestData,\n      onWithdraw,\n      userMaximum,\n      isLodding,\n      displayedBalance,\n    };\n  },\n});\n"],"mappings":"AACA,SACEA,eAAe,EACfC,GAAG,EACHC,SAAS,EACTC,QAAQ,EACRC,WAAW,EACXC,QAAQ,QACH,KAAK;AACZ,OAAOC,aAAa,MAAM,6EAA6E;AACvG,OAAOC,SAAS,MAAM,qDAAqD;AAC3E,OAAOC,gBAAgB,MAAM,qCAAqC;AAClE,OAAOC,eAAe,MAAM,oCAAoC;AAMhE,SACEC,sBAAsB,EACtBC,YAAY,QACP,yCAAyC;AAChD,OAAOC,UAAU,MAAM,cAAc;AACrC,SAASC,cAAc,QAAQ,SAAS;AACxC,SAASC,IAAI,QAAQ,QAAQ;AAC7B,SAASC,eAAe,QAAQ,kBAAkB;AAClD,OAAOC,YAAY,MAAM,gCAAgC;AACzD,SAASC,GAAG,QAAQ,wBAAwB;AAC5C,eAAejB,eAAe,CAAC;EAC7BkB,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACVb,aAAa;IACbC,SAAS;IACTC,gBAAgB;IAChBC,eAAe;IACfO;GACD;EACDI,KAAKA,CAAA;IACH,MAAMC,OAAO,GAAGlB,QAAQ,CAACS,UAAU,CAACU,MAAM,CAACC,GAAG,CAAC;IAC/C,MAAMC,QAAQ,GAAGvB,GAAG,EAAY;IAChC,MAAMwB,cAAc,GAAGxB,GAAG,CAAuB,EAAE,CAAC;IACpD,MAAMyB,cAAc,GAAGzB,GAAG,CAAC,KAAK,CAAC;IACjC,MAAM0B,uBAAuB,GAAG1B,GAAG,EAAsB;IACzD,MAAM2B,aAAa