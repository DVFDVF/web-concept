,CAAC;IACZ,CAAC;EACH,CAAC;;EAED,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EAClB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;IACP,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;IACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;MAC3B,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,EAAE;QACf,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACjD,EAAE,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,EAAE;QACtB,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACjD,EAAE,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC,EAAE;QACtB,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACjD;IACF,CAAC;EACH,CAAC;AACH,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/casino/LobbySections/battles/BattlesDown.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"main\" v-if=\"message.ranks.data.length > 0\">\n    <div class=\"top-title\">\n      <div class=\"title-place\">{{ lang.v.ba_place }}</div>\n      <div class=\"title-player\">{{ lang.v.ba_profit }}</div>\n      <div>{{ lang.v.ba_prize }}</div>\n    </div>\n    <div\n      class=\"down-item\"\n      v-for=\"(item, index) in message.ranks.data.slice(0, 3)\"\n      :key=\"item.id\"\n    >\n      <div class=\"title-place\" v-show=\"message.ranks.data\">\n        <div class=\"place-icon\" v-show=\"message.ranks.data\">\n          <img :src=\"getPlaceImage(index)\" style=\"width: 32px; height: 20px\" />\n        </div>\n        <!-- <div class=\"battle-table-gag-circle\" v-show=\"!message.enable\"></div> -->\n      </div>\n\n      <div class=\"title-player player-content\" v-show=\"message.ranks.data\">\n        <div v-show=\"message.ranks.data\">\n          <div class=\"avatar\">\n            <img\n              class=\"icon\"\n              src=\"@/assets/referral/sprite-2.png\"\n              :style=\"`object-position: ${\n                -(parseInt(message.ranks.data?.avatar ?? '1') - 1) * 24\n              }px 0`\"\n            />\n          </div>\n        </div>\n        <div class=\"battle-table-gag-circle\" v-show=\"!message.ranks.data\"></div>\n        <div class=\"player-center\">\n          <div>\n            <span class=\"use-name\" v-show=\"message.ranks.data\">{{\n              item.name\n            }}</span>\n            <div\n              class=\"battle-table-gag-line\"\n              v-show=\"!message.ranks.data\"\n            ></div>\n          </div>\n          <div>\n            <span class=\"twix-value\" v-show=\"message.ranks.data\">{{\n              item.score.toFixed(2)\n            }}</span>\n            <div\n              class=\"battle-table-gag-line-2\"\n              v-show=\"!message.ranks.data\"\n            ></div>\n          </div>\n        </div>\n      </div>\n      <div v-show=\"message.ranks.data || index != 1\">\n        <span class=\"balance\" v-show=\"message.ranks.data\"\n          >{{ dataCenter.conform }} {{ item.reward }}</span\n        >\n        <div class=\"battle-table-gag-line-3\" v-show=\"!message.ranks.data\"></div>\n      </div>\n      <div class=\"tournament-gag\" v-show=\"!message.ranks.data && index == 1\">\n        <svg\n          class=\"svg-use\"\n          width=\"20\"\n          height=\"20\"\n          focusable=\"false\"\n          aria-hidden=\"true\"\n        >\n          <use xlink:href=\"/svg/sprite.svg#icon-tournament\"></use>\n        </svg>\n        <div class=\"tournament-gag-text\">\n          {{ lang.v.ba_leader }}\n        </div>\n      </div>\n    </div>\n    <!-- <BtnRed\n      :message=\"lang.v.ba_battle\"\n      :isDisabled=\"!message.ranks.data\"\n      style=\"margin-bottom: 12px\"\n    ></BtnRed> -->\n  </div>\n  <div v-else class=\"err-title\">\n    <span>{{ lang.v.lb_zan }}</span>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { defineComponent } from 'vue';\n\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      dataCenter,\n    };\n  },\n\n  props: ['message'],\n  methods: {\n    /**前三个排行图 */\n    getPlaceImage(index: number) {\n      if (index === 0) {\n        return require('@/assets/competition/top1.png');\n      } else if (index === 1) {\n        return require('@/assets/competition/top2.png');\n      } else if (index === 2) {\n        return require('@/assets/competition/top3.png');\n      }\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.main {\n  display: flex;\n  flex-direction: column;\n  padding: 22px 12px 0px 12px;\n  margin-top: -30px;\n  background: #1c2532;\n\n  .top-title {\n    display: flex;\n    flex-direction: row;\n    text-align: left;\n    color: #55657e;\n    display: flex;\n    font-size: 11px;\n    font-weight: 600;\n    letter-spacing: -0.02em;\n    line-height: 16px;\n    padding-bottom: 8px;\n  }\n\n  .title-place {\n    width: 19%;\n  }\n\n  .place-icon {\n    display: flex;\n    align-items: flex-start;\n  }\n\n  .title-player {\n    width: 48.5%;\n  }\n\n  .down-item {\n    align-items: center;\n    border-top: 1px solid #263041;\n    display: flex;\n    flex-direction: row;\n    height: 60px;\n    padding: 12px 0;\n  }\n\n  .player-content {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n  }\n}\n.avatar {\n  width: 24px;\n  height: 24px;\n  overflow: hidden;\n}\n.icon {\n  height: 24px;\n}\n\n.player-center {\n  display: flex;\n  flex-direction: column;\n  margin-left: 8px;\n  font-size: 12px;\n  letter-spacing: -0.02em;\n  line-height: 16px;\n  text-align: left;\n\n  .use-name {\n    color: white;\n  }\n\n  .twix-value {\n    color: #93acd3;\n    font-weight: 600;\n  }\n}\n\n.balance {\n  color: white;\n  font-weight: 600;\n  letter-spacing: -0.5px;\n  margin: auto 0;\n  overflow-x: hidden;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n}\n\n.battle-table-gag-circle {\n  background: #2a3546;\n  border-radius: 50%;\n  height: 24px;\n  width: 24px;\n}\n\n.battle-table-gag-line {\n  background: #2a3546;\n  border-radius: 8px;\n  height: 6px;\n  width: 71px;\n}\n\n.battle-table-gag-line-2 {\n  background: #2a3546;\n  height: 5px;\n  margin-top: 6px;\n  height: 3px;\n  width: 80px;\n}\n\n.battle-table-gag-line-3 {\n  background: #2a3546;\n  border-radius: 8px;\n  height: 6px;\n  width: 61px;\n}\n\n.tournament-gag {\n  border-radius: 8px;\n  display: flex;\n  gap: 6px;\n  background: #2a3546;\n  margin-top: 0;\n  padding: 12px 10px;\n  width: 100%;\n  left: 0;\n}\n\n.svg-use {\n  color: #ffb636;\n  flex-shrink: 0;\n  height: 15px;\n  width: 20px;\n}\n\n.tournament-gag .tournament-gag-text {\n  color: #93acd3;\n  font-size: 12px;\n  font-weight: 500;\n}\n.err-title {\n  text-align: center;\n  padding: 22px 12px 0px 12px;\n  margin-top: -30px;\n  background: #1c2532;\n  border-radius: 0 0 10px 10px;\n}\n</style>\n"]}]}