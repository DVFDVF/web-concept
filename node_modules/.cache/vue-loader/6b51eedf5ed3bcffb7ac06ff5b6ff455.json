AAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/ReferralProgram/Invite/input/InviteBtn.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"input input-lg\">\n    <div class=\"input-inner\">\n      <span class=\"input-icon\">\n        <svg class=\"svg-use\" focusable=\"false\" aria-hidden=\"true\">\n          <use :xlink:href=\"list.icon\"></use>\n        </svg>\n      </span>\n      <div class=\"input-tag-wrapper has-value\">\n        <input class=\"input-tag input\" :value=\"list.text\" />\n        <span class=\"input-placeholder\">{{ list.title }}</span>\n      </div>\n      <button\n        class=\"button-flat button-flat-md button-flat-grey1\"\n        @click=\"linkCopy()\"\n        type=\"button\"\n      >\n        <span class=\"button-flat-inner\">\n          <span class=\"button-flat-text\">{{ lang.v.re_da_copy }}</span>\n        </span>\n      </button>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { tip } from '@/components/warn/warn';\nimport { lang } from '@/lang';\nimport { message } from 'ant-design-vue';\nimport { defineComponent } from 'vue';\n\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n    };\n  },\n  methods: {\n    linkCopy() {\n      // 创建一个临时的textarea元素\n      var textarea = document.createElement('textarea');\n      // 将要复制的文字设置为textarea的值\n      textarea.value = this.list.text;\n      // 将textarea元素添加到DOM中\n      document.body.appendChild(textarea);\n      // 选中textarea的内容\n      textarea.select();\n      // 执行复制命令\n      document.execCommand('copy');\n      // 移除textarea元素\n      document.body.removeChild(textarea);\n      tip.success('copy success');\n    },\n  },\n  props: ['list'],\n  components: {},\n});\n</script>\n\n<style lang=\"less\" scoped>\n.input:nth-child(2) {\n  margin-bottom: 8px;\n}\n\n.input {\n  width: 100%;\n  color: white;\n}\n\n.input-lg .input-inner {\n  border-radius: 10px;\n  height: 58px;\n  padding: 7px 18px;\n}\n\n.input-inner {\n  align-items: center;\n  background-color: #090c1199;\n  border: 1px solid #2a3546;\n  cursor: text;\n  display: flex;\n  position: relative;\n  transition: color 0.1s ease-in-out, border-color 0.1s ease-in-out;\n}\n\n.input-lg .input-icon {\n  height: 24px;\n  margin-left: -6px;\n  margin-right: 8px;\n  width: 24px;\n}\n\n.input-icon {\n  align-items: center;\n  display: flex;\n  flex-shrink: 0;\n  justify-content: center;\n  pointer-events: none;\n}\n\n.input-icon svg {\n  fill: #55657e;\n  height: 100%;\n  transition: fill 0.1s ease-in-out;\n  width: 100%;\n}\n\n.input-tag-wrapper {\n  display: block;\n  flex: 1 1;\n  position: relative;\n  width: 0;\n  z-index: 0;\n}\n\n.input-tag-wrapper.has-value .input-tag {\n  margin-top: 18px;\n}\n\n.input-lg .input-tag {\n  border: none;\n  font-size: 14px;\n  letter-spacing: -0.28px;\n  padding: 0;\n}\n\n.input-tag-wrapper.has-value .input-placeholder {\n  font-size: 12px;\n  letter-spacing: -0.24px;\n  line-height: 16px;\n}\n\n.input-lg .input-placeholder {\n  letter-spacing: -0.28px;\n}\n\n.input-placeholder,\n.input-tag {\n  display: block;\n  font-weight: 600;\n  line-height: 18px;\n}\n\n.input-placeholder {\n  color: #55657e;\n  left: 0;\n  pointer-events: none;\n  position: absolute;\n  right: 0;\n  top: 0;\n  transition: font-size 0.1s ease-in-out, line-height 0.1s ease-in-out;\n  white-space: nowrap;\n}\n\n.input-currency-conversion,\n.input-placeholder {\n  overflow-x: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n}\n\n.input-lg button.button-flat {\n  border-radius: 5px;\n  font-size: 14px;\n  margin-left: 8px;\n  margin-right: -12px;\n  padding: 9px 14px;\n}\n\n.button-flat-md:not(.button-flat-only-icon) {\n  padding: 10px 32px;\n}\n\n.button-flat-grey1.button-flat {\n  background-color: #2a3546;\n  border-color: transparent;\n  color: #93acd3;\n}\n\n.input-inner:hover {\n  border-color: #2283f6;\n}\n</style>\n"]}]}