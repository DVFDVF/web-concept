   </div>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport IcoOutLink from '@/assets/icon/ico-out-link.vue';\nimport dataCenter from '@/dataCenter';\nimport { getRouter } from '@/manager/view';\nimport { defineComponent } from 'vue';\n\nexport default defineComponent({\n  name: 'other-link',\n  components: {\n    IcoOutLink,\n  },\n  props: ['message'],\n  computed: {},\n  data() {\n    return {\n      dataCenter,\n      selected: '',\n    };\n  },\n  methods: {\n    onClick(item: any) {\n      if (item.link) {\n        window.open(item.link);\n      } else if (item.rout) {\n        // 获取主内容区域的元素\n        const mainContent = document.getElementById('main-content');\n        // 在主内容区域元素上设置滚动位置\n        if (mainContent) {\n          mainContent.scrollTo(0, 0);\n          getRouter().push(item.rout);\n        }\n      }\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.other-link {\n  text-align: center;\n\n  .item {\n    font-feature-settings: 'tnum' on, 'lnum' on;\n    color: #55657e;\n    font-size: 11px;\n    font-weight: 600;\n    line-height: 16px;\n    text-decoration: none;\n    text-transform: uppercase;\n    cursor: pointer;\n\n    &:not(:first-child) {\n      margin-top: 5px;\n    }\n\n    &:hover {\n      color: #93acd3;\n    }\n\n    &.selected {\n      color: #fff;\n    }\n\n    .link {\n      margin-left: 3px;\n      font-size: 11px;\n      width: 13px;\n      height: 13px;\n    }\n  }\n}\n@media only screen and (max-width: 767px) {\n  .row-center {\n    padding: 0 10px;\n  }\n}\n</style>\n"]}]}