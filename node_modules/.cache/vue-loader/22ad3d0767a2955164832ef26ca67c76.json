(newPassword, 'new')\"\n          :type=\"typeText\"\n        />\n        <span\n          :class=\"{ 'move-span': newPasswordState.moveElements }\"\n          @click=\"container('new')\"\n          z-index=\"1\"\n          class=\"input-placeholder\"\n          >{{ lang.v.xg_newPass }}</span\n        >\n        <div class=\"eyes-color\" @click.stop=\"clickEyes\">\n          <svg\n            class=\"svg-use\"\n            width=\"24\"\n            height=\"24\"\n            focusable=\"false\"\n            aria-hidden=\"true\"\n            v-show=\"newPassword.length > 0\"\n          >\n            <use :xlink:href=\"eyes\"></use>\n          </svg>\n        </div>\n      </div>\n      <div class=\"input-container\" @click=\"container('again')\">\n        <img src=\"@/assets/icon/head/userProfile/Lock.png\" class=\"input-logo\" />\n        <input\n          :class=\"{ 'move-input': againPasswordState.moveElements }\"\n          :readonly=\"againPasswordState.isDisabled\"\n          :type=\"typeText\"\n          class=\"input-dis\"\n          v-model=\"againPassword\"\n          @blur=\"handleMouseLeave(againPassword, 'again')\"\n        />\n        <span\n          :class=\"{ 'move-span': againPasswordState.moveElements }\"\n          @click=\"container('again')\"\n          z-index=\"1\"\n          class=\"input-placeholder\"\n          >{{ lang.v.xg_againPass }}</span\n        >\n        <div class=\"eyes-color\" @click.stop=\"clickEyes\">\n          <svg\n            class=\"svg-use\"\n            width=\"24\"\n            height=\"24\"\n            focusable=\"false\"\n            aria-hidden=\"true\"\n            v-show=\"againPassword.length > 0\"\n          >\n            <use :xlink:href=\"eyes\"></use>\n          </svg>\n        </div>\n      </div>\n\n      <ChangePassButton\n        :login-text=\"lang.v.xg_change\"\n        style=\"margin-bottom: 10px; height: 44px; width: 100%\"\n        @click=\"changePassAction\"\n      />\n    </div>\n  </div>\n</template>\n<!-- 这是重置密码页面 -->\n<script lang=\"ts\">\nimport { tip } from '@/components/warn/warn';\nimport { lang } from '@/lang';\nimport { tokenController } from '@/manager/token';\nimport { openModal, ModalCloseFun } from '@/manager/view';\nimport BtnBlack from '@/views/ReferralProgram/MyBalance/BtnBlack.vue';\nimport { userChangePass } from '@/views/home/api/user-api/headIndex';\nimport { ChangePassReq } from '@/views/home/api/user-api/headProtocol';\nimport ChangePassButton from '@/views/home/head/popup/walletSix/headButtonPop.vue';\nimport { RedEnvelopeFilled } from '@ant-design/icons-vue';\nimport { message } from 'ant-design-vue';\n\nimport { defineComponent, inject } from 'vue';\nconst closeFunction = inject('close') as ModalCloseFun;\n\nexport default defineComponent({\n  components: {\n    ChangePassButton,\n  },\n  data() {\n    return {\n      lang,\n      oldPassword: '',\n      newPassword: '',\n      againPassword: '',\n      oldPasswordState: {\n        isDisabled: true,\n        moveElements: false,\n      },\n      newPasswordState: {\n        isDisabled: true,\n        moveElements: false,\n      },\n      againPasswordState: {\n        isDisabled: true,\n        moveElements: false,\n      },\n      eyes: '/svg/sprite.svg#icon-eye-open',\n      typeText: '',\n    };\n  },\n  methods: {\n    async changePassAction() {\n      if (\n        this.oldPassword !== '' &&\n        this.newPassword !== '' &&\n        this.againPassword !== ''\n      ) {\n        if (this.newPassword === this.againPassword) {\n          const requestData: ChangePassReq = {\n            oldPassword: this.oldPassword,\n            newPassword: this.newPassword,\n          };\n\n          userChangePass(requestData).then((resp) => {\n            if (!resp) return;\n            closeFunction?.();\n            tokenController.update('', '');\n            location.href = '/';\n          });\n        } else {\n          tip.error(lang.v.xg_buyiz);\n        }\n      } else {\n        tip.error(lang.v.xg_kong);\n      }\n    },\n    inputContent(key: string, text: string) {\n      this.$emit('inputContent', key, text);\n    },\n    container(inputType: string) {\n      if (inputType === 'old') {\n        this.oldPasswordState.isDisabled = false;\n        this.oldPasswordState.moveElements = true;\n      } else if (inputType === 'new') {\n        this.newPasswordState.isDisabled = false;\n        this.newPasswordState.moveElements = true;\n      } else if (inputType === 'again') {\n        this.againPasswordState.isDisabled = false;\n        this.againPasswordState.moveElements = true;\n      }\n    },\n    handleMouseLeave(text: string, inputType: string | undefined) {\n      if (text.trim() === '') {\n        if (inputType === 'old') {\n          this.oldPasswordState.isDisabled = true;\n          this.oldPasswordState.moveElements = false;\n        } else if (inputType === 'new') {\n          this.newPasswordState.isDisabled = true;\n          this.newPasswordState.moveElements = false;\n        } else if (inputType === 'again') {\n          this.againPasswordState.isDisabled = true;\n          this.againPasswordState.moveElements = false;\n        }\n      }\n    },\n    /**是否显示密码 */\n    clickEyes() {\n      if (this.eyes == '/svg/sprite.svg#icon-eye-open') {\n        this.eyes = '/svg/sprite.svg#icon-eye-close';\n        this.typeText = 'password';\n      } else {\n        this.eyes = '/svg/sprite.svg#icon-eye-open';\n        this.typeText = '';\n      }\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.changePass {\n  width: 360px;\n  border-radius: 12px;\n  overflow: hidden;\n  position: relative;\n  text-align: left;\n  min-height: 62px;\n  max-width: 360px;\n  background-color: #111923;\n}\n\n.changeH {\n  background: linear-gradient(180deg, #003370, transparent);\n  border-radius: 14px 14px 0 0;\n  height: 72px;\n  width: 100%;\n  padding: 20px 24px;\n}\n\n.changeH-font {\n  font-size: 24px;\n  line-height: 135%;\n  font-weight: 700;\n  letter-spacing: -0.02em;\n  margin-bottom: 1rem;\n  margin-top: 0;\n  color: #fff;\n  margin-bottom: 16px;\n}\n\n.change-input {\n  margin-top: 0px;\n  padding: 0 24px 20px;\n}\n\n.input-dis {\n  height: 20px;\n  flex-grow: 1;\n  z-index: 0;\n  outline: none;\n  border: none;\n  background-color: transparent;\n  color: #fff;\n  font-weight: 600;\n}\n\n.input-container {\n  display: flex;\n  position: relative;\n  align-items: center;\n  width: 100%;\n  border-radius: 10px;\n  display: flex;\n  margin-bottom: 24px;\n  background-color: #090c1199;\n  border: 1px solid #2a3546;\n  color: #090c1199;\n  padding: 7px 18px;\n  height: 62px;\n}\n\n.input-logo {\n  width: 20px;\n  height: 20px;\n  margin-right: 5px;\n  color: #ffffff;\n  filter: invert(60%);\n}\n\n.input-placeholder {\n  display: block;\n  font-weight: 500;\n  line-height: 15px;\n  color: #55657e;\n  left: 0;\n  pointer-events: none;\n  position: absolute;\n  left: 45px;\n  top: 20px;\n  transition: font-size 0.1s ease-in-out, line-height 0.1s ease-in-out;\n  white-space: nowrap;\n}\n.eyes-color {\n  color: #93acd3;\n}\n\n.input-dis,\n.input-placeholder {\n  /* 应用过渡动画 */\n  transition: transform 0.3s ease-in-out;\n}\n\n.move-input {\n  transform: translateY(10px);\n  /* 下移10px */\n}\n\n.move-span {\n  transform: translateY(-12px);\n  /* 上移10px */\n}\n</style>\n"]}]}