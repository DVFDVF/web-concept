c/views/home/center/card/Cards.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"cards\">\n    <a\n      v-for=\"(item, index) in List\"\n      :key=\"item.key\"\n      :class=\"['card', { 'card-disable': item.disable }]\"\n      @click=\"pushGameList(item.key)\"\n    >\n      <div :class=\"`card-inner card-inner-casino-${index}`\">\n        <div class=\"card-image\">\n          <img\n            class=\"picture-image\"\n            draggable=\"false\"\n            :src=\"item.icon\"\n            width=\"298\"\n            height=\"191\"\n            loading=\"lazy\"\n          />\n        </div>\n        <h2 class=\"card-label h2\">{{ item.title }}</h2>\n        <p class=\"card-description b2\">{{ item.text }}</p>\n        <div class=\"btn-position\">\n          <BtnRed :message=\"item.btnText\" :is-disabled=\"item.disable\"></BtnRed>\n        </div>\n      </div>\n    </a>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { tip } from '@/components/warn/warn';\nimport { lang } from '@/lang';\nimport { enterSports } from '@/manager/view';\nimport { defineComponent } from 'vue';\nimport BtnRed from '../BtnRed.vue';\nexport default defineComponent({\n  name: 'currencies',\n  components: {\n    BtnRed,\n  },\n  data() {\n    return {\n      List: [\n        {\n          key: 'Casino',\n          icon: require('@/assets/home/center/casino@2x.png'),\n          get title() {\n            return lang.v.sy_z_casino;\n          },\n          get text() {\n            return lang.v.sy_center_casinoShow;\n          },\n          type: '0',\n          get btnText() {\n            return lang.v.sy_center_goToCasino;\n          },\n          disable: false,\n        },\n        {\n          key: 'Sports',\n          icon: require('@/assets/home/center/sports@2x.png'),\n          get title() {\n            return lang.v.sy_z_sport;\n          },\n          get text() {\n            return lang.v.sy_center_sportsShow;\n          },\n          type: '1',\n          get btnText() {\n            return lang.v.sy_center_goToSports;\n          },\n          disable: false,\n        },\n        {\n          key: 'BFG Staking',\n          icon: require('@/assets/home/center/bfg-staking@2x.png'),\n          get title() {\n            return lang.v.sy_z_coinStaking;\n          },\n          get text() {\n            return lang.v.sy_center_bfgStaingShow;\n          },\n          type: '2',\n          get btnText() {\n            return lang.v.sy_center_goToBfgStaking;\n          },\n          disable: false,\n        },\n      ],\n    };\n  },\n  methods: {\n    pushGameList(key: string) {\n      if (key == 'Casino') {\n        this.$router.push('casino');\n      } else if (key === 'Sports') {\n        enterSports();\n      } else {\n        tip.info(lang.v.me_notOpen);\n      }\n    },\n  },\n});\n</script>\n<style lang=\"less\" scoped>\n.card-disable {\n  pointer-events: none;\n}\n.card-inner-casino-0 {\n  background: radial-gradient(\n      100% 75% at 50% -10%,\n      #ff0049 0,\n      rgba(255, 0, 73, 0.11) 70%,\n      transparent 100%\n    ),\n    #111923;\n}\n\n.card-inner-casino-1 {\n  background: radial-gradient(\n      100% 75% at 50% -10%,\n      #168fff 0,\n      rgba(22, 143, 255, 0.11) 70%,\n      transparent 100%\n    ),\n    #111923;\n}\n\n.card-inner-casino-2 {\n  background: radial-gradient(\n      100% 75% at 50% -10%,\n      #49c320 0,\n      rgba(73, 195, 32, 0.11) 70%,\n      transparent 100%\n    ),\n    #111923;\n}\n\n.cards {\n  -moz-column-gap: 16px;\n  column-gap: 16px;\n  grid-template-columns: 1fr 1fr 1fr 1fr;\n  margin-bottom: 48px;\n  font-weight: 700;\n  display: flex;\n}\n\n.btn-position {\n  width: 100%;\n  margin-top: auto;\n}\n.card {\n  align-self: stretch;\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  overflow: hidden;\n  position: relative;\n  text-decoration: none;\n}\n\n.card:hover .card-inner {\n  background-color: #17212e;\n}\n\n.card:hover .card-image {\n  transform: translate(-50%) translateY(10px) scale(1.07);\n}\n\n.card:active .card-inner {\n  background-color: #0e1621;\n}\n\n.card-inner {\n  align-items: center;\n  background-color: #111923;\n  border-radius: 9px;\n  display: flex;\n  flex: 1 1;\n  flex-direction: column;\n  padding: 191px 16px 16px;\n  transition: background-color 0.1s ease-in-out, transform 0.1s ease-in-out,\n    margin-top 0.1s ease-in-out, margin-bottom 0.1s ease-in-out;\n}\n\n.card-inner button {\n  margin-top: auto;\n}\n\n.card-image {\n  display: block;\n  height: 191px;\n  left: 50%;\n  pointer-events: none;\n  position: absolute;\n  top: 0;\n  transform: translate(-50%) translateY(10px) scale(1);\n  transition: transform 0.1s ease-in-out;\n  width: 298px;\n}\n\n.card-label {\n  color: white;\n  font-size: 24px;\n  margin-bottom: 4px;\n  text-align: center;\n  white-space: nowrap;\n}\n\n.card-description {\n  color: #93acd3;\n  font-size: 14px;\n  text-align: center;\n}\n.picture-image {\n  display: block;\n  min-width: 100%;\n}\n\n@media only screen and (max-width: 1014px) {\n  .card-inner {\n    padding-top: 179px;\n  }\n  .card-label {\n    font-size: 20px;\n    text-align: left;\n  }\n  .cards {\n    margin-bottom: 36px;\n  }\n  .card-image {\n    height: 179px;\n    width: 279px;\n  }\n  .btn-position {\n    width: auto;\n  }\n  .card-description {\n    font-size: 12px;\n  }\n}\n\n@media only screen and (max-width: 719px) {\n  .cards {\n    display: grid;\n    -moz-column-gap: 16px;\n    column-gap: 16px;\n    grid-template-columns: 1fr 1fr;\n    grid-template-rows: 1fr 1fr;\n    row-gap: 16px;\n  }\n  .card-description {\n    margin-bottom: 0;\n  }\n  .btn-position {\n    margin-top: 0;\n  }\n}\n\n@media only screen and (max-width: 591px) {\n  .cards {\n    grid-template-columns: 1fr;\n    grid-template-rows: 1fr 1fr 1fr;\n    row-gap: 10px;\n  }\n\n  .card-inner {\n    align-items: flex-start;\n    margin-top: 0;\n    padding-top: 16px;\n    // flex: initial;\n  }\n\n  .card:hover .card-image {\n    transform: translate(0) translateY(-50%) scale(1.07);\n  }\n\n  .card-inner-casino-0 {\n    background: linear-gradient(170deg, rgba(255, 0, 73, 0.2), transparent),\n      linear-gradient(340deg, rgba(255, 0, 73, 0.6), transparent 70%), #111923;\n  }\n\n  .card-inner-casino-1 {\n    background: linear-gradient(170deg, rgba(22, 143, 255, 0.2), transparent),\n      linear-gradient(340deg, rgba(22, 143, 255, 0.6), transparent 70%), #111923;\n  }\n\n  .card-inner-casino-2 {\n    background: linear-gradient(170deg, rgba(73, 195, 32, 0.2), transparent),\n      linear-gradient(341deg, rgba(73, 195, 32, 0.6), transparent), #111923;\n  }\n\n  .card-inner-casino-3 {\n    background: linear-gradient(170deg, rgba(22, 143, 255, 0.2), transparent),\n      linear-gradient(340deg, rgba(22, 143, 255, 0.6), transparent 70%), #111923;\n  }\n  .card-image {\n    height: 152px;\n    left: auto;\n    right: 12px;\n    top: 50%;\n    transform: translate(0) translateY(-50%) scale(1);\n    width: 237px;\n  }\n  .picture-image {\n    width: 100%;\n    height: 100%;\n  }\n  .card-label {\n    max-width: 238px;\n    min-width: 170px;\n  }\n  .card-description {\n    color: #ffffff80;\n    max-width: 238px;\n    min-width: 170px;\n    text-align: left;\n  }\n}\n\n@media only screen and (max-width: 460px) {\n  .card-image {\n    left: 212px;\n    right: auto;\n  }\n  .card-label {\n    max-width: 200px;\n  }\n\n  .card-description {\n    max-width: 200px;\n  }\n}\n\n@media only screen and (max-width: 400px) {\n  .card-label {\n    max-width: 170px;\n  }\n  .card-image {\n    left: 182px;\n  }\n  .card-description {\n    max-width: 170px;\n  }\n}\n</style>\n"]}]}