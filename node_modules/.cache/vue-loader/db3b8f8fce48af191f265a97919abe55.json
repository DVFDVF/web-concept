CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/concept/web3-concept/src/views/home/head/wallet/PixDeposito.vue","sourceRoot":"","sourcesContent":["<template>\n  <div>\n    <div class=\"pix-size\" v-if=\"iframeLoaded\">\n      <h2 class=\"pix-title\">{{ lang.v.pix_deposito }}</h2>\n      <div>\n        <p>{{ lang.v.pix_formas }}</p>\n        <div class=\"pix-select\">\n          <PixButton\n            v-for=\"(item, index) in pixList\"\n            :key=\"index\"\n            :class=\"{ active: activeIndex === index }\"\n            @click=\"setActive(index, item.pixMin, item.pixMax)\"\n          >\n            <template v-slot:pixLevel>{{ item.pixLevel }}</template>\n            <template v-slot:pixValue\n              >{{ item.pixMin }} ~ {{ item.pixMax }}\n            </template>\n          </PixButton>\n        </div>\n      </div>\n      <div class=\"depositMain\">\n        <div class=\"amount\">\n          <input\n            type=\"text\"\n            :placeholder=\"'Min. ' + minValue\"\n            v-model=\"inputValue\"\n            class=\"amount-input\"\n            @input=\"validateInput\"\n          />\n          <span class=\"tips-close\">{{ lang.v.pix_quantia }} (BRL) </span>\n        </div>\n        <DepositNet\n          @moneyClicked=\"moneyItemClicked\"\n          :selectedValue=\"parseInt(inputValue)\"\n          :depositeoData=\"depositeoData\"\n        />\n        <DeposirarButton\n          :login-text=\"lang.v.pix_agora\"\n          class=\"topUp\"\n          @click=\"depositarAction\"\n        />\n      </div>\n      <ul class=\"tips\">\n        <li>\n          {{\n            lang.v.pix_voce.replace('{siteName}', dataCenter.websiteName_Medium)\n          }}\n          <span style=\"color: #64d23e; font-weight: 700\"\n            >Cashback em dinheiro de 25% toda semana</span\n          >\n        </li>\n        <li>\n          {{ lang.v.pix_periodo }}\n        </li>\n        <li>\n          {{ lang.v.pix_horario }}\n        </li>\n      </ul>\n    </div>\n    <PixResult\n      v-if=\"!iframeLoaded\"\n      @back-action=\"backAction\"\n      :raw=\"resultRaw\"\n      :copy-link=\"resultRaw\"\n    >\n      <template v-slot:resultAmount>{{ resultAmount }}</template>\n    </PixResult>\n    <div v-if=\"isLodding\" class=\"center lodding\">\n      <ChatsLodding />\n    </div>\n  </div>\n</template>\n\n<script setup lang=\"ts\">\nimport { lang } from '@/lang';\nimport PixButton from '@/views/home/head/wallet/widget/PixButton.vue';\nimport DeposirarButton from '@/views/home/head/popup/walletSix/headButtonPop.vue';\nimport DepositNet from '@/views/home/head/wallet/widget/DepositNet.vue';\nimport { onMounted, reactive, ref, defineEmits } from 'vue';\nimport {\n  PayRechargeReq,\n  ProcessedRechargeConfigResp,\n} from '../../api/wallet-api/walletProtocol';\nimport dataCenter from '@/dataCenter';\nimport {\n  getRechargeConfig,\n  payRecharge,\n} from '../../api/wallet-api/walletIndex';\nimport ChatsLodding from '@/views/chats/ChatsLodding.vue';\nimport PixResult from '@/views/home/head/wallet/widget/PixResult.vue';\nimport { tip } from '@/components/warn/warn';\nimport { reentryReward } from '@/views/reentryReward';\nconst emit = defineEmits();\nconst activeIndex = ref(0);\nconst minValue = ref(10);\nconst maxValue = ref(20000);\nconst inputValue = ref('0');\nconst iframeLoaded = ref(true);\nconst iframeUrl = ref('');\n/**交易金额 */\nconst resultAmount = ref('');\n/**生成二维码所需的原始数据 */\nconst resultRaw = ref('');\n/**接收充值配置 */\nconst depositeoData = ref<ProcessedRechargeConfigResp>();\nconst setActive = (index: number, min: number, max: number) => {\n  activeIndex.value = index;\n  minValue.value = min;\n  maxValue.value = max;\n};\n/**post 请求值 */\nconst create = reactive<PayRechargeReq>({\n  payType: 'PIX_QRCODE',\n  amount: parseInt(inputValue.value),\n});\nconst isLodding = ref(false);\nconst pixList = ref([\n  {\n    key: 'hkp',\n    pixLevel: 'I',\n    pixMin: 10,\n    pixMax: 20000,\n  },\n]);\nfunction moneyItemClicked(moneyValue: number) {\n  inputValue.value = moneyValue.toString();\n}\nfunction depositarAction() {\n  const input = parseInt(inputValue.value);\n  if (input) {\n    if (minValue.value <= input) {\n      if (input <= maxValue.value) {\n        isLodding.value = true;\n        create.amount = input;\n        payRecharge(create.amount, create.payType).then((resp) => {\n          if (!resp) {\n            isLodding.value = false;\n            return;\n          }\n          if (resp.type == 'HKP') {\n            iframeUrl.value = resp.url;\n            isLodding.value = false;\n            iframeLoaded.value = false;\n            emit('change', iframeLoaded.value);\n            // 创建一个URL对象\n            const urlObj = new URL(resp.url);\n\n            // 使用URLSearchParams解析查询参数\n            const queryParams = new URLSearchParams(urlObj.search);\n            resultAmount.value = queryParams.get('amount')!;\n            resultRaw.value = queryParams.get('raw')!;\n          } else if (resp.type == 'KAKA') {\n            iframeLoaded.value = false;\n            isLodding.value = false;\n            resultAmount.value = create.amount.toString();\n            resultRaw.value = resp.qrcode;\n          }\n          dataCenter.wallet.showCallbackTip = false;\n        });\n      } else {\n        tip.error(`${lang.v.dice_max} ${maxValue.value} brl`);\n      }\n    } else {\n      tip.error(`${lang.v.dice_min} 10 brl`);\n    }\n  } else {\n    tip.error(lang.v.tip_selt);\n  }\n}\n/**获取充值配置 */\nfunction getRecharge() {\n  getRechargeConfig().then((resp) => {\n    if (!resp) return;\n    depositeoData.value = resp;\n    if (reentryReward.active) {\n      if (resp.reward) {\n        resp.reward = reentryReward.filtrate(resp.reward);\n        resp.selected =\n          resp.reward?.findIndex(\n            (item) => item.initialValue === reentryReward.selected,\n          ) ?? -1;\n      }\n    }\n  });\n}\nonMounted(() => {\n  getRecharge();\n});\n/**返回 */\nfunction backAction(isLoaded: boolean) {\n  iframeLoaded.value = isLoaded;\n  emit('change', iframeLoaded.value);\n}\nconst validateInput = (event: any) => {\n  const value = event.target.value;\n  // 创建一个正则表达式，匹配除数字外的其他字符\n  const regex = /[^0-9]/g;\n  // 使用正则表达式替换掉非数字字符（包括小数点）\n  const processedValue = value.replace(regex, '');\n  // 更新 inputValue 为处理后的值，从而阻止小数点的输入\n  inputValue.value = processedValue;\n  // 更新输入框的显示值\n  event.target.value = processedValue;\n};\n</script>\n\n<style lang=\"less\" scoped>\n.pix-size {\n  height: 100%;\n  width: 100%;\n}\n.pix-title {\n  font-size: 24px;\n  line-height: 135%;\n  letter-spacing: -1px;\n  margin-bottom: 16px;\n  font-weight: bold;\n  font-family: Montserrat Variable, sans-serif;\n  color: #fff;\n}\n.pix-select {\n  display: grid;\n  gap: 10px;\n  grid-template-columns: repeat(2, 1fr);\n}\n.amount {\n  position: relative;\n}\n.amount-input {\n  font-size: 12px;\n  width: 100%;\n  margin: 12px 0 4px;\n  background-color: #1a1c20;\n  border-radius: 10px;\n  height: 42px;\n  color: #8ea7bb;\n  text-indent: 182.8px;\n  border: 1px solid rgba(0, 0, 0, 0);\n}\n.tips-close {\n  position: absolute;\n  left: 16px;\n  top: 24px;\n  font-size: 14px;\n  color: #8ea7bb;\n  font-weight: bolder;\n  z-index: 2;\n}\n.topUp {\n  margin: 24px 0 19px;\n  width: 100%;\n  height: 50px;\n  z-index: 0;\n}\n\n.tips {\n  font-size: 12px;\n  color: #7e7e7e;\n  line-height: 16px;\n  margin-left: 10px;\n}\n.tips li {\n  list-style: decimal;\n  margin-top: 10px;\n}\n\n.active {\n  color: #8ea7bb;\n  background: #232323;\n  border: 2px solid rgba(131, 153, 172, 0.5);\n  border-radius: 10px;\n}\n.lodding {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0, 0, 0, 0.5);\n}\n</style>\n"]}]}