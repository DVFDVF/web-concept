AC,CAAC;QACR,CAAC,CAAC,CAAC,CAAC,CAAC;MACP,CAAC,CAAC,CAAC,CAAC,CAAC;IACP,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EACZ,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/concept/fury-client/src/views/share/sharingInvitation.vue","sourceRoot":"","sourcesContent":["<template>\n  <PopUpNotification style=\"width: 530px; height: 260px\">\n    <template v-slot:title>\n      <div class=\"create-tip-header\">{{ lang.v.sg_invite_placeholder }}</div>\n    </template>\n    <template v-slot:body>\n      <div class=\"content\">\n        <ul class=\"sharePlatform center\">\n          <li\n            class=\"share col-center\"\n            v-for=\"item in shareList\"\n            :key=\"item.key\"\n            @click=\"socialization(item.key)\"\n          >\n            <img :src=\"item.icon\" />\n            <span>{{ item.name }}</span>\n          </li>\n        </ul>\n        <div class=\"copy center\">\n          <span class=\"omitWrap\">{{ url }}</span>\n          <span\n            class=\"copy-btn\"\n            @click=\"copyToClipboard(url)\"\n            :data-clipboard-text=\"url\"\n            >{{ lang.v.sg_copia }}\n          </span>\n        </div>\n      </div>\n    </template>\n  </PopUpNotification>\n</template>\n<script lang=\"ts\">\nimport { defineComponent } from 'vue';\nimport PopUpNotification from '../popUp/PopUpNotification.vue';\nimport { lang } from '@/lang';\nimport { tip } from '@/components/warn/warn';\nimport dataCenter from '@/dataCenter';\nimport { stringify } from 'qs';\nimport { shareUserId } from '@/utils';\n\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      url: '',\n      shareList: [\n        {\n          key: 0,\n          icon: require('@/assets/share/facebook.png'),\n          name: 'Facebook',\n        },\n        {\n          key: 1,\n          icon: require('@/assets/share/whatsApp.png'),\n          name: 'WhatsApp',\n        },\n        {\n          key: 2,\n          icon: require('@/assets/share/telegram.png'),\n          name: 'Telegram',\n        },\n        {\n          key: 3,\n          icon: require('@/assets/share/twitter.png'),\n          name: 'Twitter',\n        },\n        {\n          key: 4,\n          icon: require('@/assets/share/email.png'),\n          name: 'Email',\n        },\n      ],\n    };\n  },\n  methods: {\n    /**复制 */\n    copyToClipboard(text: string) {\n      var textarea = document.createElement('textarea');\n      textarea.value = text;\n      document.body.appendChild(textarea);\n      textarea.select();\n      document.execCommand('copy');\n      document.body.removeChild(textarea);\n      tip.success('copy success');\n    },\n    socialization(key: number) {\n      switch (key) {\n        case 0:\n          this.shareToFacebook();\n          break;\n        case 1:\n          this.shareToWhatsApp();\n          break;\n        case 2:\n          this.shareToTelegram();\n          break;\n        case 3:\n          this.shareToTwitter();\n          break;\n        case 4:\n          this.shareViaEmail();\n          break;\n        default:\n          break;\n      }\n    },\n    shareToFacebook() {\n      const url =\n        'https://www.facebook.com/sharer/sharer.php?u=' +\n        encodeURIComponent(this.url) +\n        '&quote=' +\n        encodeURIComponent(\n          'Check out this link: https://www.ppbet.club/sharecwbr/NllYqZsO',\n        );\n      window.open(url, '_blank');\n    },\n    shareToWhatsApp() {\n      const url =\n        'whatsapp://send?text=' +\n        encodeURIComponent('Check out this link: ' + this.url);\n      window.location.href = url;\n    },\n    shareToTelegram() {\n      const url = 'https://t.me/share/url?url=' + encodeURIComponent(this.url);\n      window.open(url, '_blank');\n    },\n    shareToTwitter() {\n      const url =\n        'https://twitter.com/intent/tweet?url=' + encodeURIComponent(this.url);\n      window.open(url, '_blank');\n    },\n    shareViaEmail() {\n      const subject = 'Check out this link!';\n      const body = 'I thought you might be interested in this: ' + this.url;\n      const url =\n        'mailto:?subject=' +\n        encodeURIComponent(subject) +\n        '&body=' +\n        encodeURIComponent(body);\n      window.location.href = url;\n    },\n    shareUrl(r: string, channelId: string) {\n      this.url = `${window.location.protocol}//${\n        window.location.hostname\n      }/#/casino?${stringify({\n        r,\n        channelId,\n      })}`;\n    },\n  },\n  mounted() {\n    if (dataCenter.user.data) {\n      //分享渠道号999\n      const userId = shareUserId(true);\n      this.shareUrl(userId, '999');\n    }\n  },\n  components: { PopUpNotification },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.content {\n  margin: 0;\n  user-select: none;\n}\n.sharePlatform {\n  margin: 20px -10px;\n}\n.share {\n  flex: 1;\n  font-size: 12px;\n  gap: 4px;\n  cursor: pointer;\n}\n.share img {\n  width: 50%;\n}\n.copy {\n  color: #93acd3;\n  background-color: #202124;\n  border: 1px solid #202124;\n  font-size: 11px;\n  font-weight: 600;\n  margin: 30px -10px;\n  padding: 8px 10px;\n  border-radius: 4px;\n  justify-content: space-between;\n}\n.url {\n  width: 80%;\n}\n.copy-btn {\n  color: #40a1de;\n  font-size: 13px;\n  cursor: pointer;\n}\n@media only screen and (max-width: 767px) {\n  .create-tip-header {\n    font-size: 12px;\n    margin-top: 5px;\n  }\n  .sharePlatform span {\n    font-size: 10px;\n  }\n}\n</style>\n"]}]}