assword tab-inner\">\n          <div class=\"header\">\n            <h2>Change password</h2>\n          </div>\n          <div class=\"body\">\n            <div class=\"body-inner\">\n              <div class=\"modal-resetpassword-input-wrap\">\n                <InputBox\n                  class=\"input\"\n                  :list=\"list\"\n                  @inputContent=\"intputContent\"\n                >\n                </InputBox>\n              </div>\n              <img\n                src=\"https://betfury.io/_nuxt/change-pass.377655b6.png\"\n                alt=\"betfury\"\n              />\n            </div>\n          </div>\n          <div class=\"footer\">\n            <BtnRed\n              message=\"Submit\"\n              style=\"width: 100%; padding-top: -16px\"\n              @click=\"submitForm()\"\n            ></BtnRed>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { ModalCloseFun } from '@/manager/view';\nimport { defineComponent, inject } from 'vue';\nimport BtnRed from '../center/BtnRed.vue';\nimport InputBox from './inputBox/InputBox.vue';\n\nexport default defineComponent({\n  data() {\n    return {\n      passwordText: '',\n      confirmText: '',\n      closeFunction: inject('close') as ModalCloseFun,\n      list: [\n        {\n          icon: '/svg/sprite.svg#icon-locked-new',\n          key: 'password',\n          name: 'Enter your password',\n          value: '',\n        },\n        {\n          icon: '/svg/sprite.svg#icon-locked-new',\n          key: 'confirm',\n          name: 'Confirm password',\n          value: '',\n        },\n      ],\n    };\n  },\n  methods: {\n    intputContent(key: string, text: string) {\n      if (key == 'confirm') {\n        this.confirmText = text;\n      }\n      if (key == 'password') {\n        this.passwordText = text;\n      }\n    },\n    isValidPassword(str: string): boolean {\n      const lengthRegex = /.{8,}/;\n      const uppercaseRegex = /[A-Z]/;\n      const lowercaseRegex = /[a-z]/;\n      const numberRegex = /\\d/;\n      return (\n        lengthRegex.test(str) &&\n        uppercaseRegex.test(str) &&\n        lowercaseRegex.test(str) &&\n        numberRegex.test(str)\n      );\n    },\n    submitForm() {\n      if (\n        this.isValidPassword(this.passwordText) &&\n        this.passwordText == this.confirmText\n      ) {\n        this.$emit('submitClick', this.passwordText);\n      }\n    },\n  },\n  components: { InputBox, BtnRed },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.modal-resetpassword-input-wrap .input {\n  margin-top: 10px;\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n  position: relative;\n}\n.body-inner img {\n  margin: 10px -20px 0;\n  width: calc(100% + 40px);\n}\n</style>\n"]}]}