CAAC","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/ReferralProgram/referrals/ReferralsInner.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"main\">\n    <div class=\"main-name-ref\">\n      <div class=\"main-name-div\">\n        <img class=\"main-icon-UserImage\" src=\"@/assets/referral/sprite.png\" />\n        <span class=\"user__level\">{{ userReferee.level }}</span>\n      </div>\n      <div class=\"date-name\">\n        <div class=\"date-name-name-block\">\n          <div\n            class=\"tooltip\"\n            customclass=\"\"\n            iconsize=\"22\"\n            showicon=\"true\"\n            iconcolor=\"#55657e\"\n          >\n            <span class=\"date-name-name\">{{ userReferee.username }}</span>\n          </div>\n        </div>\n        <div class=\"date-name-date\">\n          {{ lang.v.ref_created\n          }}<span class=\"datetime\">{{ createTime(userReferee.date) }}</span>\n        </div>\n      </div>\n    </div>\n    <!-- //佣兵股份暂不需要 -->\n    <!-- <div class=\"main-shares-profit\">\n      <div class=\"main-shares\">\n        <div class=\"share-block\">\n          <span class=\"share-block-percent\">100%</span>\n          <span class=\"share-block-type\">{{ lang.v.ref_commission }}</span>\n        </div>\n        <svg\n          class=\"svg-use-small share-block-icon share-block-icon\"\n          width=\"12\"\n          height=\"12\"\n          focusable=\"false\"\n          aria-hidden=\"true\"\n        >\n          <use xlink:href=\"/svg/sprite.svg#icon-arrow-arrow-right\"></use>\n        </svg>\n        <div class=\"share-block\">\n          <span class=\"share-block-percent\">0%</span>\n          <span class=\"share-block-type\">{{ lang.v.ref_shared }}</span>\n        </div>\n      </div>\n    </div> -->\n    <!-- <div class=\"main-profit\">\n      <div class=\"main-wrap\" currency=\"usd\">\n        <Image\n          width=\"24\"\n          height=\"24\"\n          :src=\"dataCenter.webConfig.currencyIcon\"\n          loading=\"lazy\"\n        />\n        <div class=\"main-wrap-currency\">\n          <span class=\"balance\">${{ userReferee.rechargeCount }}</span>\n        </div>\n      </div>\n    </div> -->\n    <div class=\"main-profit\">\n      <div class=\"main-wrap\" currency=\"usd\">\n        <amount-prompt :amountValue=\"userReferee.directBet\">\n          <div style=\"display: flex\">\n            <Image\n              width=\"24\"\n              height=\"24\"\n              :src=\"dataCenter.webConfig.currencyIcon\"\n              loading=\"lazy\"\n            />\n            <div class=\"main-wrap-currency lasttext\">\n              <span class=\"balance\">\n                <Money :value=\"userReferee.directBet\"></Money>\n              </span>\n            </div>\n          </div>\n        </amount-prompt>\n      </div>\n    </div>\n    <div class=\"main-profit\">\n      <div class=\"main-wrap\" currency=\"usd\">\n        <amount-prompt :amountValue=\"userReferee.teamBet\">\n          <div style=\"display: flex\">\n            <Image\n              width=\"24\"\n              height=\"24\"\n              :src=\"dataCenter.webConfig.currencyIcon\"\n              loading=\"lazy\"\n            />\n            <div class=\"main-wrap-currency lasttext\">\n              <span class=\"balance\">\n                <Money :value=\"userReferee.teamBet\"></Money>\n              </span>\n            </div>\n          </div>\n        </amount-prompt>\n      </div>\n    </div>\n    <div class=\"main-profit\">\n      <div class=\"main-wrap last\" currency=\"usd\">\n        <amount-prompt :amountValue=\"userReferee.value\">\n          <div style=\"display: flex\">\n            <Image\n              width=\"24\"\n              height=\"24\"\n              :src=\"dataCenter.webConfig.currencyIcon\"\n              loading=\"lazy\"\n            />\n            <div class=\"main-wrap-currency lasttext\">\n              <span class=\"balance\">\n                <Money :value=\"userReferee.value\"></Money>\n              </span>\n            </div>\n          </div>\n        </amount-prompt>\n        <!-- <svg\n          class=\"name-icon\"\n          xmlns=\"http://www.w3.org/2000/svg\"\n          viewBox=\"0 0 24 24\"\n        >\n          <use xlink:href=\"/svg/sprite.svg#icon-info-circle-2\"></use>\n        </svg> -->\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport AmountPrompt from '@/views/chats/prompt/amountPrompt.vue';\nimport { getRefereeItem } from '@/views/home/api/protocol';\nimport dayjs from 'dayjs';\nimport { defineComponent } from 'vue';\n\nexport default defineComponent({\n  data() {\n    return {\n      isSpend: false,\n      lang,\n      dataCenter,\n    };\n  },\n  computed: {},\n  methods: {\n    spendClick() {\n      this.isSpend = !this.isSpend;\n    },\n    createTime(str: string) {\n      const parsedDate = dayjs(str); // 解析字符串时间\n      return parsedDate.format('YYYY-MM-DD');\n    },\n  },\n  components: { AmountPrompt },\n  props: {\n    userReferee: {\n      default: {} as getRefereeItem,\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.main {\n  align-items: center;\n  background: rgba(22, 31, 44, 0.8);\n  display: grid;\n  grid-template-areas: 'name-refs shares-profit copy-ref controls';\n  grid-template-columns: repeat(4, 1fr);\n  padding: 16px 12px;\n  text-align: left;\n  font-weight: 700;\n  overflow: hidden;\n\n  .main-name-ref {\n    display: flex;\n    grid-area: name-refs;\n    align-items: center;\n\n    .main-name-div {\n      position: relative;\n      margin-right: 6px;\n\n      .main-icon-UserImage {\n        align-items: center;\n        height: 24px;\n        width: 24px;\n      }\n\n      .user__level {\n        background-color: #55657e;\n        border-radius: 50%;\n        color: #fff;\n        font-size: 10px;\n        height: 15px;\n        width: 15px;\n        line-height: 15px;\n        pointer-events: none;\n        position: absolute;\n        right: -4px;\n        bottom: -4px;\n        text-align: center;\n        width: 15px;\n      }\n    }\n\n    .date-name {\n      display: flex;\n      flex-direction: column;\n      width: 134px;\n      font-weight: 600;\n      letter-spacing: -0.02em;\n\n      .date-name-name-block {\n        align-items: center;\n        display: flex;\n\n        .tooltip {\n          cursor: pointer;\n\n          .date-name-name {\n            color: #fff;\n            font-size: 12px;\n            font-weight: 600;\n            letter-spacing: -0.28px;\n            line-height: 125%;\n          }\n        }\n\n        .date-name-edit {\n          padding: 0 6px;\n\n          .campaign-icon {\n            color: #55657e;\n          }\n        }\n      }\n\n      .date-name-date {\n        align-items: center;\n        color: #93acd3;\n        display: flex;\n        font-size: 11px;\n        font-weight: 600;\n        letter-spacing: -0.22px;\n        line-height: 135%;\n\n        .datetime {\n          color: #93acd3;\n          margin-left: 2px;\n        }\n      }\n    }\n\n    .campaign-divider {\n      background: linear-gradient(\n        90deg,\n        rgba(67, 83, 109, 0.1) 5%,\n        #43536d 50%,\n        rgba(67, 83, 109, 0)\n      );\n      height: 1px;\n      margin: 8px 0;\n      width: 100%;\n    }\n\n    .refs {\n      align-items: center;\n      color: white;\n      display: flex;\n      font-size: 14px;\n      font-weight: 600;\n      letter-spacing: -0.28px;\n      line-height: 125%;\n      width: 102px;\n\n      .main-name-ref .refs-title {\n        color: #93acd3;\n        display: none;\n        font-size: 11px;\n        font-weight: 600;\n        letter-spacing: -0.22px;\n        line-height: 135%;\n        padding-right: 4px;\n      }\n    }\n  }\n\n  .main-shares-profit {\n    align-items: center;\n    display: flex;\n    grid-area: shares-profit;\n\n    .main-shares {\n      align-items: center;\n      display: flex;\n\n      .share-block {\n        display: flex;\n        flex-direction: column;\n\n        .share-block-percent {\n          color: #fff;\n          font-size: 14px;\n          font-weight: 600;\n          letter-spacing: -0.28px;\n          line-height: 125%;\n        }\n\n        .share-block-type {\n          color: #93acd3;\n          font-size: 11px;\n          font-weight: 600;\n          letter-spacing: -0.22px;\n          line-height: 135%;\n          margin-top: 4px;\n        }\n      }\n\n      .share-block-icon {\n        color: #93acd3;\n        margin: 0 10px;\n      }\n    }\n\n    .main-profit {\n      grid-gap: 6px;\n      align-items: center;\n      display: grid;\n      grid-template-columns: auto 1fr;\n      grid-template-columns: 1fr;\n\n      .main-wrap {\n        grid-gap: 6px;\n        align-items: center;\n        display: grid;\n        grid-template-columns: auto 1fr;\n\n        .main-wrap-currency {\n          align-items: center;\n          display: flex;\n\n          .currency {\n            align-items: center;\n            display: inline-flex;\n            flex-shrink: 0;\n\n            .tooltip {\n              cursor: pointer;\n              display: inline-block;\n              vertical-align: middle;\n            }\n          }\n        }\n      }\n    }\n  }\n\n  .main-wrap {\n    display: flex;\n    align-items: center;\n    color: #fff;\n  }\n\n  .lasttext {\n    color: rgb(27, 184, 61);\n  }\n\n  .name-icon {\n    margin-left: 6px;\n    width: 20px;\n    height: 20px;\n  }\n\n  .main-wrap img {\n    margin-right: 4px;\n  }\n}\n\n.detailed {\n  background: #1c2532;\n  width: 100%;\n}\n\n.datetime {\n  white-space: nowrap;\n}\n\n.campaign-divider {\n  background: linear-gradient(\n    90deg,\n    rgba(67, 83, 109, 0.1) 5%,\n    #43536d 50%,\n    rgba(67, 83, 109, 0)\n  );\n  height: 1px;\n  width: 100%;\n}\n\n.tooltip {\n  cursor: pointer;\n  display: inline-block;\n  vertical-align: middle;\n}\n\n.campaign-icon {\n  color: #55657e;\n}\n\n.svg-use {\n  width: 16px;\n  height: 16px;\n}\n\n.svg-use-small {\n  width: 12px;\n  height: 12px;\n}\n\n.button-flat {\n  background-color: #2a3546;\n  border-color: transparent;\n  color: #93acd3;\n  border-radius: 6px;\n  font-size: 11px;\n  display: flex;\n  padding: 6px;\n}\n\n.campaign-toggle-btn {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n}\n\n.campaign-toggle-btn:hover {\n  color: white;\n}\n\n.svg-white {\n  color: white;\n}\n@media only screen and (max-width: 767px) {\n  .date-name {\n    width: auto !important;\n  }\n  .main-name-div {\n    display: none;\n  }\n  .main-wrap img {\n    display: none;\n  }\n}\n</style>\n"]}]}