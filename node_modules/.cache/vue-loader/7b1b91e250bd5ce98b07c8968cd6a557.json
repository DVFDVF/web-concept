,CAAC,CAAC,CAAC;QAC9C,CAAC;QACD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACrC,CAAC,CAAC,CAAC,CAAC,CAAC;IACP,CAAC,CAAC,CAAC,CAAC,CAAC;EACP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/concept/web3-concept/src/views/ReferralProgram/newRecommendation/payment/recommendationPayment.vue","sourceRoot":"","sourcesContent":["<template>\n  <div style=\"width: 100%; height: 100%\">\n    <div\n      class=\"recommendation-payment col-center\"\n      v-show=\"outRecordData.length > 0\"\n    >\n      <div class=\"title\">{{ lang.v.rc_detailed }}</div>\n      <div class=\"payment-content\">\n        <div\n          class=\"payment-item\"\n          v-for=\"titleItem in titleData\"\n          :key=\"titleItem.key\"\n        >\n          <div class=\"name\">{{ titleItem.title }}</div>\n          <div\n            class=\"text-data col-center\"\n            v-for=\"recordItem in outRecordData\"\n            :key=\"recordItem.id\"\n          >\n            <span v-if=\"titleItem.pop === 'status'\">{{\n              getStatusDescription(recordItem[titleItem.pop])\n            }}</span>\n            <span v-else>{{ recordItem[titleItem.pop] || '--' }}</span>\n          </div>\n        </div>\n      </div>\n      <a-pagination\n        style=\"margin-top: 20px\"\n        v-model:pageNumber=\"pagination.pageNumber\"\n        :total=\"pagination.total\"\n        :pageSize=\"pagination.pageSize\"\n        :showSizeChanger=\"false\"\n        show-less-items\n        @change=\"handlePageChange\"\n      />\n    </div>\n    <div class=\"rewards-table-empty\" v-show=\"outRecordData.length === 0\">\n      <div class=\"no-data\">\n        <img\n          class=\"no-data-images\"\n          src=\"@/assets/home/casino/no-data-image.webp\"\n        />\n        <span>{{ lang.v.re_da_oops }}</span>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\" setup>\nimport { onMounted, reactive, ref } from 'vue';\nimport { Pagination as APagination } from 'ant-design-vue';\nimport { getOutRecord } from '@/views/home/api';\nimport { lang } from '@/lang';\n\nconst pagination = reactive({\n  pageNumber: 1,\n  pageSize: 10,\n  total: 0,\n});\nconst titleData = [\n  {\n    key: 0,\n    get title() {\n      return lang.v.fk_riqi;\n    },\n    pop: 'time',\n  },\n  {\n    key: 1,\n    get title() {\n      return lang.v.fk_amount;\n    },\n    pop: 'change',\n  },\n  {\n    key: 2,\n    get title() {\n      return lang.v.fk_success;\n    },\n    pop: 'status',\n  },\n  {\n    key: 3,\n    get title() {\n      return lang.v.fk_yuanyi;\n    },\n    pop: 'reason',\n  },\n];\nconst outRecordData = ref({} as any);\nfunction getOutRecordData() {\n  getOutRecord(pagination.pageSize, pagination.pageNumber).then((resp) => {\n    if (!resp) return;\n    outRecordData.value = resp.data;\n    pagination.total = resp.total;\n  });\n}\nfunction handlePageChange(curren: number) {\n  pagination.pageNumber = curren;\n  getOutRecordData();\n}\nonMounted(() => {\n  getOutRecordData();\n});\n/**\n * 10 为进行中\n * 20 成功\n * 30 失败\n */\nconst statusType: { [key: string]: string } = {\n  '10': 'RUNNING',\n  '20': 'SUCCESS',\n  '-10': 'REFUND',\n};\nfunction getStatusDescription(code: number): string {\n  // 将数字转换为字符串键\n  return statusType[code.toString()] || '未知状态'; // \"Unknown Status\"\n}\n</script>\n\n<style lang=\"less\" scoped>\n.recommendation-payment {\n  background: #1c222f;\n  border-radius: 11px;\n  padding: 20px;\n  margin-right: 20px;\n  flex: 1;\n  .title {\n    color: white;\n    font-weight: bold;\n    font-size: 20px;\n  }\n  .payment-content {\n    margin-top: 10px;\n    width: 100%;\n    display: flex;\n    flex-direction: row;\n    .payment-item {\n      padding: 10px 0;\n      border-radius: 10px;\n      background: linear-gradient(180deg, #242d3d 0%, #1c222f 100%);\n      margin: 0 5px;\n      flex: 1;\n      .name {\n        color: white;\n        font-weight: bold;\n        font-size: 20px;\n        height: 40px;\n        border-bottom: 1px solid #ffffff30;\n        margin-bottom: 20px;\n      }\n      .text-data {\n        font-weight: 500;\n        font-size: 12px;\n        color: #f0f0f0;\n        height: 30px;\n        white-space: none;\n      }\n    }\n    .payment-item:nth-child(3) {\n      .text-data {\n        color: #2783f6;\n        font-weight: bold;\n      }\n    }\n    .payment-item:nth-child(1) {\n      flex: 1.1;\n    }\n    .payment-item:nth-child(4) {\n      flex: 1.5;\n    }\n  }\n}\n.rewards-table-empty {\n  display: flex;\n  height: 100%;\n  justify-content: center;\n  background: #1c222f;\n  border-radius: 11px;\n  margin-right: 20px;\n  padding: 20px;\n}\n\n.no-data {\n  align-items: center;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  margin-bottom: 44px;\n  min-height: 200px;\n}\n@media only screen and (max-width: 767px) {\n  .recommendation-payment {\n    border-radius: 11px;\n    padding: 10px;\n    margin-right: 0;\n    flex: 1;\n    .title {\n      font-size: 16px;\n    }\n    .payment-content {\n      grid-gap: 5px;\n      .payment-item {\n        .name {\n          font-size: 15px;\n          height: 30px;\n        }\n        .text-data {\n          font-size: 9px;\n          height: 30px;\n        }\n      }\n    }\n  }\n  .rewards-table-empty {\n    width: 100%;\n    margin-right: 0;\n  }\n}\n</style>\n"]}]}