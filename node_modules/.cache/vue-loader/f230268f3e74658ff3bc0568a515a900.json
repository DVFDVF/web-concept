ient/src/views/home/left/widget/menu/MenuItem.vue","sourceRoot":"","sourcesContent":["<template>\n  <div\n    :class=\"[\n      'menu-item',\n      { selected: selectedThis, 'item-disenable': data?.disenable ?? false },\n    ]\"\n    @mouseover=\"handleMouseOver(data.key)\"\n    @mouseleave=\"handleMouseLeave\"\n  >\n    <div v-if=\"data.key === '-'\">\n      <NavLine />\n    </div>\n    <div class=\"row-center item\" v-else ref=\"news\">\n      <div :class=\"getIcon\" v-show=\"data.key !== toggleLeft.leftMenuFold\">\n        <div\n          :class=\"['icon center', { 'right-none': toggleLeft.leftMenuFold }]\"\n        >\n          <component :is=\"data.icon\" />\n        </div>\n      </div>\n      <div class=\"row-center content\" v-show=\"!toggleLeft.leftMenuFold\">\n        <div class=\"name\">\n          {{ data.name }} <span>{{ data.amount }}</span>\n        </div>\n        <div class=\"extra\" v-if=\"data.extra\">{{ data.extra }}</div>\n        <div\n          :class=\"{ fold: selectedThis, more: true, center: true }\"\n          v-if=\"data.children\"\n        >\n          <svg viewBox=\"0 0 10 10\" xmlns=\"http://www.w3.org/2000/svg\">\n            <path\n              fill-rule=\"evenodd\"\n              clip-rule=\"evenodd\"\n              d=\"M2.62927 0.540023C3.02138 0.15109 3.65454 0.153666 4.04347 0.545775L7.10558 3.6329C7.85391 4.38734 7.85391 5.61266 7.10558 6.3671L4.04347 9.45422C3.65454 9.84633 3.02138 9.84891 2.62927 9.45998C2.23716 9.07104 2.23459 8.43788 2.62352 8.04577L5.64462 5L2.62352 1.95423C2.23459 1.56212 2.23716 0.928956 2.62927 0.540023Z\"\n              fill=\"currentColor\"\n            ></path>\n          </svg>\n        </div>\n      </div>\n    </div>\n\n    <transition\n      enter-active-class=\"animate-slideInDown\"\n      leave-active-class=\"animate-slideOutUp\"\n    >\n      <div v-show=\"data.children && selectedThis\">\n        <MenuSubList\n          :list=\"data.children\"\n          :selected=\"selected\"\n          v-on:change=\"$emit('subClick', $event)\"\n        />\n      </div>\n    </transition>\n\n    <transition name=\"fade\" mode=\"out-in\">\n      <div\n        v-show=\"data.children && isHandleMouseOver && toggleLeft.leftMenuFold\"\n        class=\"mouseover\"\n        :style=\"{ top: ListY + 'px', left: ListX + 'px' }\"\n      >\n        <PopUpList>\n          <template v-slot:title>{{ data.name }}</template>\n          <MenuSubList\n            :list=\"data.children\"\n            :selected=\"selected\"\n            v-on:change=\"$emit('subClick', $event)\"\n          >\n          </MenuSubList>\n        </PopUpList>\n      </div>\n    </transition>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { defineComponent } from 'vue';\nimport MenuSubList from '../menu-sub/MenuSubList.vue';\nimport NavLine from '../NavLine.vue';\nimport PopUpList from './RightPopUP/PopUpList.vue';\nexport default defineComponent({\n  name: 'menu-item',\n  components: {\n    MenuSubList,\n    NavLine,\n    PopUpList,\n  },\n  props: ['data', 'selected'],\n  computed: {\n    selectedThis(): boolean {\n      if (\n        (this.toggleLeft.leftMenuFold && this.selected?.[0] === 'CASINO') ||\n        this.selected?.[0] === '-'\n      ) {\n        return false;\n      }\n      return this.data.key === this.selected?.[0];\n    },\n    getIcon(): string {\n      return this.toggleLeft.leftMenuFold ? 'icon-small center' : 'icon center';\n    },\n  },\n  methods: {\n    handleMouseOver(key: string) {\n      if (key == '-') {\n        return;\n      }\n\n      const news = this.$refs.news as HTMLElement;\n      if (news) {\n        const rect = news.getBoundingClientRect();\n        this.ListX = rect.width + 5;\n        this.ListY = rect.top - 10;\n        this.isHandleMouseOver = true;\n      }\n    },\n    handleMouseLeave() {\n      this.isHandleMouseOver = false;\n    },\n  },\n\n  data() {\n    return {\n      toggleLeft: dataCenter.layout,\n      isHandleMouseOver: false,\n      ListX: 0,\n      ListY: 0,\n    };\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.menu-item {\n  font-feature-settings: 'tnum' on, 'lnum' on;\n  cursor: pointer;\n  position: relative;\n\n  .mouseover {\n    width: 210px;\n    background-color: #1d2736;\n    position: fixed;\n    padding: 11 9px;\n    border-radius: 9px;\n    border: 0 solid #000;\n    bottom: 20px;\n    overflow: auto;\n    z-index: 5;\n  }\n\n  &:hover,\n  &.selected {\n    background: #1d2736;\n    border-radius: 5px;\n    color: #fff;\n\n    .icon {\n      color: #2283f6;\n    }\n\n    .icon-small {\n      color: #2283f6;\n    }\n  }\n\n  .item {\n    font-size: 14px;\n    font-weight: 600;\n    letter-spacing: -0.02em;\n    line-height: 24px;\n    min-height: 24px;\n    padding: 9px;\n\n    .content {\n      width: 100%;\n    }\n  }\n\n  .icon {\n    color: #55657e;\n    height: 19px;\n    margin-right: 8px;\n  }\n  .right-none {\n    margin-right: 0;\n  }\n\n  .icon-small {\n    color: #55657e;\n    font-size: 19px;\n    width: 19px;\n    height: 19px;\n  }\n\n  .name {\n    max-width: calc(100% - 10px);\n    flex: 1;\n    overflow-x: hidden;\n    text-overflow: ellipsis;\n    white-space: nowrap;\n    text-align: left;\n    span {\n      color: #fca60f;\n    }\n  }\n\n  .extra {\n    background: #2a3546;\n    border-radius: 5px;\n    color: #1bb83d;\n    font-size: 12px;\n    font-weight: 700;\n    letter-spacing: -0.02em;\n    line-height: 130%;\n    margin-left: auto;\n    min-width: 25px;\n    padding: 4px;\n    text-align: center;\n  }\n\n  .more {\n    width: 10px;\n    height: 10px;\n    vertical-align: middle;\n    transform: rotate(-90deg);\n    font-size: 10px;\n    svg {\n      width: 100%;\n      height: 100%;\n    }\n\n    &.fold {\n      color: #2283f6;\n      transform: rotate(90deg);\n    }\n  }\n\n  .fade-enter-active,\n  .fade-leave-active {\n    transition: opacity 0.3s;\n  }\n\n  .fade-enter,\n  .fade-leave-to {\n    opacity: 0;\n  }\n}\n\n.icon svg {\n  width: 19px;\n}\n.icon img {\n  width: 19px;\n}\n\n//\n\n/* 展开动画 */\n.animate-slideInDown {\n  display: block;\n  animation: slideDown 0.5s ease forwards;\n}\n\n.animate-slideOutUp {\n  animation: slideRecovry 0.5s ease forwards;\n  display: none;\n}\n@keyframes slideDown {\n  from {\n    opacity: 0;\n    transform: translateY(-20px);\n  }\n  to {\n    opacity: 1;\n    transform: translateY(0);\n  }\n}\n\n@keyframes slideRecovry {\n  from {\n    opacity: 1;\n    transform: translateY(0px);\n  }\n  to {\n    opacity: 0;\n    transform: translateY(-20);\n  }\n}\n.item-disenable {\n  pointer-events: none;\n  color: gray;\n}\n.item-disenable .icon {\n  color: gray;\n}\n</style>\n"]}]}