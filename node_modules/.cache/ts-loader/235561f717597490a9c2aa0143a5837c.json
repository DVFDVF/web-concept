 },\n  },\n  mounted() {\n    eventBus.on(Events.MONEY_CHANGE, this.displayMoneyChange);\n    document.addEventListener('click', this.onDucumentClick);\n  },\n  unmounted() {\n    eventBus.off(Events.MONEY_CHANGE, this.displayMoneyChange);\n    document.removeEventListener('click', this.onDucumentClick);\n  },\n});\n"],"mappings":"AACA,SAASA,eAAe,EAAEC,QAAQ,EAAEC,GAAG,QAAQ,KAAK;AAGpD,SAASC,cAAc,QAAQ,SAAS;AACxC,OAAOC,aAAa,MAAM,2BAA2B;AACrD,OAAOC,UAAU,MAAM,cAAc;AACrC,OAAOC,QAAQ,IAAIC,MAAM,QAAQ,oBAAoB;AAErD,OAAOC,UAAU,MAAM,yBAAyB;AAChD,SAASC,OAAO,QAAQ,gBAAgB;AAMxC,eAAeT,eAAe,CAAC;EAC7BU,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACVP,aAAa;IACbQ,QAAQ,EAAEH;GACX;EACDI,KAAKA,CAAA;IACH,MAAMC,QAAQ,GAAGZ,GAAG,CAAC,KAAK,CAAC;IAC3B,MAAMa,OAAO,GAAGd,QAAQ,CAACI,UAAU,CAACW,MAAM,CAACC,GAAG,CAAC;IAC/C,MAAMC,QAAQ,GAAGhB,GAAG,EAAY;IAChC,MAAMiB,OAAO,GAAGjB,GAAG,CAAC,KAAK,CAAC;IAC1B,SAASkB,UAAUA,CAACC,KAAc;MAChCF,OAAO,CAACE,KAAK,GAAGA,KAAK;IACvB;IACA,SAASC,cAAcA,CAACC,UAAkB;MACxClB,UAAU,CAACW,MAAM,CAACQ,cAAc,CAACD,UAAU,CAAC;IAC9C;IAEA,OAAO;MACLT,QAAQ;MACRC,OAAO;MACPG,QAAQ;MACRf,cAAc;MACdgB,OAAO;MACPC,UAAU;MACVE,cAAc;MACdjB;KACD;EACH,CAAC;EACDoB,IAAIA,CAAA;IACF,OAAO;MACLC,YAAY,EAAE,CAAC,KAAK,EAAE,KAAK,CAAC;MAC5BC,OAAO,EAAE,CAAC,gBAAgB,EAAE,kBAAkB;KAC/C;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,QAAQA,CAACC,IAAc;MACrB,IAAIA,IAAI,CAACC,IAAI,EAAE;QACb,IAAI,CAACb,QAAQ,GAAGY,IAAI;QACpB,IAAI,CAACR,cAAc,CAACQ,IAAI,CAACE,EAAE,CAAC;;MAE9B,IAAI,CAAClB,QAAQ,GAAG,KAAK;IACvB,CAAC;IACDmB,kBAAkBA,CAACR,IAAiB;MAClC,IAAIA,IAAI,CAACJ,KAAK,IAAI,CAAC,EAAE;MACrB,IAAIb,UAAU,CAACiB,IAAI,CAACP,QAAQ,EAAEO,IAAI,CAACJ,KAAK,CAAC,CAACa,IAAI,CAC5C,IAAI,CAACC,KAAK,CAACC,OAAyB,CACrC;IACH,CAAC;IACDC,eAAeA,CAAA;MACb,IAAI,CAACvB,QAAQ,GAAG,KAAK;IACvB;GACD;EACDwB,OAAOA,CAAA;IACLhC,QAAQ,CAACiC,EAAE,CAAChC,MAAM,CAACiC,YAAY,EAAE,IAAI,CAACP,kBAAkB,CAAC;IACzDQ,QAAQ,CAACC,gBAAgB,CAAC,OAAO,EAAE,IAAI,CAACL,eAAe,CAAC;EAC1D,CAAC;EACDM,SAASA,CAAA;IACPrC,QAAQ,CAACsC,GAAG,CAACrC,MAAM,CAACiC,YAAY,EAAE,IAAI,CAACP,kBAAkB,CAAC;IAC1DQ,QAAQ,CAACI,mBAAmB,CAAC,OAA