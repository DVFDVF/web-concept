cept/web3-concept/src/views/ReferralProgram/referrals/ReferralReferrals.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"heading\">\n    <div class=\"heading-left\">\n      <div class=\"heading-tilte\">{{ lang.v.re_re_myReferrals }}</div>\n      <div class=\"numer\">{{ referee?.total }}</div>\n    </div>\n    <a-date-picker\n      v-model:value=\"value1\"\n      :allowClear=\"false\"\n      @change=\"getRefereeFunc\"\n    />\n    <!-- 暂不需要 -->\n    <!-- <div class=\"heading-right\" style=\"display: none\">\n      <div class=\"hader_top\">\n        <div class=\"parent-container myparent\">\n          <ChatBtn :onclick=\"campaigns\" :text=\"lang.v.ref_campaigns\"> </ChatBtn>\n          <ChatsDrop\n            :show-array=\"dataArray\"\n            class=\"ChatBtnStyle\"\n            v-show=\"downDrop\"\n            @select-button-clicked=\"handleSelectButtonClicked\"\n          />\n        </div>\n        <ChatBtn\n          class=\"myparent\"\n          :text=\"lang.v.re_re_chooseActivityDates\"\n          :show-image=\"true\"\n          :ishover=\"true\"\n        />\n        <div class=\"parent-container\">\n          <ChatBtn :onclick=\"Times\" :text=\"ChatBtnTimes\" class=\"disCount\" />\n          <ChatsDrop\n            :show-array=\"timeArray\"\n            class=\"ChatBtnStyle\"\n            v-show=\"downTime\"\n            @select-button-clicked=\"handleSelectButtonClickTime\"\n          />\n        </div>\n      </div>\n      <div class=\"hader_bottom\">\n        <ChatBtn\n          :text=\"lang.v.re_re_downloadAsCsvFile\"\n          class=\"ChatBtnline\"\n          :controldownload=\"true\"\n          :show-bottom=\"true\"\n        />\n      </div>\n    </div> -->\n  </div>\n  <div class=\"campaigns\" v-show=\"(referee?.total ?? 0) > 0\">\n    <div class=\"campaigns-header\">\n      <div v-for=\"item in headerData\" :key=\"item\">{{ lang.v[item] }}</div>\n    </div>\n    <div class=\"campaign\">\n      <div class=\"campaign-inner\">\n        <CampaignInner\n          v-for=\"(item, index) in referee.data\"\n          :key=\"index\"\n          :userReferee=\"item\"\n        ></CampaignInner>\n      </div>\n      <a-pagination\n        style=\"margin-top: 20px\"\n        v-model:pageNumber=\"pagination.pageNumber\"\n        :total=\"pagination.total\"\n        :pageSize=\"pagination.pageSize\"\n        :showSizeChanger=\"false\"\n        show-less-items\n        @change=\"handlePageChange\"\n      />\n    </div>\n  </div>\n  <no-data v-show=\"(referee?.total ?? 0) < 1\"></no-data>\n  <!-- <CampaignsMobile\n    class=\"mobile\"\n    v-for=\"(item, index) in referee.data\"\n    :key=\"index\"\n    :userReferee=\"item\"\n  ></CampaignsMobile> -->\n</template>\n<script lang=\"ts\">\nimport { lang } from '@/lang';\nimport { getReferee } from '@/views/home/api';\nimport { getRefereeResq } from '@/views/home/api/protocol';\nimport NoData from '@/views/noData/noData.vue';\nimport CampaignsMobile from '@/views/ReferralProgram/campaigns/CampaignInnerMobile.vue';\nimport { defineComponent } from 'vue';\nimport CampaignInner from './ReferralsInner.vue';\nimport dayjs, { Dayjs } from 'dayjs';\nimport { DatePicker as ADatePicker } from 'ant-design-vue';\nimport { Pagination as APagination } from 'ant-design-vue';\n\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      headerData: [\n        're_re_userName',\n        're_re_totalDeposits',\n        're_re_wager',\n        're_re_myProfit',\n      ],\n      downDrop: false,\n      downTime: false,\n      dataArray: ['All campaigns', 'Default Campaign'],\n      ChatBtnCampaigns: 'All campaigns',\n      timeArray: ['10', '20', '50'],\n      ChatBtnTimes: '10',\n      referee: {} as getRefereeResq,\n      value1: dayjs(),\n      pagination: {\n        pageNumber: 1,\n        pageSize: 10,\n        total: 0,\n      },\n    };\n  },\n  methods: {\n    Times() {\n      this.downTime = !this.downTime;\n    },\n    campaigns() {\n      this.downDrop = !this.downDrop;\n    },\n    handleSelectButtonClicked(item: number) {\n      this.ChatBtnCampaigns = this.dataArray[item];\n      this.downDrop = !this.downDrop;\n    },\n    handleSelectButtonClickTime(item: number) {\n      this.ChatBtnTimes = this.timeArray[item];\n      this.downTime = !this.downTime;\n    },\n    //我的推荐\n    getRefereeFunc() {\n      getReferee(\n        this.pagination.pageSize,\n        this.pagination.pageNumber,\n        dayjs(this.value1).format('YYYY-MM-DD'),\n      ).then((response) => {\n        if (!response) return;\n        this.referee = response;\n      });\n    },\n    handlePageChange(curren: number) {\n      this.pagination.pageNumber = curren;\n      this.getRefereeFunc();\n    },\n  },\n  mounted() {\n    this.getRefereeFunc();\n  },\n  components: {\n    CampaignInner,\n    //  ChatsDrop, ChatBtn,\n    // CampaignsMobile,\n    NoData,\n    ADatePicker,\n    APagination,\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.heading {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  color: white;\n  overflow: hidden;\n\n  .heading-left {\n    align-items: center;\n    display: flex;\n    flex-direction: row;\n\n    .heading-tilte {\n      font-size: 18px;\n      letter-spacing: -0.36px;\n      line-height: 135%;\n      margin: 0 8px 0 0;\n      font-style: normal;\n      font-weight: 600;\n    }\n\n    .numer {\n      background: #1c2532;\n      border-radius: 6px;\n      box-shadow: 0 1px 2px #1018280d;\n      color: #2283f6;\n      font-weight: 700;\n      font-size: 12px;\n      letter-spacing: -0.24px;\n      line-height: 130%;\n      padding: 2px 6px;\n    }\n  }\n\n  .heading-right {\n    position: relative;\n    display: flex;\n    /* 启用Flexbox布局 */\n\n    /* 使子元素均匀分布在容器中 */\n    align-items: center;\n    /* 垂直居中对齐子元素 */\n    gap: 10px;\n    /* 设置子元素之间的间距为10px */\n  }\n\n  .parent-container {\n    position: relative;\n    display: flex;\n    flex-direction: column;\n  }\n}\n\n.campaigns {\n  display: flex;\n  flex-direction: column;\n  margin-top: 12px;\n\n  .campaigns-header {\n    display: grid;\n    grid-template-columns: repeat(4, 1fr);\n    margin-bottom: 6px;\n    padding: 0 12px;\n    font-size: 11px;\n    text-align: left;\n  }\n\n  .campaigns-header > div {\n    color: #55657e;\n\n    font-style: normal;\n    font-weight: 700;\n    letter-spacing: 0.66px;\n    line-height: 150%;\n    padding: 6px 0 2px;\n    text-transform: uppercase;\n  }\n\n  .campaign {\n    margin-bottom: 12px;\n    position: relative;\n\n    .campaign-info {\n      background: #2283f6;\n      border-radius: 8px 8px 8px 0;\n      color: #fff;\n      font-size: 9px;\n      font-weight: 800;\n      left: 0;\n      letter-spacing: 0.36px;\n      line-height: 120%;\n      padding: 2px 6px;\n      position: absolute;\n      text-transform: uppercase;\n      top: -7px;\n    }\n\n    .campaign-inner {\n      border-radius: 6px;\n      overflow: hidden;\n    }\n  }\n}\n\n.mobile {\n  display: none;\n}\n@media only screen and (max-width: 767px) {\n  .heading {\n    display: flex;\n    flex-direction: column;\n    gap: 10px;\n    margin-bottom: 20px;\n  }\n  .campaigns {\n    display: flex;\n  }\n  .campaigns-header {\n    display: flex;\n    font-size: 8px !important;\n    justify-content: center;\n    align-items: center;\n    text-align: left !important;\n  }\n  .disCount {\n    display: none !important;\n  }\n\n  .mobile {\n    display: inline;\n  }\n}\n@media (max-width: 450px) {\n  .disCount {\n    display: none !important;\n  }\n\n  .heading-right {\n    display: flex;\n    flex-direction: column;\n  }\n  .hader_bottom {\n    width: 100%;\n  }\n  .hader_top {\n    display: grid;\n    width: 100%;\n    grid-template-columns: 2fr 3fr;\n  }\n  .myparent {\n    width: 100%;\n  }\n}\n.hader_top {\n  position: relative;\n  display: flex;\n  /* 启用Flexbox布局 */\n  justify-content: space-between;\n  gap: 10px;\n}\n</style>\n"]}]}