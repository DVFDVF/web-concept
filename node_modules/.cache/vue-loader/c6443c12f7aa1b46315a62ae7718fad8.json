nt, PropType } from 'vue';\n\nexport default defineComponent({\n  data(): { userBrief?: UserBriefRespItem } {\n    return { userBrief: undefined };\n  },\n  methods: {\n    closeReply() {\n      this.$emit('closeReply');\n    },\n  },\n  components: {},\n  watch: {\n    msgItem(value?: ChatData) {\n      if (value) {\n        userManager.getUser(value.userId).then((userBrief) => {\n          this.userBrief = userBrief;\n        });\n      }\n    },\n  },\n  props: {\n    msgItem: Object as PropType<ChatData>,\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.msg-content-center {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  position: relative;\n  text-align: left;\n  .meg-sub-center {\n    margin-left: 10px;\n  }\n\n  .meg-line {\n    display: flex;\n    width: 3px;\n    position: absolute;\n    top: 0;\n    bottom: 0;\n    background-color: #263041;\n  }\n\n  .msg-content-user-name {\n    font-size: 12px;\n    font-weight: 500;\n    line-height: 20px;\n  }\n}\n.header-btn {\n  display: flex;\n  flex-direction: row;\n  align-items: center;\n  justify-content: center;\n  padding: 3.5px, 5.25px;\n}\n.header-btn:hover svg {\n  color: white;\n}\n</style>\n"]}]}