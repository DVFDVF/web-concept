urcesContent":["<template>\n  <div class=\"sub-menu-list\">\n    <HeadMeunItem\n      v-for=\"item in list\"\n      :key=\"item.key\"\n      :data=\"item\"\n      :selected=\"selected\"\n      v-show=\"!item.hidden\"\n      @click=\"onSelected(item)\"\n      v-on:subClick=\"onSubSelected\"\n    />\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent } from 'vue';\nimport HeadMeunItem from './HeadMeunItem.vue';\nimport { MenuData } from '@/views/home/left/interface';\n\nexport default defineComponent({\n  name: 'headmenu-list',\n  components: {\n    HeadMeunItem,\n  },\n  props: ['list'],\n  data() {\n    return {\n      selected: [] as string[],\n    };\n  },\n  methods: {\n    onSelected(item: MenuData) {\n      if (item.children && item.key === this.selected[0]) {\n        this.selected = ['', this.selected[1]];\n      } else {\n        this.selected = [item.key, this.selected[1]];\n      }\n      this.$emit('selected', item);\n    },\n    onSubSelected(item: MenuData) {\n      this.selected = [this.selected[0], item.key];\n      this.$emit('selected', item);\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.sub-menu-list {\n  margin: 6px 0;\n  color: #93acd3;\n  font-family: Montserrat Variable, sans-serif;\n}\n</style>\n"]}]}