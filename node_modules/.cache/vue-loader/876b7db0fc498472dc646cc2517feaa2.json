CAAC;EAC3D,CAAC,CAAC,CAAC,CAAC,CAAC;EACL,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/App.vue","sourceRoot":"","sourcesContent":["<template>\n  <div v-if=\"!ready\" class=\"x-load-box\">\n    <div class=\"x-load\">\n      <div class=\"x-e x-e1\"></div>\n      <div class=\"x-e x-e2\"></div>\n      <div class=\"x-e x-e3\"></div>\n      <div class=\"x-e x-e4\"></div>\n    </div>\n  </div>\n  <div id=\"main\" v-if=\"ready\">\n    <InstallTipsTop></InstallTipsTop>\n    <HeadWidget />\n    <!-- <a-button type=\"primary\" @click=\"openNotification\" style=\"z-index: 99999\"\n      >Open the notification box</a-button\n    > -->\n    <div class=\"body\">\n      <LeftWidget />\n      <div\n        class=\"body-center\"\n        id=\"main-content\"\n        ref=\"bodyCenter\"\n        @scroll=\"handleScroll\"\n      >\n        <div class=\"center-content\">\n          <router-view />\n        </div>\n        <DownFooter></DownFooter>\n      </div>\n      <ChatWidget v-if=\"dataCenter.user.data\"></ChatWidget>\n      <CustomerWidget></CustomerWidget>\n      <Notifications />\n      <CompetitionPage />\n      <CompetitionPendant />\n    </div>\n    <div class=\"tabbar\">\n      <TabbarWidget></TabbarWidget>\n    </div>\n    <!-- 活动转盘 -->\n    <redEnvelopeRainBtn\n      :isScroll=\"isScroll\"\n      @change=\"onChange\"\n    ></redEnvelopeRainBtn>\n    <turntable :isScroll=\"isScroll\" @change=\"onChange\"></turntable>\n    <FloatingFacebook :isScroll=\"isScroll\"></FloatingFacebook>\n  </div>\n  <InstallTips></InstallTips>\n</template>\n\n<script lang=\"ts\">\nimport HeadWidget from '@/views/home/HeadWidget.vue'; // @ is an alias to /src\nimport LeftWidget from '@/views/home/left/index.vue'; // @ is an alias to /src\nimport { defineAsyncComponent, defineComponent, ref } from 'vue';\n//tabbar\nimport dataCenter from '@/dataCenter';\nimport { autoLogin, webConfig } from './api/fetcher';\nimport { openNotification } from './components/notice/Notice';\nimport { tip } from './components/warn/warn';\nimport { lang } from './lang';\nimport signer from './manager/signer';\nimport { tokenController } from './manager/token';\nimport { getRouter, openModal, registerRouter } from './manager/view';\nimport { metaPixel, reportingEvents, roundRobin } from './utils';\nimport { getUserBalance } from './views/home/api/user-api/headIndex';\nimport { jiugongge } from './views/home/other/game/jiugongge';\nimport { gameActive } from './views/active';\nimport { gridNineAdvertisingIsShow } from './views/advertising';\nimport { getGoogleCode } from './views/home/login/googleLogin';\nimport DrawLottery from './views/home/casino/drawLottery/DrawLottery.vue';\n\nexport default defineComponent({\n  name: 'Home',\n  data() {\n    return {\n      openNotification,\n      timer: 0,\n      dataCenter,\n      jiugongge,\n    };\n  },\n\n  mounted() {\n    // getGoogleCode();\n    metaPixel();\n    //轮询上报\n    this.timer = roundRobin();\n    lang.change(lang.m);\n\n    //监听来自子iframe的消息\n    window.addEventListener('message', (event: MessageEvent) => {\n      if (event.data == 'iframe to home') {\n        getRouter().push('/casino');\n        return;\n      }\n    });\n    (window as any).receiveValue = this.receiveValue;\n  },\n  beforeUnmount() {\n    clearInterval(this.timer);\n  },\n\n  components: {\n    HeadWidget,\n    LeftWidget,\n    // CenterWidget,\n    DownFooter: defineAsyncComponent(\n      () => import('@/views/home/footer/DownFooter.vue'),\n    ),\n    ChatWidget: defineAsyncComponent(() => import('@/views/chats/index.vue')),\n    // CustomerBtn,\n    CustomerWidget: defineAsyncComponent(\n      () => import('@/views/customerService/index.vue'),\n    ),\n    Notifications: defineAsyncComponent(\n      () => import('@/views/home/head/notify/Notifications.vue'),\n    ),\n    CompetitionPage: defineAsyncComponent(\n      () => import('@/views/home/competition/CompetitionPage.vue'),\n    ),\n    CompetitionPendant: defineAsyncComponent(\n      () => import('@/views/home/competition/widget/CompetitionPendant.vue'),\n    ),\n    TabbarWidget: defineAsyncComponent(\n      () => import('@/views/tabbar/index.vue'),\n    ),\n    Turntable: defineAsyncComponent(\n      () => import('@/views/hoverScolling/turntable.vue'),\n    ),\n    redEnvelopeRainBtn: defineAsyncComponent(\n      () => import('@/views/redEnvelopeRain/redEnvelopeRainBtn.vue'),\n    ),\n    InstallTips: defineAsyncComponent(\n      () => import('./views/popUp/installTips.vue'),\n    ),\n    InstallTipsTop: defineAsyncComponent(\n      () => import('./views/popUp/installTipsTop.vue'),\n    ),\n    FloatingFacebook: defineAsyncComponent(\n      () => import('@/views/floatingFacebook/floatingFacebook.vue'),\n    ),\n  },\n  setup() {\n    const ready = ref(false);\n    const isScroll = ref(false);\n    const scrollTimeout = ref(0);\n    const now = Date.now();\n    webConfig().then(async (config) => {\n      if (config) {\n        dataCenter.webConfig.update(config, (Date.now() - now) >> 1);\n      } else {\n        tip.error(lang.v.zd_error);\n        return;\n      }\n      await signer.start();\n      autoLogin()\n        .then((data) => {\n          if (data) {\n            // 登陆事件\n            reportingEvents('login', data.pixelId);\n            dataCenter.user.update(data);\n            tokenController.updateToken(data.token);\n            getUserBalance().then((wallet) => {\n              dataCenter.wallet.update(wallet);\n            });\n            dataCenter.siteMessage.init();\n            //初始化活动\n            gameActive.init();\n            ready.value = true;\n          } else {\n            setTimeout(() => {\n              ready.value = true;\n            }, 500);\n          }\n        })\n        .finally(() => {\n          gridNineAdvertisingIsShow();\n        });\n    });\n    function handleScroll() {\n      isScroll.value = true;\n      clearTimeout(scrollTimeout.value);\n      scrollTimeout.value = setTimeout(() => {\n        isScroll.value = false;\n      }, 400);\n    }\n    function onChange() {\n      isScroll.value = false;\n    }\n\n    return { ready, handleScroll, isScroll, onChange };\n  },\n  created() {\n    registerRouter(this.$router);\n  },\n  methods: {\n    receiveValue(authCode: string) {\n      tip.success('拿到了' + authCode);\n    },\n  },\n});\n</script>\n\n<style lang=\"less\">\n#main {\n  display: flex;\n  flex-direction: column;\n  width: 100%;\n  height: 100%;\n  user-select: none;\n  overflow: hidden;\n}\n\n.body {\n  flex: 1;\n  display: flex;\n  height: 100%;\n  overflow: hidden;\n  position: relative;\n}\n\n.body-center {\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  overflow: auto;\n  .center-content {\n    padding: 0 40px;\n  }\n}\n\n::-webkit-scrollbar {\n  width: 0;\n}\n\nbutton {\n  background-color: transparent;\n  border: none;\n  padding: 0;\n  margin: 0;\n  font-family: inherit;\n  cursor: pointer;\n}\n\nul {\n  list-style-type: none;\n  padding: 0;\n  margin: 0;\n}\n\nli {\n  list-style: none;\n  margin: 0;\n  padding: 0;\n}\n\np {\n  margin-top: 0;\n}\n\n.c7,\np {\n  margin-bottom: 1rem;\n}\n\nbody {\n  font-feature-settings: 'tnum' on, 'lnum' on;\n  background-color: #0d131c;\n  color: white;\n  font-family: Montserrat Variable, sans-serif;\n  font-size: 14px;\n  font-weight: 500;\n  line-height: 1.5;\n}\n\ninput {\n  border: none;\n  outline: none;\n  background-color: transparent;\n}\n\n.h3,\n.h4,\nh3,\nh4 {\n  font-weight: 600;\n  letter-spacing: -0.02em;\n  margin-bottom: 1rem;\n  margin-top: 0;\n}\n\n.h3,\nh3 {\n  font-size: 18px;\n  line-height: 135%;\n}\n.tabbar {\n  display: none;\n}\n@media only screen and (max-width: 767px) {\n  body {\n    overflow: hidden;\n  }\n  #app {\n    position: fixed;\n    bottom: 0;\n    top: 0;\n    transition: height 2s ease-in-out;\n  }\n  .tabbar {\n    display: inline;\n  }\n  .body {\n    margin-bottom: 65px;\n  }\n  .center-content {\n    padding: 0 10px !important;\n    background: rgba(0, 0, 0, 0);\n  }\n  //禁用自带的回弹效果\n  body {\n    overscroll-behavior-x: none;\n    overscroll-behavior-y: none;\n    overscroll-behavior: none;\n    overflow: hidden;\n  }\n  // .body-center {\n  //   margin-bottom: 50px;\n  // }\n}\n</style>\n"]}]}