n// To map mutations\n// 通知是否显示\nconst notifClick = () => {\n  dataCenter.layout.toggleNotify();\n};\n//聊天室是否显示\nconst chatClick = () => {\n  if (dataCenter.user.data) {\n    dataCenter.layout.toggleChat();\n  } else {\n    //登录\n    openModal(LoginWidget);\n  }\n};\n\n//显示搜索\nconst searchClick = () => {\n  openModal(LobbySearch);\n};\nconst unreadCount = () => {\n  return dataCenter.siteMessage.getUnreadCount();\n};\nfunction enterCasion() {\n  location.href = '/#/casino';\n  window.location.reload();\n}\n</script>\n\n<style lang=\"less\" scoped>\nheader {\n  align-items: center;\n  background: #1b2636;\n  box-shadow: 0 4px 26px #0d131c78;\n  display: flex;\n  height: 64px;\n  left: 0;\n  padding: 0 24px;\n  top: 0;\n  width: 100%;\n  z-index: 104;\n  position: relative;\n}\n\n.header-right {\n  align-items: center;\n  display: flex;\n  margin-left: auto;\n}\n\n.twoBtn {\n  display: flex;\n  gap: 8px;\n  margin-right: 21px;\n}\n\n/* 隐藏 */\n\n.header-rigt-hild {\n  align-items: center;\n  display: flex;\n  margin-left: auto;\n}\n\n.hildtn {\n  display: flex;\n  position: relative;\n}\n\n.notif-count {\n  background: rgb(27, 184, 61);\n  border: 2px solid rgb(27, 38, 54);\n  border-radius: 5px;\n  cursor: pointer;\n  font-family: Montserrat Variable, sans-serif;\n  color: rgb(255, 255, 255);\n  font-size: 12px;\n  font-weight: 800;\n  height: 22px;\n  min-width: 22px;\n  padding: 0px 2.5px;\n  position: absolute;\n  right: -8px;\n  text-align: center;\n  top: -8.5px;\n}\n\n.iconColor {\n  color: #2283f6;\n}\n.img-logo-small {\n  display: none;\n}\n\n@media only screen and (max-width: 767px) {\n  .expand-button {\n    display: none;\n  }\n\n  .head-all,\n  .head-settings,\n  .img-logo,\n  .icon-search,\n  .icon-text {\n    display: none;\n  }\n  .col-middle {\n    display: none;\n  }\n  .center {\n    display: none;\n  }\n  .center {\n    display: none;\n  }\n  .img-logo-small {\n    display: inline;\n  }\n  header {\n    padding: 0 12px;\n  }\n}\n</style>\n"]}]}