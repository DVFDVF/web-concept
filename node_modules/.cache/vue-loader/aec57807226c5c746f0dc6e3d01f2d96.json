AC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACpB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;AACf;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACV,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EACf,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAChB;AACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;EACN,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AACvD","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/head/wallet/widget/DepositNet.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"deposit\">\n    <div\n      class=\"deposit-item center\"\n      v-for=\"(item, index) in props.depositeoData?.reward\"\n      :key=\"index\"\n      :class=\"{\n        active: activeItem(index),\n      }\"\n      @click=\"huiChuan(index, item.initialValue)\"\n    >\n      <span class=\"iconImg hot\" v-if=\"award(item)\"></span>\n      <div class=\"amountContent\">\n        <span class=\"iconfont icon-unit-brl\"></span\n        ><span>{{ item.initialValue }}</span>\n      </div>\n      <span v-if=\"award(item)\" class=\"amountTips\"\n        >Obter {{ item.obterValue }}</span\n      >\n    </div>\n  </div>\n</template>\n<!-- 这是充值页面 -- 显示金额组件，显示第一次充值组件 （包含金额倍率计算） -->\n<script setup lang=\"ts\">\nimport { ref, defineEmits, defineProps, watch } from 'vue';\nimport {\n  ProcessedRechargeConfigResp,\n  RewardItem,\n} from '@/views/home/api/wallet-api/walletProtocol';\nimport { preferential } from '@/views/home/login/preferential/index';\nimport { reentryReward } from '@/views/reentryReward';\nimport dataCenter from '@/dataCenter';\n\nconst props = defineProps({\n  selectedValue: Number,\n  depositeoData: Object as () => ProcessedRechargeConfigResp,\n});\n/**初始化为-1，表示未匹配到 */\nconst activeIndex = ref(-1);\nconst activeItem = (index: number) => {\n  let selected =\n    (activeIndex.value == -1\n      ? props.depositeoData?.selected\n      : activeIndex.value) === index;\n  if (selected) {\n    emit('moneyClicked', props.depositeoData?.reward[index].initialValue);\n    //重置\n    reentryReward.selected = -1;\n  }\n  return selected;\n};\nconst emit = defineEmits(['moneyClicked']);\nfunction huiChuan(index: number, moneyValue: number) {\n  activeIndex.value = index;\n  emit('moneyClicked', moneyValue);\n}\n\nwatch(\n  () => props.selectedValue,\n  (newValue) => {\n    updateActiveIndex(newValue!);\n  },\n  { immediate: true },\n);\n\nfunction award(rewardItem: RewardItem) {\n  if (!dataCenter.user.data?.isFirstRecharge) {\n    return false;\n  }\n  if (reentryReward.active) {\n    if (reentryReward.configuration?.config) {\n      return (reentryReward.configuration.config as RewardItem[]).find(\n        (item) => item.initialValue === rewardItem.initialValue,\n      );\n    } else {\n      return false;\n    }\n  }\n  return preferential.firstBonus();\n}\n\nfunction updateActiveIndex(newValue: number) {\n  if (props.depositeoData?.reward) {\n    const index = props.depositeoData.reward.findIndex(\n      (item) => Number(item.initialValue) === Number(newValue), // 确保类型匹配\n    );\n    activeIndex.value = index;\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.deposit {\n  display: grid;\n  gap: 10px;\n  grid-template-columns: repeat(3, 1fr);\n}\n.deposit-item {\n  height: 48px;\n  border-radius: 10px;\n  color: #fff;\n  font-size: 12px;\n  background: #494949;\n  flex-direction: column;\n  justify-content: center;\n  margin-top: 12px;\n  position: relative;\n  overflow: hidden;\n}\n.iconImg {\n  background: url(~@/assets/configuration/wallet/hot.png) 0 0 no-repeat;\n  background-size: 32px auto;\n}\n.hot {\n  display: inline-block;\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 32px;\n  height: 32px;\n}\n.amountContent {\n  display: flex;\n  align-items: center;\n}\n.icon-unit-brl {\n  margin-right: 6px;\n}\n.icon-unit-brl:before {\n  content: 'R$';\n}\n.amountTips {\n  font-size: 12px;\n  color: #ff9500;\n}\n.active {\n  background: linear-gradient(180deg, #278e47, #64d23e);\n}\n</style>\n"]}]}