eight: 12px;\n  margin-top: -3px;\n  width: 12px;\n}\n\n.dapps-top-range-help {\n  position: absolute;\n  right: 3px;\n  top: 5px;\n  z-index: 9;\n}\n\n.dapps-top-button {\n  align-items: center;\n  background-color: #161f2c;\n  border: none;\n  border-radius: 8px;\n  display: flex;\n  height: 30px;\n  justify-content: center;\n  min-width: 30px;\n  overflow: visible;\n  padding: 8px;\n  position: relative;\n  transition: all 0.3s;\n}\n\n.dapps-top-button:not(:last-child) {\n  margin-right: 5px;\n}\n\n.dapps-top-button:disabled {\n  cursor: not-allowed;\n  pointer-events: none;\n}\n\n.dapps-top-button .dapps-top-svg {\n  fill: #55657e;\n  height: 14px;\n  pointer-events: none;\n  transition: all 0.3s;\n  width: 14px;\n}\n\n.dapps-top-button label {\n  background-color: #283343;\n  border-radius: 5px;\n  box-shadow: 0 0 15px #00000075;\n  color: #55657e;\n  display: block;\n  font-size: 9px;\n  opacity: 0;\n  padding: 8px;\n  pointer-events: none;\n  position: absolute;\n  text-align: center;\n  top: 0;\n  transform: translateY(5px);\n  transition: all 0.3s;\n  visibility: hidden;\n  white-space: nowrap;\n}\n\n.dapps-top-button:hover {\n  background-color: #223044;\n}\n\n.dapps-top-button:hover label {\n  opacity: 1;\n  transform: translateY(calc(-100% - 5px));\n  visibility: visible;\n  z-index: 10;\n}\n\n.dapps-top-button.on {\n  background-color: #161f2c;\n}\n\n.dapps-top-button.on .dapps-top-svg {\n  fill: #2283f6;\n}\n\n.dapps-top-button.on .dapps-top-svg-off {\n  fill: #55657e;\n}\n\n.dapps-top-sound-tooltip {\n  position: relative;\n}\n\n.dapps-top-sound-tooltip .dapps-top-sound-block {\n  left: 50%;\n  margin-left: -40px;\n  opacity: 0;\n  position: absolute;\n  text-align: center;\n  top: 100%;\n  transition: opacity 0.3s;\n  visibility: hidden;\n  width: 80px;\n  z-index: 9;\n}\n\n.down {\n  background: #1c2532;\n  padding: 2px 5px 5px;\n  border-radius: 8px;\n  box-shadow: 2px 2px 20px #0000004d;\n  margin-top: 6px;\n  color: #fff;\n}\n.icon {\n  width: 14