AAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACjC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACnB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACnB,CAAC;EACH,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/concept/web3-concept/src/views/home/head/popup/walletSix/widget/VaultSelector/VaultDeposit.vue","sourceRoot":"","sourcesContent":["<template>\n  <div\n    class=\"langedvs center\"\n    @click.stop=\"isClickable && (showPopover = !showPopover)\"\n  >\n    <div class=\"langedv row-center\">\n      <Image :src=\"currency?.icon\" class=\"langedvs-images\" />\n      <div class=\"triggerue\">\n        <span class=\"triggerue-lantext\">{{\n          displayBalance(currency, balance)\n        }}</span>\n      </div>\n      <span class=\"curklt\">\n        <icoBottom\n          class=\"curren-ialtm\"\n          focusable=\"false\"\n          aria-hidden=\"true\"\n          :style=\"{ transform: showPopover ? 'rotate(180deg)' : 'none' }\"\n        />\n      </span>\n    </div>\n    <MyCurrencyPop\n      class=\"depcy\"\n      :inputHidden=\"true\"\n      :toggleButtons=\"[false]\"\n      :dynamicTexts=\"['hb_hideBalance']\"\n      v-show=\"showPopover\"\n      @change=\"onChange\"\n    />\n  </div>\n</template>\n<!-- 提现和充值 -->\n<script lang=\"ts\">\nimport { defineComponent, nextTick, reactive, ref } from 'vue';\nimport icoBottom from '@/assets/icon/head/ico-bottom.vue';\nimport { Currency } from '@/api/protocol';\nimport MyCurrencyPop from '../../../MyCurrencyPop.vue';\nimport { displayBalance } from '@/utils';\nimport dataCenter from '@/dataCenter';\ninterface DataStruct {\n  selected?: Currency;\n  showPopover: boolean;\n}\n\nexport default defineComponent({\n  components: {\n    icoBottom,\n    MyCurrencyPop,\n  },\n  data(): DataStruct {\n    return {\n      selected: undefined,\n      showPopover: false,\n    };\n  },\n  mounted() {\n    document.addEventListener('click', this.onDucumentClick);\n  },\n  unmounted() {\n    document.removeEventListener('click', this.onDucumentClick);\n  },\n  methods: {\n    onChange(item: Currency) {\n      this.currency = item;\n      this.$emit('change', item);\n    },\n    onDucumentClick() {\n      this.showPopover = false;\n    },\n  },\n  //先让显示第一条数据\n  setup(props, { emit }) {\n    const balance = reactive(dataCenter.wallet.map);\n    const currency = ref<Currency>();\n\n    nextTick(() => {\n      emit('change', emit);\n    });\n\n    return {\n      currency,\n      displayBalance,\n      balance,\n      nextTick,\n    };\n  },\n  props: {\n    isClickable: {\n      type: Boolean,\n      default: true, // 是否开启点击事件\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n@icon-color: #93acd3;\n\n.langedvs {\n  border-radius: 10px;\n  height: 100%;\n  padding: 10px 17px;\n  background: rgba(68, 88, 116, 0.24);\n  border: 1px solid transparent;\n  box-shadow: 0 2px 4px #0000002e;\n  cursor: pointer;\n  z-index: 15;\n  .langedv {\n    flex: 1 1;\n\n    .triggerue {\n      display: flex;\n      flex-direction: column;\n      align-items: flex-start;\n      margin-left: 10px;\n      width: 100%;\n\n      &-placeholder {\n        display: block;\n        font-size: 14px;\n        transform-origin: left top;\n        font-family: Montserrat Variable, sans-serif;\n        font-weight: 600;\n        line-height: 18px;\n      }\n\n      &-lantext {\n        color: #fff;\n        display: block;\n        font-size: 14px;\n        line-height: 24px;\n        margin-left: 6px;\n        transform-origin: left top;\n        font-family: Montserrat Variable, sans-serif;\n        font-weight: 600;\n      }\n    }\n  }\n\n  .curklt {\n    margin: 0 6px;\n    width: 24px;\n    height: 24px;\n    display: flex;\n    justify-content: flex-end;\n    align-items: center;\n\n    .curren-ialtm {\n      transition: transform 0.3s ease, filter 0.3s ease;\n      width: 10px;\n      height: 10px;\n      fill: @icon-color;\n    }\n  }\n}\n\n.langedvs-images {\n  width: 24px;\n  height: 24px;\n}\n.blue-text {\n  color: #2283f6;\n}\n\n.rwt-text {\n  color: #55657e;\n}\n.depcy {\n  position: absolute;\n  margin-top: 90px;\n  max-width: 233px;\n  z-index: 15;\n}\n@media only screen and (max-width: 767px) {\n  .depcy {\n    max-width: calc(100% - 45px) !important;\n    z-index: 17 !important;\n  }\n}\n</style>\n"]}]}