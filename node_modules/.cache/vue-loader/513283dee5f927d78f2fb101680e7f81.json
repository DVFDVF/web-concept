>{{ lang.v.pix_cry }}</h2>\n    <div>\n      <!-- <h4 class=\"main-title\">Saque com</h4> -->\n      <div class=\"pix-select\">\n        <PixButton\n          v-for=\"(item, index) in pixList\"\n          :key=\"item.key\"\n          :class=\"{ active: activeIndex === index }\"\n          v-show=\"isDrawOut(item.key)\"\n          @click=\"setActive(index, item.pixMin, item.pixMax)\"\n        >\n          <template v-slot:pixLevel>{{ item.pixLevel }}</template>\n          <template v-slot:pixValue\n            >{{ item.pixMin }} ~ {{ item.pixMax }}</template\n          >\n        </PixButton>\n      </div>\n      <div>\n        <p class=\"withdraw-text\">\n          {{ lang.v.pix_certifique }}\n        </p>\n        <ul class=\"tips\">\n          <li>Mín 10, máx 10000</li>\n          <li>2% {{ lang.v.pix_taxa }}</li>\n        </ul>\n      </div>\n    </div>\n    <div>\n      <p class=\"accountTip\">{{ lang.v.pix_tipo }}</p>\n      <div class=\"accountType\">\n        <a-select\n          v-model:value=\"province\"\n          style=\"width: 110px\"\n          :options=\"\n            provinceData.map((pro) => ({\n              value: pro.pixTitle,\n            }))\n          \"\n          @change=\"handleSelectChange\"\n        ></a-select>\n        <div v-if=\"typeValue == 'PIX_PHONE' || typeValue == 'PHONE'\">\n          {{ dataCenter.webConfig.countryCode }}\n        </div>\n        <input\n          style=\"margin-left: 10px\"\n          :placeholder=\"lang.v.pix_code\"\n          v-model=\"accountNoValue\"\n          @input=\"formatInput\"\n        />\n      </div>\n      <div class=\"ngAmount\">\n        {{ lang.v.pix_saldo }}:<i class=\"iconfont icon-unit-brl\"></i>\n        {{ availableValue }}\n        <input\n          class=\"withdrawMain-input\"\n          type=\"text\"\n          v-model=\"amountValue\"\n          @input=\"validateInput\"\n        /><span class=\"withdrawMain-input-span\"\n          >{{ lang.v.pix_quantia }} (BRL)</span\n        >\n      </div>\n    </div>\n    <ul class=\"bottom-tips\">\n      <li>\n        {{ lang.v.pix_valor }}\n        <span style=\"color: #ffa200; font-weight: 700\"\n          >{{ amountValue }} BRL</span\n        >\n      </li>\n      <li>\n        {{ lang.v.pix_para }}\n        <span style=\"color: #64d23e; font-weight: 700\">{{\n          lang.v.pix_aposte\n        }}</span>\n      </li>\n      <li>\n        {{ lang.v.tr_withdrawal_record }}\n        <span\n          style=\"color: #64d23e; font-weight: 700\"\n          @click=\"displayContent\"\n          >{{ lang.v.tr_click_view }}</span\n        >\n      </li>\n    </ul>\n    <DeposirarButton\n      login-text=\"Retirada\"\n      class=\"topUp\"\n      @click=\"retiradaAction\"\n    />\n    <div v-if=\"isLodding\" class=\"center lodding\">\n      <ChatsLodding />\n    </div>\n  </div>\n</template>\n\n<script setup lang=\"ts\">\nimport { lang } from '@/lang';\nimport PixButton from '@/views/home/head/wallet/widget/PixButton.vue';\nimport DeposirarButton from '@/views/home/head/popup/walletSix/headButtonPop.vue';\nimport {\n  createPayout,\n  payoutAmount,\n  kakaPayout,\n  payPayout,\n} from '../../api/wallet-api/walletIndex';\nimport { inject, onMounted, reactive, ref } from 'vue';\nimport dataCenter from '@/dataCenter';\nimport { Select as ASelect } from 'ant-design-vue';\nimport ChatsLodding from '@/views/chats/ChatsLodding.vue';\nimport { tip } from '@/components/warn/warn';\nimport { ModalCloseFun, getRouter } from '@/manager/view';\nconst provinceData = [\n  { pixTitle: 'CPF', pixValue: 'PIX_CPF', kakaPix: 'CPF' },\n  { pixTitle: 'PHONE', pixValue: 'PIX_PHONE', kakaPix: 'PHONE' },\n  { pixTitle: 'EMAIL', pixValue: 'PIX_EMAIL', kakaPix: 'EMAIL' },\n];\nconst closeFunction = inject('close') as ModalCloseFun;\nconst province = ref(provinceData[0].pixTitle);\nconst typeValue = ref('PIX_CPF');\nconst kakaType = ref('CPF');\n/**可提现余额 */\nconst availableValue = ref(0);\nconst amountValue = ref(10);\nconst accountNoValue = ref('');\n/**加载 */\nconst isLodding = ref(false);\nconst activeIndex = ref(0);\nconst minValue = ref(10);\nconst maxValue = ref(20000);\nconst setActive = (index: number, min: number, max: number) => {\n  activeIndex.value = index;\n  minValue.value = min;\n  maxValue.value = max;\n};\n\nconst pixList = reactive([\n  {\n    key: 'hkp',\n    pixLevel: 'I',\n    pixMin: 10,\n    pixMax: 20000,\n  },\n]);\nfunction retiradaAction() {\n  if (amountValue.value && amountValue.value >= 10) {\n    if (amountValue.value <= availableValue.value) {\n      if (accountNoValue.value) {\n        let accountNo;\n        if (typeValue.value == 'PIX_PHONE' || kakaType.value == 'PHONE') {\n          accountNo = `${dataCenter.webConfig.countryCode}${accountNoValue.value}`;\n        } else {\n          accountNo = accountNoValue.value;\n        }\n        if (!dataCenter.user.data) return;\n        isLodding.value = true;\n        payPayout(amountValue.value, typeValue.value, accountNo)\n          .then((resp) => {\n            if (!resp) {\n              return;\n            }\n            // 扣钱\n            dataCenter.wallet.change(\n              dataCenter.wallet.selectId,\n              -amountValue.value,\n            );\n            payoutAmo();\n            if (resp.type == 1) {\n              tip.success(resp.msg);\n            } else {\n              tip.success(resp.msg);\n            }\n          })\n          .finally(() => {\n            isLodding.value = false;\n          });\n      } else {\n        tip.error(lang.v.pix_code);\n      }\n    } else {\n      tip.error(lang.v.pix_excee);\n    }\n  } else {\n    tip.error(`${lang.v.pix_cannot} 10`);\n  }\n}\nfunction payoutAmo() {\n  if (!dataCenter.user.data) return;\n  payoutAmount(dataCenter.user.data.id).then((resp) => {\n    if (!resp) return;\n    availableValue.value = Math.floor(resp);\n  });\n}\nonMounted(() => {\n  payoutAmo();\n});\n\n/**类型选择 */\nfunction handleSelectChange(value: any) {\n  accountNoValue.value = '';\n  const selectedPixTitle = value;\n  const selectedItem = provinceData.find(\n    (item) => item.pixTitle === selectedPixTitle,\n  );\n  if (selectedItem) {\n    typeValue.value = selectedItem.pixValue;\n    kakaType.value = selectedItem.pixTitle;\n  }\n}\nconst validateInput = (event: any) => {\n  const value = event.target.value;\n  const regex = /[^0-9]/g;\n  let processedValue = value.replace(regex, '');\n  amountValue.value = processedValue;\n  event.target.value = processedValue;\n};\n//判断是否使用该提现\nconst isDrawOut = (key: string) => {\n  return dataCenter.webConfig.drawOut.includes(key);\n};\nfunction formatInput(event: Event) {\n  if (typeValue.value == 'PIX_CPF' || kakaType.value == 'CPF') {\n    accountNoValue.value = dataCenter.wallet.pixCpf(event);\n  }\n}\nfunction displayContent() {\n  closeFunction();\n  getRouter().push('/Userprofe/Transactions');\n}\n</script>\n\n<style lang=\"less\" scoped>\n.pix-size {\n  height: 100%;\n  width: 100%;\n}\n.pix-title {\n  font-size: 24px;\n  line-height: 135%;\n  letter-spacing: -1px;\n  margin-bottom: 16px;\n  font-weight: bold;\n  font-family: Montserrat Variable, sans-serif;\n  color: #fff;\n}\n.pix-select {\n  display: grid;\n  gap: 10px;\n  grid-template-columns: repeat(2, 1fr);\n}\n.withdraw-text {\n  margin-top: 10px;\n  font-size: 12px;\n  color: #fff;\n}\n.tips {\n  font-size: 8px;\n  color: #7e7e7e;\n  line-height: 10px;\n  margin-left: 10px;\n}\n.tips li {\n  list-style: decimal;\n}\n.accountTip {\n  margin-top: 12px;\n  color: #aab3ba;\n}\n.accountType {\n  display: flex;\n  align-items: center;\n  background: #1d1e22;\n  border-radius: 2px;\n  text-indent: 14px;\n  margin: 7px 0;\n  height: 40px;\n}\n.ngAmount {\n  font-size: 12px;\n  margin-top: 13px;\n  color: #aab3ba;\n  position: relative;\n}\n.icon-unit-brl:before {\n  content: 'R$';\n  font-size: 16px;\n  font-style: normal;\n}\n.withdrawMain-input {\n  width: 100%;\n  background-color: #1d1e22;\n  border-radius: 4px;\n  height: 40px;\n  color: #868686;\n  text-indent: 150px;\n  font-size: 13px;\n}\n.withdrawMain-input-span {\n  color: #8ea7bb;\n  position: absolute;\n  bottom: 12px;\n  left: 14px;\n  font-size: 15px;\n}\n.bottom-tips {\n  font-size: 14px;\n  color: #7e7e7e;\n  line-height: 18px;\n  margin-top: 10px;\n}\n.topUp {\n  margin: 24px 0 19px;\n  width: 100%;\n  height: 50px;\n  z-index: 0;\n}\n.lodding {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0, 0, 0, 0.5);\n}\n.active {\n  color: #8ea7bb;\n  background: #232323;\n  border: 2px solid rgba(131, 153, 172, 0.5);\n  border-radius: 10px;\n}\n</style>\n"]}]}