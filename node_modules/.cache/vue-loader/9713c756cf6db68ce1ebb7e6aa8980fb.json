pan class=\"toggle-title\"> {{ lang.v.sz_music }} </span>\n    </div>\n    <!-- 音乐音量滑动条 -->\n    <a-config-provider :theme=\"{ token: { colorPrimary: '#2283f6' } }\">\n      <a-slider\n        class=\"tens\"\n        v-model:value=\"musicVale\"\n        :disabled=\"!disabled\"\n        :max=\"100\"\n        @change=\"handleSliderChange\"\n      />\n    </a-config-provider>\n  </div>\n</template>\n\n<script lang=\"ts\" setup>\nimport { ref, watch } from 'vue';\nimport icoTrumpet from '@/assets/icon/head/ico-trumpet.vue';\nimport { lang } from '@/lang';\nimport dataCenter from '@/dataCenter';\n// 按需引入的\nimport {\n  ConfigProvider as AConfigProvider,\n  Switch as ASwitch,\n  Slider as ASlider,\n} from 'ant-design-vue';\n\n// Define reactive state with ref\nconst musicVale = ref<number>(50);\n\nconst disabled = ref<boolean>(true);\n\n/**\n * 设置音量\n * 将值转换为 0 到 1 之间的浮点数\n *\n * */\nconst handleSliderChange = () => {\n  const volume = musicVale.value / 100;\n  dataCenter.gameConfiguration.soundVolume = volume;\n};\n/**\n * 是否打开声音\n */\nconst openSound = () => {\n  if (disabled.value) {\n    const volume = musicVale.value / 100;\n    dataCenter.gameConfiguration.soundVolume = volume;\n  } else {\n    dataCenter.gameConfiguration.soundVolume = 0;\n  }\n};\n/**\n * 监视 musicVale 的变化\n * 当音量为 0 时，设置滑块为可用\n */\nwatch(musicVale, (newValue) => {\n  if (newValue === 0) {\n    disabled.value = false;\n  }\n});\n</script>\n\n<style lang=\"less\" scoped>\n@icon-color: #93acd3;\n\n.modal-body {\n  border-radius: 10px;\n  margin-top: 8px;\n  padding: 10px 17px;\n  background: rgba(68, 88, 116, 0.24);\n  border: 1px solid transparent;\n  box-shadow: 0 2px 4px #0000002e;\n  cursor: pointer;\n}\n\n.tens {\n  border-radius: 4px;\n  background: linear-gradient(\n    to right,\n    var(#2283f6) 0,\n    var(#2283f6) var(51%),\n    var(rgba(67, 81, 101, 0.8)) var(51%),\n    var(rgba(67, 81, 101, 0.8)) 100%\n  );\n  height: 7px;\n  width: 100%;\n  z-index: 9;\n}\n\n.toggleWitch {\n  display: flex;\n  justify-content: space-between;\n}\n\n.sounds-hr {\n  background-color: rgba(125, 125, 125, 0.3);\n  height: 2px;\n  border: none;\n}\n\n.toggmty {\n  display: grid;\n  justify-items: start;\n}\n\n.toggle-title {\n  font-size: 12px;\n  color: @icon-color;\n  font-family: Montserrat Variable, sans-serif;\n  text-decoration: inherit;\n  font-weight: 600;\n  letter-spacing: -0.02em;\n  line-height: 20px;\n}\n\n.svg-useLeft {\n  margin-left: 5px;\n  width: 12px;\n  height: 12px;\n  fill: @icon-color;\n}\n</style>\n"]}]}