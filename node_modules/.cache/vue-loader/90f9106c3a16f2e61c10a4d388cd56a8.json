mounted() {\n    document.addEventListener('click', this.onDucumentClick);\n  },\n  unmounted() {\n    document.removeEventListener('click', this.onDucumentClick);\n  },\n  methods: {\n    onChange(item: Currency) {\n      this.selected = item;\n      this.showPopover = false;\n      this.$emit('change', item);\n    },\n    onDucumentClick() {\n      this.showPopover = false;\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n@icon-color: #93acd3;\n\n.langedvs {\n  position: relative;\n\n  .langedv {\n    align-items: center;\n    cursor: pointer;\n    display: flex;\n    height: 100%;\n    position: relative;\n    width: 100%;\n    border-radius: 10px;\n    padding: 8px 12px;\n    background-color: #2a3546;\n\n    .triggerue {\n      display: flex;\n      flex-direction: column;\n      align-items: flex-start;\n      width: 100%;\n\n      &-placeholder {\n        display: block;\n        font-size: 14px;\n        transform-origin: left top;\n        font-family: Montserrat Variable, sans-serif;\n        font-weight: 600;\n        line-height: 18px;\n      }\n\n      &-lantext {\n        color: #fff;\n        display: block;\n        font-size: 14px;\n        line-height: 24px;\n        margin-left: 6px;\n        transform-origin: left top;\n        font-family: Montserrat Variable, sans-serif;\n        font-weight: 600;\n      }\n    }\n  }\n\n  .curklt {\n    width: 24px;\n    height: 24px;\n    display: flex;\n    justify-content: flex-end;\n    align-items: center;\n\n    .curren-ialtm {\n      transition: transform 0.3s ease, filter 0.3s ease;\n      width: 10px;\n      height: 10px;\n      fill: @icon-color;\n    }\n  }\n}\n\n.langedvs-images {\n  width: 24px;\n  height: 24px;\n}\n.depcy {\n  position: absolute;\n  left: 0;\n  right: 0;\n  z-index: 17;\n}\n@media only screen and (max-width: 767px) {\n  .depcy {\n    position: absolute;\n    top: 100% !important;\n    margin-top: 8px;\n    left: 0;\n    right: 0;\n    z-index: 17;\n    width: 100%;\n  }\n}\n</style>\n"]}]}