\n  <footer class=\"footer\">\n    <div class=\"container\">\n      <div class=\"footer-inner\">\n        <div class=\"footer-menus\">\n          <div class=\"submenu\" v-for=\"(item, index) in list\" :key=\"item.title\">\n            <div class=\"sub-title\" @click=\"spandClick(index)\">\n              {{ item.title }}\n              <svg class=\"svg-use\">\n                <use xlink:href=\"/svg/sprite.svg#icon-arrow-down\"></use>\n              </svg>\n            </div>\n            <SubItem\n              :message=\"item.content\"\n              :class=\"{ 'sub-item': !item.isSpand }\"\n            ></SubItem>\n          </div>\n          <FooterInfo></FooterInfo>\n        </div>\n        <FooterLogosWrapper></FooterLogosWrapper>\n        <FooterSocial></FooterSocial>\n      </div>\n    </div>\n  </footer>\n</template>\n<script lang=\"ts\">\nimport { lang } from '@/lang';\nimport { defineComponent } from 'vue';\nimport FooterInfo from './FooterInfo.vue';\nimport FooterLogosWrapper from './FooterLogosWrapper.vue';\nimport FooterSocial from './FooterSocial.vue';\nimport SubItem from './SubItem.vue';\nexport default defineComponent({\n  name: 'currencies',\n\n  components: {\n    SubItem,\n    FooterInfo,\n    FooterLogosWrapper,\n    FooterSocial,\n  },\n  data() {\n    return {\n      list: [\n        {\n          get title() {\n            return lang.v.sy_d_igaming;\n          },\n          isSpand: false,\n          content: [\n            {\n              key: 'All games',\n              rout: '/casino',\n              get name() {\n                return lang.v.sy_d_allgame;\n              },\n            },\n            {\n              key: 'Dice',\n              rout: '/casino/dice?gameId=1',\n              get name() {\n                return lang.v.sy_d_dice;\n              },\n            },\n            {\n              key: 'LIMBO',\n              rout: '/casino/limbo?gameId=3',\n              get name() {\n                return lang.v.sy_d_slots;\n              },\n            },\n          ],\n        },\n        {\n          get title() {\n            return lang.v.sy_d_features;\n          },\n          isSpand: false,\n          content: [\n            {\n              key: 'Refer',\n              rout: '/referral/dashboard',\n              get name() {\n                return lang.v.sy_z_refer;\n              },\n            },\n            {\n              key: 'Bitcoin',\n              get name() {\n                return lang.v.sy_d_farming;\n              },\n              link: 'https://bitcoin.org/',\n            },\n            {\n              key: 'Vip',\n              rout: '/vip-club',\n              get name() {\n                return lang.v.sy_z_rank;\n              },\n            },\n\n            {\n              key: 'Cryptocurrency',\n              get name() {\n                return lang.v.sy_d_affiliate;\n              },\n              link: 'https://www.cryptocompare.com/',\n            },\n          ],\n        },\n        {\n          get title() {\n            return lang.v.sy_d_promo;\n          },\n          isSpand: false,\n          content: [\n            {\n              key: 'Battle',\n              rout: '/CompetitionDetails',\n              get name() {\n                return lang.v.sy_z_promotions;\n              },\n            },\n          ],\n        },\n        {\n          get title() {\n            return lang.v.sy_d_about;\n          },\n          isSpand: false,\n          content: [\n            {\n              key: 'News',\n              rout: '/LatestNews',\n              get name() {\n                return lang.v.sy_d_news;\n              },\n            },\n            {\n              key: 'Betfury team',\n              rout: '/TeamIntroduction',\n              get name() {\n                return lang.v.sy_z_betfuryTeam;\n              },\n            },\n          ],\n        },\n        {\n          get title() {\n            return lang.v.sy_d_help;\n          },\n          isSpand: false,\n          content: [\n            {\n              key: 'Fairness',\n              rout: '/Fair',\n              get name() {\n                return lang.v.sy_d_fairness;\n              },\n            },\n            {\n              key: 'Coindesk',\n              get name() {\n                return lang.v.sy_d_privacypolicy;\n              },\n              link: 'https://www.coindesk.com/',\n            },\n            {\n              key: 'Newsbtc',\n              get name() {\n                return lang.v.sy_d_termsofservice;\n              },\n              link: 'https://www.newsbtc.com/',\n            },\n            {\n              key: 'Blockchain',\n              get name() {\n                return lang.v.sy_d_sportsbetting;\n              },\n              link: 'https://the-blockchain.com/',\n            },\n            {\n              key: 'sports',\n              rout: '/sports',\n              get name() {\n                return lang.v.sy_d_bugbounty;\n              },\n            },\n            {\n              key: '在线客服',\n              get name() {\n                return lang.v.sy_d_livesupport;\n              },\n            },\n          ],\n        },\n      ],\n    };\n  },\n  methods: {\n    spandClick(index: number) {\n      this.list[index].isSpand = !this.list[index].isSpand;\n    },\n  },\n});\n</script>\n<style lang=\"less\" scoped>\n.footer {\n  background-color: #161f2c;\n  padding-top: 40px;\n  display: flex;\n  z-index: 3;\n\n  .container {\n    margin: 0 auto;\n    padding-left: 24px;\n    padding-right: 24px;\n    width: 100%;\n\n    .footer-inner {\n      display: flex;\n      flex-direction: column;\n      .footer-menus {\n        border-bottom: 1px solid rgba(85, 101, 126, 0.15);\n        display: grid;\n        gap: 15px;\n        grid-template-columns: auto auto auto auto auto 266px;\n        margin-bottom: 20px;\n        padding-bottom: 40px;\n\n        .submenu {\n          display: inline-block;\n          position: relative;\n          color: #93acd3;\n          display: block;\n          font-size: 11px;\n          font-weight: 600;\n          line-height: 16px;\n          margin-bottom: 15px;\n          margin-top: 0;\n          text-transform: uppercase;\n        }\n      }\n    }\n  }\n}\n\np {\n  text-align: left;\n}\n.svg-use {\n  display: none;\n}\n.sub-title {\n  text-align: left;\n  margin-bottom: 10px;\n}\n@media only screen and (max-width: 767px) {\n  .footer-menus {\n    display: flex !important;\n    flex-direction: column;\n    width: 100%;\n    padding-bottom: 10px !important;\n    gap: 0 !important;\n  }\n  .submenu {\n    margin-bottom: 10px !important;\n  }\n  .sub-title {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    justify-content: space-between;\n    cursor: pointer;\n  }\n  .svg-use {\n    width: 12px;\n    height: 12px;\n    display: inline;\n  }\n  .sub-item {\n    display: none;\n  }\n}\n</style>\n"]}]}