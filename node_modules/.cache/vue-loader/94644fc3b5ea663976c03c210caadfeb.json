AC,CAAC,CAAC;QAC/B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,EAAE,CAAC;MACtC;QACE,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACtE,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;MACf,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACV,CAAC,CAAC,CAAC,CAAC,CAAC;EACP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/concept/web3-concept/src/views/home/head/notify/widget/Personal.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"persnal\">\n    <div class=\"persnal-rles\">\n      <div class=\"persnal-left\">\n        <Image class=\"persnal-left-imge\" :src=\"contentData.item?.image\" />\n      </div>\n\n      <div class=\"persnal-right\">\n        <p class=\"persnal-times\">{{ formatDateTime(PersonalMessage.time) }}</p>\n        <h2 class=\"persnal-title\">\n          {{ contentData.item?.title }}\n        </h2>\n        <span class=\"persnal-text\">{{ contentData.item?.text }}</span>\n      </div>\n    </div>\n    <div class=\"persnal-footer\">\n      <a class=\"link\" @click=\"execute(PersonalMessage.cmd)\">{{\n        contentData.item?.message\n      }}</a>\n      <button\n        class=\"notify-mark-btn\"\n        @click=\"$emit('Personal-click')\"\n        v-show=\"PersonalMessage.status === 0\"\n      >\n        <span class=\"mark-read mark-btn_text\">{{ lang.v.ts_markAsRead }}</span>\n        <img src=\"\" />\n      </button>\n    </div>\n  </div>\n</template>\n<!-- Personal 组件 -->\n<script lang=\"ts\">\nimport { Message } from '@/api/protocol';\nimport { PropType, defineComponent, ref } from 'vue';\nimport dayjs from 'dayjs';\nimport { lang } from '@/lang';\nimport { execute } from '@/manager/cmd';\n//内容定义\ninterface PersonalMessageType {\n  item?: {\n    image: string;\n    title: string;\n    text: string;\n    message: string;\n  };\n}\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      execute,\n    };\n  },\n  props: {\n    PersonalMessage: {\n      type: Object as PropType<Message>,\n      required: true,\n    },\n  },\n  setup(props) {\n    //内容处理\n    const contentData = ref<PersonalMessageType>({});\n    // 检查 PlatformItem 和 content 是否存在\n    if (\n      props.PersonalMessage &&\n      typeof props.PersonalMessage.content === 'string'\n    ) {\n      try {\n        contentData.value = JSON.parse(props.PersonalMessage.content);\n      } catch (error) {\n        console.error('解析 JSON 数据时发生错误:', error);\n      }\n    }\n    return { contentData };\n  },\n  methods: {\n    // 格式化日期时间的函数\n    formatDateTime(dateTimeStr: string): string {\n      return dayjs(dateTimeStr).format('MM/DD/YY HH:mm:ss A');\n    },\n  },\n});\n</script>\n<style lang=\"less\" scoped>\n.persnal {\n  margin-bottom: 8px;\n  background-color: rgb(28, 37, 50);\n  border-radius: 8px;\n  cursor: pointer;\n  position: relative;\n  transition: background-color 0.1s ease-in-out 0s,\n    border-color 0.1s ease-in-out 0s;\n  display: flex;\n  flex-direction: column;\n  min-height: 44px;\n  padding: 12px 12px 0px;\n}\n\n.persnal-left-imge {\n  width: 40px;\n  height: 40px;\n}\n.persnal-left {\n  align-items: center;\n  display: flex;\n  flex-direction: column;\n  margin-right: 12px;\n  min-width: 40px;\n}\n.persnal-rles {\n  display: flex;\n  justify-content: space-between;\n}\n.persnal-right {\n  flex-grow: 1;\n  text-align: left;\n  margin-bottom: 12px;\n}\n\n.persnal-times {\n  color: rgb(85, 101, 126);\n  font-size: 12px;\n  font-weight: 500;\n  line-height: 18px;\n  display: flex;\n  justify-content: space-between;\n  margin-bottom: 4px;\n}\n.persnal-title {\n  font-size: 16px;\n  font-weight: 600;\n  margin-bottom: 8px;\n  color: #fff;\n}\n.persnal-text {\n  color: #93acd3;\n  font-size: 12px;\n  margin-bottom: 0;\n  overflow: hidden;\n  transition: all 0.3s;\n}\n\n.persnal-footer {\n  background-color: #202a39;\n  border-bottom-left-radius: 8px;\n  border-bottom-right-radius: 8px;\n  border-top: 1px solid #2a3546;\n  justify-content: space-between;\n  margin: 0px -12px;\n  padding-inline: 2px;\n  padding: 10px 12px;\n  display: flex;\n}\n.notify-mark-btn {\n  background-color: transparent;\n  border: none;\n  align-items: center;\n  color: #55657e;\n  display: flex;\n  font-size: 11px;\n  font-weight: 600;\n  gap: 4px;\n  letter-spacing: 0.02em;\n  margin-left: auto;\n  text-align: right;\n}\n.link {\n  align-items: center;\n  color: #2283f6;\n  display: flex;\n  font-weight: 600;\n  padding: 0;\n  text-decoration: none;\n  text-align: left;\n}\n</style>\n"]}]}