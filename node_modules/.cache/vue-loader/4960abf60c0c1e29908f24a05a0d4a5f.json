\n        <div class=\"content\" @click=\"onClickMenu(item)\">\n          <div :class=\"iconSizeClass\">\n            <svg\n              class=\"svg-use\"\n              :width=\"iconSize\"\n              :height=\"iconSize\"\n              focusable=\"false\"\n              aria-hidden=\"true\"\n            >\n              <use :xlink:href=\"item.icon\"></use>\n            </svg>\n            <!-- <component :is=\"item.icon\" /> -->\n          </div>\n        </div>\n      </a-tooltip>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent } from 'vue';\nimport { MenuData } from '../../left/interface';\nimport { message, Tooltip } from 'ant-design-vue';\nimport { lang } from '@/lang';\nimport { handleGoogleLogin } from '../googleLogin';\nimport { tip } from '@/components/warn/warn';\nexport default defineComponent({\n  name: 'home-left-share',\n  components: { ATooltip: Tooltip },\n  computed: {\n    iconSize(): number {\n      return this.type == '0' ? 24 : 18;\n    },\n    iconSizeClass(): string {\n      return this.type == '0' ? 'icon' : 'icon-fillet';\n    },\n    backSizeClass(): string {\n      return this.type == '0' ? 'back back-size' : 'back back-size-fillet';\n    },\n  },\n  props: {\n    type: {\n      required: true,\n    },\n  },\n  data() {\n    return {\n      lang,\n      list: [\n        {\n          icon: '/svg/sprite.svg#icon-google-auth',\n          key: 'gmail',\n          get name() {\n            return lang.v.sh_gmail;\n          },\n        },\n        {\n          icon: '/svg/sprite.svg#icon-telegram-auth-new',\n          key: 'telegram',\n          get name() {\n            return lang.v.sh_telegram;\n          },\n        },\n        {\n          icon: '/svg/sprite.svg#icon-metamask',\n          key: 'metMask',\n          get name() {\n            return lang.v.sh_metMask;\n          },\n        },\n        {\n          icon: '/svg/sprite.svg#icon-tronwallet-new',\n          key: 'TronWallet',\n          get name() {\n            return lang.v.sh_tronWallet;\n          },\n        },\n        {\n          icon: '/svg/sprite.svg#icon-binance',\n          key: 'Binance Chain',\n          get name() {\n            return lang.v.sh_binance;\n          },\n        },\n        {\n          icon: '/svg/sprite.svg#icon-coin98-new',\n          key: 'coin 98',\n          get name() {\n            return lang.v.sh_coin;\n          },\n        },\n      ],\n    };\n  },\n  methods: {\n    onClickMenu(item: MenuData) {\n      if (item.key == 'gmail') {\n        handleGoogleLogin();\n      } else {\n        tip.warning(lang.v.me_notOpen);\n      }\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.content {\n  display: flex;\n  cursor: pointer;\n\n  .back-size {\n    width: 42px;\n    height: 42px;\n    border-radius: 8px;\n  }\n\n  .back-size-fillet {\n    width: 30px;\n    height: 30px;\n    border-radius: 15px;\n  }\n\n  .back {\n    margin-top: 0px;\n    margin-right: 6px;\n    background-color: #2a3546;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n\n    .content {\n      display: grid;\n      place-items: center;\n\n      .icon {\n        width: 24px;\n        height: 24px;\n      }\n\n      .icon-fillet {\n        width: 18px;\n        height: 18px;\n      }\n    }\n  }\n\n  .back:hover {\n    background-color: #3c485c;\n    color: #ffffff;\n  }\n}\n</style>\n"]}]}