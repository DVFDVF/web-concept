omi/Desktop/项目/concept/fury-client/src/games/wheel/WheelRow.vue?vue&type=script&lang=ts"],"sourcesContent":["\nimport { defineComponent } from 'vue';\nimport { AwardResp } from './api/protocol';\nimport dayjs from 'dayjs';\nimport dataCenter from '@/dataCenter';\nimport userManager from '@/manager/userManager';\nimport Avatar from '@/components/Avatar.vue';\n\nexport default defineComponent({\n  components: {\n    Avatar,\n  },\n  data() {\n    return {\n      /**临时存储用户数据 */\n      userData: { name: '', avatar: '' },\n    };\n  },\n  props: {\n    record: {\n      type: Object as () => AwardResp,\n      required: true,\n    },\n  },\n  watch: {\n    'record.userId': function (value) {\n      this.getUserData(value.userId);\n    },\n  },\n  mounted() {\n    this.getUserData(this.record.userId);\n  },\n  methods: {\n    /**转换时间 */\n    conversionTime(wheelTime: string): string {\n      return dayjs(wheelTime).format('HH:mm:ss');\n    },\n    //查询用户信息\n    getUserData(userId: number) {\n      userManager.getUser(userId)?.then((resp) => {\n        if (!resp) return;\n        this.userData = { name: resp.name, avatar: resp.avatar };\n      });\n    },\n    //查询货币icon\n    getCuurrencyIcon(currencyid: number) {\n      if (dataCenter.webConfig.cryptoCurrencies.concat()) {\n        const foundCurrency = dataCenter.webConfig.cryptoCurrencies\n          .concat()\n          .find((currency) => currency.id === currencyid);\n        return foundCurrency?.icon;\n      }\n    },\n  },\n});\n"],"mappings":"AACA,SAASA,eAAe,QAAQ,KAAK;AAErC,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,UAAU,MAAM,cAAc;AACrC,OAAOC,WAAW,MAAM,uBAAuB;AAC/C,OAAOC,MAAM,MAAM,yBAAyB;AAE5C,eAAeJ,eAAe,CAAC;EAC7BK,UAAU,EAAE;IACVD;GACD;EACDE,IAAIA,CAAA;IACF,OAAO;MACL;MACAC,QAAQ,EAAE;QAAEC,IAAI,EAAE,EAAE;QAAEC,MAAM,EAAE;MAAE;KACjC;EACH,CAAC;EACDC,KAAK,EAAE;IACLC,MAAM,EAAE;MACNC,IAAI,EAAEC,MAAyB;MAC/BC,QAAQ,EAAE;;GAEb;EACDC,KAAK,EAAE;IACL,eAAe,EAAE,SAAAC,CAAUC,KAAK;MAC9B,IAAI,CAACC,WAAW,CAACD,KAAK,CAACE,MAAM,CAAC;IAChC;GACD;EAC