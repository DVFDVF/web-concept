C,CAAC,CAAC,EAAE;EACxC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;IACP,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;IAC3B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EACd;AACF","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/casino/LobbySections/card/CardEmpty.vue","sourceRoot":"","sourcesContent":["<template>\n  <div\n    class=\"scroll-container\"\n    ref=\"emblaContent\"\n    v-on:mousedown=\"startDrag\"\n    v-on:mousemove=\"onDrag\"\n    v-on:mouseup=\"stopDrag\"\n  >\n    <ul\n      class=\"list-item-row\"\n      :style=\"{ transform: 'translateX(' + transX + 'px)' }\"\n    >\n      <li v-for=\"index in 10\" :key=\"index\">\n        <div class=\"content\">\n          <div class=\"card-wrapper\" role=\"button\" tabindex=\"0\"></div>\n        </div>\n      </li>\n    </ul>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { GetGamesResp } from '@/views/home/api/protocol';\nimport { defineComponent } from 'vue';\nexport default defineComponent({\n  props: {\n    gameData: {\n      type: Object as () => GetGamesResp[],\n    },\n    search: {\n      default: false,\n    },\n  },\n  data() {\n    return {\n      dragging: false,\n      startX: 0,\n      lastX: 0,\n      transX: -0,\n    };\n  },\n  methods: {\n    startDrag(e: MouseEvent) {\n      document.addEventListener('mousemove', this.onDrag);\n      document.addEventListener('mouseup', this.stopDrag);\n      this.dragging = true;\n      this.startX = this.transX;\n      this.lastX = e.clientX;\n    },\n\n    onDrag(e: MouseEvent) {\n      if (this.dragging) {\n        const container = this.$refs.emblaContent as HTMLElement;\n        const container1 = this.$refs.viewportContent as HTMLElement;\n        if (!container || !container1 || !this.gameData) return;\n        const rect = container.getBoundingClientRect();\n        const rect1 = container1.getBoundingClientRect();\n        const length = this.gameData.length;\n        const percentage = (e.clientX - this.lastX) * 2 + this.startX;\n        if (percentage > rect.width * 0.4) {\n          this.transX = rect.width * 0.4;\n        } else if (\n          percentage <\n          rect.width * 0.6 - rect1.width * (length + 1) + 6 * (length - 1)\n        ) {\n          this.transX =\n            rect.width * 0.6 - rect1.width * (length + 1) + 6 * (length - 1);\n        } else {\n          this.transX = percentage;\n        }\n      }\n    },\n\n    stopDrag(e: MouseEvent) {\n      document.removeEventListener('mousemove', this.onDrag);\n      document.removeEventListener('mouseup', this.stopDrag);\n      const container = this.$refs.emblaContent as HTMLElement;\n      const container1 = this.$refs.viewportContent as HTMLElement;\n      if (!container || !container1 || !this.gameData) return;\n      const rect = container.getBoundingClientRect();\n      const rect1 = container1.getBoundingClientRect();\n      const length = this.gameData.length;\n      const percentage = (e.clientX - this.lastX) * 2 + this.startX;\n      if (percentage > 0) {\n        this.transX = 0;\n      } else if (\n        percentage <\n        rect.width - rect1.width * (length + 1) - 16 * (length - 1)\n      ) {\n        this.transX =\n          rect.width - rect1.width * (length + 1) - 16 * (length - 1);\n      } else {\n        this.transX = percentage;\n      }\n      this.dragging = false;\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.scroll-container {\n  overflow: hidden;\n  user-select: none;\n}\n\n.list-item-row {\n  display: flex;\n  flex-direction: row;\n  transition: transform 0.2s;\n}\n\nul,\nli {\n  list-style: none;\n  padding: 0;\n  margin: 0;\n}\n\nli {\n  margin: 6px;\n}\n\n.card-wrapper {\n  border-radius: 14px;\n  container-type: inline-size;\n  position: relative;\n  height: 100%;\n}\n\n.card-wrapper {\n  aspect-ratio: 182/240;\n  background: linear-gradient(\n      150.15deg,\n      rgba(34, 131, 246, 0.147) 27.17%,\n      rgba(34, 131, 246, 0.084) 46.76%,\n      rgba(34, 131, 246, 0) 61.36%\n    ),\n    #263041;\n  width: 100%;\n}\n\n.content {\n  width: 172px;\n  height: 227px;\n}\n@media only screen and (max-width: 767px) {\n  .content {\n    width: calc(33.33vw - 20px);\n    height: auto;\n  }\n}\n</style>\n"]}]}