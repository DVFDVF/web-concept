CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC;MACZ,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC;MACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC;MACb,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC;MACR,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;MACd,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;MACjB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;MAClB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC;MACjB,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACrC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACpB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACZ,CAAC;EACH,CAAC;AACH,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/login/inputBox/InputBoxContent.vue","sourceRoot":"","sourcesContent":["<template>\n  <div\n    :class=\"[\n      'content',\n      'input-lg',\n      {\n        click: clickInput,\n        'error-color': isError,\n      },\n    ]\"\n    @click=\"focuInpust(data.key)\"\n  >\n    <div>\n      <a-popover\n        :trigger=\"isShowPopover(data.key) ? 'click' : ''\"\n        color=\"#2a3546\"\n        :overlayStyle=\"{ 'pointer-events': 'none' }\"\n      >\n        <template #content>\n          <div :class=\"['popover', { background: 'red' }]\">\n            <p\n              :class=\"[\n                'text-type',\n                {\n                  'text-type-nomal': checkLength(\n                    data.key == 'password' ? password : confirm,\n                  ),\n                },\n              ]\"\n            >\n              {{\n                checkLength(data.key == 'password' ? password : confirm)\n                  ? '✓'\n                  : 'X'\n              }}\n              {{ lang.v.dl_minimum }}\n            </p>\n            <p\n              :class=\"[\n                'text-type',\n                {\n                  'text-type-nomal': checkUpperCaseLowerCase(\n                    data.key == 'password' ? password : confirm,\n                  ),\n                },\n              ]\"\n            >\n              {{\n                checkUpperCaseLowerCase(\n                  data.key == 'password' ? password : confirm,\n                )\n                  ? '✓'\n                  : 'X'\n              }}\n              {{ lang.v.dl_lowercase }}\n            </p>\n            <p\n              :class=\"[\n                'text-type',\n                {\n                  'text-type-nomal': checkNumber(\n                    data.key == 'password' ? password : confirm,\n                  ),\n                },\n              ]\"\n            >\n              {{\n                checkNumber(data.key == 'password' ? password : confirm)\n                  ? '✓'\n                  : 'X'\n              }}\n              {{ lang.v.dl_digits }}\n            </p>\n          </div>\n        </template>\n        <div\n          :class=\"['input-inner', { 'mobile-content': data.key == 'mobile' }]\"\n        >\n          <span class=\"input-icon\" v-show=\"data.key != 'mobile'\">\n            <div class=\"icon\">\n              <svg\n                class=\"svg-use\"\n                width=\"20\"\n                height=\"20\"\n                focusable=\"false\"\n                aria-hidden=\"true\"\n              >\n                <use :xlink:href=\"data.icon\"></use>\n              </svg>\n            </div>\n          </span>\n          <span v-show=\"data.key == 'mobile'\" class=\"country-code\"\n            >{{ dataCenter.webConfig.countryCode }} ｜</span\n          >\n          <div class=\"input-tag-wrapper\">\n            <span class=\"input-placeholder\">\n              {{ data.name }}\n            </span>\n            <input\n              :class=\"[\n                'input-tag',\n                {\n                  'input-height': clickInput || isNotEmpty,\n                  'input-height-nomal': !clickInput && !isNotEmpty,\n                },\n              ]\"\n              placeholder=\"\"\n              autocomplete=\"off\"\n              :ref=\"data.key\"\n              @blur=\"handleBlur(data.key)\"\n              @input=\"updateInputValue\"\n              v-model=\"inputText\"\n              :type=\"isShowPopover(data.key) ? typeText : ''\"\n            />\n          </div>\n          <div class=\"eyes-color\" @click.stop=\"clickEyes\">\n            <svg\n              class=\"svg-use\"\n              width=\"24\"\n              height=\"24\"\n              focusable=\"false\"\n              aria-hidden=\"true\"\n              v-show=\"isShowPopover(data.key)\"\n            >\n              <use :xlink:href=\"eyes\"></use>\n            </svg>\n            <!-- <component :is=\"item.icon\" /> -->\n          </div>\n        </div>\n      </a-popover>\n    </div>\n  </div>\n  <span class=\"input-error-text\" v-show=\"isError\">{{ errorText }}</span>\n</template>\n\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { phoneValid } from '@/utils';\nimport { Popover as APopover } from 'ant-design-vue';\nimport { defineComponent } from 'vue';\nexport default defineComponent({\n  name: 'home-login-inpuBox',\n  computed: {\n    errorText() {\n      if (this.data.key == 'email') {\n        return 'Invalid email format';\n      }\n      if (this.data.key == 'mobile') {\n        return 'Phone number format error';\n      }\n      if (this.data.key == 'password') {\n        return 'Password doesn’t meet the requirements';\n      }\n      if (this.data.key == 'confirm') {\n        if (this.confirm != this.password) {\n          return 'passwords are not the same';\n        }\n        return 'Password doesn’t meet the requirements';\n      }\n      return '';\n    },\n  },\n\n  components: { APopover },\n  props: ['data'],\n  emits: ['inputContent'],\n  methods: {\n    updateInputValue(event: Event) {\n      const text = (event.target as HTMLInputElement).value;\n      this.$emit('inputContent', this.data.key, text);\n      if (text) {\n        this.isNotEmpty = true;\n      } else {\n        this.isNotEmpty = false;\n      }\n      if (this.data.key == 'email') {\n        this.email = text;\n\n        const emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\n        this.isError = !emailRegex.test(text);\n      } else if (this.data.key == 'mobile') {\n        this.email = text;\n        this.isError = !phoneValid(text);\n      } else if (this.data.key == 'password') {\n        this.password = text;\n        // this.clickLoginPassword(text);\n        this.isError = !(\n          this.checkUpperCaseLowerCase(this.password) &&\n          this.checkLength(this.password) &&\n          this.checkNumber(this.password)\n        );\n      } else if (this.data.key == 'confirm') {\n        this.confirm = text;\n        // this.clickLoginPassword(text);\n        this.isError = !(\n          this.checkUpperCaseLowerCase(this.confirm) &&\n          this.checkLength(this.confirm) &&\n          this.checkNumber(this.confirm) &&\n          this.confirm != this.password\n        );\n      } else {\n        this.code = text;\n        // this.clickLoginPassword(text);\n      }\n      if (text.trim().length === 0) {\n        this.isError = false;\n      }\n    },\n\n    checkLength(str: string) {\n      return str.length >= 8;\n    },\n\n    checkUpperCaseLowerCase(str: string) {\n      return /[A-Z]/.test(str) && /[a-z]/.test(str);\n    },\n\n    checkNumber(str: string) {\n      return /\\d/.test(str);\n    },\n    focuInpust(key: string) {\n      const inputRef = this.$refs[key] as HTMLInputElement;\n      if (inputRef) {\n        inputRef.focus();\n      }\n      if (this.data.key == key) {\n        this.clickInput = true;\n      }\n    },\n    handleBlur(key: string) {\n      if (this.data.key == key) {\n        this.clickInput = false;\n        this.firtstClick = true;\n      }\n    },\n    clickEyes() {\n      if (this.eyes == '/svg/sprite.svg#icon-eye-open') {\n        this.eyes = '/svg/sprite.svg#icon-eye-close';\n\n        this.typeText = '';\n      } else {\n        this.eyes = '/svg/sprite.svg#icon-eye-open';\n        this.typeText = 'password';\n      }\n    },\n    isShowPopover(key: string) {\n      if (key == 'password') {\n        return true;\n      }\n      if (key == 'confirm') {\n        return true;\n      }\n\n      return false;\n    },\n  },\n\n  mounted() {\n    if (this.data.key == 'code') {\n      const text = localStorage.getItem('ReferralCode') as string;\n      if (text) {\n        this.inputText = text;\n        this.clickInput = true;\n        this.$emit('inputContent', this.data.key, this.inputText);\n      }\n    }\n  },\n  data() {\n    return {\n      lang,\n      email: '',\n      password: '',\n      confirm: '',\n      inputText: '',\n      code: '',\n      isError: false,\n      isNotEmpty: false,\n      firtstClick: false,\n      clickInput: false,\n      eyes: '/svg/sprite.svg#icon-eye-open',\n      typeText: 'password',\n      dataCenter,\n    };\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.content {\n  font-size: 14px;\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n  position: relative;\n  background-color: #090c1199;\n  border: 1px solid var(--input-border, #2a3546);\n  border-radius: 10px;\n}\n\n.content:hover {\n  border: 1px solid #2283f6;\n}\n\n.input-lg .input-inner {\n  height: 58px;\n  padding: 7px 18px;\n}\n.input-lg .mobile-content {\n  height: 48px;\n}\n\n.input-inner {\n  align-items: center;\n  cursor: text;\n  display: flex;\n  position: relative;\n  transition: color 0.1s ease-in-out, border-color 0.1s ease-in-out;\n}\n\n.input-lg .input-icon {\n  height: 24px;\n  margin-left: -6px;\n  margin-right: 8px;\n  width: 24px;\n}\n\n.input-icon {\n  align-items: center;\n  display: flex;\n  flex-shrink: 0;\n  justify-content: center;\n  pointer-events: none;\n  display: grid;\n  place-items: center;\n}\n\n.input-icon {\n  -webkit-touch-callout: none;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  user-select: none;\n}\n\n.input-lg .input-tag {\n  border: none;\n  font-size: 14px;\n  letter-spacing: -0.28px;\n  padding: 0;\n}\n.country-code {\n  font-size: 14px;\n}\n\n.input-tag-wrapper {\n  display: block;\n  flex: 1 1;\n  position: relative;\n  width: 0;\n  margin-left: 10px;\n  z-index: 0;\n\n  .input-placeholder,\n  .input-tag {\n    display: block;\n    position: relative;\n    font-weight: 600;\n    line-height: 18px;\n  }\n\n  .input-tag {\n    width: 100%;\n    background-color: #00000000;\n    /* height: var(--input-height, 0); */\n    color: white;\n    margin-top: 0;\n    outline: none;\n    transition: margin-top 0.1s ease-in-out;\n  }\n\n  /* .input-placeholder {\n    font-size: var(--input-font, 14px);\n\n  } */\n}\n\n.icon {\n  width: 24px;\n  height: 24px;\n  /* color: #93acd3; */\n  cursor: pointer;\n}\n\n.eyes-color {\n  color: #93acd3;\n}\n\n.click {\n  font-size: 12px;\n  border: 1px solid #2283f6;\n}\n\n.input-height {\n  height: auto;\n  transition: 3s;\n}\n\n.input-height-nomal {\n  height: 0;\n  transition: 3s;\n}\n\n:root {\n  --input-height: auto;\n  --input-font: 12px;\n  --transition-duration: 0.3s;\n  --transition-timing-function: ease;\n}\n\n.popover {\n  font-family: Montserrat Variable, sans-serif;\n  font-size: 11px;\n  font-weight: 600;\n  line-height: 1.5;\n\n  .text-type {\n    color: #93acd3;\n    margin-bottom: 0px;\n    padding: 0;\n  }\n\n  .text-type-nomal {\n    color: #1bb83d;\n  }\n}\n\n.error-color {\n  border: 1px solid #ed1d49;\n}\n\n.input-error-text {\n  margin-top: -10px;\n  padding: 0 18px;\n  color: #ed1d49;\n  font-size: 12px;\n  font-weight: 500;\n  line-height: 18px;\n}\n</style>\n"]}]}