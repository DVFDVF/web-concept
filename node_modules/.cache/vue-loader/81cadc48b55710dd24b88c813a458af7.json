C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;MAC7D,CAAC;IACH,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACN,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EAC1C,CAAC,CAAC,CAAC,CAAC,CAAC;;EAEL,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACtB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IAClB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IAC5B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACtB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IAChB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACjB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EACtB,CAAC","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/head/MyCurrency.vue","sourceRoot":"","sourcesContent":["<template>\n  <div v-if=\"!dataCenter.layout.isMobile\">\n    <a-tooltip :title=\"displayBalance(currency, balance)\" color=\"#1b2636\">\n    </a-tooltip>\n  </div>\n  <div class=\"curren row-center\" @click.stop=\"popState = !popState\" v-else>\n    <Image\n      class=\"curren-icon\"\n      :src=\"currency?.icon\"\n      :alt=\"currency?.name\"\n      loading=\"lazy\"\n    />\n    <span class=\"curren-fontsy\">{{\n      displayBalance(currency, balance, useUsdt)\n    }}</span>\n    <span class=\"curren-impsp center\">\n      <img\n        src=\"@/assets/header/MoneyBottom.svg\"\n        class=\"curren-ialtm\"\n        focusable=\"false\"\n        aria-hidden=\"true\"\n        :class=\"{ icon: popState }\"\n        :style=\"{ transform: popState ? 'rotate(180deg)' : 'none' }\"\n      />\n    </span>\n    <div class=\"floater\" ref=\"floater\"></div>\n  </div>\n\n  <MyCurrencyPop\n    class=\"my-currency-pop\"\n    :inputHidden=\"true\"\n    :toggleButtons=\"buttonStates\"\n    :dynamicTexts=\"actions\"\n    v-show=\"popState\"\n    @change=\"onChange\"\n    @useUsdt=\"updateUsdt\"\n  />\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent, reactive, ref } from 'vue';\n\nimport { Currency } from '@/api/protocol';\nimport { displayBalance } from '@/utils';\nimport MyCurrencyPop from './popup/MyCurrencyPop.vue';\nimport dataCenter from '@/dataCenter';\nimport eventBus, { Events } from '@/manager/eventBus';\nimport { MoneyChange } from '@/dataCenter/wallet';\nimport MoneyFloat from '@/components/MoneyFloat';\nimport { Tooltip } from 'ant-design-vue';\n\ninterface DataStuct {\n  buttonStates: boolean[];\n  actions: string[];\n}\nexport default defineComponent({\n  name: 'mycurrency',\n  components: {\n    MyCurrencyPop,\n    ATooltip: Tooltip,\n  },\n  setup() {\n    const popState = ref(false);\n    const balance = reactive(dataCenter.wallet.map);\n    const currency = ref<Currency>();\n    const useUsdt = ref(false);\n    function updateUsdt(value: boolean) {\n      useUsdt.value = value;\n    }\n    function updateCurrency(currencyId: number) {\n      dataCenter.wallet.selectCurrency(currencyId);\n    }\n\n    return {\n      popState,\n      balance,\n      currency,\n      displayBalance,\n      useUsdt,\n      updateUsdt,\n      updateCurrency,\n      dataCenter,\n    };\n  },\n  data(): DataStuct {\n    return {\n      buttonStates: [false, false],\n      actions: ['hb_hideBalance', 'hb_displayInFiat'],\n    };\n  },\n  methods: {\n    onChange(item: Currency) {\n      if (item.icon) {\n        this.currency = item;\n        this.updateCurrency(item.id);\n      }\n      this.popState = false;\n    },\n    displayMoneyChange(data: MoneyChange) {\n      if (data.value <= 0) return;\n      new MoneyFloat(data.currency, data.value).show(\n        this.$refs.floater as HTMLDivElement,\n      );\n    },\n    onDucumentClick() {\n      this.popState = false;\n    },\n  },\n  mounted() {\n    eventBus.on(Events.MONEY_CHANGE, this.displayMoneyChange);\n    document.addEventListener('click', this.onDucumentClick);\n  },\n  unmounted() {\n    eventBus.off(Events.MONEY_CHANGE, this.displayMoneyChange);\n    document.removeEventListener('click', this.onDucumentClick);\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.curren {\n  background: rgba(79, 92, 109, 0.25);\n  border-radius: 8px 0 0 8px;\n  box-shadow: 0 2px 3px #0000001f;\n  height: 36px;\n  padding-left: 10px;\n  cursor: pointer;\n  position: relative;\n\n  .floater {\n    position: absolute;\n  }\n\n  &-icon {\n    width: 20px;\n    height: 20px;\n    margin-right: 0.5em;\n  }\n\n  &-fontsy {\n    font-size: 12px;\n    font-family: Montserrat Variable, sans-serif;\n    font-weight: 600;\n    letter-spacing: -0.5px;\n    color: white;\n  }\n\n  &-impsp {\n    margin: 0 6px;\n    width: 24px;\n    height: 24px;\n  }\n\n  &-ialtm {\n    width: 10px;\n    height: 10px;\n    transition: transform 0.3s ease, filter 0.3s ease;\n  }\n}\n\n.icon {\n  filter: brightness(0) saturate(100%) invert(34%) sepia(96%) saturate(7500%)\n    hue-rotate(201deg) brightness(102%) contrast(102%);\n}\n.my-currency-pop {\n  width: 240px;\n}\n@media only screen and (max-width: 767px) {\n  .curren-fontsy {\n    font-size: 11px;\n  }\n}\n</style>\n"]}]}