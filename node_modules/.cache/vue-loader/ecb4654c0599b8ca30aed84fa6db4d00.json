AAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;AAC1D,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/concept/fury-client/src/views/game/dice/left/BoardIn.vue","sourceRoot":"","sourcesContent":["<template>\n  <div :class=\"boardType\">\n    <div class=\"board-left\">\n      <div class=\"block-prediction\">\n        <span>{{ NumberString }}</span>\n        <span>{{ lang.v.dice_prediction }}</span>\n      </div>\n    </div>\n    <div class=\"board-center\">\n      <div id=\"dice\" :style=\"'animation-duration: ' + trans + 's;'\">\n        <div class=\"side front\">\n          <div class=\"dot center\"></div>\n        </div>\n        <div class=\"side front inner\"></div>\n        <div class=\"side top\">\n          <div class=\"dot dtop dleft\"></div>\n          <div class=\"dot dbottom dright\"></div>\n        </div>\n        <div class=\"side top inner\"></div>\n        <div class=\"side right\">\n          <div class=\"dot dtop dleft\"></div>\n          <div class=\"dot center\"></div>\n          <div class=\"dot dbottom dright\"></div>\n        </div>\n        <div class=\"side right inner\"></div>\n        <div class=\"side left\">\n          <div class=\"dot dtop dleft\"></div>\n          <div class=\"dot dtop dright\"></div>\n          <div class=\"dot dbottom dleft\"></div>\n          <div class=\"dot dbottom dright\"></div>\n        </div>\n        <div class=\"side left inner\"></div>\n        <div class=\"side bottom\">\n          <div class=\"dot center\"></div>\n          <div class=\"dot dtop dleft\"></div>\n          <div class=\"dot dtop dright\"></div>\n          <div class=\"dot dbottom dleft\"></div>\n          <div class=\"dot dbottom dright\"></div>\n        </div>\n        <div class=\"side bottom inner\"></div>\n        <div class=\"side back\">\n          <div class=\"dot dtop dleft\"></div>\n          <div class=\"dot dtop dright\"></div>\n          <div class=\"dot dbottom dleft\"></div>\n          <div class=\"dot dbottom dright\"></div>\n          <div class=\"dot center dleft\"></div>\n          <div class=\"dot center dright\"></div>\n        </div>\n        <div class=\"side back inner\"></div>\n        <div class=\"side cover x\"></div>\n        <div class=\"side cover y\"></div>\n        <div class=\"side cover z\"></div>\n      </div>\n    </div>\n    <div class=\"board-right\">\n      <div class=\"block-luckynumber\">\n        <span>{{ luckyNumberString }}</span>\n        <span>{{ lang.v.dice_luckyNumber }}</span>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { lang } from '@/lang';\nimport { defineComponent } from 'vue';\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n    };\n  },\n  methods: {},\n  computed: {\n    boardType() {\n      if (this.isRollUnder) {\n        if (this.number > this.luckyNumber) {\n          return 'board in';\n        } else {\n          return 'board out';\n        }\n      } else {\n        if (this.number > this.luckyNumber) {\n          return 'board out';\n        } else {\n          return 'board in';\n        }\n      }\n    },\n    luckyNumberString() {\n      return this.luckyNumber.toLocaleString('en-US', {\n        minimumIntegerDigits: 2,\n        useGrouping: false,\n      });\n    },\n    NumberString() {\n      return this.number.toLocaleString('en-US', {\n        minimumIntegerDigits: 2,\n        useGrouping: false,\n      });\n    },\n  },\n  components: {},\n\n  props: ['number', 'isRollUnder', 'trans', 'luckyNumber'],\n});\n</script>\n\n<style lang=\"less\" scoped>\n.board.in {\n  background: linear-gradient(\n      270deg,\n      rgba(41, 126, 229, 0.75),\n      rgba(22, 31, 44, 0) 60%\n    ),\n    url(../../../../assets/dice/board-bg.085ff1b4.svg);\n  background-position: 50%;\n  background-size: cover;\n}\n\n.board.out {\n  background: linear-gradient(\n      270deg,\n      rgba(250, 48, 92, 0.75),\n      rgba(22, 31, 44, 0) 60%\n    ),\n    url(../../../../assets/dice/board-bg.085ff1b4.svg);\n  background-position: 50%;\n  background-size: cover;\n}\n\n.board {\n  align-items: center;\n  background: #161f2c;\n  background-position: 50%;\n  background-size: cover;\n  border-radius: 15px;\n  display: flex;\n  height: 150px;\n  margin-bottom: 20px;\n}\n\n.board-left {\n  flex: 1 1;\n}\n\n.block-prediction {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: center;\n}\n\n.block-prediction span:first-child {\n  color: #fff;\n  font-size: 70px;\n  font-weight: 700;\n  height: 80px;\n  line-height: 80px;\n  text-align: center;\n}\n\n.block-prediction span:nth-child(2) {\n  color: #55657e;\n  font-size: 15px;\n  font-weight: 600;\n  margin-top: 5px;\n  text-align: center;\n  text-transform: uppercase;\n  width: 100%;\n}\n\n.board-center {\n  height: 100px;\n  overflow: hidden;\n  position: relative;\n  width: 100px;\n}\n\nelement.style {\n  animation-duration: 50s;\n}\n\n#dice {\n  animation: dice3D 50s linear infinite;\n  left: 50%;\n  margin-left: -30px;\n  margin-top: -30px;\n  top: 50%;\n  transform-style: preserve-3d;\n}\n\n@keyframes dice3D {\n  0% {\n    transform: translateZ(-100px) rotateX(0) rotateY(0) rotate(0);\n  }\n\n  16% {\n    transform: translateZ(-100px) rotateX(180deg) rotateY(180deg) rotate(0);\n  }\n\n  33% {\n    transform: translateZ(-100px) rotateX(1turn) rotateY(90deg) rotate(180deg);\n  }\n\n  50% {\n    transform: translateZ(-100px) rotateX(1turn) rotateY(1turn) rotate(1turn);\n  }\n\n  66% {\n    transform: translateZ(-100px) rotateX(180deg) rotateY(1turn) rotate(270deg);\n  }\n\n  83% {\n    transform: translateZ(-100px) rotateX(270deg) rotateY(180deg) rotate(180deg);\n  }\n\n  100% {\n    transform: translateZ(-100px) rotateX(1turn) rotateY(1turn) rotate(1turn);\n  }\n}\n\n.side {\n  background: #fff;\n  border-radius: 12px;\n  box-shadow: inset 0 0 12px #ccc;\n}\n\n#dice,\n.side {\n  height: 60px;\n  position: absolute;\n  width: 60px;\n}\n\n.dot.center {\n  margin: 23px 0 0 23px;\n}\n\n.dot {\n  background: #444;\n  border-radius: 7px;\n  box-shadow: inset 5px 0 10px #000;\n  height: 14px;\n  position: absolute;\n  width: 14px;\n}\n\n#dice .front.inner {\n  transform: translateZ(28px);\n}\n\n#dice .front {\n  transform: translateZ(30px);\n}\n\n.dot.dleft {\n  margin-left: 40px;\n}\n\n.dot.dtop {\n  margin-top: 6px;\n}\n\n.dot.dbottom {\n  margin-top: 40px;\n}\n\n.dot.dright {\n  margin-left: 6px;\n}\n\n#dice .top.inner {\n  transform: rotateX(90deg) translateZ(28px);\n}\n\n#dice .top {\n  transform: rotateX(90deg) translateZ(30px);\n}\n\n#dice .right {\n  transform: rotateY(90deg) translateZ(30px);\n}\n\n#dice .right.inner {\n  transform: rotateY(90deg) translateZ(28px);\n}\n\n#dice .left {\n  transform: rotateY(-90deg) translateZ(30px);\n}\n\n#dice .left.inner {\n  transform: rotateY(-90deg) translateZ(28px);\n}\n\n#dice .bottom {\n  transform: rotateX(-90deg) translateZ(30px);\n}\n\n#dice .bottom.inner {\n  transform: rotateX(-90deg) translateZ(28px);\n}\n\n#dice .bottom {\n  transform: rotateX(-90deg) translateZ(30px);\n}\n\n#dice .back {\n  transform: rotateX(-180deg) translateZ(30px);\n}\n\n#dice .back.inner {\n  transform: rotateX(-180deg) translateZ(28px);\n}\n\n#dice .cover.x {\n  transform: rotateY(90deg);\n}\n\n#dice .cover {\n  border-radius: 0;\n  transform: translateZ(0);\n}\n\n#dice .cover,\n#dice .inner {\n  background: #e0e0e0;\n  box-shadow: none;\n}\n\n#dice .cover.z {\n  transform: rotateX(90deg);\n}\n\n.board-right {\n  flex: 1 1;\n}\n\n.block-luckynumber {\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: center;\n}\n\n.block-luckynumber span:first-child {\n  color: #fff;\n  font-size: 70px;\n  font-weight: 700;\n  height: 80px;\n  line-height: 80px;\n}\n\n.block-luckynumber span {\n  white-space: nowrap;\n}\n\n.block-luckynumber span:nth-child(2) {\n  color: #fff;\n  font-size: 15px;\n  font-weight: 600;\n  margin-top: 5px;\n  text-align: center;\n  text-transform: uppercase;\n  width: 100%;\n}\n@media only screen and (max-width: 767px) {\n  .board {\n    height: 125px !important;\n    margin-bottom: 10px;\n  }\n  .board-center {\n    display: none;\n  }\n  .block-prediction span:first-child,\n  .block-luckynumber span:first-child {\n    font-size: 50px;\n  }\n  .block-prediction span:nth-child(2),\n  .block-luckynumber span:nth-child(2) {\n    font-size: 15px;\n    font-weight: 600;\n  }\n}\n</style>\n"]}]}