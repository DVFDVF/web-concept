,CAAC;IACZ,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;EACX;AACF","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/games/limbo/Target.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"auto-stop\">\n    <div class=\"auto-stop-row\">\n      <button\n        class=\"auto-stop-btn\"\n        type=\"button\"\n        @click=\"sub\"\n        :disabled=\"isStart\"\n      >\n        -\n      </button>\n      <a-popconfirm\n        :title=\"lang.v.limbo_greater\"\n        placement=\"bottom\"\n        :open=\"isPopconfirmVisible\"\n        @openChange=\"handleVisibleChange\"\n        style=\"pointer-events: none\"\n      >\n        <template #okButton> </template>\n        <template #cancelButton> </template>\n        <input\n          type=\"number\"\n          min=\"1.2\"\n          max=\"9999\"\n          step=\"any\"\n          class=\"auto-stop-input\"\n          v-model=\"inputText\"\n          :disabled=\"isStart\"\n          @input=\"handleInput()\"\n        />\n      </a-popconfirm>\n      <button\n        class=\"auto-stop-btn\"\n        type=\"button\"\n        @click=\"add\"\n        :disabled=\"isStart\"\n      >\n        +\n      </button>\n    </div>\n    <div class=\"auto-stop-row\">\n      <span class=\"auto-stop-label\">{{ lang.v.limbo_target }}</span>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { Popconfirm } from 'ant-design-vue';\nimport { defineComponent } from 'vue';\nexport default defineComponent({\n  components: {\n    APopconfirm: Popconfirm,\n  },\n  data() {\n    return {\n      lang,\n      inputText: '2',\n    };\n  },\n  watch: {\n    inputText: function (value) {\n      this.$emit('change', value);\n    },\n  },\n  methods: {\n    add() {\n      if (Number(this.inputText) >= 9999) {\n        this.inputText = '9999';\n        return;\n      }\n      if (this.inputText == '1.2') {\n        this.inputText = '2';\n      } else {\n        this.inputText = String(Number(this.inputText) + 1);\n      }\n    },\n    sub() {\n      if (Number(this.inputText) < 1.2) {\n        return;\n      }\n      if (this.inputText <= '2') {\n        this.inputText = '1.2';\n      } else {\n        this.inputText = String(Number(this.inputText) - 1);\n      }\n    },\n    handleInput() {\n      this.inputText = this.inputText.toString().replace(/[^\\d.]/g, '');\n      const dotCount = this.inputText.split('.').length - 1;\n      if (this.inputText.indexOf('.') === 0) {\n        this.inputText = this.inputText.slice(1);\n      }\n      if (dotCount > 1) {\n        this.inputText = this.inputText.slice(\n          0,\n          this.inputText.lastIndexOf('.'),\n        );\n      }\n    },\n    handleKeyDown(event: KeyboardEvent) {\n      if (dataCenter.gameConfiguration.HotkeyEnabled) {\n        const isQPressed = event.key === 'q';\n        const isWPressed = event.key === 'w';\n        // 处理热键触发后的逻辑\n        if (isQPressed) {\n          this.sub();\n        }\n        if (isWPressed) {\n          this.add();\n        }\n      }\n    },\n    //是否显示提示框\n    handleVisibleChange() {\n      this.$emit('closePromptBox');\n    },\n  },\n  mounted() {\n    window.addEventListener('keydown', this.handleKeyDown);\n  },\n  beforeUnmount() {\n    window.removeEventListener('keydown', this.handleKeyDown);\n  },\n  props: ['targetNumber', 'isStart', 'isPopconfirmVisible'],\n});\n</script>\n\n<style lang=\"less\" scoped>\n.auto-stop {\n  background: rgb(22, 31, 44);\n  border: 1px solid rgb(75, 91, 116);\n  border-radius: 10px;\n  padding: 10px;\n  position: relative;\n}\n\n.auto-stop-row {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.auto-stop-input {\n  background-color: transparent;\n  border: none;\n  color: rgb(255, 255, 255);\n  font-size: 20px;\n  font-weight: 700;\n  outline: none;\n  padding: 0px;\n  text-align: center;\n  width: 50px;\n}\n\n.auto-stop-input::-webkit-inner-spin-button,\n.auto-stop-input::-webkit-outer-spin-button {\n  appearance: none;\n  margin: 0px;\n}\n\n.auto-stop-btn {\n  background-color: rgb(40, 51, 67);\n  border: 1px solid transparent;\n  border-radius: 7px;\n  color: rgb(85, 101, 126);\n  font-size: 12px;\n  font-weight: 600;\n  height: 25px;\n  line-height: 23px;\n  margin-bottom: 5px;\n  padding: 0px;\n  transition: all 0.3s ease 0s;\n  user-select: none;\n  width: 25px;\n}\n\n.auto-stop-btn:hover {\n  background: rgb(18, 46, 79);\n  border: 1px solid rgb(41, 127, 229);\n  color: rgb(255, 255, 255);\n}\n\n.auto-stop-btn:active,\n.auto-stop-btn:focus {\n  background: rgb(41, 127, 229);\n  color: rgb(255, 255, 255);\n}\n\n.auto-stop-btn:disabled {\n  pointer-events: none;\n}\n\n.auto-stop-label {\n  color: rgb(85, 101, 126);\n  display: block;\n  font-size: 14px;\n  font-weight: 600;\n  text-align: center;\n  text-transform: uppercase;\n  width: 100%;\n}\n@media only screen and (max-width: 767px) {\n  .auto-stop {\n    display: flex;\n    flex-direction: row;\n    justify-content: space-between;\n  }\n  .auto-stop-row:nth-child(2) {\n    order: -1;\n  }\n  .auto-stop-input {\n    width: 80px;\n  }\n  .auto-stop-btn {\n    width: 25px;\n    height: 25px;\n    margin: 0;\n  }\n}\n</style>\n"]}]}