{"remainingRequest":"/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/babel-loader/lib/index.js!/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/ts-loader/index.js??ref--15-2!/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/cache-loader/dist/cjs.js??ref--1-0!/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/vue-loader-v16/dist/index.js??ref--1-1!/Users/nuomi/Desktop/项目/concept/fury-client/src/views/home/casino/LobbySections/card/CardContent.vue?vue&type=script&lang=ts","dependencies":[{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/src/views/home/casino/LobbySections/card/CardContent.vue","mtime":1718864777018},{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/babel.config.js","mtime":1716171552917},{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/cache-loader/dist/cjs.js","mtime":1716171608994},{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/babel-loader/lib/index.js","mtime":1716171609486},{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/ts-loader/index.js","mtime":1716171609531},{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/cache-loader/dist/cjs.js","mtime":1716171608994},{"path":"/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/vue-loader-v16/dist/index.js","mtime":1716171609468}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["dataCenter","enterGame","MIN_MOVE_DISTANCE","defineComponent","CardAll","CardWrapper","props","gameData","type","Object","search","default","headerData","clickDirection","data","isSmallScreen","isRoll","dragging","startX","lastX","transX","moveDistance","methods","startDrag","e","document","addEventListener","onDrag","stopDrag","clientX","container","$refs","emblaContent","container1","viewportContent","rect","getBoundingClientRect","rect1","length","percentage","Math","abs","width","removeEventListener","round","cardClick","index","layout","isMobile","gameItem","selectedGroups","$router","push","id","checkScreenSize","window","innerWidth","components","watch","newValue","_this$headerData","text"],"sources":["/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/cache-loader/dist/cjs.js??ref--1-0!/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/vue-loader-v16/dist/index.js??ref--1-1!/Users/nuomi/Desktop/项目/concept/fury-client/src/views/home/casino/LobbySections/card/CardContent.vue?vue&type=script&lang=ts"],"sourcesContent":["\nimport dataCenter from '@/dataCenter';\nimport { enterGame } from '@/manager/view';\nimport { MIN_MOVE_DISTANCE } from '@/utils/define';\nimport { getGameGroupResp, GetGamesResp } from '@/views/home/api/protocol';\nimport { defineComponent } from 'vue';\nimport CardAll from './CardAll.vue';\nimport CardWrapper from './CardWrapper.vue';\n\nexport default defineComponent({\n  props: {\n    gameData: {\n      type: Object as () => GetGamesResp,\n    },\n    search: {\n      default: false,\n    },\n    headerData: {\n      type: Object as () => getGameGroupResp,\n    },\n    clickDirection: {},\n  },\n  data() {\n    return {\n      isSmallScreen: false,\n      isRoll: false,\n      dragging: false,\n      startX: 0,\n      lastX: 0,\n      transX: -0,\n      moveDistance: 0, // 移动距离，如果超过MIN_MOVE_DISTANCE,则认为是移动\n    };\n  },\n  methods: {\n    startDrag(e: MouseEvent) {\n      document.addEventListener('mousemove', this.onDrag);\n      document.addEventListener('mouseup', this.stopDrag);\n      this.dragging = true;\n      this.moveDistance = 0;\n      this.startX = this.transX;\n      this.lastX = e.clientX;\n    },\n\n    onDrag(e: MouseEvent) {\n      if (this.dragging) {\n        this.isRoll = true;\n        const container = this.$refs.emblaContent as HTMLElement;\n        const container1 = this.$refs.viewportContent as HTMLElement;\n        if (!container || !container1 || !this.gameData) return;\n        const rect = container.getBoundingClientRect();\n        const rect1 = container1.getBoundingClientRect();\n        const length = this.gameData.data.length;\n        const percentage = e.clientX - this.lastX + this.startX;\n        // 累积移动距离，超过阈值就不触发点击事件\n        this.moveDistance += Math.abs(percentage);\n\n        if (percentage > rect.width * 0.4) {\n          this.transX = rect.width * 0.4;\n        } else if (\n          percentage <\n          rect.width * 0.6 - rect1.width * (length + 1) + 6 * (length - 1)\n        ) {\n          if (rect1.width * (length + 1) - 16 * (length - 1) < rect.width) {\n            this.transX = percentage;\n          } else {\n            this.transX =\n              rect.width * 0.6 - rect1.width * (length + 1) + 6 * (length - 1);\n          }\n        } else {\n          this.transX = percentage;\n        }\n      }\n    },\n\n    stopDrag(e: MouseEvent) {\n      document.removeEventListener('mousemove', this.onDrag);\n      document.removeEventListener('mouseup', this.stopDrag);\n      const container = this.$refs.emblaContent as HTMLElement;\n      const container1 = this.$refs.viewportContent as HTMLElement;\n      if (!container || !container1 || !this.gameData) return;\n      const rect = container.getBoundingClientRect();\n      const rect1 = container1.getBoundingClientRect();\n      const length = this.gameData.data.length;\n      const percentage = e.clientX - this.lastX + this.startX;\n\n      this.isRoll = false;\n      if (percentage > 0) {\n        this.transX = 0;\n      } else if (\n        percentage <\n        rect.width - rect1.width * (length + 1) - 16 * (length - 1)\n      ) {\n        if (rect1.width * (length + 1) - 16 * (length - 1) < rect.width) {\n          this.transX = 0;\n        } else {\n          this.transX =\n            rect.width - rect1.width * (length + 1) - 16 * (length - 1);\n        }\n      } else {\n        this.transX = Math.round(percentage / 184) * 184;\n      }\n      this.dragging = false;\n    },\n    cardClick(index: number) {\n      if (!dataCenter.layout.isMobile) {\n        if (this.moveDistance > MIN_MOVE_DISTANCE) {\n          // 说明是移动的\n          return;\n        }\n        if (!this.gameData) {\n          return;\n        }\n        const gameItem = this.gameData.data[index];\n        enterGame(gameItem);\n      }\n    },\n    selectedGroups() {\n      if (!this.dragging) {\n        if (this.headerData) {\n          // dataCenter.gameConfiguration.currentGameGroup = this.headerData;\n          this.$router.push(`/casino/originals?groupId=${this.headerData.id}`);\n        }\n      }\n    },\n    checkScreenSize() {\n      // 根据窗口宽度判断屏幕大小\n      this.isSmallScreen = window.innerWidth < 768; // 你可以根据需要调整阈值\n    },\n  },\n\n  components: { CardAll, CardWrapper },\n  watch: {\n    clickDirection(newValue) {\n      if (newValue.id == this.headerData?.id) {\n        const container = this.$refs.emblaContent as HTMLElement;\n        const container1 = this.$refs.viewportContent as HTMLElement;\n        if (!container || !container1 || !this.gameData) return;\n        const rect = container.getBoundingClientRect();\n        const rect1 = container1.getBoundingClientRect();\n        if (!rect && !rect1) {\n          return;\n        }\n        if (!this.gameData) return;\n        const length = this.gameData.data.length;\n        if (newValue.text == 'left') {\n          this.transX = 0;\n        } else {\n          if (rect1.width * (length + 1) - 16 * (length - 1) < rect.width) {\n            this.transX = 0;\n          } else {\n            this.transX =\n              rect.width - rect1.width * (length + 1) - 16 * (length - 1);\n          }\n        }\n      }\n    },\n  },\n});\n"],"mappings":";AACA,OAAOA,UAAU,MAAM,cAAc;AACrC,SAASC,SAAS,QAAQ,gBAAgB;AAC1C,SAASC,iBAAiB,QAAQ,gBAAgB;AAElD,SAASC,eAAe,QAAQ,KAAK;AACrC,OAAOC,OAAO,MAAM,eAAe;AACnC,OAAOC,WAAW,MAAM,mBAAmB;AAE3C,eAAeF,eAAe,CAAC;EAC7BG,KAAK,EAAE;IACLC,QAAQ,EAAE;MACRC,IAAI,EAAEC;KACP;IACDC,MAAM,EAAE;MACNC,OAAO,EAAE;KACV;IACDC,UAAU,EAAE;MACVJ,IAAI,EAAEC;KACP;IACDI,cAAc,EAAE;GACjB;E