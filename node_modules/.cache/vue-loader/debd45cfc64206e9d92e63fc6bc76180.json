mponent } from 'vue';\nimport { OtherLink } from './interface';\nexport default defineComponent({\n  name: 'other-link',\n  components: {},\n  data() {\n    return {\n      dataCenter,\n      toggleLeft: dataCenter.layout,\n      selected: '',\n      list: [\n        {\n          key: 'Fairness',\n          get name() {\n            return lang.v.sy_z_fairness;\n          },\n        },\n        {\n          key: 'Betfury team',\n          get name() {\n            return lang.v.sy_z_betfuryTeam;\n          },\n        },\n      ],\n    };\n  },\n  methods: {\n    onClick(item: OtherLink) {\n      this.selected = item.key;\n      if (item.key === 'Fairness') {\n        this.$router.push('/Fair');\n      } else if (item.key === 'Betfury team') {\n        this.$router.push('/TeamIntroduction');\n      }\n    },\n  },\n  watch: {\n    $route() {\n      if (this.$route.path == '/TeamIntroduction') {\n        this.selected = 'Betfury team';\n      }\n      if (this.$route.path == '/Fair') {\n        this.selected = 'Fairness';\n      }\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.other-link {\n  text-align: left;\n  padding: 0 9px;\n\n  .item {\n    font-feature-settings: 'tnum' on, 'lnum' on;\n    color: #55657e;\n    font-size: 11px;\n    font-weight: 700;\n    justify-content: space-between;\n    letter-spacing: 0.06em;\n    line-height: 150%;\n    padding: 5px 0;\n    text-decoration: none;\n    text-transform: uppercase;\n    cursor: pointer;\n\n    &:not(:first-child) {\n      margin-top: 5px;\n    }\n\n    &:hover {\n      color: #93acd3;\n    }\n\n    &.selected {\n      color: #fff;\n    }\n\n    > span {\n      flex: 1;\n    }\n\n    .link {\n      font-size: 13px;\n      width: 13px;\n      height: 13px;\n    }\n  }\n}\n</style>\n"]}]}