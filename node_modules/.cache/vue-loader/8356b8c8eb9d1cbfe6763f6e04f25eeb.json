@/manager/view';\nimport { lang } from '@/lang';\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      list: [\n        {\n          get title() {\n            return lang.v.rew_premio;\n          },\n          content: [\n            {\n              get text() {\n                return lang.v.rew_recc;\n              },\n            },\n            {\n              get text() {\n                return lang.v.rew_except;\n              },\n            },\n          ],\n        },\n        {\n          get title() {\n            return lang.v.rew_calcu;\n          },\n          content: [\n            {\n              get text() {\n                return lang.v.rew_user;\n              },\n            },\n          ],\n        },\n        {\n          get title() {\n            return lang.v.rew_duration;\n          },\n          content: [\n            {\n              get text() {\n                return lang.v.rew_hours;\n              },\n            },\n            {\n              get text() {\n                return lang.v.rew_excif;\n              },\n            },\n          ],\n        },\n        {\n          get title() {\n            return lang.v.rew_note;\n          },\n          content: [\n            {\n              get text() {\n                return lang.v.rew_event;\n              },\n            },\n          ],\n        },\n        {\n          get title() {\n            return lang.v.rew_rules;\n          },\n          content: [\n            {\n              get text() {\n                return lang.v.rew_wagered;\n              },\n            },\n          ],\n        },\n      ],\n      closeFunction: inject('close') as ModalCloseFun,\n    };\n  },\n  methods: {},\n  components: { PopUpNotification, BtnBlack },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.text--grey {\n  color: #a7b5ca;\n  font-weight: 500;\n  line-height: 150%;\n  font-size: 16px;\n}\n.lines {\n  background: linear-gradient(\n    90deg,\n    #ffffff0d -3%,\n    #ffffff26 48.66%,\n    #ffffff0d 103.12%\n  );\n  height: 1px;\n  margin: 16px 0;\n  width: 312px;\n}\n.text--regular {\n  color: #fff;\n  font-size: 13px;\n}\n.fechar {\n  margin-left: 10%;\n  width: 80%;\n  margin-bottom: 10px;\n}\n</style>\n"]}]}