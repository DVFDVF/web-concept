   :key=\"index\"\n        >\n          <span class=\"list-item-num\">{{ index }}</span>\n          <div class=\"list-item-user\">\n            <div class=\"item-user\">\n              <div class=\"item-user-userpick\">\n                <div class=\"user-avatar\" style=\"width: 26px; height: 26px\">\n                  <Avatar :size=\"24\" :user=\"item\" />\n                  <span class=\"user-level\">{{ item.level }}</span>\n                </div>\n                <div class=\"user-wrap\"></div>\n              </div>\n              <span class=\"item-user-name\">{{ item.name }}</span>\n              <!-- 暂不需要 -->\n              <!-- <span class=\"item-user-betsize\">110 BFG</span> -->\n            </div>\n          </div>\n          <div class=\"list-item-expected\">\n            <amount-prompt :amountValue=\"item.win\">\n              <div class=\"currency\">\n                <span>\n                  <span style=\"color: #fff\" class=\"font-digits\">\n                    <Money :value=\"item.win\"></Money>\n                  </span>\n                </span>\n                <Image class=\"currency-coin\" :src=\"usd\" loading=\"lazy\" />\n              </div>\n            </amount-prompt>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport Avatar from '@/components/Avatar.vue';\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport AmountPrompt from '@/views/chats/prompt/amountPrompt.vue';\nimport { gameLeaderboard } from '@/views/home/api';\nimport { GameLeaderboardResq } from '@/views/home/api/protocol';\nimport { defineComponent } from 'vue';\n\nexport default defineComponent({\n  components: { Avatar, AmountPrompt },\n  data() {\n    return {\n      timer: 0,\n      leaderboardData: [] as GameLeaderboardResq[],\n      icon: dataCenter.webConfig.currencyMap[0]?.icon,\n      usd: dataCenter.webConfig.currencyIcon,\n      lang,\n      gameId: this.$route.query.gameId as string,\n    };\n  },\n  methods: {\n    //排行榜\n    gameWinRank() {\n      gameLeaderboard(parseInt(this.gameId)).then((response) => {\n        if (response) {\n          this.leaderboardData = response;\n        }\n      });\n    },\n  },\n  mounted() {\n    this.gameWinRank();\n    // 在组件挂载后，启动定时器\n    this.timer = setInterval(this.gameWinRank, 20000); // 20秒\n  },\n  beforeUnmount() {\n    // 在组件销毁前清除定时器，以防止内存泄漏\n    clearInterval(this.timer);\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.auction-list {\n  overflow: auto;\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n}\n\n.list-inner {\n  scrollbar-width: none;\n}\n\n.list-inner::-webkit-scrollbar {\n  width: 0;\n}\n\n.list {\n  margin-top: 25px;\n  margin-left: 30px;\n}\n\n.list:after {\n  background: linear-gradient(0deg, #0d131c, rgba(21, 22, 31, 0));\n  content: '';\n  height: 150px;\n  pointer-events: none;\n}\n\n.list--is-panel:after {\n  content: none;\n}\n\n.list-head {\n  display: flex;\n  height: 20px;\n  justify-content: space-between;\n  padding: 0 20px 0 0px;\n}\n\n.list-head span {\n  color: #55657e;\n  font-size: 12px;\n  line-height: 11px;\n}\n\n.list-inner {\n  height: calc(100% - 20px);\n  overflow-x: hidden;\n  overflow-y: scroll;\n}\n\n.list-item {\n  align-items: center;\n  background: #1b2233;\n  border: 1px solid rgba(85, 101, 126, 0.28);\n  border-radius: 7px;\n  display: flex;\n  height: 53px;\n  margin-bottom: 10px;\n  padding: 5px 10px;\n  width: 350px;\n}\n\n.list-item-num {\n  color: #798dad;\n  font-size: 32px;\n  font-weight: 900;\n  margin-right: 8px;\n  min-width: 38px;\n  opacity: 0.24;\n  text-align: center;\n  white-space: nowrap;\n}\n\n.list-item-user {\n  flex: 1 1;\n}\n\n.list-item-expected {\n  color: #fff;\n  font-size: 13px;\n  font-weight: 600;\n}\n\n.list-item.sticky:last-child {\n  margin-bottom: 0;\n}\n\n.list-item.sticky {\n  background-color: #1d2d42;\n  border: 1px solid #297fe5;\n  bottom: 0;\n  position: sticky;\n  z-index: 1;\n}\n\n.list-item.sticky .list-item-num {\n  color: #297fe5;\n  opacity: 1;\n}\n\n.item-user {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  min-height: 30px;\n  padding-left: 35px;\n  position: relative;\n  text-align: left;\n}\n\n.item-user-name {\n  color: #fff;\n  font-size: 15px;\n  font-weight: 600;\n}\n\n.item-user-betsize {\n  color: #297fe5;\n  font-size: 12px;\n  font-weight: 500;\n}\n\n.item-user-userpick {\n  left: 0;\n  position: absolute !important;\n  top: 50%;\n  transform: translateY(-50%);\n}\n\n.currency {\n  align-items: center;\n  display: flex;\n}\n\n.currency-coin {\n  flex-shrink: 0;\n  height: 14px;\n  margin-left: 5px;\n  width: 14px;\n}\n\n.user-level {\n  background-color: #55657e;\n  border-radius: 50%;\n  bottom: -4px;\n  color: white;\n  font-size: 10px;\n  height: 15px;\n  line-height: 15px;\n  pointer-events: none;\n  position: absolute;\n  right: -4px;\n  text-align: center;\n  width: 15px;\n}\n\n.user-avatar {\n  flex-shrink: 0;\n  position: relative;\n}\n\n*,\n:after,\n:before {\n  background-repeat: no-repeat;\n  box-sizing: border-box;\n}\n</style>\n"]}]}