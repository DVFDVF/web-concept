ms","pixelType","get","pixelId","client","r","channelId","pixel","reportingEvents","ev","parts","reportRechargeEvents","_dataCenter$user$data","retrievedPixelId","user","isFirst","amount","roundRobin","addEventListener","visibilityState","queryRecharge","setInterval","pixelReport","pixelReportTime","Date","now","overQuery","then","respense","wallet","update","forEach","isFirstRecharge","find","_dataCenter$webConfig","currencyMap","currencyId","success","v","pix_sur","finally","phoneValid","phone","country","charAt","test","installApp","open","downloadLink","countDownTime","time","duration","diff","days","hours","padStart","minutes","seconds","shareUserId","isEncryption","_dataCenter$user$data2","_dataCenter$user$data3","showUserId","userId","btoa","toLocaleUpperCase","decryptionId","parseInt","atob","error","greenToBrazil","brazilTime","subtract","format"],"sources":["/Users/nuomi/Desktop/项目/concept/fury-client/node_modules/eslint-loader/index.js??ref--14-0!/Users/nuomi/Desktop/项目/concept/fury-client/src/utils/index.ts"],"sourcesContent":["import { Configs, Currency } from '@/api/protocol';\nimport dataCenter from '@/dataCenter';\nimport { Balance } from '@/dataCenter/wallet';\nimport { getEventReport, updateEventReport } from '@/views/home/api';\nimport { getEventReportResq } from '@/views/home/api/protocol';\nimport { formatFix9Money, toFloor } from './currency';\nimport dayjs from 'dayjs';\nimport utc from 'dayjs/plugin/utc';\nimport timezone from 'dayjs/plugin/timezone';\nimport { getUserBalance } from '@/views/home/api/user-api/headIndex';\nimport { tip } from '@/components/warn/warn';\nimport { lang } from '@/lang';\nimport { reentryReward } from '@/views/reentryReward';\ndayjs.extend(utc);\ndayjs.extend(timezone);\n\n/**\n * 格式化金额\n * @param currency 货币类型\n * @param balance 总的钱包数据\n * @param showUsdt 是否格式化为usdt\n * @returns\n */\nexport function displayBalance(\n  currency?: Currency,\n  balance?: Balance,\n  showUsdt = false,\n): string {\n  if (!currency || !balance) {\n    if (showUsdt) {\n      return '$0.00';\n    }\n    return formatFix9Money(\n      currency ? balance?.[currency.id].value : 0,\n      currency,\n    );\n  }\n  const value = balance[currency.id];\n  if (value) {\n    if (showUsdt) {\n      // 不应该四舍五入，所以用toFloor\n      return `$${toFloor(value.value * currency.usdtRate, 2)}`;\n    } else {\n      return formatFix9Money(value.value, currency);\n    }\n  }\n  if (showUsdt) {\n    return '$0.00';\n  }\n  return formatFix9Money(value, currency);\n}\n\n/**格式化数值 */\nexport function formatBalance(\n  currency: Currency,\n  value: number,\n  showUsdt = false,\n): string {\n  if (showUsdt) {\n    return `$${toFloor(value * currency.usdtRate, 2)}`;\n  } else {\n    return formatFix9Money(value, currency);\n  }\n}\n/**格式化美元 */\nexport function formatDollar(value: number, showUsdt = false): string {\n  if (showUsdt) {\n    return toFloor(value, 2);\n  } else {\n    return formatFix9Money(value);\n  }\n}\n\n/**localStorage添加 */\nexport function addArrayToLocal(name: string, value: number): void {\n  let data = localStorage.getItem(name);\n  if (data) {\n    // 将字符串分割成数组\n    const arrayOfValues = splitString(data);\n    // 检查数组中是否已经存在相同的值\n    if (!arrayOfValues.includes(value)) {\n      // 如果不存在，则添加新值\n      arrayOfValues.push(value);\n    }\n    // 将数组合并成字符串\n    data = arrayOfValues.join(',');\n  } else {\n    data = value.toString();\n  }\n\n  // 保存数据到本地存储\n  localStorage.setItem(name, data);\n}\n\n/** 移除localStorage数组部分数据*/\nexport function removeArrayToLocal(name: string, value: number): void {\n  let data = localStorage.getItem(name);\n  if (data) {\n    // 将字符串分割成数组\n    const arrayOfValues = splitString(data);\n    // 使用数组的过滤方法移除特定值\n    const filteredArray = arrayOfValues.filter(\n      (filterValue) => filterValue !== value,\n    );\n    // 将过滤后的数组合并成字符串\n    data = filteredArray.join(',');\n    // 保存数据到本地存储\n    localStorage.setItem(name, data);\n  }\n}\n/**获取数组结果 */\nexport function splitArrData(name: string): number[] {\n  // 使用split函数将字符串按逗号分隔\n  const data = localStorage.getItem(name);\n  if (data) {\n    return splitString(data);\n  } else {\n    return [];\n  }\n}\n\n/**将字符串生成数组 */\nexport function splitString(str: string): number[] {\n  // 使用split函数将字符串按逗号分隔\n  return str.split(',').map((item) => Number(item));\n}\n//获取等级\nexport function getLevel(level: number, exp: number): number {\n  const levelConfigs = dataCenter.webConfig.levelConfigs;\n  for (let index = 0; index < levelConfigs.length; index++) {\n    if (level == levelConfigs[levelConfigs.length - 1].level) {\n      return 1;\n    } else if (level == levelConfigs[index].level) {\n      return (\n        (exp - levelConfigs[index].chip) /\n        (levelConfigs[index + 1].chip - levelConfigs[index].chip)\n      );\n    }\n  }\n  return 0;\n}\n// 97.38032655581696 53.36966696635521\nexport function updateSVGPath(degrees: number): string {\n  // 计算新的路径坐标\n  const x = 50 + 47.5 * Math.cos((degrees - 90) * (Math.PI / 180));\n  const y = 50 + 47.5 * Math.sin((degrees - 90) * (Math.PI / 180));\n  const arcCommand = degrees >= 180 ? '0 1' : '0 0';\n  return `M ${x} ${y} A 47.5 47.5 ${arcCommand} 0 50 2.5`;\n}\n\nexport function isPhone(): boolean {\n  return window.screen.width <= 768;\n}\n\nexport function filteredLevelConfigs(): Configs[] {\n  // 使用 filter 方法进行过滤\n  return dataCenter.webConfig.levelConfigs.filter((config, index, array) => {\n    // 如果是数组的第一项，或者当前项的 chip 与前一项不相同，保留该项\n    return index === 0 || config.chip !== array[index - 1].chip;\n  });\n}\nexport function addPixelEvent(eventSrc: string): void {\n  // 创建一个新的img元素\n  const img = document.createElement('img');\n  // 设置img的src属性\n  img.src = eventSrc;\n  // 将img元素添加到容器中\n  document.querySelector('body')!.appendChild(img);\n  // 监听图片加载事件\n  img.onload = img.onerror = () => {\n    img.remove();\n  };\n}\n//初始化像素id\nexport function metaPixel(): void {\n  const hashParams = window.location.hash.split('?')[1];\n  const params = new URLSearchParams(hashParams);\n  // 获取特定参数的值\n  const pixelType = params.get('pixelType');\n  const pixelId = params.get('pixelId');\n  const client = params.get('client');\n  //推荐码\n  const r = params.get('r');\n\n  if (r) {\n    localStorage.setItem('ReferralCode', r as string);\n  }\n  //传递手机注册类型\n  if (client) {\n    localStorage.setItem('client', client);\n  }\n  //channelId\n  const channelId = params.get('channelId');\n  if (channelId) {\n    localStorage.setItem('channelId', channelId);\n  }\n  let pixel = '';\n  if (pixelId && pixelType) {\n    pixel = `${pixelType}=${pixelId}`;\n    // 将参数保存到本地存储\n    localStorage.setItem('pixel', pixel);\n  }\n\n  addPixelEvent(`https://www.facebook.com/tr?id=${pixelId}&ev=PageView`);\n}\n\n//上报事件\nexport function reportingEvents(ev: string, pixel?: string | null): void {\n  if (pixel) {\n    const parts: string[] = pixel.split('=');\n    if (parts[0] == 'fb') {\n      addPixelEvent(`https://www.facebook.com/tr?id=${parts[1]}&ev=${ev}`);\n    }\n  }\n}\n//上报充值事件\nexport function reportRechargeEvents(data: getEventReportResq): void {\n  const retrievedPixelId = dataCenter.user.data?.pixelId;\n  const currency = data.currency;\n  if (retrievedPixelId) {\n    const parts: string[] = retrievedPixelId.split('=');\n    if (parts[0] == 'fb') {\n      if (retrievedPixelId) {\n        if (data.isFirst == 1) {\n          addPixelEvent(\n            `https://www.facebook.com/tr?id=${parts[1]}&ev=FirstDeposit&cd[currency]=${currency}&cd[value]=${data.amount}`,\n          );\n          addPixelEvent(\n            `https://www.facebook.com/tr?id=${parts[1]}&ev=InitiateCheckout&cd[currency]=${currency}&cd[value]=${data.amount}`,\n          );\n        }\n        addPixelEvent(\n          `https://www.facebook.com/tr?id=${parts[1]}&ev=Purchase&cd[currency]=${currency}&cd[value]=${data.amount}`,\n        );\n      }\n    }\n  }\n}\n\n// 轮循上报\nexport function roundRobin(): number {\n  document.addEventListener('visibilitychange', function () {\n    if (document.visibilityState === 'hidden') {\n      // 页面不可见，可以推断手机进入了熄屏状态\n    } else {\n      queryRecharge();\n    }\n  });\n  return setInterval(pixelReport, 60000);\n}\n\nlet pixelReportTime = 0;\nfunction pixelReport() {\n  if (Date.now() - pixelReportTime > 2 * 60 * 1000) {\n    queryRecharge();\n  }\n}\nlet overQuery = false;\n//查询充值\nexport function queryRecharge(): void {\n  if (overQuery) return;\n  overQuery = true;\n  if (dataCenter.user.data) {\n    pixelReportTime = Date.now();\n    getEventReport(dataCenter.user.data.id.toString())\n      .then((respense) => {\n        if (respense) {\n          getUserBalance().then((wallet) => {\n            if (!wallet) return;\n            dataCenter.wallet.update(wallet);\n          });\n          respense.forEach((data) => {\n            updateEventReport(data.id.toString()).then((respense) => {\n              if (respense) {\n                if (data.isFirst == 1) {\n                  //如果第一次充值，在充值后改变状态\n                  dataCenter.user.data!.isFirstRecharge = 0;\n                }\n                reportRechargeEvents(data);\n                getUserBalance().then((wallet) => {\n                  if (!wallet) return;\n                  const currency = wallet.find(\n                    (item) =>\n                      dataCenter.webConfig.currencyMap[item.currencyId]\n                        ?.name === data.currency,\n                  );\n                  if (\n                    currency?.value ==\n                    data.amount +\n                      dataCenter.wallet.map[currency!.currencyId].value\n                  ) {\n                    dataCenter.wallet.update(wallet);\n                    tip.success(lang.v.pix_sur);\n                  }\n                });\n              }\n            });\n          });\n        }\n      })\n      .finally(() => {\n        overQuery = false;\n      });\n  }\n}\n\n//手机号验证\nexport function phoneValid(phone: string, country?: string) {\n  //默认巴西\n  return phone.length === 11 && phone.charAt(2) === '9' && /^\\d+$/.test(phone);\n}\n\n//下载app\nexport function installApp() {\n  window.open(dataCenter.webConfig.downloadLink);\n}\n\n//倒计时\nexport function countDownTime(time: string) {\n  const duration = dayjs.duration(dayjs(time).diff(dayjs()));\n\n  return `${duration.days()}d ${duration\n    .hours()\n    .toString()\n    .padStart(2, '0')}:${duration\n    .minutes()\n    .toString()\n    .padStart(2, '0')}:${duration.seconds().toString().padStart(2, '0')}`;\n}\n//用户id加密\nexport function shareUserId(isEncryption: boolean, id?: string) {\n  const showUserId = (window as any).showUserId;\n  const userId = id ? id : dataCenter.user.data?.id ?? 0;\n  if (isEncryption) {\n    return showUserId\n      ? userId.toString()\n      : btoa(userId.toString(16).toLocaleUpperCase());\n  } else {\n    if (showUserId) {\n      return id ?? '';\n    } else {\n      try {\n        const decryptionId = parseInt(atob(userId.toString()), 16);\n        return decryptionId ? decryptionId.toString() : '';\n      } catch (error) {\n        return '';\n      }\n    }\n  }\n}\n\n//格林时间转巴西时间\nexport function greenToBrazil(time: string) {\n  const brazilTime = dayjs(time).subtract(3, 'hour');\n  return brazilTime.format('YYYY-MM-DD HH:mm:ss');\n}\n"],"mappings":";;;;;AACA,OAAOA,UAAU,MAAM,cAAc;AAErC,SAASC,cAAc,EAAEC,iBAAiB,QAAQ,kBAAkB;AAEpE,SAASC,eAAe,EAAEC,OAAO,QAAQ,YAAY;AACrD,OAAOC,KAAK,MAAM,OAAO;AACzB,OAAOC,GAAG,MAAM,kBAAkB;AAClC,OAAOC,QAAQ,MAAM,uBAAuB;AAC5C,SAASC,cAAc,QAAQ,qCAAqC;AACpE,SAASC,GAAG,QAAQ,wBAAwB;AAC5C,SAASC,IAAI,QAAQ,QAAQ;AAE7BL,KAAK,CAACM,MAAM,CAACL,GAAG,CAAC;AACjBD,KAAK,CAACM,MAAM,CAACJ,QAAQ,CAAC;AAEtB;;;;;;;AAOA,OAAM,SAAUK,cAAcA,CAC5BC,QAAmB,EACnBC,OAAiB,EACjBC,QAAQ,GAAG,KAAK;EAEhB,IAAI,CAACF,QAAQ,IAAI,CAACC,OAAO,EAAE;IACzB,IAAIC,QAAQ,EAAE;MACZ,OAAO,OAAO;;IAEhB,OAAOZ,eAAe,CACpBU,QAAQ,GAAGC,OAAO,aAAPA,OAAO,uBAAPA,OAAO,CAAGD,QAAQ,CAACG,EAAE,CAAC,CAACC,KAAK,GAAG,CAAC,EAC3CJ,QAAQ,CACT;;EAEH,MAAMI,KAAK,GAAGH,OAAO,CAACD,QAAQ,CAACG,EAAE,CAA