                :login-text=\"lang.v.par_play\"\n                style=\"height: 44px\"\n                @click=\"sportIframe\"\n              />\n            </div>\n          </div>\n        </div>\n        <iframe\n          class=\"slot-frame\"\n          :src=\"iframeUrl\"\n          frameborder=\"0\"\n          allowfullscreen\n        ></iframe>\n        <PGQuit v-if=\"isPhone()\" />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { loginGame } from '@/games';\nimport { lang } from '@/lang';\nimport { isPhone } from '@/utils';\nimport { defineComponent, onMounted, ref, watch } from 'vue';\nimport { getGames } from '../api';\nimport ChatsLodding from '@/views/chats/ChatsLodding.vue';\nimport playButton from '@/views/home/head/popup/walletSix/headButtonPop.vue';\nimport dataCenter from '@/dataCenter';\nimport { openModal } from '@/manager/view';\nimport LoginWidget from '@/views/home/login/Login.vue';\nimport PGQuit from '@/views/home/other/widget/PGQuit.vue';\nimport { useRoute } from 'vue-router';\nexport default defineComponent({\n  components: {\n    ChatsLodding,\n    playButton,\n    PGQuit,\n  },\n  data() {\n    return {\n      lang,\n      user: dataCenter.user,\n    };\n  },\n  setup() {\n    const route = useRoute();\n    const iframeLoaded = ref(false);\n    const iframeUrl = ref('');\n    const lange = route.query.lange as string;\n    function getSport() {\n      getGames(0, 10, 1, 2).then((gameData) => {\n        if (!gameData) {\n          return;\n        }\n        loginGame(gameData.data[0].id.toString()).then((resp) => {\n          if (!resp) return;\n          const device = isPhone() ? 'm' : 'd';\n          if (resp.includes('?')) {\n            iframeUrl.value = `https:${resp}&lang=${lange}&device=${device}`;\n          } else {\n            iframeUrl.value = `https:${resp}?lang=${lange}&device=${device}`;\n          }\n        });\n      });\n      iframeLoaded.value = false;\n    }\n    function sportIframe() {\n      if (dataCenter.user.data) {\n        iframeLoaded.value = true;\n      } else {\n        openModal(LoginWidget);\n      }\n    }\n    watch(() => dataCenter.user.data, getSport);\n    onMounted(() => {\n      if (dataCenter.user.data) {\n        getSport();\n      } else {\n        openModal(LoginWidget);\n      }\n    });\n    return { iframeLoaded, iframeUrl, sportIframe, isPhone };\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.wrapper-inner {\n  padding: 10px 0;\n  flex: 1 1;\n  min-height: 400px;\n  display: flex;\n}\n.container {\n  position: relative;\n  width: 100%;\n}\n.slot-game {\n  align-items: center;\n  display: flex;\n  height: 100vh;\n  justify-content: center;\n  max-height: 800px;\n  position: relative;\n}\n.slot-currency-block {\n  align-items: center;\n  background-color: rgba(22, 31, 44, 0.9);\n  background-size: 100% 100%;\n  border-radius: 14px;\n  bottom: -1px;\n  display: flex;\n  justify-content: center;\n  left: -1px;\n  position: absolute;\n  right: -1px;\n  top: -1px;\n  z-index: 2;\n}\n.slot-choose-currency {\n  display: flex;\n  flex-direction: column;\n}\n.slot-currency-wrap {\n  align-items: center;\n  display: flex;\n  gap: 12px;\n  margin-bottom: 16px;\n}\n.slot-frame {\n  height: 100%;\n  left: 0;\n  position: absolute;\n  top: 0;\n  width: 100%;\n}\n@media only screen and (max-width: 768px) {\n  .wrapper-inner {\n    padding-bottom: 10px;\n    padding-top: 10px;\n  }\n  .container {\n    padding-left: 16px;\n    padding-right: 16px;\n  }\n  .slot-game {\n    background-color: #000;\n    bottom: 0;\n    height: 100vh;\n    left: 0;\n    margin-top: 0;\n    max-height: 100%;\n    position: fixed !important;\n    right: 0;\n    top: 0;\n    z-index: 225;\n  }\n  .slot-currency-block {\n    align-items: center;\n    background-color: rgba(22, 31, 44, 0.9);\n    background-size: 100% 100%;\n    border-radius: 0px;\n    bottom: -1px;\n    display: flex;\n    justify-content: center;\n    left: -1px;\n    position: absolute;\n    right: -1px;\n    top: -1px;\n    z-index: 2;\n  }\n  .slot-frame {\n    border-radius: 0px;\n    height: 100%;\n    left: 0;\n    position: absolute;\n    top: 0;\n    width: 100%;\n  }\n}\n</style>\n"]}]}