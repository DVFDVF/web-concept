  {{ lang.v.sg_ainda }}\n              {{ (100 - parseFloat(moneyValue)).toFixed(2) }}\n              {{ lang.v.sg_para }}\n            </a-timeline-item>\n\n            <a-timeline-item\n              :color=\"parseInt(moneyValue) >= 100 ? 'green' : 'gray'\"\n              :class=\"['timeline-title', { grey: parseInt(moneyValue) < 100 }]\"\n              >100 R$ {{ lang.v.sg_serao }}</a-timeline-item\n            >\n          </a-timeline>\n        </div>\n        <FeedbackButton\n          :login-text=\"\n            parseInt(moneyValue) < 100 ? lang.v.sg_convide : lang.v.sg_reward\n          \"\n          style=\"width: 100%; margin-top: 15px\"\n          @click=\"withdrawalClick\"\n        ></FeedbackButton>\n      </div>\n    </div>\n  </div>\n</template>\n<script setup lang=\"ts\">\nimport {\n  Timeline as ATimeline,\n  TimelineItem as ATimelineItem,\n} from 'ant-design-vue';\nimport FeedbackButton from '@/views/home/casino/about/FeedbackButton.vue';\nimport { ModalCloseFun, openModal } from '@/manager/view';\nimport sharingInvitation from '@/views/share/sharingInvitation.vue';\nimport { ref, defineProps, inject } from 'vue';\nimport { receiveReward } from '../api';\nimport { tip } from '@/components/warn/warn';\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { numberAddMotion } from '../jiugongge';\nconst closeFunction = inject('close') as ModalCloseFun;\nconst moneyValue = ref('0');\nconst isClick = ref(true);\nconst props = defineProps<{\n  update: () => void;\n}>();\nfunction withdrawalClick() {\n  if (isClick.value) {\n    isClick.value = false;\n    if (dataCenter.gridNine) {\n      if (100 <= dataCenter.gridNine.curtAmount) {\n        receiveReward()\n          .then((resq) => {\n            if (resq) {\n              closeFunction();\n              if (dataCenter.gridNine) {\n                dataCenter.gridNine.curtAmount = 0;\n              }\n              props.update();\n            }\n          })\n          .finally(() => {\n            isClick.value = true;\n          });\n      } else {\n        openModal(sharingInvitation);\n        isClick.value = true;\n      }\n    }\n  }\n}\nnumberAddMotion((result: string) => {\n  if (result) {\n    moneyValue.value = result;\n  }\n});\n</script>\n\n<style lang=\"less\" scoped>\n.withdraw-main {\n  max-width: 600px;\n  background: #111923;\n  padding: 10px;\n  border-radius: 20px;\n  user-select: none;\n}\n.withdraw-total {\n  padding: 10px;\n  border-radius: 20px;\n  background: #27292d;\n  align-items: center;\n  text-align: center;\n}\n.total-money {\n  font-size: 25px;\n  color: red;\n  font-family: Montserrat Variable, sans-serif;\n}\n.total-conta {\n  display: flex;\n  justify-content: space-between;\n}\n.progress {\n  background: #232327;\n  border-radius: 10px;\n  text-align: left;\n  padding: 10px;\n  margin-top: 10px;\n}\n.timeline {\n  position: relative;\n}\n.timeline-title {\n  color: #6ddf39;\n  font-size: 12px;\n  height: 30px !important;\n}\n.grey {\n  color: gray;\n}\n.timeline::before {\n  content: '';\n  position: absolute;\n  top: 0;\n  height: 60px;\n  left: 5px;\n  width: 2px;\n  background-color: #6ddf39;\n  margin-left: -1px;\n}\n@media only screen and (max-width: 767px) {\n  .withdraw-main {\n    width: calc(100vw - 20px) !important;\n  }\n}\n</style>\n"]}]}