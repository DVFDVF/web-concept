ner\"\n          :style=\"{\n            transform: `translateY(${translateY}%)`,\n            transition: `transform 0.5s ease 0s`,\n          }\"\n        >\n          <div class=\"time col-middle\">\n            {{\n              redEnvelopeRain.status.isStart\n                ? lang.v.hb_agarre\n                : `${lang.v.hb_comecas} ${redEnvelopeRain.status.time}`\n            }}\n          </div>\n          <div class=\"time col-middle\">{{ lang.v.hb_chuva }}</div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { openModal } from '@/manager/view';\nimport { defineComponent } from 'vue';\nimport index from './index.vue';\nimport receive from './receive.vue';\nimport { redEnvelopeRain } from './game';\nimport { lang } from '@/lang';\nimport dataCenter from '@/dataCenter';\n\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      translateY: 0,\n      timer: 0,\n      redEnvelopeRain,\n    };\n  },\n  computed: {\n    redRainIsShow() {\n      return (\n        redEnvelopeRain.status.time &&\n        dataCenter.user.data &&\n        (this.$route.path == '/' || this.$route.path == '/casino')\n      );\n    },\n  },\n  methods: {\n    styleSwitching() {\n      this.translateY = this.translateY == 0 ? -100 : 0;\n    },\n    open() {\n      redEnvelopeRain.open();\n    },\n  },\n  mounted() {\n    this.timer = setInterval(this.styleSwitching, 2000);\n  },\n  beforeUnmount() {\n    clearInterval(this.timer);\n  },\n  props: ['isScroll'],\n});\n</script>\n\n<style lang=\"less\" scoped>\n.icons-container {\n  position: fixed;\n  right: 20px;\n  bottom: 260px;\n  z-index: 10;\n  transition: right 0.3s;\n  align-items: flex-end;\n  cursor: pointer;\n}\n.scroll {\n  right: -32px;\n}\n.redicon {\n  width: 64px;\n  height: 69px;\n  background: url(~@/assets/redEnvelope/redicon.png) no-repeat;\n  background-size: 64px 69px;\n  animation: scaleAnimation 2s ease forwards;\n}\n@keyframes scaleAnimation {\n  