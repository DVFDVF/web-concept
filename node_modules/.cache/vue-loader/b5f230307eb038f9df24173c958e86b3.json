    href=\"https://fonts.googleapis.com/css?family=Montserrat+Alternates:100,100italic,200,200italic,300,300italic,regular,italic,500,500italic,600,600italic,700,700italic,800,800italic,900,900italic\"\n      rel=\"stylesheet\"\n    />\n    <div class=\"close\" @click=\"onClose()\">\n      <img src=\"@/assets/install/close.jpg\" />\n    </div>\n    <div class=\"left\">\n      {{ lang.v.tb_xiazai }}\n    </div>\n    <div class=\"install center\" @click=\"onSubmit()\">\n      <img src=\"@/assets/install/phone.jpg\" />\n      {{ lang.v.tb_xia_down }}\n    </div>\n  </div>\n</template>\n<script lang=\"ts\" setup>\nimport { ref } from 'vue';\nimport { installApp } from '@/utils';\nimport { lang } from '@/lang';\nconst open = ref<boolean>(true);\nconst onSubmit = () => {\n  open.value = false;\n  installApp();\n};\n\nconst onClose = () => {\n  const installTipsNumber = localStorage.getItem('installTipsTopNumber');\n  if (installTipsNumber) {\n    const number = parseInt(installTipsNumber) + 1;\n    localStorage.setItem('installTipsTopNumber', `${number}`);\n  } else {\n    localStorage.setItem('installTipsTopNumber', '1');\n  }\n  open.value = false;\n};\nconst isShow = () => {\n  const client = localStorage.getItem('client');\n  if (client != 'H5') {\n    open.value = false;\n    return;\n  }\n  const installTipsNumber = localStorage.getItem('installTipsTopNumber');\n  if (installTipsNumber) {\n    const number = parseInt(installTipsNumber);\n    if (number > 3) {\n      open.value = false;\n    }\n  }\n};\nisShow();\n</script>\n<style lang=\"less\" scoped>\n.top-pop-up {\n  font-family: Montserrat, Montserrat;\n  justify-content: space-between;\n  background: linear-gradient(234deg, #7f38f0 0%, #2f76d8 100%);\n  position: relative;\n  height: 60px;\n  padding: 0 20px;\n  .close {\n    position: absolute;\n    top: 0;\n    left: 0;\n    background: #1e202634;\n    border-radius: 0 0 44px 0;\n    height: 25px;\n    width: 30px;\n    img {\n      width: 10px;\n      height: 10px;\n      margin-right: 4px;\n    }\n  }\n  .left {\n    font-weight: 900;\n    font-size: 13px;\n    color: #48bcef;\n    text-align: left;\n    font-style: normal;\n    text-transform: none;\n    margin-left: 10px;\n    span {\n      color: white;\n    }\n  }\n  .install {\n    background: white;\n    font-family: Montserrat, Montserrat;\n    font-weight: 900;\n    font-size: 12px;\n    color: #7f38f0;\n    text-align: left;\n    font-style: normal;\n    text-transform: none;\n    padding: 5px 15px;\n    border-radius: 6px;\n    gap: 6px;\n    img {\n      height: 15px;\n    }\n  }\n}\n</style>\n"]}]}