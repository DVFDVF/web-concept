raw'\"\n            @change=\"onCurrencyChange\"\n          />\n        </div>\n      </div>\n      <MyCurrencyPop\n        :inputHidden=\"false\"\n        class=\"depcy\"\n        :toggleButtons=\"toggleButts\"\n        :dynamicTexts=\"dynamTexts\"\n        v-show=\"isHidMyCurrency\"\n      />\n      <PublicInput\n        style=\"width: 100%; height: 58px\"\n        :showImage=\"currency?.icon\"\n        ref=\"amount\"\n        :enter-text=\"'Amount'\"\n        :showButtonFlatStr=\"'Max'\"\n        :inputMoney=\"allMoneyStr\"\n        :show-button-flat=\"true\"\n        :show-input-icon=\"true\"\n        @button-flat-click=\"maxAllClick\"\n      />\n      <HeadButtonPop\n        class=\"withdaw\"\n        :login-text=\"loginText()\"\n        style=\"margin-bottom: 20px; height: 44px\"\n      />\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent, ref, reactive, inject } from 'vue';\nimport VaultDeposit from '@/views/home/head/popup/walletSix/widget/VaultSelector/VaultDeposit.vue';\nimport MyCurrencyPop from './MyCurrencyPop.vue';\nimport HeadButtonPop from './walletSix/headButtonPop.vue';\nimport PublicInput from '@/views/home/head/popup/walletSix/widget/RetiradaSelector/RetiradaInput.vue';\nimport { Currency } from '@/api/protocol';\nimport { displayBalance } from '@/utils';\nimport dataCenter from '@/dataCenter';\nimport { ModalCloseFun } from '@/manager/view';\n\nexport default defineComponent({\n  name: 'DepositoPop',\n  components: {\n    VaultDeposit,\n    MyCurrencyPop,\n    HeadButtonPop,\n    PublicInput,\n  },\n  data() {\n    return {\n      closeFunction: inject('close') as ModalCloseFun,\n      toggleButts: [false],\n      dynamTexts: ['hb_hideBalance'],\n    };\n  },\n  setup() {\n    const isHidMyCurrency = ref(false);\n    const currency = ref<Currency>();\n    const allMoneyStr = ref<string>();\n    const activeTab = ref('Deposit');\n    const balance = reactive(dataCenter.wallet.map);\n    return { isHidMyCurrency, currency, allMoneyStr, balance, activeTab };\n  },\n  methods: {\n    onCurrencyChange(item: Currency) {\n      this.currency = item;\n    },\n    maxAllClick() {\n      this.allMoneyStr = displayBalance(this.currency, this.balance);\n    },\n    loginText() {\n      return this.activeTab === 'Withdraw'\n        ? 'Withdraw from Vault'\n        : 'Deposit to Vault';\n    },\n    closePopUp() {\n      this.closeFunction();\n    },\n  },\n});\n</script>\n<style lang=\"less\" scoped>\n.vaultAll {\n  max-width: 530px;\n  width: 530px;\n  background-color: #111923;\n  border-radius: 14px;\n\n  &-psi {\n    background: linear-gradient(180deg, #003370, #08264a 40%, #111923);\n    border-radius: 14px;\n    padding: 20px 24px;\n    position: relative;\n  }\n\n  &-dsn {\n    font-weight: 700;\n    margin-top: 0;\n    font-size: 32px;\n    color: #fff;\n    font-family: Montserrat Variable, sans-serif;\n  }\n\n  .vaultAll-spron {\n    position: relative;\n    padding: 0 24px;\n    display: grid;\n    gap: 26px;\n  }\n\n  .tabs {\n    background: rgba(55, 70, 92, 0.3);\n    border-radius: 10px;\n    display: inline-flex;\n    padding: 6px;\n    width: 100%;\n    gap: 8px;\n  }\n\n  .tab-active {\n    background-color: #2283f6;\n    color: #fff;\n  }\n  .tab-active span {\n    color: white;\n  }\n\n  .tab {\n    border-radius: 8px;\n    display: flex;\n    flex: 1 1;\n    justify-content: center;\n    height: 40px;\n    border: none;\n    cursor: pointer;\n    color: #55657e;\n  }\n\n  .tab-label {\n    font-size: 14px;\n    line-height: 18px;\n    font-weight: 600;\n    text-align: center;\n    white-space: nowrap;\n  }\n\n  .vaultAll-buntsy {\n    display: grid;\n    gap: 16px;\n    grid-template-columns: 1fr 1fr;\n    margin-bottom: 24px;\n  }\n\n  .vaultAll-uensty {\n    color: #55657e;\n    display: inline-block;\n    font-weight: 600;\n    font-size: 12px;\n    font-family: Montserrat Variable, sans-serif;\n    display: grid;\n    gap: 6px;\n  }\n\n  .depcy {\n    position: absolute;\n    margin-top: 88px;\n    left: 20px;\n    width: 233px;\n    z-index: 11;\n  }\n}\n@media only screen and (max-width: 767px) {\n  .vaultAll {\n    position: fixed;\n    top: 50%;\n    transform: translate(0, -50%);\n    left: 0;\n    z-index: 10;\n    margin: 0 10px;\n    max-height: 100% !important;\n    width: calc(100% - 20px) !important;\n  }\n  .vaultAll-dsn {\n    font-size: 32px;\n  }\n  .vaultAll-buntsy {\n    display: flex !important;\n    flex-direction: column;\n  }\n  .vaultAll-uensty:nth-child(1) {\n    z-index: 2;\n  }\n  .vaultAll-uensty:nth-child(2) {\n    z-index: 1;\n  }\n  .vaultAll-spron {\n    gap: 10px !important;\n  }\n  .withdaw {\n    margin-top: 10px;\n  }\n}\n</style>\n"]}]}