AC;QAC7C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACV,CAAC,CAAC,CAAC,CAAC,CAAC;MACL,CAAC,CAAC,CAAC;QACD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACb,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MAC1D,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACR,CAAC,CAAC,CAAC,CAAC,CAAC;IACL,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MAClB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACP,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACpC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACtB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACjB,CAAC;IACH,CAAC,CAAC,CAAC,CAAC,CAAC;EACP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/head/popup/WalletButtonPop.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"walpop\">\n    <button title=\"Close modal\" class=\"modal-btn-close\" @click=\"closePopUp\">\n      <svg\n        class=\"svg-use\"\n        width=\"24\"\n        height=\"24\"\n        focusable=\"false\"\n        aria-hidden=\"true\"\n      >\n        <use xlink:href=\"/svg/sprite.svg#icon-close\"></use>\n      </svg>\n    </button>\n    <div class=\"leftve\">\n      <div class=\"topve\">\n        <button\n          v-for=\"(item, index) in list\"\n          :key=\"index\"\n          :class=\"['topbtn', { active: index === selectedIndex }]\"\n          type=\"button\"\n          @click=\"selectButton(index)\"\n        >\n          <span class=\"toplit center\">\n            <img :src=\"item.icon\" style=\"width: 14px; height: 14px\" />\n          </span>\n          <span class=\"topspan\">{{ item.name }}</span>\n        </button>\n      </div>\n      <div\n        class=\"shadow\"\n        v-if=\"dataCenter.wallet.showCallbackTip && !iframeLoaded\"\n      ></div>\n    </div>\n    <div class=\"rightve\">\n      <component\n        :is=\"componentMapping[selectedIndex]\"\n        ref=\"childComponentRef\"\n        @change=\"change\"\n      />\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { defineComponent, inject } from 'vue';\nimport DepositoPop from '@/views/home/head/popup/walletSix/DepositoPop.vue';\nimport Retirada from '@/views/home/head/popup/walletSix/Retirada.vue';\nimport PixDeposito from '@/views/home/head/wallet/PixDeposito.vue';\nimport PixRetirada from '@/views/home/head/wallet/PixRetirada.vue';\nimport { lang } from '@/lang';\nimport { ModalCloseFun, openModal } from '@/manager/view';\nimport dataCenter from '@/dataCenter';\nimport wheelPop from '@/games/wheel/wheelPop.vue';\nimport Receba from '../../other/Receba.vue';\ninterface childComponent {\n  iframeLoaded: boolean;\n}\nexport default defineComponent({\n  name: 'WalletButtonPop',\n  components: {\n    DepositoPop,\n    Retirada,\n    PixDeposito,\n    PixRetirada,\n  },\n  data() {\n    return {\n      closeFunction: inject('close') as ModalCloseFun,\n      selectedIndex: 0,\n      iframeLoaded: true,\n      list: [\n        {\n          icon: require('@/assets/configuration/wallet/rs-deposit.png'),\n          key: 'PixDeposito',\n          get name() {\n            return lang.v.pix_deposito;\n          },\n        },\n        {\n          icon: require('@/assets/configuration/wallet/rs-retirada.png'),\n          key: 'Retirada',\n          get name() {\n            return lang.v.pix_cry;\n          },\n        },\n        // {\n        //   icon: require('@/assets/configuration/wallet/usdtDeposit.png'),\n        //   key: 'Deposito',\n        //   get name() {\n        //     return lang.v.qb_deposito;\n        //   },\n        // },\n        // {\n        //   icon: require('@/assets/configuration/wallet/cryptotrading.png'),\n        //   key: 'Retirada',\n        //   get name() {\n        //     return lang.v.qb_retirada;\n        //   },\n        // },\n      ],\n      dataCenter,\n    };\n  },\n  methods: {\n    selectButton(index: number) {\n      if (index == this.selectedIndex) return;\n      this.selectedIndex = index;\n      dataCenter.wallet.showCallbackTip = false;\n    },\n    closePopUp() {\n      dataCenter.layout.closeAll();\n      this.closeFunction();\n      if (this.type == 'preferential') {\n        openModal(Receba);\n      }\n    },\n    change(value: boolean) {\n      this.iframeLoaded = value;\n    },\n  },\n  computed: {\n    componentMapping(): { [key: number]: any } {\n      return {\n        0: PixDeposito,\n        1: PixRetirada,\n        // 2: DepositoPop,\n        // 3: Retirada,\n      };\n    },\n  },\n  mounted() {\n    dataCenter.wallet.showCallbackTip = !this.iframeLoaded;\n  },\n\n  watch: {\n    'dataCenter.wallet.showCallbackTip'(newVal) {\n      if (this.selectedIndex == 1) {\n        dataCenter.wallet.showCallbackTip = false;\n        return;\n      }\n    },\n  },\n  props: ['type'],\n});\n</script>\n\n<style lang=\"less\" scoped>\n.walpop {\n  display: flex;\n  max-width: 680px;\n  border-radius: 12px;\n  background: #111923;\n  overflow: hidden;\n  width: 680px;\n}\n\n.leftve {\n  background: linear-gradient(180deg, #01336f, #131c27 79.41%);\n  padding: 24px 16px;\n  width: 225px;\n  position: relative;\n}\n\n.rightve {\n  background-color: #111923;\n  padding: 24px;\n  flex: 1;\n  position: relative;\n}\n\n.topve {\n  display: flex;\n  flex-direction: column;\n\n  .topbtn {\n    background-color: transparent;\n    border: 2px solid transparent;\n    color: #93acd3;\n    cursor: pointer;\n    opacity: 1;\n    margin-bottom: 4px;\n    padding: 8px 12px;\n    border-radius: 3rem;\n    display: flex;\n    flex-direction: row;\n  }\n\n  .active {\n    background-color: #2283f6;\n    color: #fff;\n  }\n\n  .toplit {\n    margin-left: -4px;\n    width: 21px;\n    height: 21px;\n  }\n\n  .topspan {\n    margin-left: 8px;\n    font-size: 14px;\n    font-family: Montserrat Variable, sans-serif;\n    letter-spacing: -0.24px;\n    line-height: 20px;\n    font-weight: 600;\n    white-space: nowrap;\n  }\n}\n\n@media only screen and (max-width: 767px) {\n  .walpop {\n    display: block;\n    margin: 10px 10px 50px 10px;\n    min-height: 62px;\n    height: calc(100vh - 60px);\n    width: calc(100vw - 20px);\n  }\n  .leftve {\n    padding: 32px 24px 0;\n    width: auto;\n  }\n  .rightve {\n    max-height: calc(100vh - 120px);\n    overflow-y: auto;\n  }\n  .topve {\n    width: auto;\n    flex-direction: row;\n    overflow: auto;\n    scrollbar-color: #2a3546 rgba(13, 19, 28, 0.8);\n    scrollbar-width: thin;\n  }\n}\n.shadow {\n  position: absolute;\n  top: 0;\n  left: 0;\n  right: 0;\n  bottom: 0;\n  background-color: rgba(0, 0, 0, 0.4);\n  // background: black;\n  z-index: 30;\n}\n</style>\n"]}]}