n>\n      </div>\n    </div>\n    <div class=\"pople\" v-if=\"isPopoverVisible\">\n      <HeadMenuList :list=\"LanguageList\" @selected=\"onClickMenu\" />\n    </div>\n    <Tanchuang />\n  </div>\n</template>\n<!-- 语言切换 和 全局声音-->\n<script lang=\"ts\">\nimport { defineComponent } from 'vue';\n\nimport HeadMenuList from '../menu/HeadMenuList.vue';\nimport Tanchuang from './Tanchuang.vue';\nimport icoBottom from '@/assets/icon/head/ico-bottom.vue';\nimport { lang } from '@/lang';\nimport {\n  IconMapping,\n  LanguageList,\n  NameMapping,\n  getLangeIcon,\n} from '@/lang/localizationLang';\nexport default defineComponent({\n  name: 'home-common-menus',\n  components: {\n    HeadMenuList,\n    Tanchuang,\n    icoBottom,\n  },\n  data() {\n    return {\n      lang,\n      LanguageList,\n      isPopoverVisible: false,\n      isIconRotated: false, // 控制图标旋转\n      isPlaceholderBlue: false, // 初始状态下应为 false\n      selectedLanguage: {\n        iconComponent: IconMapping[lang.m],\n        text: NameMapping[lang.m],\n      },\n    };\n  },\n  methods: {\n    onClickMenu(item: any) {\n      lang.change(item.key);\n      this.selectedLanguage.iconComponent = item.icon;\n      this.selectedLanguage.text = item.name;\n      this.isPopoverVisible = false;\n      this.$emit('language-selected', this.selectedLanguage.iconComponent);\n    },\n    togglePopover() {\n      this.isPopoverVisible = !this.isPopoverVisible;\n      this.isIconRotated = !this.isIconRotated; // 切换图标旋转状态\n      this.isPlaceholderBlue = !this.isPlaceholderBlue; // 切换占位符字体颜色状态\n    },\n    updateSelectedLanguageComponent(languageKey: string) {\n      this.selectedLanguage.iconComponent = IconMapping[languageKey];\n    },\n  },\n  mounted() {\n    getLangeIcon();\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n@icon-color: #93acd3;\n\n.swige {\n  background: #242f3f;\n  border-radius: 10px;\n  padding: 16px;\n  position: absolute;\n  right: 40px;\n  top: 64px;\n\n  .langedvs {\n    border-radius: 10px;\n    height: 58px;\n    padding: 10px 17px;\n    background: rgba(68, 88, 116, 0.24);\n    border: 1px solid transparent;\n    box-shadow: 0 2px 4px #0000002e;\n    cursor: pointer;\n    width: 224px;\n\n    .langedv {\n      flex: 1 1;\n\n      .triggerue {\n        display: flex;\n        flex-direction: column;\n        align-items: flex-start;\n        margin-left: 10px;\n        width: 100%;\n\n        &_placeholder {\n          display: block;\n          font-size: 14px;\n          transform-origin: left top;\n          font-family: Montserrat Variable, sans-serif;\n          font-weight: 600;\n          line-height: 18px;\n        }\n\n        &_lantext {\n          color: #fff;\n          display: block;\n          font-size: 16px;\n          transform-origin: left top;\n          font-family: Montserrat Variable, sans-serif;\n          font-weight: 600;\n          line-height: 18px;\n        }\n      }\n    }\n\n    .curklt {\n      margin: 0 6px;\n      width: 24px;\n      height: 24px;\n      display: flex;\n      justify-content: flex-end;\n      align-items: center;\n\n      .curren-ialtm {\n        transition: transform 0.3s ease, filter 0.3s ease;\n        width: 10px;\n        height: 10px;\n        fill: @icon-color;\n      }\n    }\n  }\n\n  .pople {\n    align-items: center;\n    background: #1b2636;\n    box-shadow: 0 4px 26px #0d131c78;\n    min-width: 204px;\n    margin-top: 8px;\n    border: 2px;\n    border-radius: 10px;\n    padding: 12px;\n    background: #1b2636;\n    position: absolute;\n    z-index: 12;\n  }\n\n  .blue-text {\n    color: #2283f6;\n  }\n\n  .rwt-text {\n    color: #55657e;\n  }\n}\n@media only screen and (max-width: 767px) {\n  .swige {\n    top: 60px;\n    left: 10px;\n    z-index: 20;\n    width: calc(65vw - 20px);\n    .langedvs {\n      width: auto;\n      height: auto;\n      padding: 5px 10px;\n      .langedv {\n        .triggerue_placeholder {\n          font-size: 12px;\n        }\n        .triggerue_lantext {\n          font-size: 12px;\n        }\n      }\n    }\n  }\n}\n</style>\n"]}]}