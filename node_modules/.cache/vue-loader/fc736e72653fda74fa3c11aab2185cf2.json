concept/web3-concept/src/views/game/dice/left/RollControls.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"controls\">\n    <button\n      :class=\"[\n        'controls-left btn btn-large',\n        { 'btn-blue': isRollUnder, 'btn-red ': !isRollUnder },\n      ]\"\n      :disabled=\"isStart\"\n      @click=\"RollUnder\"\n    >\n      <span>{{ lang.v.dice_rollUnder }}</span>\n    </button>\n    <div class=\"controls-prediction\">\n      <span>{{ lang.v.dice_multiplier }}</span>\n      <span>{{ returnRate }}</span>\n    </div>\n    <div class=\"controls-chance\">\n      <span>{{ lang.v.dice_chancetoWin }}</span>\n      <span>{{ number }}%</span>\n    </div>\n    <button\n      :class=\"[\n        'controls-right btn btn-large',\n        { 'btn-blue': !isRollUnder, 'btn-red ': isRollUnder },\n      ]\"\n      @click=\"RollOver\"\n      :disabled=\"isStart\"\n    >\n      <span>{{ lang.v.dice_rollCoer }}</span>\n    </button>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { defineComponent } from 'vue';\n\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      minNumber: 1,\n      maxNumber: 95,\n      isRollUnder: true,\n      multiplier: dataCenter.webConfig.profitRate * 100 ?? 99.07,\n    };\n  },\n  methods: {\n    RollUnder() {\n      this.minNumber = 1;\n      this.maxNumber = 95;\n      this.isRollUnder = true;\n      this.$emit(\n        'RollControlsData',\n        this.minNumber,\n        this.maxNumber,\n        this.isRollUnder,\n      );\n    },\n    RollOver() {\n      this.minNumber = 4;\n      this.maxNumber = 98;\n      this.isRollUnder = false;\n      this.$emit(\n        'RollControlsData',\n        this.minNumber,\n        this.maxNumber,\n        this.isRollUnder,\n      );\n    },\n    handleKeyDown(event: KeyboardEvent) {\n      if (dataCenter.gameConfiguration.HotkeyEnabled) {\n        const isQPressed = event.key === 'q';\n        // 处理热键触发后的逻辑\n        if (isQPressed) {\n          if (this.isRollUnder) {\n            this.RollOver();\n          } else {\n            this.RollUnder();\n          }\n        }\n      }\n    },\n  },\n\n  mounted() {\n    window.addEventListener('keydown', this.handleKeyDown);\n  },\n  beforeUnmount() {\n    window.removeEventListener('keydown', this.handleKeyDown);\n  },\n  components: {},\n  props: ['number', 'isStart', 'returnRate'],\n});\n</script>\n\n<style lang=\"less\" scoped>\n.controls {\n  align-items: center;\n  background: #161f2c;\n  border-radius: 8px;\n  display: flex;\n  justify-content: space-between;\n  margin: 0 20px 45px;\n  padding: 3px;\n  position: relative;\n}\n\n.controls:after {\n  background-color: #54658040;\n  content: '';\n  display: block;\n  height: 34px;\n  left: 50%;\n  margin-top: -17px;\n  position: absolute;\n  top: 50%;\n  width: 1px;\n}\n\n.controls .btn {\n  margin-bottom: -2px;\n  margin-top: -2px;\n  max-width: calc(50% - 5px);\n  position: relative;\n  width: 170px;\n}\n\n.controls-left {\n  margin-left: -20px;\n  transform: skew(-11deg);\n  white-space: pre-wrap;\n}\n\n.btn-large {\n  font-size: 18px;\n  padding: 15px 30px;\n}\n\n.btn-blue {\n  background-color: #2283f6;\n}\n\n.btn-blue:hover {\n  background-color: #0a75f5;\n}\n\n.btn-blue:hover,\n.btn-blue:hover.btn-shadow {\n  box-shadow: 0 14px 20px #2283f64d;\n}\n\n.controls-left span {\n  display: block;\n  font-size: 15px;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  text-transform: uppercase;\n  transform: skew(11deg);\n}\n\n.controls-left {\n  margin-left: -20px;\n  transform: skew(-11deg);\n  white-space: pre-wrap;\n}\n\n.btn-large {\n  font-size: 18px;\n  padding: 15px 30px;\n}\n\n.controls div {\n  display: flex;\n  flex-direction: column;\n}\n\n.controls-prediction {\n  min-width: 90px;\n  position: relative;\n}\n\n*,\n:after,\n:before {\n  background-repeat: no-repeat;\n  box-sizing: border-box;\n}\n\n.controls div span:first-child {\n  color: #55657e;\n  font-size: 12px;\n  font-weight: 500;\n  text-align: center;\n}\n\n.controls div span:nth-child(2) {\n  color: #fff;\n  font-size: 20px;\n  font-weight: 700;\n  text-align: center;\n}\n\n.controls-right {\n  margin-right: -20px;\n  transform: skew(11deg);\n  white-space: pre-wrap;\n}\n\n.btn-large {\n  font-size: 18px;\n  padding: 15px 30px;\n}\n\n.btn-red {\n  background-color: #ed1d49;\n  border-color: #ed1d49;\n}\n\n.btn-red:hover,\n.btn-red:hover.btn-shadow {\n  box-shadow: 0 14px 20px #ed1d494d;\n}\n\n.btn-red:hover {\n  background-color: #df123d;\n}\n\n.controls-right span {\n  display: block;\n  font-size: 15px;\n  font-weight: 600;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  text-transform: uppercase;\n  transform: skew(-11deg);\n}\n\n.btn {\n  border: 1px solid transparent;\n  border-radius: 7px;\n  color: #fff;\n  display: inline-block;\n  font-style: normal;\n  font-weight: 600;\n  line-height: 1.4;\n  position: relative;\n  text-align: center;\n  text-decoration: none;\n  transition: all 0.3s;\n  -webkit-user-select: none;\n  -moz-user-select: none;\n  user-select: none;\n  white-space: nowrap;\n}\n.controls-left span,\n.controls-right span {\n  overflow-wrap: break-word;\n  white-space: normal;\n}\n@media only screen and (max-width: 767px) {\n  .controls-left span,\n  .controls-left {\n    transform: skew(0);\n    margin: 0;\n  }\n  .controls-right span,\n  .controls-right {\n    transform: skew(0);\n    margin: 0;\n  }\n\n  .controls {\n    display: grid;\n    grid-template-columns: auto auto;\n    gap: 10px;\n    margin: 0 0 45px;\n  }\n  .controls-prediction,\n  .controls-chance {\n    grid-row: 1;\n  }\n  .controls .btn {\n    max-width: none !important;\n  }\n  .controls-left span,\n  .controls-right span {\n    font-size: 12px;\n  }\n}\n</style>\n"]}]}