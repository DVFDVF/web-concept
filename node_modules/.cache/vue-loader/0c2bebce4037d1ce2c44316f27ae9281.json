ACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACpB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;IACvD;MACE,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACxD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACT,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UAChB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACnB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QAC1B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MAChB,CAAC,CAAC,CAAC,CAAC;MACJ,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACvB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MAC/D,CAAC,CAAC,CAAC,CAAC;IACN,CAAC,CAAC,CAAC,CAAC;EACN,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/casino/LobbySections/card/CardContent.vue","sourceRoot":"","sourcesContent":["<template>\n  <div\n    class=\"scroll-container\"\n    ref=\"emblaContent\"\n    v-on:mousedown=\"startDrag\"\n    v-on:mousemove=\"onDrag\"\n    v-on:mouseup=\"stopDrag\"\n  >\n    <ul\n      class=\"list-item-row\"\n      :style=\"{ transform: 'translateX(' + transX + 'px)' }\"\n    >\n      <li v-for=\"(item, index) in gameData?.data\" :key=\"item.id\">\n        <CardWrapper\n          :game-data=\"item\"\n          class=\"card-wrapper\"\n          @click=\"cardClick(index)\"\n        ></CardWrapper>\n      </li>\n      <li ref=\"viewportContent\">\n        <CardAll v-show=\"!search\" @click=\"selectedGroups()\"></CardAll>\n      </li>\n    </ul>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { enterGame } from '@/manager/view';\nimport { MIN_MOVE_DISTANCE } from '@/utils/define';\nimport {\n  GamesDatas,\n  getGameGroupResp,\n  GetGamesResp,\n} from '@/views/home/api/protocol';\nimport { defineComponent } from 'vue';\nimport CardAll from './CardAll.vue';\nimport GameDetailsPopUp from '@/views/popUp/gameDetailsPopUp.vue';\nimport CardWrapper from './CardWrapper.vue';\n\nexport default defineComponent({\n  props: {\n    gameData: {\n      type: Object as () => GetGamesResp,\n    },\n    search: {\n      default: false,\n    },\n    headerData: {\n      type: Object as () => getGameGroupResp,\n    },\n    clickDirection: {},\n  },\n  data() {\n    return {\n      isSmallScreen: false,\n      isRoll: false,\n      dragging: false,\n      startX: 0,\n      lastX: 0,\n      transX: -0,\n      moveDistance: 0, // 移动距离，如果超过MIN_MOVE_DISTANCE,则认为是移动\n    };\n  },\n  methods: {\n    startDrag(e: MouseEvent) {\n      document.addEventListener('mousemove', this.onDrag);\n      document.addEventListener('mouseup', this.stopDrag);\n      this.dragging = true;\n      this.moveDistance = 0;\n      this.startX = this.transX;\n      this.lastX = e.clientX;\n    },\n\n    onDrag(e: MouseEvent) {\n      if (this.dragging) {\n        this.isRoll = true;\n        const container = this.$refs.emblaContent as HTMLElement;\n        const container1 = this.$refs.viewportContent as HTMLElement;\n        if (!container || !container1 || !this.gameData) return;\n        const rect = container.getBoundingClientRect();\n        const rect1 = container1.getBoundingClientRect();\n        const length = this.gameData.data.length;\n        const percentage = e.clientX - this.lastX + this.startX;\n        // 累积移动距离，超过阈值就不触发点击事件\n        this.moveDistance += Math.abs(percentage);\n\n        if (percentage > rect.width * 0.4) {\n          this.transX = rect.width * 0.4;\n        } else if (\n          percentage <\n          rect.width * 0.6 - rect1.width * (length + 1) + 6 * (length - 1)\n        ) {\n          if (rect1.width * (length + 1) - 16 * (length - 1) < rect.width) {\n            this.transX = percentage;\n          } else {\n            this.transX =\n              rect.width * 0.6 - rect1.width * (length + 1) + 6 * (length - 1);\n          }\n        } else {\n          this.transX = percentage;\n        }\n      }\n    },\n\n    stopDrag(e: MouseEvent) {\n      document.removeEventListener('mousemove', this.onDrag);\n      document.removeEventListener('mouseup', this.stopDrag);\n      const container = this.$refs.emblaContent as HTMLElement;\n      const container1 = this.$refs.viewportContent as HTMLElement;\n      if (!container || !container1 || !this.gameData) return;\n      const rect = container.getBoundingClientRect();\n      const rect1 = container1.getBoundingClientRect();\n      const length = this.gameData.data.length;\n      const percentage = e.clientX - this.lastX + this.startX;\n\n      this.isRoll = false;\n      if (percentage > 0) {\n        this.transX = 0;\n      } else if (\n        percentage <\n        rect.width - rect1.width * (length + 1) - 16 * (length - 1)\n      ) {\n        if (rect1.width * (length + 1) - 16 * (length - 1) < rect.width) {\n          this.transX = 0;\n        } else {\n          this.transX =\n            rect.width - rect1.width * (length + 1) - 16 * (length - 1);\n        }\n      } else {\n        this.transX = Math.round(percentage / 184) * 184;\n      }\n      this.dragging = false;\n    },\n    cardClick(index: number) {\n      if (!dataCenter.layout.isMobile) {\n        if (this.moveDistance > MIN_MOVE_DISTANCE) {\n          // 说明是移动的\n          return;\n        }\n        if (!this.gameData) {\n          return;\n        }\n        const gameItem = this.gameData.data[index];\n        enterGame(gameItem);\n      }\n    },\n    selectedGroups() {\n      if (!this.dragging) {\n        if (this.headerData) {\n          // dataCenter.gameConfiguration.currentGameGroup = this.headerData;\n          this.$router.push(`/casino/originals?groupId=${this.headerData.id}`);\n        }\n      }\n    },\n    checkScreenSize() {\n      // 根据窗口宽度判断屏幕大小\n      this.isSmallScreen = window.innerWidth < 768; // 你可以根据需要调整阈值\n    },\n  },\n\n  components: { CardAll, CardWrapper },\n  watch: {\n    clickDirection(newValue) {\n      if (newValue.id == this.headerData?.id) {\n        const container = this.$refs.emblaContent as HTMLElement;\n        const container1 = this.$refs.viewportContent as HTMLElement;\n        if (!container || !container1 || !this.gameData) return;\n        const rect = container.getBoundingClientRect();\n        const rect1 = container1.getBoundingClientRect();\n        if (!rect && !rect1) {\n          return;\n        }\n        if (!this.gameData) return;\n        const length = this.gameData.data.length;\n        if (newValue.text == 'left') {\n          this.transX = 0;\n        } else {\n          if (rect1.width * (length + 1) - 16 * (length - 1) < rect.width) {\n            this.transX = 0;\n          } else {\n            this.transX =\n              rect.width - rect1.width * (length + 1) - 16 * (length - 1);\n          }\n        }\n      }\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.list-item-row {\n  display: flex;\n  flex-direction: row;\n  transition: transform 0.1s;\n}\n\nul,\nli {\n  list-style: none;\n  padding: 0;\n  margin: 0;\n}\n\nli {\n  margin: 6px;\n  padding: 0;\n}\nli:first-child {\n  margin-left: 0;\n}\n\n.scroll-container {\n  overflow: hidden;\n  user-select: none;\n}\n.card-wrapper {\n  width: 172px;\n  height: 227px;\n}\n\n@media only screen and (max-width: 767px) {\n  .scroll-container {\n    overflow-x: scroll;\n    margin-bottom: 10px;\n  }\n  .card-wrapper {\n    width: calc(33.33vw - 20px);\n    height: auto;\n  }\n  .list-item-row {\n    display: grid;\n    grid-template-columns: repeat(6, auto);\n    transition: transform 0.2s ease-out;\n  }\n}\n</style>\n"]}]}