cept/fury-client/src/views/chats/chatsArea/ChatsCenter.vue?vue&type=script&lang=ts"],"sourcesContent":["\nimport { UserBriefRespItem } from '@/api/protocol';\nimport Avatar from '@/components/Avatar.vue';\nimport { ChatData } from '@/dataCenter/chatRoom';\nimport userManager from '@/manager/userManager';\nimport { openModal } from '@/manager/view';\nimport { getLevel, updateSVGPath } from '@/utils';\nimport { defineComponent } from 'vue';\nimport { getChat } from '../api/index';\nimport { GetChatResp } from '../api/protocol';\nimport ShareGame from './ShareGame.vue';\nimport ShareProfits from './ShareProfits.vue';\nimport TipsForPerson from './tips/TipsForPerson.vue';\n\nexport default defineComponent({\n  components: { ShareGame, ShareProfits, Avatar },\n  data() {\n    return {\n      isReply: false,\n      userBrief: {} as UserBriefRespItem,\n      // replyBrief: {} as UserBriefRespItem,\n      replyBrief: {} as GetChatResp,\n      experienceRatio: updateSVGPath(0),\n    };\n  },\n  props: {\n    msgItem: {\n      type: Object as () => ChatData,\n    },\n  },\n  methods: {\n    openTips() {\n      openModal(TipsForPerson);\n    },\n    replyClick(msgItem: ChatData | undefined) {\n      if (msgItem) {\n        this.$emit('replyClick', msgItem, this.userBrief);\n      }\n    },\n    replyData() {\n      if (this.msgItem?.replyId) {\n        if (this.msgItem.replyId != 0) {\n          getChat(this.msgItem.replyId).then((replyBrief) => {\n            if (!replyBrief) return;\n            this.replyBrief = replyBrief;\n          });\n        }\n      }\n    },\n  },\n  mounted() {\n    if (this.msgItem?.userId) {\n      userManager.getUser(this.msgItem.userId).then((userBrief) => {\n        if (userBrief) {\n          this.userBrief = userBrief;\n          this.experienceRatio = updateSVGPath(\n            getLevel(userBrief.level, userBrief.exp) * 360,\n          );\n          this.replyData();\n        }\n      });\n    }\n  },\n});\n"],"mappings":"AAEA,OAAOA,MAAM,M