"sourcesContent":["\nimport { defineComponent } from 'vue';\nimport Login from '../../login/Login.vue';\nimport { contactRoller } from '../contacRoller';\nexport default defineComponent({\n  props: ['numberList', 'itemList'],\n  computed: {},\n  methods: {\n    selectNumber(text: string) {\n      return this.selectItemData.key != text\n        ? 'dropdown-menu-link'\n        : 'dropdown-menu-link dropdown-menu-link-click';\n    },\n    selectBtn(item: any) {\n      this.selectItemData = item;\n      this.isSpend = false;\n      this.spendIcon = '/svg/sprite.svg#icon-arrow-down';\n      this.clickBtn(item.key);\n    },\n    clickBtn(name: string) {\n      if (contactRoller.selectItem == name) return;\n      contactRoller.selectItem = name;\n      contactRoller.selectItem = name;\n      if (contactRoller.selectItem == 'My Bets') {\n        contactRoller.selectType = '4';\n      } else {\n        if (contactRoller.selectItem == 'High Rollers') {\n          contactRoller.selectType = '3';\n        }\n        if (contactRoller.selectItem == 'Rare Wins') {\n          contactRoller.selectType = '1';\n        }\n        if (contactRoller.selectItem == 'All Bets') {\n          contactRoller.selectType = '1';\n        }\n      }\n      if (contactRoller.selectItem == 'High Rollers') {\n        contactRoller.selectType = '3';\n      } else if (contactRoller.selectItem == 'Sports Bets') {\n        contactRoller.selectType = '3';\n      } else if (contactRoller.selectItem == 'Casino Bets') {\n        contactRoller.selectType = '1';\n      }\n      // this.$emit('childEvent', this.selectItem, selectType);\n    },\n    showColor(name: string) {\n      return this.selectItemData.text == name\n        ? 'switcher-label select-lable'\n        : 'switcher-label';\n    },\n    dropdownExpand() {\n      this.isSpend = !this.isSpend;\n      if (this.isSpend) {\n        this.spendIcon = '/svg/sprite.svg#icon-arrow-up';\n      } else {\n        this.spendIcon = '/svg/sprite.svg#icon-arrow-down';\n      }\n    },\n  },\n  mounted() {\n    this.clickBtn(contactRoller.selectItem);\n  },\n  data() {\n    return {\n      isSpend: false,\n      spendIcon: '/svg/sprite.svg#icon-arrow-down',\n      selectItem: contactRoller.selectItem,\n      selectItemData: this.itemList[0],\n      selectNumberText: '10',\n    };\n  },\n  components: { Login },\n});\n"],"mappings":"AACA,SAASA,eAAe,QAAQ,KAAK;AACrC,OAAOC,KAAK,MAAM,uBAAuB;AACzC,SAASC,aAAa,QAAQ,iBAAiB;AAC/C,eAAeF,eAAe,CAAC;EAC7BG,KAAK,EAAE,CAAC,YAAY,EAAE,UAAU,CAAC;EACjCC,QAAQ,EAAE,EAAE;EACZC,OAAO,EAAE;IACPC,YAAYA,CAACC,IAAY;MACvB,OAAO,IAAI,CAACC,cAAc,CAACC,GAAG,IAAIF,IAAI,GAClC,oBAAoB,GACpB,6CAA6C;IACnD,CAAC;IACDG,SAASA,CAACC,IAAS;MACjB,IAAI,CAACH,cAAc,GAAGG,IAAI;MAC1B,IAAI,CAACC,OAAO,GAAG,KAAK;MACpB,IAAI,CAACC,SAAS,GAAG,iCAAiC;MAClD,IAAI,CAACC,QAAQ,CAACH,IAAI,CAACF,GAAG,CAAC;IACzB,CAAC;IACDK,QAAQA,CAACC,IAAY;MACnB,IAAIb,aAAa,CAACc,UAAU,IAAID,IAAI,EAAE;MACtCb,aAAa,CAACc,UAAU,GAAGD,IAAI;MAC/Bb,aAAa,CAACc,UAAU,GAAGD,IAAI;MAC/B,IAAIb,aAAa,CAACc,UAAU,IAAI,SAAS,EAAE;QACzCd,aAAa,CAACe,UAAU,GAAG,GAAG;OAC/B,MAAM;QACL,IAAIf,aAAa,CAACc,UAAU,IAAI,cAAc,EAAE;UAC9Cd,aAAa,CAACe,UAAU,GAAG,GAAG;;QAEhC,IAAIf,aAAa,CAACc,UAAU,IAAI,WAAW,EAAE;UAC3Cd,aAAa,CAACe,UAAU,GAAG,GAAG;;QAEhC,IAAIf,aAAa,CAACc,UAAU,IAAI,UAAU,EAAE;UAC1Cd,aAAa,CAACe,UAAU,GAAG,GAAG;;;MAGlC,IAAIf,aAAa,CAACc,UAAU,IAAI,cAAc,EAAE;QAC9Cd,aAAa,CAACe,UAAU,GAAG,GAAG;OAC/B,MAAM,IAAIf,aAAa,CAACc,UAAU,IAAI,aAAa,EAAE;QACpDd,aAAa,CAACe,UAAU,GAAG,GAAG;OAC/B,MAAM,IAAIf,aAAa,CAACc,UAAU,IAAI,aAAa,EAAE;QACpDd,aAAa,CAACe,UAAU,GAAG,GAAG;;MAEhC;IACF,CAAC;;IACDC,SAASA,CAACH,IAAY;MACpB,OAAO,IAAI,CAACP,cAAc,CAACD,IAAI,IAAIQ,IAAI,GACnC,6BAA6B,GAC7B,gBAAgB;IACtB,CAAC;IACDI,cAAcA,CAAA;MACZ,IAAI,CAACP,OAAO,GAAG,CAAC,IAAI,CAACA,OAAO;MAC5B,IAAI,IAAI,CAACA,OAAO,EAAE;QAChB,IAAI,CAACC,SAAS,GAAG,+BAA+B;OACjD,MAAM;QACL,IAAI,CAACA,SAAS,GAAG,iCAAiC;;IAEtD;GACD;EACDO,OAAOA,CAAA;IACL,IAAI,CAACN,QAAQ,CAACZ,aAAa,CAACc,UAAU,CAAC;EACzC,CAAC;EACDK,IAAIA,CAAA;IACF,OAAO;MACLT,OAAO,EAAE,KAAK;MACdC,SAAS,EAAE,iC