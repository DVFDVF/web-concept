AAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MAClD,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;IACrC,CAAC,CAAC,CAAC,CAAC,CAAC;IACL,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MAC1C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;IACjB,CAAC,CAAC,CAAC,CAAC,CAAC;EACP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/head/popup/walletSix/Retirada.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"depov\">\n    <h2 class=\"depov-title\">{{ lang.v.qb_retirada }}</h2>\n    <div class=\"curencies\">\n      <CurrencySelector @change=\"onCurrencyChange\" style=\"z-index: 1000\" />\n      <NetWorkSelector\n        :list=\"netWorkOptions\"\n        @changes=\"onNetWorkChange\"\n        v-show=\"isNetworkTitle\"\n      />\n    </div>\n    <!--  -->\n    <RetiradaInput\n      class=\"addressInput\"\n      @update:value=\"requestData.payoutAddress = $event\"\n      :input-money=\"requestData.payoutAddress\"\n      input-tops=\"12px\"\n      :-enter-text=\"'Enter Address'\"\n      :is-input-focuseds=\"true\"\n    />\n    <div class=\"retd-disty\">\n      <RetiradaInput\n        class=\"amountInput\"\n        input-tops=\"12px\"\n        :is-input-focuseds=\"true\"\n        :-enter-text=\"'Enter amount'\"\n        :input-money=\"requestData.amount.toString()\"\n        @update:value=\"requestData.amount = $event\"\n        @button-flat-click=\"userMaximum\"\n      />\n      <RetiradaInput\n        class=\"currencyInput\"\n        :isInputmoney=\"true\"\n        :showImage=\"currency?.icon\"\n        input-tops=\"12px\"\n        :is-input-focuseds=\"true\"\n        -enter-text=\"balance\"\n        :inputMoney=\"displayedBalance\"\n        :show-input-icon=\"true\"\n        :isDisabled=\"true\"\n      />\n    </div>\n    <p class=\"retd-upfont\">\n      ⚠️ {{ lang.v.qb_h_montante }} <span> {{ currency?.name }}</span>\n    </p>\n    <HeadBupop\n      login-text=\"Withdraw\"\n      style=\"margin-bottom: 10px; height: 44px; z-index: 0\"\n      @click=\"onWithdraw\"\n    />\n    <p class=\"retd-gray\" v-show=\"currency?.name === 'BTC'\">\n      {{ lang.v.qb_h_envie }} <span>1</span> {{ lang.v.qb_h_confirma }}\n    </p>\n    <p class=\"retd-poutn\">\n      {{ lang.v.qb_h_taxa }} - <span>0.00025 {{ currency?.name }} </span>\n    </p>\n    <div class=\"select-chain-overlay\" v-if=\"isTipsTitle\">\n      <span>{{ lang.v.qb_network }}</span>\n    </div>\n    <div v-if=\"isLodding\" class=\"center lodding\">\n      <ChatsLodding />\n    </div>\n  </div>\n</template>\n<!-- 提现页面 -->\n<script lang=\"ts\">\nimport {\n  defineComponent,\n  ref,\n  onMounted,\n  reactive,\n  onUnmounted,\n  computed,\n} from 'vue';\nimport RetiradaInput from '@/views/home/head/popup/walletSix/widget/RetiradaSelector/RetiradaInput.vue';\nimport HeadBupop from '@/views/home/head/popup/walletSix/headButtonPop.vue';\nimport CurrencySelector from './widget/CurrencySelector/index.vue';\nimport NetWorkSelector from './widget/NetWorkSelector/index.vue';\nimport { Currency, NetWork } from '@/api/protocol';\nimport {\n  TransitionRespItem,\n  WithdrawReq,\n} from '@/views/home/api/wallet-api/walletProtocol';\nimport {\n  getCryptoTransitionAll,\n  userWithdraw,\n} from '@/views/home/api/wallet-api/walletIndex';\nimport dataCenter from '@/dataCenter';\nimport { displayBalance } from '@/utils';\nimport { lang } from '@/lang';\nimport { extenalOfficial } from '@/utils/currency';\nimport ChatsLodding from '@/views/chats/ChatsLodding.vue';\nimport { tip } from '@/components/warn/warn';\nexport default defineComponent({\n  name: 'DepositoPop',\n  components: {\n    RetiradaInput,\n    HeadBupop,\n    CurrencySelector,\n    NetWorkSelector,\n    ChatsLodding,\n  },\n  setup() {\n    const balance = reactive(dataCenter.wallet.map);\n    const currency = ref<Currency>();\n    const transitionList = ref<TransitionRespItem[]>([]);\n    const isNetworkTitle = ref(false);\n    const TransitionRespItemValue = ref<TransitionRespItem>();\n    const walletAddress = ref('');\n    const netWorkOptions = ref<NetWork[]>([]);\n    const isTipsTitle = ref(false);\n    const isLodding = ref(false);\n    // 提现条件\n    const requestData: WithdrawReq = {\n      currencyId: 1,\n      networkId: 1,\n      payoutAddress: '',\n      amount: 0,\n    };\n    let timer = 0;\n\n    function transitionPlet() {\n      getCryptoTransitionAll().then((resp) => {\n        if (!resp) return;\n        transitionList.value = resp;\n        const currency = extenalOfficial(\n          dataCenter.webConfig.cryptoCurrencies,\n          true,\n        )[0];\n        onCurrencyChange(currency);\n      });\n    }\n\n    const onCurrencyChange = (item: Currency) => {\n      requestData.currencyId = item.id;\n      currency.value = item;\n      const list = transitionList.value.filter(\n        (transitionItem) => transitionItem.currencyId === item.id,\n      );\n\n      isNetworkTitle.value = list.length > 1;\n\n      if (list.length <= 1) {\n        isTipsTitle.value = false;\n      } else {\n        isTipsTitle.value = true;\n        netWorkOptions.value = list.map(\n          (item) => dataCenter.webConfig.networkMap[item.networkId]!,\n        );\n      }\n    };\n\n    const onNetWorkChange = (item: NetWork) => {\n      if (item) {\n        requestData.networkId = item.id;\n        isTipsTitle.value = false;\n      }\n    };\n\n    /**复制 */\n    function copyToClipboard(text: string) {\n      var textarea = document.createElement('textarea');\n      textarea.value = text;\n      document.body.appendChild(textarea);\n      textarea.select();\n      document.execCommand('copy');\n      document.body.removeChild(textarea);\n      tip.success('copy success');\n    }\n\n    //用户提现\n    function onWithdraw() {\n      if (requestData.amount != 0 && requestData.payoutAddress != '') {\n        isLodding.value = true;\n        userWithdraw(requestData).then((resp) => {\n          if (!resp) {\n            isLodding.value = false;\n            return;\n          }\n          isLodding.value = false;\n          // 扣钱\n          dataCenter.wallet.change(\n            dataCenter.wallet.selectId,\n            -requestData.amount,\n          );\n          tip.success(lang.v.me_success);\n        });\n      } else {\n        tip.info(lang.v.me_mess);\n      }\n    }\n    //最大余额\n    function userMaximum() {\n      requestData.amount = Number(displayBalance(currency.value, balance));\n    }\n\n    const displayedBalance = computed(() =>\n      displayBalance(currency.value, balance),\n    );\n\n    onMounted(() => {\n      transitionPlet();\n    });\n\n    onUnmounted(() => {\n      clearTimeout(timer);\n    });\n\n    return {\n      currency,\n      transitionList,\n      isNetworkTitle,\n      TransitionRespItemValue,\n      walletAddress,\n      onCurrencyChange,\n      onNetWorkChange,\n      lang,\n      isTipsTitle,\n      copyToClipboard,\n      balance,\n      displayBalance,\n      netWorkOptions,\n      requestData,\n      onWithdraw,\n      userMaximum,\n      isLodding,\n      displayedBalance,\n    };\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.depov {\n  height: 100%;\n  width: 100%;\n\n  &-title {\n    font-size: 24px;\n    line-height: 135%;\n    letter-spacing: -0.02em;\n    margin-bottom: 1rem;\n    margin-top: 0;\n    font-weight: bold;\n    font-family: Montserrat Variable, sans-serif;\n    color: #fff;\n  }\n\n  .curencies {\n    column-gap: 16px;\n    display: grid;\n    grid-template-columns: 2fr 1fr;\n    margin-bottom: 16px;\n    min-height: 58px;\n    position: relative;\n  }\n\n  .addressInput {\n    width: 100%;\n    height: 60px;\n  }\n  .amountInput {\n    width: 100%;\n    height: 60px;\n  }\n  .currencyInput {\n    width: 220px;\n    height: 60px;\n  }\n  .retd-disty {\n    margin-top: 16px;\n    display: flex;\n    gap: 12px;\n    justify-content: space-between;\n    margin-bottom: 16px;\n  }\n\n  .retd-upfont {\n    font-size: 11px;\n    font-weight: 500;\n    font-family: Montserrat Variable, sans-serif;\n    color: #fff;\n    margin-bottom: 16px;\n  }\n\n  .retd-gray {\n    color: #55657e;\n    font-size: 11px;\n    font-weight: 500;\n    font-family: Montserrat Variable, sans-serif;\n    margin-bottom: 16px;\n  }\n\n  .retd-poutn {\n    color: #fff;\n    font-size: 11px;\n    font-weight: 500;\n    font-family: Montserrat Variable, sans-serif;\n    margin-bottom: 16px;\n  }\n\n  .depcy {\n    position: absolute;\n    margin-top: 74px;\n    width: 260px;\n    z-index: 11;\n  }\n\n  .pople {\n    align-items: center;\n    background: #1b2636;\n    box-shadow: 0 4px 26px #0d131c78;\n    min-width: 130px;\n    margin-top: -8px;\n    border: 2px;\n    border-radius: 10px;\n    padding: 12px;\n    right: 25px;\n    background: #1b2636;\n    position: absolute;\n    z-index: 11;\n  }\n}\n\n.select-chain-overlay {\n  align-items: center;\n  background: rgba(17, 25, 35, 0.9);\n  bottom: 0;\n  display: flex;\n  filter: none !important;\n  height: calc(100% - 120px);\n  justify-content: center;\n  left: 0;\n  position: absolute;\n  color: #fff;\n  width: 100%;\n  z-index: 9;\n}\n.lodding {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(0, 0, 0, 0.5);\n}\n\n@media only screen and (max-width: 767px) {\n  .curencies {\n    gap: 16px;\n    grid-template-columns: repeat(1, 1fr) !important;\n  }\n  .retd-disty {\n    display: block;\n    flex-direction: column;\n  }\n  .currencyInput {\n    width: 100% !important;\n  }\n}\n</style>\n"]}]}