:"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/center/Currencies/PopUpAll.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"modal\" style=\"width: 530px; height: 565px\">\n    <div class=\"modal-container bottom-shadow has-height\">\n      <button title=\"Close modal\" class=\"modal-btn-close\" @click=\"close\">\n        <svg\n          class=\"svg-use\"\n          width=\"24\"\n          height=\"24\"\n          focusable=\"false\"\n          aria-hidden=\"true\"\n        >\n          <use xlink:href=\"/svg/sprite.svg#icon-close\"></use>\n        </svg>\n      </button>\n      <div class=\"default-tab\">\n        <div class=\"tab-inner\">\n          <div class=\"header\">\n            <div class=\"header-title\">\n              <h1>{{ lang.v.sy_center_cryptos }}</h1>\n            </div>\n          </div>\n          <div class=\"body\">\n            <div class=\"input input_md\">\n              <div class=\"input-inner\">\n                <span class=\"input-icon\">\n                  <svg class=\"svg-use\" focusable=\"false\" aria-hidden=\"true\">\n                    <use xlink:href=\"/svg/sprite.svg#icon-search-new\"></use>\n                  </svg>\n                </span>\n                <div class=\"input-tag-wrapper\">\n                  <input\n                    class=\"input-tag\"\n                    :placeholder=\"lang.v.sy_center_search\"\n                    autocomplete=\"off\"\n                    tabindex=\"1\"\n                  />\n                </div>\n              </div>\n            </div>\n            <div>\n              <div v-for=\"(item, index) in netTitle\" :key=\"index\">\n                <h2 class=\"result-title\">\n                  <div class=\"result-title-icon\">\n                    <svg\n                      class=\"svg-use\"\n                      width=\"20\"\n                      height=\"20\"\n                      fill=\"#55657E\"\n                      focusable=\"false\"\n                      aria-hidden=\"true\"\n                    >\n                      <use\n                        :xlink:href=\"'/svg/sprite.svg#icon-menu-networks'\"\n                      ></use>\n                    </svg>\n                  </div>\n                  {{ lang.v[item] }}\n                </h2>\n                <div class=\"result-center\">\n                  <div class=\"sup\" v-if=\"index == 0\">\n                    <Cryptocurrencies\n                      v-for=\"item in config.cryptoNetworks\"\n                      :key=\"item.id\"\n                      :item=\"item\"\n                    >\n                    </Cryptocurrencies>\n                  </div>\n                  <div class=\"cry\" v-else>\n                    <SupportedBtn\n                      v-for=\"item in config.cryptoCurrencies\"\n                      :key=\"item.id\"\n                      :item=\"item\"\n                    >\n                    </SupportedBtn>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { ModalCloseFun } from '@/manager/view';\nimport { defineComponent, inject } from 'vue';\nimport Cryptocurrencies from './popBtn/Cryptocurrencies.vue';\nimport SupportedBtn from './popBtn/SupportedBtn.vue';\n\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      icon: '/svg/sprite.svg#icon-menu-networks',\n      netTitle: ['sy_center_upported', 'sy_center_cryptocurrencies'],\n    };\n  },\n  setup() {\n    const close = inject('close') as ModalCloseFun;\n    return { close, config: dataCenter.webConfig };\n  },\n  methods: {},\n\n  components: { SupportedBtn, Cryptocurrencies },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.input {\n  width: 100%;\n}\n\n.input_md .input-inner {\n  border-radius: 8px;\n  height: 36px;\n  padding: 3px 12px;\n}\n\n.input-inner {\n  align-items: center;\n  background-color: #090c1199;\n  border: 1px solid #2a3546;\n  cursor: text;\n  display: flex;\n  position: relative;\n  transition: color 0.1s ease-in-out, border-color 0.1s ease-in-out;\n}\n\n.input_md .input-icon {\n  height: 20px;\n  margin-left: -2px;\n  margin-right: 6px;\n  width: 20px;\n}\n\n.input-icon {\n  align-items: center;\n  display: flex;\n  flex-shrink: 0;\n  justify-content: center;\n  pointer-events: none;\n}\n\n.input-tag-wrapper {\n  display: block;\n  flex: 1 1;\n  position: relative;\n  width: 0;\n  z-index: 0;\n}\n\n.input_md .input-tag {\n  border: none;\n  font-size: 12px;\n  letter-spacing: -0.24px;\n  padding: 0;\n}\n\n.input-tag {\n  color: white;\n  margin-top: 0;\n  outline: none;\n  transition: margin-top 0.1s ease-in-out;\n  width: 100%;\n}\n\n.input-placeholder,\n.input-tag {\n  display: block;\n  font-weight: 600;\n  line-height: 18px;\n}\n\n.input-inner:hover,\n.input-inner:focus,\n.input-inner:active {\n  border-color: #2283f6;\n}\n\ninput {\n  border: none;\n  outline: none;\n  background: none;\n}\n\n.body {\n  padding: 0 20px;\n  display: flex;\n  flex-direction: column;\n  height: 100%;\n  overflow: hidden;\n}\n\n.result-title {\n  color: #fff;\n  display: flex;\n  align-items: center;\n\n  font-size: 16px;\n  font-weight: 600;\n  letter-spacing: -0.02em;\n  line-height: 20px;\n  margin: 14px 0;\n  order: 1;\n}\n\n.result-title-icon {\n  color: #55657e;\n  margin-right: 4px;\n}\n\nsvg {\n  width: 20;\n  height: 20;\n  fill: rgb(85, 101, 126);\n}\n\n.result-center {\n  display: flex;\n  flex-direction: row;\n}\n\n.sup {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 5px;\n}\n\n.cry {\n  width: 100%;\n  display: flex;\n  // flex-direction: row;\n  flex-wrap: wrap;\n  gap: 5px;\n}\n</style>\n"]}]}