sor: pointer;\n  outline: none;\n\n  .button-3d-outer {\n    padding: 10px 32px;\n    border-radius: 10px;\n    background: linear-gradient(1turn, #c4003b 0.8%, #fb1949);\n    box-shadow: 0 3px 16px #ff234180, inset 0 4px 3px #ffffff4d;\n    display: block;\n    height: 100%;\n    position: relative;\n    transition: transform 0.1s ease-in-out;\n    width: 100%;\n    z-index: 1;\n\n    .button-3d-text {\n      color: white;\n      font-variant-numeric: lining-nums tabular-nums;\n      font-weight: 600;\n      line-height: 24px;\n      font-size: 14px;\n      letter-spacing: -0.28px;\n    }\n  }\n}\n\n.button-3d:after {\n  border-radius: 10px;\n  height: 44px;\n  background-color: #61001d;\n  bottom: 0;\n  content: '';\n  display: block;\n  left: 0;\n  pointer-events: none;\n  position: absolute;\n  right: 0;\n  z-index: 0;\n}\n\n.button-3d:hover .button-3d-outer {\n  transform: translateY(-2px);\n  background: linear-gradient(3360deg, #db0a49 0.8%, #fb2b57);\n  box-shadow: 0 3px 20px #ff234199, inset 0 4px 4px #ffffff5e;\n}\n\n.button-3d-md.button-3d {\n  font-size: 14px;\n  padding-bottom: 4px;\n}\n\n.button-3d:disabled {\n  opacity: 0.4;\n  pointer-events: none;\n}\n\n.button-3d-fullwidth {\n  display: flex;\n  justify-content: center;\n  width: 100%;\n}\n\n.loader {\n  margin: auto;\n  border: 4px solid #f3f3f3;\n  border-top: 4px solid #3498db;\n  border-radius: 50%;\n  width: 30px;\n  height: 30px;\n  animation: spin 2s linear infinite;\n}\n\n@keyframes spin {\n  0% {\n    transform: rotate(0deg);\n  }\n\n  100% {\n    transform: rotate(360deg);\n  }\n}\n</style>\n"]}]}