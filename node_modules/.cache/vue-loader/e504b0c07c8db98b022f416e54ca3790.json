iv class=\"share-card-center\">\n        <p class=\"share-card-title\">\n          {{ lang.v.chats_mwin }}\n          <strong class=\"strong\">{{ lang.v.chats_limb }}</strong>\n        </p>\n        <div class=\"share-card-link\" @click=\"shareLink()\">\n          <span class=\"currency\"\n            ><span style=\"color: rgb(255, 255, 255)\">\n              <div>\n                <div class=\"currency-body\" style=\"font-size: 19px\">\n                  <img\n                    width=\"16\"\n                    height=\"16\"\n                    src=\"@/assets/home/footer/bfg.jpg\"\n                    loading=\"lazy\"\n                  />\n                  <div class=\"currency-data\">\n                    <span class=\"balance\">$0.37</span>\n                  </div>\n                </div>\n              </div>\n            </span></span\n          >\n          <svg\n            class=\"link-icon\"\n            width=\"13\"\n            height=\"13\"\n            focusable=\"false\"\n            aria-hidden=\"true\"\n          >\n            <use xlink:href=\"/svg/sprite.svg#icon-arrow-right\"></use>\n          </svg>\n          <div class=\"share-card-link-label\">\n            <span>x 1.23</span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { lang } from '@/lang';\nimport { openModal } from '@/manager/view';\nimport { defineComponent } from 'vue';\nimport BetDetailsVue from '../popUp/BetDetails.vue';\n\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n    };\n  },\n  methods: {\n    shareLink() {\n      openModal(BetDetailsVue);\n    },\n  },\n  components: {},\n});\n</script>\n\n<style lang=\"less\" scoped>\n.msg-content {\n  border-radius: 12px;\n  padding: 8px 12px;\n  background: linear-gradient(rgb(0, 201, 130) 0%, rgb(28, 37, 50) 100%);\n}\n.msg-content {\n  background-color: #1c2532;\n  min-width: 237px;\n  padding: 10px 12px 12px;\n}\n.msg-content .msg-content-header {\n  align-items: center;\n  color: #fff;\n  display: flex;\n  margin-bottom: 2px;\n  width: 100%;\n}\n.msg-content-header {\n  color: #ffffffb3;\n  justify-content: space-between;\n  margin-bottom: 4px;\n}\n.msg-content-header-left {\n  align-items: center;\n  display: flex;\n  gap: 5px;\n}\n.msg-content .msg-content-user-name {\n  font-size: 12px;\n  font-weight: 500;\n  line-height: 20px;\n}\n.new-chat p,\n.new-chat span {\n  transition: font-size 0.1s ease-in-out;\n}\n.msg-content-user-name {\n  color: #ffffffb3;\n}\n.msg-content-header-right {\n  color: #55657e;\n  display: flex;\n  gap: 4px;\n  justify-content: flex-end;\n  margin-right: 2px;\n}\n\n.msg-content .msg-action {\n  color: #ffffff80;\n  padding: 0;\n  transition: color 0.1s ease-in-out;\n}\n.share-card {\n  display: grid;\n  gap: 5px 9px;\n  grid-template-areas:\n    'preview title'\n    'preview timestamp';\n  grid-template-columns: fit-content(1px);\n}\n.share-card {\n  margin: 6px 0 9px;\n}\n.share-card {\n  border-radius: 12px;\n  display: flex;\n  // flex-direction: column;\n  gap: 6px;\n  margin: 4px 0;\n}\n.share-card-preview img {\n  border-radius: 5px;\n  height: 76px;\n  width: 55px;\n}\n.share-card-title {\n  font-size: 11px;\n  display: flex;\n  flex-direction: column;\n}\n\n.share-card .share-card-title {\n  color: #fff;\n  line-height: 1.5;\n  white-space: pre-wrap;\n}\n.msg-content :where(p) {\n  color: #93acd3;\n  margin: 0;\n  word-break: break-word;\n}\n.strong {\n  font-size: 14px;\n  font-weight: 600;\n  letter-spacing: -0.28px;\n}\n.share-card.share-card-link {\n  padding: 9px 1px 9px 8px;\n}\n.share-card-link {\n  cursor: pointer;\n  transition: background-color 0.1s ease-in-out;\n  align-items: center;\n  background-color: #ffffff1a;\n  border-radius: 6px;\n  display: flex;\n  gap: 3px;\n  height: 38px;\n  justify-content: flex-start;\n  padding: 10px 8px;\n  position: relative;\n}\n.msg-content .share-card-link > span {\n  display: inline-flex;\n  line-height: 1 !important;\n}\n.currency {\n  align-items: center;\n  display: inline-flex;\n  flex-shrink: 0;\n}\n\n.share-card-link-label {\n  font-feature-settings: 'tnum' on, 'lnum' on;\n  background-color: #fed700;\n  border-radius: 8px 0 8px 8px;\n  bottom: 0;\n  color: #0b2251;\n  display: flex;\n  flex-shrink: 0;\n  font-size: 11px;\n  font-weight: 800 !important;\n  gap: 10px;\n  letter-spacing: -0.02em;\n  line-height: 1.2;\n  padding: 1px 6px;\n  position: absolute;\n  right: 0;\n  text-transform: uppercase;\n  transform: translateY(calc(100% - 6px));\n}\n\n.currency-body {\n  align-items: center;\n  display: flex;\n}\n\n.share-card-link .link-icon {\n  margin-left: auto;\n  margin-right: 8px;\n}\n.currency-name,\n.link-icon {\n  opacity: 0.5;\n  transition: opacity 0.1s ease-in-out;\n}\n.share-card-center {\n  display: flex;\n  flex-direction: column;\n  text-align: left;\n  width: 100%;\n}\n.share-card-link:hover {\n  background-color: #ffffff40;\n}\n.share-card-link:hover.link-icon {\n  opacity: 1;\n  color: white;\n}\n</style>\n"]}]}