AAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EACd,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EACf,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;EAChB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;EACjB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC;EACjB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC,CAAC,CAAC;AACrB","file":"/Users/nuomi/Desktop/项目/concept/fury-client/src/views/home/login/ResetPassword.vue","sourceRoot":"","sourcesContent":["<template>\n  <div>\n    <ModifyPassword\n      @submitClick=\"submitClick\"\n      v-show=\"isModifyPassword\"\n    ></ModifyPassword>\n    <div class=\"modal\" v-show=\"!isModifyPassword\">\n      <button title=\"Close modal\" class=\"modal-btn-close\" @click=\"resetBack\">\n        <svg\n          class=\"svg-use\"\n          width=\"24\"\n          height=\"24\"\n          focusable=\"false\"\n          aria-hidden=\"true\"\n        >\n          <use xlink:href=\"/svg/sprite.svg#icon-close\"></use>\n        </svg>\n      </button>\n      <div>\n        <div class=\"modal-container row-center\">\n          <EmailVerification\n            v-show=\"selectItem == 1\"\n            :email=\"emailValue\"\n            @goBack=\"resetBack\"\n            @sendAgain=\"codeSend\"\n            @confirmEmail=\"confirmEmail\"\n            @onChange=\"onChange\"\n          ></EmailVerification>\n          <div v-show=\"selectItem == 0\">\n            <div class=\"top-content\">\n              <div class=\"reset-back\" @click=\"resetBack()\">\n                <div class=\"icon\">\n                  <svg\n                    class=\"svg-use\"\n                    width=\"20\"\n                    height=\"20\"\n                    focusable=\"false\"\n                    aria-hidden=\"true\"\n                  >\n                    <use :xlink:href=\"backIcon\"></use>\n                  </svg>\n                </div>\n              </div>\n              <div class=\"reset-title\">{{ lang.v.dl_reset }}</div>\n            </div>\n            <div v-show=\"selectItem == 0\">\n              <div class=\"modal-btn\">\n                <InputBoxContent\n                  :data=\"item\"\n                  @inputContent=\"inputContent\"\n                ></InputBoxContent>\n              </div>\n              <p>\n                {{ lang.v.dl_letter }}\n              </p>\n            </div>\n            <div class=\"is-letter\" v-show=\"selectItem == 1\">\n              <img\n                class=\"reset-pass-letter\"\n                src=\"@/assets/home/login/letter.png\"\n                alt=\"letter\"\n              />\n              <p>Please check your email</p>\n            </div>\n          </div>\n        </div>\n        <div class=\"reset-down\" v-show=\"selectItem == 0\">\n          <BtnLogin :message=\"sendSubmitBtn\" @click=\"sendLetter\"></BtnLogin>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\nimport { tip } from '@/components/warn/warn';\nimport { lang } from '@/lang';\nimport { ModalCloseFun } from '@/manager/view';\nimport { message } from 'ant-design-vue';\nimport { defineComponent, inject } from 'vue';\nimport { forgetPassword, getCode } from '../api';\nimport BtnLogin from './BtnLogin.vue';\nimport EmailVerification from './emailVerification/emailVerification.vue';\nimport InputBoxContent from './inputBox/InputBoxContent.vue';\nimport ModifyPassword from './modifyPassword.vue';\n\nexport default defineComponent({\n  name: 'or-line',\n  components: {\n    InputBoxContent,\n    BtnLogin,\n    EmailVerification,\n    ModifyPassword,\n  },\n  data() {\n    return {\n      lang,\n      closeFunction: inject('close') as ModalCloseFun,\n      item: {\n        icon: '/svg/sprite.svg#icon-email-new',\n        key: 'email',\n        get name() {\n          return lang.v.dl_entr;\n        },\n      },\n      backIcon: '/svg/sprite.svg#icon-arrow-left',\n      sendSubmitBtn: {\n        type: '0',\n        get text() {\n          return lang.v.dl_senter;\n        },\n        status: '',\n      },\n      selectItem: 0,\n      isSendEmail: false,\n      isModifyPassword: false,\n      emailValue: '',\n      emailCodeValue: '',\n    };\n  },\n  methods: {\n    resetBack() {\n      if (this.selectItem == 0) {\n        this.closeFunction();\n      } else {\n        this.selectItem = 0;\n      }\n    },\n    sendLetter() {\n      if (this.isValidEmail(this.emailValue)) {\n        this.codeSend();\n      }\n    },\n    isValidEmail(email: string): boolean {\n      const emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\n      return emailRegex.test(email);\n    },\n    //验证码\n    codeSend() {\n      getCode(this.emailValue, 1).then((response) => {\n        if (response) {\n          tip.success('Successfully sent');\n          this.selectItem = 1;\n        }\n      });\n    },\n    //获取input输入\n    inputContent(key: string, value: string) {\n      this.emailValue = value;\n    },\n    //弹出忘记密码界面\n    confirmEmail() {\n      this.isModifyPassword = true;\n    },\n    //获取邮件验证码\n    onChange(codeArr: string[]) {\n      this.emailCodeValue = codeArr.join('');\n    },\n    //提交修改\n    submitClick(password: string) {\n      forgetPassword(this.emailValue, this.emailCodeValue, password).then(\n        (response) => {\n          if (!response) return;\n          this.closeFunction();\n          tip.success(response);\n        },\n      );\n    },\n  },\n});\n</script>\n<style lang=\"less\" scoped>\n.modal {\n  background-color: #111923;\n  border-radius: 12px;\n  text-align: left;\n  width: 360px;\n  align-items: flex-start;\n  flex-direction: column;\n}\n\n.modal-container {\n  padding: 16px 26px;\n  text-align: left;\n  display: flex;\n  align-items: flex-start;\n  flex-direction: column;\n\n  .top-content {\n    height: 33px;\n    display: flex;\n    align-items: center;\n\n    .reset-back {\n      width: 33px;\n      height: 33px;\n      padding: 6.5px;\n      background-color: #2a3546;\n      color: #93acd3;\n      border-radius: 8px;\n      cursor: pointer;\n    }\n\n    .reset-title {\n      font-size: 18px;\n      margin-left: 0.5em;\n      margin-right: 0.5em;\n      color: white;\n      font-weight: 600;\n    }\n  }\n\n  .modal-btn {\n    width: 100%;\n    margin: 10px 0;\n    padding: 7px 0px;\n    color: #55657e;\n  }\n}\n\np {\n  color: #93acd3;\n  font-size: 16px;\n  font-weight: 500;\n  letter-spacing: 0;\n  line-height: 24px;\n  margin: 2px 0 0px;\n  font-family: Montserrat Variable, sans-serif;\n}\n\nbutton {\n  text-rendering: auto;\n  text-transform: none;\n  text-indent: 0px;\n  text-shadow: none;\n  padding-block: 1px;\n  padding-inline: 6px;\n  writing-mode: horizontal-tb !important;\n}\n\n.reset-down {\n  padding: 5px 26px 20px;\n  text-align: left;\n  display: flex;\n  align-items: flex-start;\n  flex-direction: column;\n  background-color: #161f2c;\n  border-top: 1px solid rgba(183, 207, 255, 0.05);\n  border-top-width: 1px;\n  border-top-style: solid;\n}\n.reset-down:hover {\n  background-color: #202a39;\n  color: #93acd3;\n}\n.is-letter {\n  display: flex;\n  width: 100%;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  margin: 30px 0 10px;\n}\n.reset-pass-letter {\n  margin: 0 auto;\n  max-width: 187px;\n}\n.is-letter p {\n  color: #93acd3;\n  font-size: 16px;\n  font-weight: 500;\n  letter-spacing: 0;\n  line-height: 24px;\n  margin: 12px 0 20px;\n}\n</style>\n"]}]}