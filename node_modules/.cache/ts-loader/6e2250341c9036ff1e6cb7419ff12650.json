{"remainingRequest":"/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/babel-loader/lib/index.js!/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/ts-loader/index.js??ref--15-2!/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/cache-loader/dist/cjs.js??ref--1-0!/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/vue-loader-v16/dist/index.js??ref--1-1!/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/head/popup/MyCurrencyPop.vue?vue&type=script&lang=ts","dependencies":[{"path":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/head/popup/MyCurrencyPop.vue","mtime":1718971765467},{"path":"/Users/nuomi/Desktop/项目/fury-client/fury-client/babel.config.js","mtime":1716171552917},{"path":"/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/cache-loader/dist/cjs.js","mtime":1716171608994},{"path":"/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/babel-loader/lib/index.js","mtime":1716171609486},{"path":"/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/ts-loader/index.js","mtime":1716171609531},{"path":"/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/cache-loader/dist/cjs.js","mtime":1716171608994},{"path":"/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/vue-loader-v16/dist/index.js","mtime":1716171609468}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"names":["dataCenter","displayBalance","HSearchIcon","computed","defineComponent","nextTick","ref","lang","extenalOfficial","components","data","props","toggleButtons","type","Array","required","dynamicTexts","default","inputHidden","Boolean","excludeBRL","setup","emit","searchText","showClearButton","isFocused","list","balance","wallet","map","showUsdt","value","webConfig","cryptoCurrencies","concat","currency","toggleClearButton","length","clearInput","filteredList","trim","filtered","slice","filter","item","name","toLowerCase","includes","buttonStates","toggleToggleButton","index","currencies","_balance$item$id","id","console","log","selectItem","selectedItem","display","myToggleButtons"],"sources":["/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/cache-loader/dist/cjs.js??ref--1-0!/Users/nuomi/Desktop/项目/fury-client/fury-client/node_modules/vue-loader-v16/dist/index.js??ref--1-1!/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/head/popup/MyCurrencyPop.vue?vue&type=script&lang=ts"],"sourcesContent":["\nimport { Currency } from '@/api/protocol';\nimport dataCenter from '@/dataCenter';\nimport { displayBalance } from '@/utils';\nimport HSearchIcon from '@/assets/icon/head/ico-search.vue';\nimport { computed, defineComponent, nextTick, PropType, ref } from 'vue';\nimport { lang } from '@/lang';\nimport { extenalOfficial } from '@/utils/currency';\n\nexport default defineComponent({\n  components: {\n    HSearchIcon,\n  },\n  data() {\n    return {\n      lang,\n    };\n  },\n  props: {\n    toggleButtons: {\n      type: Array as PropType<boolean[]>,\n      required: true,\n    },\n    dynamicTexts: {\n      type: Array as PropType<string[]>,\n      default: () => [],\n    },\n    inputHidden: {\n      type: Boolean as PropType<boolean>,\n      required: true,\n    },\n    excludeBRL: {\n      type: Boolean as PropType<boolean>,\n      default: false, // 设置默认值为false\n    },\n  },\n  setup(props, { emit }) {\n    const searchText = ref('');\n    const showClearButton = ref(false);\n    const isFocused = ref(false);\n    const list = ref<Currency[]>([]);\n    const balance = dataCenter.wallet.map;\n    const showUsdt = ref(false);\n    list.value = dataCenter.webConfig.cryptoCurrencies.concat();\n\n    nextTick(() => {\n      const currency = extenalOfficial(list.value, props.excludeBRL);\n      if (currency[0]) {\n        emit('change', currency[0]);\n      }\n    });\n\n    /**自动显示清除按钮 */\n    const toggleClearButton = () => {\n      showClearButton.value = searchText.value.length > 0;\n    };\n    const clearInput = () => {\n      searchText.value = '';\n    };\n\n    // 搜索\n    const filteredList = computed(() => {\n      // 检查 searchText 是否为空或只包含空白字符\n      if (!searchText.value || searchText.value.trim() === '') {\n        return extenalOfficial(list.value, props.excludeBRL);\n      }\n\n      // 根据 searchText 过滤列表,slice 主要去除usdt\n      let filtered = list.value.slice(0, 1).filter((item) => {\n        return item.name\n          .toLowerCase()\n          .includes(searchText.value.toLowerCase().trim());\n      });\n      // 如果 excludeBRL 为 true，则过滤掉含有 BRL 的数据\n      filtered = extenalOfficial(filtered, props.excludeBRL);\n      return filtered;\n    });\n\n    const buttonStates = ref(props.toggleButtons); // Renamed to avoid conflicts\n    /**底部的3个切换按钮 */\n    const toggleToggleButton = (index: number) => {\n      buttonStates.value[index] = !buttonStates.value[index];\n      if (index === 0) {\n        // 隐藏金额为0的\n        const currencies = dataCenter.webConfig.cryptoCurrencies;\n        if (currencies) {\n          if (buttonStates.value[index]) {\n            list.value = currencies.filter((item) => {\n              return balance[item.id]?.value;\n            });\n            if (list.value.length < 1) {\n              list.value = [currencies[0]];\n            }\n          } else {\n            list.value = currencies;\n          }\n        } else {\n          console.log('货币列表为空');\n        }\n      }\n      // todo index===1 Show vault balance\n      if (index === 1) {\n        // 显示汇率\n        showUsdt.value = !showUsdt.value;\n        emit('useUsdt', showUsdt.value);\n      }\n    };\n\n    const selectItem = (index: number) => {\n      // 检查索引是否有效\n      if (index < 0 || index >= filteredList.value.length) return;\n\n      // 获取被选中的项\n      const selectedItem = filteredList.value[index];\n      // 使用 emit 发送选中的数据项\n      emit('change', selectedItem);\n    };\n    function display(currency: Currency) {\n      return displayBalance(currency, balance, showUsdt.value);\n    }\n    // 如果 excludeBRL 为 true，则过滤掉含有 BRL 的数据\n\n    return {\n      searchText,\n      showClearButton,\n      isFocused,\n      myToggleButtons: buttonStates,\n      list,\n      balance,\n      filteredList,\n      selectItem,\n      toggleClearButton,\n      clearInput,\n      toggleToggleButton,\n      display,\n    };\n  },\n});\n"],"mappings":"AAEA,OAAOA,UAAU,MAAM,cAAc;AACrC,SAASC,cAAc,QAAQ,SAAS;AACxC,OAAOC,WAAW,MAAM,mCAAmC;AAC3D,SAASC,QAAQ,EAAEC,eAAe,EAAEC,QAAQ,EAAYC,GAAG,QAAQ,KAAK;AACxE,SAASC,IAAI,QAAQ,QAAQ;AAC7B,SAASC,eAAe,QAAQ,kBAAkB;AAElD,eAAeJ,eAAe,CAAC;EAC7BK,UAAU,EAAE;IACVP;GACD;EACDQ,IAAIA,CAAA;IACF,OAAO;MACLH;KACD;EACH,CAAC;EACDI,KAAK,EAAE;IACLC,aAAa,EAAE;MACbC,IAAI,EAAEC,KAA4B;MAClCC,QAAQ,EAAE;KACX;IACDC,YAAY,EAAE;MACZH,IAAI,EAAEC,KAA2B;MACjCG,OAAO,EAAEA,CAAA,KAAM;KAChB;IACDC,WAAW,EAAE;MACXL,IAAI,EAAEM,OAA4B;MAClCJ,QAAQ,EAAE;KACX;IACDK,UAAU,EAAE;MACVP,IAAI,EAAEM,OAA4B;MAClCF,OAAO,EAAE;;GAEZ;EACDI,KAAKA,CAACV,KAAK,EAAE;IAAEW;EAAI,CAAE;IACnB,MAAMC,UAAU,GAAGjB,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMkB,eAAe,GAAGlB,GAAG,CAAC,KAAK,CAAC;IAClC,MAAMmB,SAAS,GAAGnB,GAAG,CAAC,KAAK,CAAC;IAC5B,MAAMoB,IAAI,GAAGpB,GAAG,CAAa,EAAE,CAAC;IAChC,MAAMqB,OAAO,GAAG3B,UAAU,CAAC4B,MAAM,CAACC,GAAG;IACrC,MAAMC,QAAQ,GAAGxB,GAAG,CAAC,KAAK,CAAC;IAC3BoB,IAAI,CAACK,KAAK,GAAG/B,UAAU,CAACgC,SAAS,CAACC,gBAAgB,CAACC,MAAM,EAAE;IAE3D7B,QAAQ,CAAC,MAAK;MACZ,MAAM8B,QAAQ,GAAG3B,eAAe,CAACkB,IAAI,CAACK,KAAK,EAAEpB,KAAK,CAACS,UAAU,CAAC;MAC9D,IAAIe,QAAQ,CAAC,CAAC,CAAC,EAAE;QACfb,IAAI,CAAC,QAAQ,EAAEa,QAAQ,CAAC,CAAC,CAAC,CAAC;;IAE/B,CAAC,CAAC;IAEF;IACA,MAAMC,iBAAiB,GAAGA,CAAA,KAAK;MAC7BZ,eAAe,CAACO,KAAK,GAAGR,UAAU,CAACQ,KAAK,CAACM,MAAM,GAAG,CAAC;IACrD,CAAC;IACD,MAAMC,UAAU,GAAGA,CAAA,KAAK;MACtBf,UAAU,CAACQ,KAAK,GAAG,EAAE;IACvB,CAAC;IAED;IACA,MAAMQ,YAAY,GAAGpC,QAAQ,CAAC,MAAK;MACjC;MACA,IAAI,CAACoB,UAAU,CAACQ,KAAK,IAAIR,UAAU,CAACQ,KAAK,CAACS,IAAI,EAAE,KAAK,EAAE,EAAE;QACvD,OAAOhC,eAAe,CAACkB,IAAI,CAACK,KAAK,EAAEpB,KAAK,CAACS,UAAU,CAAC;;MAGtD;MACA,IAAIqB,QAAQ,GAAGf,IAAI,CAACK,KAAK,CAACW,