-show=\"toggleLeft.leftMenuFold\">\n      <img\n        width=\"29\"\n        height=\"31\"\n        :src=\"require('@/assets/home/left/gift-3d.webp')\"\n      />\n    </div>\n    <div class=\"fury col-middle\" v-show=\"!toggleLeft.leftMenuFold\">\n      <div class=\"label\">\n        <span>{{ dataCenter.websiteName_Large }}</span>\n        <span>{{ lang.v.zp_wheel }}</span>\n      </div>\n      <div class=\"wheel\">\n        <div class=\"wrapper\">\n          <img\n            class=\"wheel-main\"\n            :src=\"require('@/assets/home/left/wheel.webp')\"\n          />\n        </div>\n        <img\n          class=\"wheel-center\"\n          :src=\"require('@/assets/home/left/wheel-center.webp')\"\n        /><img\n          class=\"wheel-coin-lg\"\n          :src=\"require('@/assets/home/left/coin-lg.webp')\"\n        /><img\n          class=\"wheel-coin-sm\"\n          :src=\"require('@/assets/home/left/coin-sm.webp')\"\n        />\n      </div>\n    </div>\n    <div class=\"nav-line-wrap\">\n      <NavLine />\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\n// 顶部福利区域\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { defineComponent } from 'vue';\nimport NavLine from './widget/NavLine.vue';\n\nimport wheelPop from '@/games/wheel/wheelPop.vue';\nimport { openModal } from '@/manager/view';\nimport LoginWidget from '@/views/home/login/Login.vue';\nexport default defineComponent({\n  name: 'home-welfare',\n  components: {\n    NavLine,\n  },\n  data() {\n    return {\n      lang,\n      dataCenter,\n      toggleLeft: dataCenter.layout,\n      fold: false,\n    };\n  },\n  methods: {\n    furyClick() {\n      if (dataCenter.user.data) {\n        openModal(wheelPop);\n      } else {\n        //登录\n        openModal(LoginWidget);\n      }\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.welfare {\n  cursor: pointer;\n  .list {\n    margin: 0 9px;\n    width: 188px;\n    height: 100px;\n  }\n\n  .fury {\n    width: 100%;\n    height: 48px;\n    border-radius: 8px;\n    box-sizing: border-box;\n    overflow: hidden;\n    padding: 8px;\n    position: relative;\n    transform: translateZ(0);\n    background: linear-gradient(287deg, #5200ff, #1c2532 96.78%), #1c2532;\n\n    .label {\n      display: flex;\n      flex-direction: column;\n      text-align: left;\n\n      span {\n        transition: color 0.1s ease-in-out;\n        line-height: 1;\n        text-transform: uppercase;\n      }\n\n      span:first-child {\n        color: #fff;\n        font-size: 13px;\n        font-weight: 900;\n      }\n\n      span:last-child {\n        color: #ffffffa6;\n        font-size: 10px;\n        font-weight: 700;\n      }\n    }\n\n    .wrapper {\n      animation: rolling 3s linear infinite;\n      bottom: -14px;\n      height: 52px;\n      position: absolute;\n      right: -14px;\n      width: 52px;\n\n      .wheel-main {\n        width: 52px;\n        height: 52px;\n      }\n    }\n\n    .wheel-center {\n      bottom: 2px;\n      position: absolute;\n      right: 3px;\n      width: 18px;\n    }\n\n    .wheel-coin-lg {\n      animation: coin-lg 4s linear infinite;\n      position: absolute;\n      right: 16px;\n      top: 6px;\n      width: 30px;\n    }\n\n    .wheel-coin-sm {\n      animation: coin-sm 4s linear infinite;\n      position: absolute;\n      right: 4px;\n      top: -3px;\n      width: 22px;\n    }\n  }\n\n  .nav-line-wrap {\n    padding-top: 12px;\n    padding-bottom: 1px;\n  }\n}\n\n.bonuses-short {\n  align-items: center;\n  background: linear-gradient(291deg, #3a9800 -83.06%, #1c2532 96.77%), #1c2532;\n  border-radius: 6px;\n  cursor: pointer;\n  width: 40px;\n  height: 35px;\n  justify-content: center;\n}\n</style>\n"]}]}