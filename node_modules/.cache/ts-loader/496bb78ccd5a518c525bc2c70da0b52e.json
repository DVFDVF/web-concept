String;\n}\nclass Lang {\n  v: Record<string, string> & typeof pt_br = pt_br; // 设置默认语言\n  m = getLanguageCode();\n\n  change(key: string) {\n    this.m = key;\n    localStorage.setItem('language', key);\n    import(`./${key}`).then((v) => (this.v = v.default));\n  }\n}\n\nexport const lang = reactive(new Lang());\n\n/**真PG & 三方游戏 语言映射 */\nexport const PgLangMap: Record<string, string> = {\n  en: 'EN',\n  zh: 'ZH_CN',\n  es: 'ES_ES',\n  ina: 'ID_ID',\n  pt_br: 'PT_PT',\n  vn: 'VI_VN',\n};\n\n/**假PG 语言映射 */\nexport const fakePgMap: Record<string, string> = {\n  en: 'en',\n  zh: 'zh',\n  es: 'es',\n  ina: 'id',\n  pt_br: 'pt',\n  vn: 'vi',\n};\n"],"mappings":"AAAA;AACA,SAASA,QAAQ,QAAQ,KAAK;AAC9B,OAAOC,KAAK,MAAM,SAAS;AAC3B;AACA,SAASC,eAAeA,CAAA;EACtB,MAAMC,UAAU,GAAGC,YAAY,CAACC,OAAO,CAAC,UAAU,CAAC;EACnD,IAAI,CAACF,UAAU,EAAE;IACf,OAAO,OAAO;;EAEhB;EACA,IAAIA,UAAU,CAACG,QAAQ,CAAC,GAAG,CAAC,EAAE;IAC5B,MAAMC,IAAI,GAAGC,IAAI,CAACC,KAAK,CAACN,UAAU,CAAC,CAACO,GAAa;IACjD;IACAN,YAAY,CAACO,OAAO,CAAC,UAAU,EAAEJ,IAAI,CAAC;IACtC,OAAOA,IAAI;;EAEb,OAAOJ,UAAU;AACnB;AACA,MAAMS,IAAI;EAAVC,YAAA;IACE,KAAAC,CAAC,GAA0Cb,KAAK,CAAC,CAAC;IAClD,KAAAc,CAAC,GAAGb,eAAe,EAAE;EAOvB;EALEc,MAAMA,CAACC,GAAW;IAChB,IAAI,CAACF,CAAC,GAAGE,GAAG;IACZb,YAAY,CAACO,OAAO,CAAC,UAAU,EAAEM,GAAG,CAAC;IACrC,MAAM,CAAC,KAAKA,GAAG,EAAE,CAAC,CAACC,IAAI,CAAEJ,CAAC,IAAM,IAAI,CAACA,CAAC,GAAGA,CAAC,CAACK,OAAQ,CAAC;EACtD;;AAGF,OAAO,MAAMC,IAAI,GAAGpB,QAAQ,CAAC,IAAIY,IAAI,EAAE,CAAC;AAExC;AACA,OAAO,MAAMS,SAAS,GAA2B;EAC/CC,EAAE,EAAE,IAAI;EACRC,EAAE,EAAE,OAAO;EACXC,EAAE,EAAE,OAAO;EACXC,GAAG,EAAE,OAAO;EACZxB,KAAK,EAAE,OAAO;EACdyB,EAAE,EAAE;CACL;AAED;AACA,OAAO,MAAMC,SAAS,GAA2B;EAC/CL,EAAE,EAAE,IAAI;EACRC,EAAE,EAAE,IAAI;EACRC,EAAE,EAAE,IAAI;EACRC,GAAG,EAAE,IAAI;EACTxB,KAAK,EAAE,IAAI;EACXyB,EAAE,EAAE;CACL"}]}