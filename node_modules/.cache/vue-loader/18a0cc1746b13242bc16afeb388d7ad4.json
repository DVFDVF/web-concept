C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACvB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACf,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACL,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UAC/B,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UAClB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;UACd,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACjC;QACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;MACX,CAAC,CAAC,CAAC,CAAC,CAAC;MACL,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACvB,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC;MACjC,CAAC,CAAC,CAAC,CAAC,CAAC;IACP,CAAC,CAAC,CAAC,CAAC,CAAC;EACP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/concept/fury-client/src/games/limbo/index.vue","sourceRoot":"","sourcesContent":["<template>\n  <div class=\"limbo\" ref=\"limbo\">\n    <div class=\"content\">\n      <div class=\"center\">\n        <div class=\"dapps-top-buttons\">\n          <TopIcon :topIcons=\"topIcons\"> </TopIcon>\n        </div>\n        <div ref=\"container\" class=\"game\">\n          <div id=\"limbo-stage\"></div>\n          <div class=\"front-bg\"></div>\n          <div id=\"limbo-animate-layer\"></div>\n          <div class=\"ui-layer\">\n            <div class=\"history\">\n              <span\n                class=\"history-item\"\n                v-for=\"item in history\"\n                :key=\"item.id\"\n                :class=\"item.isWin ? 'win' : 'lose'\"\n                >{{ item.value }}x</span\n              >\n            </div>\n          </div>\n        </div>\n        <div class=\"openingMenu\">\n          <AmontMenu\n            :isEnable=\"isStart\"\n            :bettingAmount=\"bettingAmount\"\n            @updateValue=\"updateValue\"\n            class=\"amonut-menu\"\n          >\n          </AmontMenu>\n          <Target\n            :is-start=\"isStart\"\n            @change=\"onTargetChange\"\n            :is-popconfirm-visible=\"isPopconfirmVisible\"\n            @closePromptBox=\"closePromptBox\"\n            class=\"target\"\n          ></Target>\n          <StartGame\n            @click=\"startGame\"\n            :is-start=\"isStart\"\n            :auto-count=\"childAutobet.count\"\n            :is-stop=\"isStop\"\n            class=\"start-game\"\n          >\n          </StartGame>\n        </div>\n        <!-- 自动模式选项 -->\n        <DiceGameMode\n          :is-auto=\"isAuto\"\n          @modeClick=\"modeClick\"\n          class=\"dice-game-mode\"\n        ></DiceGameMode>\n        <Autobet\n          @getAutoBetData=\"getAutoBetData\"\n          :is-start=\"isStart\"\n          v-show=\"isAuto\"\n          :auto-count=\"childAutobet.count\"\n        >\n        </Autobet>\n      </div>\n      <div v-show=\"showRanking\">\n        <GameRight class=\"game-right\" />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script lang=\"ts\">\ninterface HistoryItem {\n  id: number;\n  value: string;\n  isWin: boolean;\n}\nimport SizeObserver from '@/components/SizeObserver';\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { openModal } from '@/manager/view';\nimport { addArrayToLocal } from '@/utils';\nimport Autobet from '@/views/game/dice/left/Autobet.vue';\nimport DiceGameMode from '@/views/game/dice/left/DiceGameMode.vue';\nimport { Configuration } from '@/views/game/dice/left/interface';\nimport GameRight from '@/views/game/dice/right/GameRight.vue';\nimport WalletButtonPop from '@/views/home/head/popup/WalletButtonPop.vue';\nimport Login from '@/views/home/login/Login.vue';\nimport { Modal } from 'ant-design-vue';\nimport { defineComponent } from 'vue';\nimport { playGameRocket } from '..';\nimport { amountAdd, amountMax } from '../methods';\nimport AmontMenu from './AmontMenu.vue';\nimport { Limbo } from './game';\nimport StartGame from './StartGame.vue';\nimport Target from './Target.vue';\nimport TopIcon from './TopIcon.vue';\nimport { formatMoney } from '@/utils/currency';\nexport default defineComponent({\n  name: 'game-limbo',\n  components: {\n    GameRight,\n    TopIcon,\n    DiceGameMode,\n    Autobet,\n    StartGame,\n    Target,\n    AmontMenu,\n  },\n  setup(): { game?: Limbo; getBalance: () => number } {\n    return {\n      game: undefined,\n      getBalance: () => dataCenter.wallet.getBalance(),\n    };\n  },\n  data() {\n    return {\n      history: [] as HistoryItem[],\n      isStart: false,\n      isStop: false,\n      isPopconfirmVisible: false,\n      topIcons: [\n        // 暂不需要\n        // {\n        //   icon: '/svg/sprite.svg#icon-statistic',\n        //   get text() {\n        //     return lang.v.dice_statictic;\n        //   },\n        //   key: 'Statistic',\n        // },\n        {\n          icon: '/svg/sprite.svg#icon-speed',\n          get text() {\n            return lang.v.dice_fast;\n          },\n          key: 'Fast',\n        },\n        {\n          icon: '/svg/sprite.svg#icon-fairness',\n          get text() {\n            return lang.v.dice_fairness;\n          },\n          key: 'Fairness',\n        },\n        {\n          icon: '/svg/sprite.svg#icon-rules',\n          get text() {\n            return lang.v.dice_rules;\n          },\n          key: 'Rules',\n        },\n        {\n          icon: '/svg/sprite.svg#icon-sound-on',\n          text: 'sound',\n        },\n        {\n          icon: '/svg/sprite.svg#icon-hotkeys',\n          get text() {\n            return lang.v.dice_hotkeys;\n          },\n          key: 'Hotkeys',\n        },\n      ],\n      bettingAmount: amountMax(),\n      CurrentAmount: 0,\n      TotalAmount: 1000,\n      target: 2,\n      isAuto: false,\n      isWin: false,\n      childAutobet: {\n        count: '',\n        stopOnProfit: '',\n        StopOnLoss: '',\n        MaxBetAmount: '',\n        OnWin: '',\n        OnLoss: '',\n        onWinisReset: true,\n        OnLessisReset: true,\n      },\n      isUnmounted: false,\n      sizeObserver: {} as SizeObserver,\n      showRanking: false,\n    };\n  },\n  mounted() {\n    this.sizeObserver = new SizeObserver((size) => {\n      this.showRanking = size.width > 1200;\n    });\n    this.sizeObserver.observe(this.$refs['limbo'] as HTMLElement);\n    window.addEventListener('keydown', this.handleKeyDown);\n    this.$nextTick(() => {\n      const container = this.$refs.container as HTMLDivElement;\n      this.game = new Limbo(container);\n      this.game.start();\n    });\n  },\n  beforeCreate() {\n    dataCenter.gameConfiguration.gameId = 3;\n  },\n  unmounted() {\n    this.$nextTick(() => {\n      this.game?.stop();\n    });\n  },\n\n  beforeUnmount() {\n    this.sizeObserver.unobserve();\n    dataCenter.gameConfiguration.gameId = 0;\n    this.isUnmounted = true;\n    window.removeEventListener('keydown', this.handleKeyDown);\n  },\n  methods: {\n    //关闭提示框\n    closePromptBox() {\n      this.isPopconfirmVisible = false;\n    },\n    onTargetChange(value: string) {\n      if (value == '0') {\n        this.target = 0;\n        return;\n      }\n      if (Number(value)) {\n        this.target = Number(value);\n      }\n    },\n    delay(ms: number) {\n      return new Promise((resolve) => setTimeout(resolve, ms));\n    },\n    fire() {\n      this.game?.setFastMode(dataCenter.gameConfiguration.isFast);\n\n      playGameRocket(\n        this.target,\n        Number(this.bettingAmount),\n        dataCenter.wallet.selectId,\n      ).then(async (response) => {\n        if (!response) {\n          this.isStart = false;\n          return;\n        }\n\n        addArrayToLocal('Recent', 3);\n        const result = response.result;\n        const threshold = this.target;\n        this.isWin = response.isWin == 0 ? true : false;\n        // 扣钱\n        dataCenter.wallet.change(\n          dataCenter.wallet.selectId,\n          -this.bettingAmount,\n        );\n        this.game?.fire(result, threshold, {\n          onFired: () => {\n            this.history.unshift({\n              id: Date.now(),\n              value: result.toFixed(2),\n              isWin: this.isWin,\n            });\n            // 更新新的金额\n            dataCenter.wallet.set(dataCenter.wallet.selectId, response.balance);\n          },\n          onFinished: async () => {\n            if (this.isAuto) {\n              await this.delay(800);\n            }\n            if (this.isWin) {\n              this.CurrentAmount += Number(this.bettingAmount);\n              this.TotalAmount += Number(this.bettingAmount);\n\n              if (this.childAutobet.OnWin != '' && this.isAuto) {\n                this.bettingAmount = amountAdd(\n                  this.bettingAmount,\n                  this.childAutobet.OnWin,\n                );\n              }\n            } else {\n              this.CurrentAmount -= Number(this.bettingAmount);\n              this.TotalAmount -= Number(this.bettingAmount);\n              if (this.childAutobet.OnLoss != '' && this.isAuto) {\n                this.bettingAmount = amountAdd(\n                  this.bettingAmount,\n                  this.childAutobet.OnLoss,\n                );\n              }\n            }\n\n            let count = String(Number(this.childAutobet.count));\n            if (this.childAutobet.count != '') {\n              count = String(Number(this.childAutobet.count) - 1);\n              this.childAutobet.count = count;\n            }\n\n            this.downForm();\n            if (count == '0' || count == '-1') {\n              this.childAutobet.count = '';\n            }\n\n            if (this.isStop) {\n              this.isStart = false;\n              this.isStop = false;\n            }\n            if (this.isStart && !this.isUnmounted) {\n              this.fire();\n            }\n          },\n        });\n      });\n      this.isStart = true;\n    },\n    startGame() {\n      if (this.target > 9999.99 || this.target < 1.01) {\n        this.isPopconfirmVisible = true;\n        return;\n      }\n      if (!dataCenter.user.data) {\n        openModal(Login);\n        return;\n      }\n\n      if (\n        this.getBalance() < Number(this.bettingAmount) ||\n        Number(this.bettingAmount) < dataCenter.webConfig.min\n      ) {\n        openModal(WalletButtonPop);\n        return;\n      }\n\n      if (this.isStart) {\n        this.isStop = true;\n      } else {\n        this.fire();\n      }\n    },\n    formatInput(text: string) {\n      this.bettingAmount = text;\n    },\n    modeClick(isAuto: boolean) {\n      this.isAuto = isAuto;\n    },\n    getAutoBetData(value: Configuration) {\n      this.childAutobet.count = value.mechanism[0].input;\n      this.childAutobet.stopOnProfit = value.mechanism[1].input;\n      this.childAutobet.StopOnLoss = value.mechanism[2].input;\n      this.childAutobet.MaxBetAmount = value.mechanism[3].input;\n      this.childAutobet.OnWin = value.WinOrLose[0].input;\n      this.childAutobet.OnLoss = value.WinOrLose[1].input;\n    },\n    updateValue(text: string) {\n      this.bettingAmount = text;\n    },\n    downForm() {\n      this.isStart = this.isAuto;\n      if (this.childAutobet.count != '') {\n        if (Number(this.childAutobet.count) < 1) {\n          this.isStart = false;\n        }\n      }\n      if (\n        this.CurrentAmount >= Number(this.childAutobet.stopOnProfit) &&\n        this.childAutobet.stopOnProfit != ''\n      ) {\n        this.isStart = false;\n      }\n      if (\n        this.CurrentAmount <= Number(this.childAutobet.StopOnLoss) &&\n        this.childAutobet.StopOnLoss != ''\n      ) {\n        this.isStart = false;\n      }\n      if (\n        this.CurrentAmount >= Number(this.childAutobet.MaxBetAmount) &&\n        this.childAutobet.MaxBetAmount != ''\n      ) {\n        this.isStart = false;\n      }\n      if (this.isStop) {\n        this.isStart = false;\n      }\n    },\n    handleKeyDown(event: KeyboardEvent) {\n      if (dataCenter.gameConfiguration.HotkeyEnabled) {\n        const isSpacePressed = event.key === ' ';\n\n        if (isSpacePressed) {\n          event.preventDefault();\n          this.startGame();\n        }\n      }\n    },\n  },\n  created() {\n    dataCenter.gameConfiguration.ruleData = [\n      {\n        icon: require('@/assets/game/limbo/step-1.png'),\n        get text() {\n          return lang.v.limbo_predict;\n        },\n      },\n      {\n        icon: require('@/assets/game/limbo/step-2.png'),\n        get text() {\n          return lang.v.limbo_press;\n        },\n      },\n    ];\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.limbo {\n  opacity: 1;\n  padding: 34px 24px;\n  font-family: Montserrat Variable, sans-serif;\n\n  .content {\n    display: flex;\n    flex-direction: row;\n    align-items: flex-start;\n    justify-content: center;\n\n    .center {\n      display: flex;\n      flex-direction: column;\n      width: 767px;\n\n      .dapps-top-buttons {\n        cursor: pointer;\n        display: flex;\n        justify-content: flex-end;\n        margin-bottom: 12px;\n        margin-top: 2px;\n        width: 100%;\n        gap: 3px;\n      }\n\n      .game {\n        width: 767px;\n        height: 602px;\n        background: url('~@/assets/game/limbo/bg.svg') no-repeat center;\n        background-size: cover;\n        border-radius: 10px;\n        overflow: hidden;\n        position: relative;\n\n        > div {\n          position: absolute;\n          left: 0;\n          top: 0;\n          width: 100%;\n          height: 100%;\n        }\n\n        .front-bg {\n          background: url('~@/assets/game/limbo/up.png') no-repeat center;\n       