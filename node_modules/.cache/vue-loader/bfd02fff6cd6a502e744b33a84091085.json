ass=\"level-title center\">\n        <img src=\"@/assets/reentryReward/576.png\" class=\"title-icon\" />\n        <span class=\"level-reward\"\n          >{{ lang.v.rew_reward\n          }}<span class=\"level-number\">{{ (index ?? 0) + 1 }} </span></span\n        >\n      </div>\n      <div class=\"level-money center\">\n        <div class=\"col-center\">\n          <div class=\"level-value\">{{ rules.initialValue }}R$</div>\n          <div class=\"level-recarge\">\n            {{ lang.v.rew_recarga }} ${{ rules.initialValue }}R\n          </div>\n          <img class=\"level-down\" src=\"@/assets/reentryReward/5815.png\" />\n        </div>\n        <img src=\"@/assets/reentryReward/578.png\" />\n      </div>\n      <div class=\"obter\">\n        <span class=\"obter-title\">{{ lang.v.rew_obter }}</span>\n        <span class=\"obter-number\">{{ rules.obterValue }}</span>\n        <span class=\"obter-currecy\">R$</span>\n      </div>\n      <div class=\"level-submit\">\n        <BtnRed message=\"Deposito\" @click=\"deposito\"></BtnRed>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { defineComponent, inject } from 'vue';\nimport BtnRed from '../ReferralProgram/MyBalance/BtnRed.vue';\nimport { ModalCloseFun, openModal } from '@/manager/view';\nimport WalletButtonPop from '../home/head/popup/WalletButtonPop.vue';\nimport { reentryReward } from '.';\nimport { lang } from '@/lang';\nexport default defineComponent({\n  data() {\n    return {\n      closeFunction: inject('close') as ModalCloseFun,\n      lang,\n    };\n  },\n  methods: {\n    deposito() {\n      reentryReward.selected = this.rules.initialValue;\n      this.closeFunction();\n      openModal(WalletButtonPop);\n    },\n  },\n  props: ['rules', 'index'],\n  components: { BtnRed },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.reentry-level {\n  margin: 5px;\n  padding-left: 1.5%;\n  aspect-ratio: 534 / 726;\n  position: relative;\n}\n.back-img {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  object-fit: fill;\n}\n.level-center {\n  position: relative;\n  height: 100%;\n  padding: 0 10%;\n}\n.level-title {\n  height: 10%;\n  margin-top: 5%;\n}\n.title-icon {\n  height: 100%;\n  object-fit: fill;\n}\n.level-money {\n  margin-top: 20%;\n  gap: 5%;\n}\n.level-money img {\n  width: 40%;\n  object-fit: fill;\n}\n.level-value {\n  font-size: 17px;\n  color: #000;\n}\n.level-recarge {\n  padding: 2px 6px;\n  border-radius: 2px;\n  font-size: 9px;\n  background-color: #000;\n}\n.level-money .level-down {\n  margin-top: 15%;\n  margin-bottom: 10%;\n  width: 20%;\n}\n.obter {\n  width: 85%;\n  padding: 0 5px;\n  margin-left: -7%;\n  background-color: #ef3e52;\n}\n.obter-title {\n  font-size: 12px;\n  margin-right: 8%;\n}\n.obter-number {\n  font-size: 12px;\n}\n.obter-currecy {\n  font-size: 13px;\n}\n.level-submit {\n  position: absolute;\n  bottom: -8px;\n}\n// .level-submit button {\n//   height: 34px;\n// }\n:deep(.button-3d .button-3d__outer) {\n  padding: 1px 20px;\n}\n:deep(.button-3d .button-3d__outer .button-3d__text) {\n  font-size: 11px;\n}\n.level-reward {\n  font-size: 13px;\n  font-weight: 900;\n}\n.level-number {\n  margin-left: 3px;\n  font-size: 18px;\n}\n\n@media only screen and (max-width: 767px) {\n  .reentry-level {\n    flex: none;\n    width: 45%;\n  }\n  .level-reward {\n    font-weight: 900;\n  }\n  .level-number {\n    font-size: 15px;\n  }\n  .level-value {\n    font-size: 14px;\n  }\n  .level-recarge {\n    padding: 1px 4px;\n    border-radius: 2px;\n    font-size: 6px;\n  }\n  .obter-title {\n    font-size: 10px;\n  }\n  .obter-number {\n    font-size: 14px;\n  }\n  .obter-currecy {\n    font-size: 12px;\n  }\n}\n</style>\n"]}]}