v class=\"level\">{{ props.userRefereeInfo?.level }}</div>\n      </div>\n      <div class=\"text\">{{ lang.v.fk_nivel }}</div>\n    </div>\n    <div class=\"right\">\n      <div class=\"amount-header center\">\n        <div class=\"total-page\">\n          <div class=\"amount red\">\n            R$ <Money :value=\"props.userRefereeInfo?.totalOut\"></Money>\n          </div>\n          <div class=\"text\">{{ lang.v.fk_topa }}</div>\n        </div>\n        <div class=\"total-page\">\n          <div class=\"amount white\">\n            R$ <Money :value=\"props.userRefereeInfo?.currentAmount\"></Money>\n          </div>\n          <div class=\"text\">{{ lang.v.fk_togo }}</div>\n        </div>\n      </div>\n      <div class=\"receive\" @click=\"refereeToBalance\">\n        {{ lang.v.fk_trans }}\n      </div>\n\n      <div class=\"tips\">{{ lang.v.fk_valor }}</div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\" setup>\nimport { lang } from '@/lang';\nimport { transferRefereeToBalance } from '@/views/home/api';\nimport { UserRefereeInfoResp } from '@/views/home/api/protocol';\nimport { defineProps } from 'vue';\nimport { tip } from '@/components/warn/warn';\nimport Money from '@/components/Money.vue';\nconst props = defineProps({\n  userRefereeInfo: Object as () => UserRefereeInfoResp,\n});\nfunction refereeToBalance() {\n  if (props.userRefereeInfo?.currentAmount) {\n    transferRefereeToBalance(props.userRefereeInfo?.currentAmount).then(\n      (resp) => {\n        if (!resp) return;\n        tip.success(resp);\n      },\n    );\n  } else {\n    tip.error(lang.v.no_reward);\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.recommendation-table {\n  height: 200px;\n  width: 100%;\n  background: #1c222f;\n  border-radius: 11px;\n  .left-table {\n    background: linear-gradient(\n      180deg,\n      hsla(218, 26%, 19%, 0.3) 0%,\n      hsla(347, 89%, 57%, 0.3) 100%\n    );\n    height: 100%;\n    width: 12%;\n    font-family: Montserrat Alternates, Montserrat Alternates;\n    font-weight: 900;\n  }\n  .recommendation-level {\n    width: 25%;\n    gap: 6px;\n    .icon-level {\n      width: 100%;\n      position: relative;\n      img {\n        width: 50%;\n      }\n      .level {\n        position: absolute;\n        top: 50%;\n        font-weight: bold;\n        font-size: 19px;\n      }\n    }\n    .text {\n      font-size: 12px;\n      color: #acacac;\n    }\n  }\n  .right {\n    flex: 1;\n    padding: 0 20px;\n    .amount-header {\n      gap: 60px;\n      .total-page {\n        text-align: center;\n        font-weight: bold;\n        .amount {\n          font-size: 34px;\n        }\n        .red {\n          color: #ed0650;\n        }\n        .white {\n          color: white;\n        }\n        .text {\n          color: #acacac;\n          font-size: 13px;\n        }\n      }\n    }\n    .receive {\n      font-weight: bold;\n      font-size: 17px;\n      color: #fff;\n      background: linear-gradient(1turn, #0c5eff 0.8%, #1398f9);\n      box-shadow: 0 3px 28px #2283f666, inset 0 3px 3px #ffffff4d;\n      border-radius: 5px;\n      text-align: center;\n      padding: 5px 0;\n      margin-top: 20px;\n      cursor: pointer;\n    }\n    .tips {\n      text-align: center;\n      color: #acacac;\n      font-size: 10px;\n      margin-top: 10px;\n    }\n  }\n}\n@media only screen and (max-width: 767px) {\n  .recommendation-table {\n    height: 150px;\n    .left-table {\n      width: 15%;\n      font-size: 13px;\n    }\n    .recommendation-level {\n      width: 25%;\n      .icon-level {\n        .level {\n          font-size: 15px;\n        }\n      }\n      .text {\n        margin-top: 10px;\n        font-size: 9px;\n      }\n    }\n    .right {\n      padding: 0 10px;\n      .amount-header {\n        gap: 20px;\n        .total-page {\n          .amount {\n            font-size: 17px;\n          }\n          .text {\n            font-size: 10px;\n          }\n        }\n      }\n      .receive {\n        font-size: 9px;\n        margin-top: 10px;\n      }\n      .tips {\n        font-size: 8px;\n      }\n    }\n  }\n}\n</style>\n"]}]}