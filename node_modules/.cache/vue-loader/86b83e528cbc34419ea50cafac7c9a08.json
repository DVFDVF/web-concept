<preferential-time :time=\"countdownTime.hour\"></preferential-time>\n              :\n              <preferential-time :time=\"countdownTime.min\"></preferential-time>\n              :\n              <preferential-time :time=\"countdownTime.sec\"></preferential-time>\n            </div>\n            <btn-red message=\"Deposito\" @click=\"recharge\"></btn-red>\n          </div>\n          <div class=\"inner-text countdown-text\">\n            {{ lang.v.par_dentro }}\n          </div>\n        </div>\n      </div>\n    </template>\n    <template v-slot:footer></template>\n  </PopUpNotification>\n</template>\n<script lang=\"ts\">\nimport { ModalCloseFun, openModal } from '@/manager/view';\nimport PopUpNotification from '@/views/popUp/PopUpNotification.vue';\nimport dayjs from 'dayjs';\nimport { defineComponent, inject } from 'vue';\nimport BtnRed from '../../center/BtnRed.vue';\nimport WalletButtonPopVue from '../../head/popup/WalletButtonPop.vue';\nimport PreferentialTime from './preferentialTime.vue';\nimport { lang } from '@/lang';\nimport wheelPop from '@/games/wheel/wheelPop.vue';\nimport duration from 'dayjs/plugin/duration';\ndayjs.extend(duration);\nexport default defineComponent({\n  data() {\n    return {\n      lang,\n      type: 'preferential',\n      countdownTime: {\n        hour: '00',\n        min: '00',\n        sec: '00.00',\n      },\n      intervalId: 0,\n      closeFunction: inject('close') as ModalCloseFun,\n    };\n  },\n  methods: {\n    recharge() {\n      this.closeFunction();\n      openModal(WalletButtonPopVue, { type: this.type });\n    },\n    updateCountdown(): void {\n      //时区似乎并不相符\n      // const target = dayjs(targetTime).add(2, 'hour');\n      //单次显示，只在当前时间进行计算\n      const target = dayjs().add(2, 'hour');\n      this.intervalId = setInterval(() => {\n        const now = dayjs();\n        const diff = target.diff(now);\n        if (diff <= 0) {\n          clearInterval(this.intervalId);\n          return;\n        }\n        const duration = dayjs.duration(diff);\n        this.countdownTime.hour = duration.hours().toString().padStart(2, '0');\n        this.countdownTime.min = duration.minutes().toString().padStart(2, '0');\n        let seconds = duration.seconds().toString().padStart(2, '0');\n        let milliseconds = duration\n          .milliseconds()\n          .toString()\n          .padStart(3, '0')\n          .slice(0, 2); //只保留前两位\n        this.countdownTime.sec = `${seconds}.${milliseconds}`;\n      }, 1);\n    },\n  },\n  components: { PopUpNotification, PreferentialTime, BtnRed },\n  mounted() {\n    this.updateCountdown();\n  },\n  beforeUnmount() {\n    clearInterval(this.intervalId);\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.inner-body {\n  width: 100%;\n  margin: 50px 0;\n}\n.inner-img {\n  width: 80px;\n  height: 80px;\n}\n.inner-title {\n  font-size: 26px;\n  font-weight: 700;\n  color: #ffffff;\n  line-height: 40px;\n  margin-top: 10px;\n}\n.inner-text {\n  font-size: 18px;\n  font-weight: 300;\n  color: #ffffff;\n  line-height: 30px;\n}\n.inner-countdown {\n  // width: calc(100% + 40px);\n  margin-top: 50px;\n  padding-bottom: 40px;\n  background: #1c2638;\n  border-radius: 11px;\n}\n.countdown-text {\n  font-size: 12px;\n  padding: 0 20px;\n  line-height: 15px;\n}\n.count-time {\n  gap: 8px;\n  font-size: 16px;\n  font-weight: 900;\n}\n.countdow-row {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  width: 100%;\n  padding: 20px;\n}\n:deep(.button-3d) {\n  margin-top: 0 !important;\n  width: 100px;\n}\n:deep(.button-3d-md):not(.button-3d-only-icon) .button-3d-outer {\n  padding: 6px;\n}\n.pop-up {\n  height: 500px !important;\n}\n@media only screen and (max-width: 767px) {\n  .inner-body {\n    margin-top: 100px;\n  }\n  .inner-countdown {\n    width: calc(100% + 40px);\n    margin-top: 40px;\n  }\n  .pop-up {\n    height: 80vh !important;\n  }\n  .inner-img {\n    width: 60px;\n    height: 60px;\n  }\n  .inner-title {\n    font-size: 20px;\n    margin-top: 0;\n  }\n  .inner-text {\n    font-size: 14px;\n  }\n}\n</style>\n"]}]}