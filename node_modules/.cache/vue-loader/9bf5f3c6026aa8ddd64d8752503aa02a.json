s: ['0', '1'],\n      logosList: [\n        [\n          {\n            logo1: '/fury/MonetaryStatistics/bitcoin.avif',\n            logo2: '/fury/MonetaryStatistics/bitcoin_gray.avif',\n            lingk: 'https://bitcoin.org/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/ethereum.avif',\n            logo2: '/fury/MonetaryStatistics/ethereum_gray.avif',\n            lingk: 'https://ethereum.org/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/tether.avif',\n            logo2: '/fury/MonetaryStatistics/tether_gray.avif',\n            lingk: 'https://tether.to/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/tron.avif',\n            logo2: '/fury/MonetaryStatistics/tron_gray.avif',\n            lingk: 'https://tron.network/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/binance.avif',\n            logo2: '/fury/MonetaryStatistics/binance_gray.avif',\n            lingk: 'https://accounts.binance.com/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/biswap.avif',\n            logo2: '/fury/MonetaryStatistics/biswap_gray.avif',\n            lingk: 'https://biswap.org/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/babyswap.avif',\n            logo2: '/fury/MonetaryStatistics/babyswap_gray.avif',\n            lingk: 'https://home.babyswap.finance/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/boggedfinance.avif',\n            logo2: '/fury/MonetaryStatistics/boggedfinance_gray.avif',\n            lingk: 'https://app.bogged.finance/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/cryptorank.avif',\n            logo2: '/fury/MonetaryStatistics/cryptorank_gray.avif',\n            lingk: 'https://cryptorank.io/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/coinhub.avif',\n            logo2: '/fury/MonetaryStatistics/coinhub_gray.avif',\n            lingk: 'https://www.coinhub.org/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/apeswap.avif',\n            logo2: '/fury/MonetaryStatistics/apeswap_gray.avif',\n            lingk: 'https://apeswap.finance/',\n          },\n        ],\n        [\n          {\n            logo1: '/fury/MonetaryStatistics/cointiger.avif',\n            logo2: '/fury/MonetaryStatistics/cointiger_gray.avif',\n            lingk: 'https://m.cointiger.com/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/coin98.avif',\n            logo2: '/fury/MonetaryStatistics/coin98_gray.avif',\n            lingk: 'https://coin98.com/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/onto.avif',\n            logo2: '/fury/MonetaryStatistics/onto_gray.avif',\n            lingk: 'https://on.to/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/vulkania.avif',\n            logo2: '/fury/MonetaryStatistics/vulkania_gray.avif',\n            lingk: 'https://vulkania.io/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/airnfts.avif',\n            logo2: '/fury/MonetaryStatistics/airnfts_gray.avif',\n            lingk: 'https://www.airnfts.com/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/hotbit.avif',\n            logo2: '/fury/MonetaryStatistics/hotbit_gray.avif',\n            lingk: 'https://hotbit.zendesk.com/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/arken_finance.avif',\n            logo2: '/fury/MonetaryStatistics/arken_finance_gray.avif',\n            lingk: 'https://arken.finance/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/dappcasino.avif',\n            logo2: '/fury/MonetaryStatistics/dappcasino_gray.avif',\n            lingk: 'https://dappcasino.io/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/klever.avif',\n            logo2: '/fury/MonetaryStatistics/klever_gray.avif',\n            lingk: 'https://klever.io/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/chengelly.avif',\n            logo2: '/fury/MonetaryStatistics/chengelly_gray.avif',\n            lingk: 'https://changelly.com/',\n          },\n          {\n            logo1: '/fury/MonetaryStatistics/coingecko.avif',\n            logo2: '/fury/MonetaryStatistics/coingecko_gray.avif',\n            lingk: 'https://www.coingecko.com/',\n          },\n        ],\n      ],\n    };\n  },\n  methods: {\n    toggleHover(value: boolean, item: string) {\n      this.isHovered = value;\n      this.selectImg = item;\n    },\n    /**跳转详情 */\n    checkDetails(item: string) {\n      window.open(item);\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n:deep(.slick-slide) {\n  text-align: center;\n  overflow: hidden;\n  padding-bottom: 20px;\n  margin-bottom: 18px;\n  border-bottom: 1px solid rgba(85, 101, 126, 0.15);\n}\n\n:deep(.slick-slide h3) {\n  color: #fff;\n}\n.content {\n  display: flex;\n  flex-direction: row;\n}\n</style>\n"]}]}