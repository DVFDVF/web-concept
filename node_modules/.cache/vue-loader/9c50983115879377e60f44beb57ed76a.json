odeClick', auto);\n    },\n    openGameIntroduction() {\n      openModal(GameIntroduction, { class: 'test' });\n    },\n  },\n  props: ['isAuto', 'isStart'],\n});\n</script>\n\n<style lang=\"less\" scoped>\n.autobet-btns {\n  display: flex;\n  justify-content: center;\n  margin-bottom: 30px;\n  margin-top: 30px;\n  position: relative;\n  width: 100%;\n}\n\n.autobet-btns-btn {\n  background-color: transparent;\n  border: none;\n  border-bottom: 1px solid #55657e;\n  border-radius: 0;\n  color: #55657e;\n  font-size: 14px;\n  font-weight: 600;\n  margin-bottom: 2px;\n  min-width: 65px;\n  padding: 2px 5px;\n  position: relative;\n  text-align: center;\n  text-transform: uppercase;\n  transition: all 0.3s;\n}\n\n.autobet-btns-btn.active {\n  border-bottom: 3px solid #2283f6;\n  color: #fff;\n  margin-bottom: 0;\n}\n\n.autobet-btns-btn:disabled {\n  cursor: not-allowed;\n}\n\n.autobet-btns-info-btn {\n  align-items: center;\n  background: #273141;\n  border-radius: 50%;\n  display: flex;\n  height: 19px;\n  justify-content: center;\n  position: absolute;\n  right: -20px;\n  top: 3px;\n  width: 19px;\n}\n\n.autobet-btns-info-btn:hover svg {\n  fill: #fff;\n}\n\n.autobet-btns-info-ico {\n  fill: #778cae;\n  display: block;\n  height: 10px;\n  margin: auto;\n  width: 10px;\n}\n\n.autobet-btns .rtp-wrap {\n  align-items: center;\n  background: #161f2c;\n  border-radius: 32px;\n  display: flex;\n  font-size: 14px;\n  font-weight: 600;\n  height: 36px;\n  letter-spacing: -0.02em;\n  line-height: 18px;\n  margin-top: -4px;\n  padding: 6px 12px;\n  position: absolute;\n  right: 0;\n}\n\n.autobet-btns .rtp-wrap-value {\n  color: #93acd3;\n}\n\n.autobet-btns .rtp-wrap-text {\n  color: #55657e;\n}\n.autobet-btns {\n  display: flex;\n  justify-content: center;\n  margin-bottom: 30px;\n  margin-top: 30px;\n  position: relative;\n}\n.autobet-btns-btn {\n  background-color: transparent;\n  border: none;\n  border-bottom: 1px solid #55657e;\n  border-radius: 0;\n  color: #55657e;\n  fo