efineComponent({\n  data() {\n    return {\n      lang,\n      dataCenter,\n      introduce: [\n        {\n          key: 0,\n          icon: require('@/assets/install/rocket.png'),\n          get title() {\n            return lang.v.ph_lightning;\n          },\n          get text() {\n            return lang.v.ph_enjoy.replace(\n              '{siteName}',\n              dataCenter.websiteName_Medium,\n            );\n          },\n        },\n        {\n          key: 1,\n          icon: require('@/assets/install/bell.png'),\n          get title() {\n            return lang.v.ph_firn;\n          },\n          get text() {\n            return lang.v.ph_miss;\n          },\n        },\n        {\n          key: 2,\n          icon: require('@/assets/install/phone-in-hand.png'),\n          get title() {\n            return lang.v.ph_quic;\n          },\n          get text() {\n            return lang.v.ph_withs.replace(\n              '{siteName}',\n              dataCenter.websiteName_Medium,\n            );\n          },\n        },\n      ],\n    };\n  },\n  methods: {},\n  components: {},\n});\n</script>\n\n<style lang=\"less\" scoped>\n.section-features {\n  margin-top: 20px;\n  margin-bottom: 100px;\n  color: white;\n}\n.section-inner {\n  margin-inline: auto;\n  max-width: 1168px;\n  padding-inline: 40px;\n}\n.features-list {\n  gap: 16px;\n  margin-right: 30px;\n  align-items: stretch;\n}\n.features-list-item {\n  flex-basis: 33%;\n}\n.feature {\n  align-items: center;\n  align-self: stretch;\n  background: linear-gradient(180deg, #1c2532, #1c253200);\n  border-radius: 14px;\n  display: flex;\n  flex: 1 0 0;\n  flex-direction: column;\n  gap: 9px;\n  padding: 90px 24px 64px 24px;\n  position: relative;\n}\n.feature-img {\n  left: 50%;\n  position: absolute;\n  top: -52px;\n  transform: translate(-50%);\n  width: 170px;\n}\n.feature-title {\n  font-size: 20px;\n  font-style: normal;\n  font-variant-numeric: lining-nums tabular-nums;\n  font-weight: 700;\n  letter-spacing: -0.4px;\n  line-height: 26px;\n  margin: 0 0 5px;\n  text-align: center;\n}\n.feature-text {\n  color: #93acd3;\n  font-size: 16px;\n  font-weight: 500;\n  line-height: 24px;\n  margin-bottom: 0;\n  text-align: center;\n}\n@media only screen and (max-width: 767px) {\n  .features-list {\n    flex-direction: column;\n    margin: 0 20px;\n  }\n  .features-list-item {\n    flex-basis: 100%;\n  }\n}\n</style>\n"]}]}