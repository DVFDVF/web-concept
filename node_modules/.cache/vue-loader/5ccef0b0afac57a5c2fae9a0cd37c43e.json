svg>\n      </button>\n      <!-- 左边 -->\n\n      <div class=\"wheel-ground\">\n        <div\n          :class=\"[\n            'content',\n            {\n              'wheel-center-mobile': layout.isMobile,\n              'wheel-center-start': wheel.isStartWheel && !layout.isMobile,\n              'wheel-center-mobile-start':\n                wheel.isStartWheel && layout.isMobile,\n            },\n          ]\"\n          ref=\"wheelCent\"\n        ></div>\n      </div>\n\n      <div\n        :class=\"[\n          'wheel-tray',\n          {\n            'wheel-tray-computer': !layout.isMobile,\n            'wheel-tray-mobile': layout.isMobile,\n          },\n        ]\"\n        ref=\"watwheel\"\n        v-show=\"!wheel.isFunWin\"\n      >\n        <img\n          class=\"wheel-wapl-imgs\"\n          src=\"~@/assets/game/wheel/wheel-1.png\"\n          ref=\"wheel\"\n        />\n        <img class=\"wheel-wapl-img\" src=\"@/assets/game/wheel/arrow.webp\" />\n        <div v-for=\"i in 8\" v-bind:key=\"i\" class=\"wheel-wapl\">\n          <img :class=\"wheel.wheelClass(i)\" :src=\"wheel.getWheelSrc(i)\" />\n        </div>\n\n        <div class=\"fury-wheel-btn\">\n          <div class=\"fury-wbtn-wrap\">\n            <div class=\"fury-wbtn-content\">\n              <img\n                class=\"fury-wbtn-img\"\n                src=\"@/assets/game/wheel/btn-text.png\"\n                @click=\"\n                  wheel.isTiming\n                    ? null\n                    : wheel.startWheel(\n                        $refs.watwheel as HTMLElement,\n                        $refs.wheelCent as HTMLElement,\n                      )\n                \"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- 右边 -->\n      <div\n        :class=\"[\n          'wheel-fury',\n          {\n            'wheel-fury-mobile': layout.isMobile,\n            'wheel-fury-web': !layout.isMobile,\n          },\n        ]\"\n        ref=\"wheelFury\"\n        v-if=\"!wheel.isWheeling\"\n      >\n        <img style=\"width: 328px\" src=\"@/assets/game/wheel/racoon.png\" />\n        <button\n          v-if=\"!wheel.isTiming\"\n          class=\"wheel-spin\"\n          @click=\"\n            wheel.startWheel(\n              $refs.watwheel as HTMLElement,\n              $refs.wheelCent as HTMLElement,\n            )\n          \"\n        >\n          <span>{{ lang.v.zp_spinWin }}</span>\n        </button>\n        <div class=\"wheel-cliver\" v-if=\"wheel.isTiming\">\n          <span>{{ lang.v.wh_start }}</span>\n          <TimeWidget\n            :hours=\"wheel.hours\"\n            :minutes=\"wheel.minutes\"\n            :seconds=\"wheel.seconds\"\n          />\n        </div>\n        <button class=\"wheel-recent\" @click=\"wheel.displayRanking('530px')\">\n          <span>{{ lang.v.zp_recentBigWin }}</span>\n        </button>\n      </div>\n      <!-- 下边 -->\n      <div class=\"wheel-footer\" v-if=\"!wheel.isWheeling\">\n        <div class=\"wheel-bit\">\n          <div\n            v-for=\"(awardVaule, index) in reversedAwardData\"\n            v-bind:key=\"index\"\n            class=\"wheel-winner\"\n          >\n            <Image\n              class=\"wheel-winn-icon\"\n              :src=\"wheel.getCuurrencyIcon(awardVaule.currencyId)\"\n            />\n            <span class=\"wheel-winner-text\">{{ awardVaule.value }}</span>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <wheelSettlement\n      v-show=\"wheel.isFunWin\"\n      :-setlements=\"wheel.WheelRespData\"\n    />\n    <div class=\"wheel-history\">\n      <wheelRanking\n        ref=\"ranking\"\n        :right-number=\"wheel.isDisplayRanking\"\n        @button-out=\"displayRanking('0px')\"\n      />\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { computed, defineComponent, inject, ref } from 'vue';\n\n//win 最后页面\nimport wheelRanking from '@/games/wheel/wheelRanki