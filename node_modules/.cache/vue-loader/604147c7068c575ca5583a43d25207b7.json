ntry: lang.v.country_TG },\n  { key: 676, country: lang.v.country_TO },\n  { key: 1868, country: lang.v.country_TT },\n  { key: 216, country: lang.v.country_TN },\n  { key: 90, country: lang.v.country_TR },\n  { key: 993, country: lang.v.country_TM },\n  { key: 688, country: lang.v.country_TV },\n  { key: 256, country: lang.v.country_UG },\n  { key: 380, country: lang.v.country_UA },\n  { key: 971, country: lang.v.country_AE },\n  { key: 44, country: lang.v.country_GB },\n  { key: 1, country: lang.v.country_US },\n  { key: 598, country: lang.v.country_UY },\n  { key: 998, country: lang.v.country_UZ },\n  { key: 678, country: lang.v.country_VU },\n  { key: 58, country: lang.v.country_VE },\n  { key: 84, country: lang.v.country_VN },\n  { key: 967, country: lang.v.country_YE },\n  { key: 260, country: lang.v.country_ZM },\n  { key: 263, country: lang.v.country_ZW },\n];\nfunction forgotClick() {\n  openModal(ResetPassword);\n}\nfunction isValidEmail(email: string): boolean {\n  const emailRegex = /^[^\\s@]+@[^\\s@]+\\.[^\\s@]+$/;\n  return emailRegex.test(email);\n}\n//手机注册\nfunction isValidMobile(email: string): boolean {\n  return phoneValid(email);\n}\nfunction isValidPassword(password: string): boolean {\n  const lengthRegex = /.{8,}/;\n  const uppercaseRegex = /[A-Z]/;\n  const lowercaseRegex = /[a-z]/;\n  const numberRegex = /\\d/;\n\n  return (\n    lengthRegex.test(password) &&\n    uppercaseRegex.test(password) &&\n    lowercaseRegex.test(password) &&\n    numberRegex.test(password)\n    // password.trim() !== ''\n  );\n}\n\nfunction registerAccount() {\n  if (!isAgreeService.value) {\n    return;\n  }\n  loginBtn.value.type = '1';\n  let code = shareUserId(false, codeText.value);\n  //生成随机头像\n  const randomInteger = Math.floor(Math.random() * 9) + 1;\n  const randomString = randomInteger.toString();\n  let pixel = localStorage.getItem('pixel');\n  let channelId = localStorage.getItem('channelId');\n  if (selectLoginMode.value == 0) {\n    registerByPhone(\n      selectLoginMode.value,\n      emailText.value,\n      passwordText.value,\n      randomString,\n      countryName.value.toString(),\n      code ? code : undefined,\n      pixel ? pixel : undefined,\n      channelId ? channelId : undefined,\n    ).then((response) => {\n      loginBtn.value.type = '0';\n      if (response === 'success') {\n        reportingEvents('CompleteRegistration', pixel);\n        loginFuc(true);\n      }\n    });\n  } else {\n    regiter(\n      selectLoginMode.value,\n      emailText.value,\n      passwordText.value,\n      randomString,\n      countryName.value.toString(),\n      emailCodeText.value,\n      code,\n      pixel ? pixel : undefined,\n      channelId ? channelId : undefined,\n    ).then((response) => {\n      loginBtn.value.type = '0';\n      if (response === 'success') {\n        reportingEvents('CompleteRegistration', pixel);\n        loginFuc(true);\n      }\n    });\n  }\n}\n\n//验证码返回上一级\nfunction goBack() {\n  isInputCode.value = false;\n}\n//重发验证吗\nfunction sendAgain() {\n  codeSend();\n}\n//验证码\nfunction codeSend() {\n  getCode(emailText.value, 1).then((response) => {\n    if (response) {\n      isInputCode.value = true;\n    }\n  });\n}\n\nfunction submitForm() {\n  if (isValidPassword(passwordText.value)) {\n    if (selectLoginMode.value == 0 && isValidMobile(emailText.value)) {\n      if (isLogin.value) {\n        loginBtn.value.type = '1';\n        loginFuc(false);\n      } else {\n        //手机注册跳过收验证码环节\n        registerAccount();\n      }\n    } else if (selectLoginMode.value == 1 && isValidEmail(emailText.value)) {\n      if (isLogin.value) {\n        loginBtn.value.type = '1';\n        loginFuc(false);\n      } else {\n        codeSend();\n      }\n    } else {\n      loginBtn.value.type = '2';\n    }\n  } else {\n    loginBtn.value.type = '2';\n  }\n}\nfunction loginFuc(isRegister: boolean) {\n  let client = localStorage.getItem('client');\n  if (selectLoginMode.value == 0) {\n    loginByPhone(\n      emailText.value,\n      passwordText.value,\n      client ? client : undefined,\n    ).then((response) => {\n      if (!isRegister) {\n        loginBtn.value.type = '0';\n      }\n      if (!response) return;\n      // 登陆事件\n      reportingEvents('login', response.pixelId);\n      tokenController.update(response.token, response.freshToken);\n      dataCenter.user.update(response);\n      getUserBalance().then((wallet) => {\n        dataCenter.wallet.update(wallet);\n      });\n      closeFunction?.();\n      if (isRegister) {\n        openModal(preferentialVue);\n      } else {\n        //九宫格广告\n        gridNineAdvertisingIsShow();\n      }\n      //初始化活动\n      gameActive.init();\n    });\n  } else {\n    login(\n      emailText.value,\n      passwordText.value,\n      client ? client : undefined,\n    ).then((response) => {\n      if (!isRegister) {\n        loginBtn.value.type = '0';\n      }\n      if (!response) return;\n      // 登陆事件\n      reportingEvents('login', response.pixelId);\n      tokenController.update(response.token, response.freshToken);\n      dataCenter.user.update(response);\n      getUserBalance().then((wallet) => {\n        dataCenter.wallet.update(wallet);\n      });\n      closeFunction?.();\n      if (isRegister) {\n        openModal(preferentialVue);\n      } else {\n        //九宫格广告\n        gridNineAdvertisingIsShow();\n      }\n      //初始化活动\n      gameActive.init();\n    });\n  }\n}\nfunction updatecountry(newValue: number) {\n  countryName.value = newValue;\n}\nfunction inputContent(key: string, text: string) {\n  if (key == 'email' || key == 'mobile') {\n    emailText.value = text;\n  }\n  if (key == 'password') {\n    passwordText.value = text;\n  }\n\n  if (key == 'code') {\n    codeText.value = text;\n  }\n}\n/**登陆方式 */\nfunction selectModeItem(key: number) {\n  if (selectLoginMode.value == key) {\n    return;\n  }\n  emailText.value = '';\n  selectLoginMode.value = key;\n  if (key == 0) {\n    list[0] = mobileItem;\n  } else if (key == 1) {\n    list[0] = emailItem;\n  }\n}\n//获取邮件验证码\nfunction onChange(codeArr: string[]) {\n  emailCodeText.value = codeArr.join('');\n}\nwatch(\n  () => {\n    return emailText.value;\n  },\n  () => {\n    loginBtn.value.type = '0';\n  },\n);\nwatch(\n  () => {\n    return passwordText.value;\n  },\n  () => {\n    loginBtn.value.type = '0';\n  },\n);\n\nfunction btnClick(isClickLogin: boolean) {\n  isLogin.value = isClickLogin;\n  if (isClickLogin) {\n    loginBtn.value.text = 'Login';\n  } else {\n    loginBtn.value.text = 'Create account';\n  }\n}\nfunction OptionalClick() {\n  isOptional.value = !isOptional.value;\n}\n</script>\n<style lang=\"less\" scoped>\n.container {\n  display: flex;\n  min-width: 547px;\n  border-radius: 12px;\n  overflow: hidden;\n\n  .left,\n  .right {\n    flex: 1;\n  }\n\n  .right {\n    background-color: #111923;\n    color: #55657e;\n    width: 364px;\n    /* height: 547px; */\n    padding: 24px;\n\n    .btn-content {\n      width: 188px;\n      height: 44px;\n      padding: 4px;\n      background-color: #2a3546;\n      border-radius: 8px;\n      display: flex;\n      justify-content: space-between;\n\n      .btn {\n        width: 88px;\n        height: 36px;\n        border: none;\n        border-radius: 8px;\n        margin: 0;\n        cursor: pointer;\n\n        .tab-label {\n          font-size: 12px;\n          line-height: 16px;\n        }\n      }\n    }\n\n    .other {\n      margin-top: 10px;\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n\n      .login-signup-or {\n        font-size: 12px;\n        font-weight: 500;\n        letter-spacing: 0.06em;\n        line-height: 150%;\n        margin-bottom: 0;\n        width: 40px;\n        text-align: center;\n        text-transform: uppercase;\n      }\n\n      .line {\n        background-color: #1c2532;\n        height: 1px;\n        flex: 1;\n      }\n    }\n\n    .input {\n      margin-top: 10px;\n      display: flex;\n      flex-dire