href=\"/svg/sprite.svg#icon-battle\"></use>\n          </svg>\n          <p class=\"battle-name\">20k {{ lang.v.dice_daily }}</p>\n        </div> -->\n      </div>\n    </div>\n    <BoardIn\n      :number=\"number\"\n      :is-roll-under=\"isRollUnder\"\n      :trans=\"trans\"\n      :lucky-number=\"luckyNumber\"\n    ></BoardIn>\n    <RollControls\n      :number=\"number\"\n      :is-start=\"isStart\"\n      :multiplier=\"multiplier\"\n      :returnRate=\"returnRate\"\n      @RollControlsData=\"RollControlsData\"\n    ></RollControls>\n    <Slider\n      @SliderNumber=\"SliderNumber\"\n      :min-number=\"minNumber\"\n      :max-number=\"maxNumber\"\n      :is-start=\"isStart\"\n      :is-roll-under=\"isRollUnder\"\n    >\n    </Slider>\n    <GameForm\n      :childAutobet=\"childAutobet\"\n      :is-start=\"isStart\"\n      :is-enable=\"isEnable\"\n      :betting-amount=\"bettingAmount\"\n      :is-auto=\"isAuto\"\n      :returnRate=\"returnRate\"\n      @startGame=\"startGame\"\n      @getLuckyNumber=\"getLuckyNumber\"\n      @formatInput=\"formatInput\"\n      @updateValue=\"updateValue\"\n    >\n    </GameForm>\n    <DiceGameMode\n      :is-auto=\"isAuto\"\n      @modeClick=\"modeClick\"\n      :is-start=\"isStart\"\n    ></DiceGameMode>\n    <Autobet\n      @getAutoBetData=\"getAutoBetData\"\n      :is-start=\"isStart\"\n      v-show=\"isAuto\"\n      :auto-count=\"childAutobet.count\"\n    >\n    </Autobet>\n  </div>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { lang } from '@/lang';\nimport { soundManager } from '@/manager/SoundManager';\nimport { openModal } from '@/manager/view';\nimport { addArrayToLocal } from '@/utils';\nimport DicePlace from '@/views/game/dice/audio/dice-place.mp3';\nimport DiceWin from '@/views/game/dice/audio/dice-win.mp3';\nimport WalletButtonPop from '@/views/home/head/popup/WalletButtonPop.vue';\nimport LoginWidget from '@/views/home/login/Login.vue';\nimport { defineComponent } from 'vue';\nimport { playGame } from './api';\nimport Autobet from './Autobet.vue';\nimport BoardIn from './BoardIn.vue';\nimport DiceGameMode from './DiceGameMode.vue';\nimport GameForm from './GameForm.vue';\nimport { Configuration, NumberRecordData } from './interface';\nimport LastBets from './LastBets.vue';\nimport RollControls from './RollControls.vue';\nimport Slider from './Slider.vue';\nimport TopIcon from './TopIcon.vue';\nimport { formatMoney } from '@/utils/currency';\nimport { amountAdd } from '@/games/methods';\nexport default defineComponent({\n  setup(): { getBalance: () => number } {\n    return {\n      getBalance: () => dataCenter.wallet.getBalance(),\n    };\n  },\n  data() {\n    return {\n      lang,\n      number: 50,\n      multiplier: dataCenter.webConfig.profitRate * 100 ?? 99.07,\n      amount: 200,\n      minNumber: 1,\n      maxNumber: 95,\n      trans: 50,\n      luckyNumber: 0,\n      currentAmount: 0,\n      bettingAmount: '10.00',\n      isRollUnder: true,\n      isStart: false,\n      isEnable: true,\n      isAuto: false,\n      isWin: false,\n      numberList: [] as NumberRecordData[],\n      childAutobet: {\n        count: '',\n        stopOnProfit: '',\n        StopOnLoss: '',\n        MaxBetAmount: '',\n        OnWin: '',\n        OnLoss: '',\n        onWinisReset: true,\n        OnLessisReset: true,\n      },\n\n      topIcons: [\n        // 统计暂不需要\n        // {\n        //   icon: '/svg/sprite.svg#icon-statistic',\n        //   get text() {\n        //     return lang.v.dice_statictic;\n        //   },\n        //   key: 'Statistic',\n        // },\n        {\n          icon: '/svg/sprite.svg#icon-speed',\n          get text() {\n            return lang.v.dice_fast;\n          },\n          key: 'Fast',\n        },\n        {\n          icon: '/svg/sprite.svg#icon-fairness',\n          get text() {\n            return lang.v.dice_fairness;\n          },\n          key: 'Fairness',\n        },\n        {\n          icon: '/svg/sprite.svg#icon-rules',\n          get text() {\n            return lang.v.dice_rules;\n          },\n          key: 'Rules',\n        },\n        {\n          icon: '/svg/sprite.svg#icon-sound-on',\n          text: 'sound',\n        },\n        {\n          icon: '/svg/sprite.svg#icon-hotkeys',\n          get text() {\n            return lang.v.dice_hotkeys;\n          },\n          key: 'Hotkeys',\n        },\n      ],\n      topNumber: ['5x', '5x', '5x', '5x', '5x', '5x', '5x', '5x'],\n    };\n  },\n  created() {\n    dataCenter.gameConfiguration.ruleData = [\n      {\n        icon: require('@/assets/dice/step-1.webp'),\n        get text() {\n          return lang.v.dice_ru_placeabet;\n        },\n      },\n      {\n        icon: require('@/assets/dice/step-2.webp'),\n        get text() {\n          return lang.v.dice_ho_randomNumberGenerator;\n        },\n      },\n    ];\n  },\n  beforeUnmount() {\n    this.isStart = false;\n  },\n  computed: {\n    returnRate() {\n      return (this as any).caculateRate();\n    },\n  },\n  methods: {\n    caculateRate() {\n      // 回报率\n      return this.isRollUnder\n        ? (this.multiplier / this.number).toFixed(4)\n        : (this.multiplier / (this.maxNumber - this.number + 1)).toFixed(4);\n    },\n    SliderNumber(number: number) {\n      this.number = number;\n    },\n    RollControlsData(\n      minNumber: number,\n      maxNumber: number,\n      isRollUnder: boolean,\n    ) {\n      this.minNumber = minNumber;\n      this.maxNumber = maxNumber;\n      this.isRollUnder = isRollUnder;\n    },\n    getAutoBetData(value: Configuration) {\n      this.childAutobet.count = value.mechanism[0].input;\n      this.childAutobet.stopOnProfit = value.mechanism[1].input;\n      this.childAutobet.StopOnLoss = value.mechanism[2].input;\n      this.childAutobet.MaxBetAmount = value.mechanism[3].input;\n      this.childAutobet.OnWin = value.WinOrLose[0].input;\n      this.childAutobet.OnLoss = value.WinOrLose[1].input;\n    },\n    async startGame() {\n      if (!dataCenter.user.data) {\n        openModal(LoginWidget);\n        return;\n      }\n\n      if (\n        this.getBalance() < Number(this.bettingAmount) ||\n        Number(this.bettingAmount) < dataCenter.webConfig.min\n      ) {\n        openModal(WalletButtonPop);\n        return;\n      }\n      this.isStart = !this.isStart;\n      this.currentAmount = 0;\n      this.startGamePlay();\n    },\n    startGamePlay() {\n      if (this.isStart) {\n        if (this.childAutobet.count != '') {\n          this.childAutobet.count = String(Number(this.childAutobet.count) - 1);\n        }\n\n        playGame(\n          this.number,\n          this.isRollUnder ? 0 : 1,\n          Number(this.bettingAmount),\n          dataCenter.wallet.selectId,\n        ).then(async (response) => {\n          if (!response) {\n            this.isStart = false;\n            return;\n          }\n          addArrayToLocal('Recent', 1);\n\n          this.trans = 2;\n          //播放音效\n          soundManager.play(DicePlace);\n          // 扣钱\n          dataCenter.wallet.change(\n            dataCenter.wallet.selectId,\n            -this.bettingAmount,\n          );\n          this.isWin = response.isWin == 0 ? true : false;\n          //判断是否为快速模式\n          if (!dataCenter.gameConfiguration.isFast) {\n            //判断数字间是否小于30\n            let isLess =\n              response.betResult - this.luckyNumber < 30 &&\n              response.betResult - this.luckyNumber > -30;\n\n            // 开始循环\n            while (this.luckyNumber != response.betResult || isLess) {\n              if (this.luckyNumber == response.betResult) {\n                isLess = false;\n              }\n              if (this.luckyNumber < 99) {\n                this.luckyNumber += 1;\n              } else {\n                this.luckyNumber = 0;\n              }\n              await this.delay(20);\n            }\n            await this.delay(200);\n            this.trans = 50;\n            // 更新新的金额\n            dataCenter.wallet.set(dataCenter.wallet.selectId, response.balance);\n            //当次循环结束，如果不继续循环，让按钮可以使用\n            if (!this.isStart) {\n              this.trans = 50;\n              this.isEnable = true;\n            }\n          } else {\n            // 快速模式\n            await this.delay(400);\n            dataCenter.wallet.set(dataCenter.wallet.selectId, response.balance);\n            this.luckyNumber = response.betResult;\n            this.isEnable = true;\n            this.trans = 50;\n          }\n          //添加游戏记录\n          this.numberList.push({\n            name: this.luckyNumber,\n            isWin: this.isWin,\n          });\n          if (this.isWin) {\n            //如果赢了播放音效\n            soundManager.play(DiceWin);\n          }\n          const currency = dataCenter.wallet.getSelectedCurrency();\n          //判断是否为自动模式\n          if (this.isAuto) {\n            if (this.isWin) {\n              // 计算当前盈利\n              this.currentAmount += Number(response.betAmount);\n              //判断是否开启金额累加\n              if (\n                this.childAutobet.OnWin != '' &&\n                this.childAutobet.onWinisReset\n              ) {\n                this.bettingAmount = amountAdd(\n                  this.bettingAmount,\n                  this.childAutobet.OnWin,\n                );\n              }\n            } else {\n              // 计算当前盈利\n              this.currentAmount -= Number(this.bettingAmount);\n              if (\n                this.childAutobet.OnLoss != '' &&\n                this.childAutobet.OnLessisReset\n              ) {\n                this.bettingAmount = amountAdd(\n                  this.bettingAmount,\n                  this.childAutobet.OnLoss,\n                );\n              }\n            }\n            this.downForm();\n            if (this.isStart) {\n              setTimeout(this.startGamePlay, 0);\n            }\n          } else {\n            this.isStart = false;\n            return;\n          }\n        });\n      } else {\n        this.currentAmount = 0;\n        this.isEnable = false;\n        this.trans = 50;\n      }\n    },\n    getLuckyNumber(luckyNumber: number) {\n      this.luckyNumber = luckyNumber;\n    },\n    delay(ms: number) {\n      return new Promise((resolve) => setTimeout(resolve, ms));\n    },\n    generateRandomNumber(min: number, max: number): number {\n      return Math.floor(Math.random() * (max - min + 1)) + min;\n    },\n    formatInput(text: string) {\n      this.bettingAmount = text;\n    },\n    downForm() {\n      if (\n        this.currentAmount >= Number(this.childAutobet.stopOnProfit) &&\n        this.childAutobet.stopOnProfit != ''\n      ) {\n        this.isStart = false;\n      }\n      if (\n        this.currentAmount <= Number(-this.childAutobet.StopOnLoss) &&\n        this.childAutobet.StopOnLoss != ''\n      ) {\n        this.isStart = false;\n      }\n      if (\n        this.currentAmount >= Number(this.childAutobet.MaxBetAmount) &&\n        this.childAutobet.MaxBetAmount != ''\n      ) {\n        this.isStart = false;\n      }\n      if (!this.isEnable) {\n        this.isStart = false;\n      }\n      if (\n        Number(this.childAutobet.count) < 1 &&\n        this.childAutobet.count != ''\n      ) {\n        this.childAutobet.count = '';\n        this.isStart = false;\n      }\n    },\n    updateValue(text: string) {\n      this.bettingAmount = text;\n    },\n    modeClick(isAuto: boolean) {\n      this.isAuto = isAuto;\n    },\n  },\n\n  components: {\n    TopIcon,\n    BoardIn,\n    RollControls,\n    Slider,\n    GameForm,\n    DiceGameMode,\n    Autobet,\n    LastBets,\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.main {\n  display: flex;\n  flex-direction: column;\n  max-width: 700px;\n  position: relative;\n  width: 767px;\n}\n\n.dapps-top-container {\n  flex-direction: column;\n  display: flex;\n  width: 100%;\n  // margin-top: 20px;\n}\n\n.multi-battles-shield {\n  max-width: 161px;\n  align-items: center;\n  background-color: #161f2c;\n  border-radius: 69px;\n  display: flex;\n  max-height: 24px;\n  justify-content: center;\n}\n\n.v-popper {\n  margin: 4px 6px 0px 8px;\n}\n\n.multi-battles-shield-title {\n  color: #55657e;\n  font-size: 12px;\n  font-weight: 600;\n  line-height: 16px;\n}\n\n.battle-name {\n  color: #93acd3;\n  cursor: pointer;\n  font-size: 12px;\n  font-weight: 600;\n  line-height: 16px;\n  margin: 0;\n  max-width: 220px;\n  overflow: hidden;\n  padding-right: 10px;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n}\n\n.dapps-top-buttons {\n  cursor: pointer;\n  display: flex;\n  justify-content: flex-end;\n  margin-bottom: 12px;\n  margin-top: 2px;\n  width: 100%;\n  gap: 3px;\n}\n\n.top {\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n  width: 220px;\n\n  .numbers {\n    display: flex;\n    flex-direction: row;\n    margin-bottom: 7px;\n  }\n\n  .numbers div {\n    border-radius: 5px;\n    color: #55667f;\n    font-size: 12px;\n    font-weight: 600;\n    padding: 1px 2px;\n  }\n\n  .svg-use {\n    fill: #5b6773;\n    height: 16px;\n    transition: all 0.3s;\n    width: 16px;\n  }\n}\n\n.title {\n  display: flex;\n  flex-direction: column;\n  justify-content: flex-start;\n\n  .dice-jackpot-img {\n    width: 146px;\n    height: 36px;\n  }\n\n  .dice-jackpot-sum {\n    text-align: left;\n    color: #ffc51c;\n    font-size: 18px;\n    font-weight: 700;\n  }\n}\n\nbody button {\n  background-color: transparent;\n  border: none;\n  cursor: pointer;\n}\n\n.topMenu {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  margin-bottom: 20px;\n}\n@media only screen and (max-width: 767px) {\n  .main {\n    width: 100%;\n    overflow: hidden;\n  }\n\n  .topMenu {\n    display: flex;\n    flex-direction: column;\n    margin-bottom: 20px;\n  }\n  .dapps-top-container {\n    order: -1;\n  }\n  .dapps-top-buttons {\n    position: relative;\n    flex-direction: row;\n    justify-content: center;\n    width: 100%;\n  }\n  .multi-battles-shield {\n    display: none;\n  }\n  .top {\n    display: flex;\n    flex-direction: row;\n    justify-content: center;\n    gap: 10px;\n  }\n  .numbers {\n    order: 1;\n  }\n  .title {\n    position: relative;\n  }\n  .dice-jackpot-sum {\n    position: absolute;\n    left: 100%;\n    margin-left: 10px;\n    bottom: -5px;\n    width: 100%;\n    font-size: 15px !important;\n  }\n  .last-bets {\n    display: none;\n  }\n}\n</style>\n"]}]}