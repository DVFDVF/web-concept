,CAAC,CAAC,CAAC,EAAE,CAAC;MACvB,CAAC,CAAC,CAAC;IACL,CAAC,CAAC,CAAC,CAAC,CAAC;EACP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/originals/OriginalsContent.vue","sourceRoot":"","sourcesContent":["<template>\n  <div v-show=\"isEmpty\">\n    <div class=\"wrapper\">\n      <img src=\"@/assets/other/noData.png\" alt=\"no data\" class=\"img\" />\n      <p class=\"h5\">{{ emptyTitle }}</p>\n      <p class=\"b3\">{{ emptyText }}</p>\n    </div>\n  </div>\n  <div v-show=\"!isEmpty\">\n    <ul>\n      <li class=\"content\">\n        <div class=\"title\">\n          <OriginalsHeader :currentGroup=\"currentGroup\"></OriginalsHeader>\n        </div>\n        <CardOriginalsEmpty v-show=\"games.length < 1\"></CardOriginalsEmpty>\n        <ul class=\"list-item-row\" v-show=\"games.length > 0\">\n          <li v-for=\"(item, index) in games\" :key=\"index\" class=\"list-item-li\">\n            <CardWrapper\n              :gameData=\"item\"\n              class=\"card-wrapper\"\n              @click=\"cardClick(item)\"\n            ></CardWrapper>\n          </li>\n        </ul>\n      </li>\n    </ul>\n    <div class=\"show-more\">\n      <!-- 如果大于显示更多 -->\n      <div\n        class=\"show-more-button\"\n        v-if=\"total > 48 && currentCount < total\"\n        @click=\"showMore\"\n      >\n        <span>{{ lang.v.org_show }}</span>\n      </div>\n      <div class=\"progress\">\n        <span class=\"progress-value\" :style=\"'width:' + progress + '%'\"> </span>\n      </div>\n      <p class=\"caption\">\n        {{ lang.v.org_so }} {{ currentCount }} {{ lang.v.org_of }} {{ total }}\n        {{ lang.v.org_games }}\n      </p>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\" setup>\nimport { watch } from 'vue';\nimport {\n  getGameById,\n  getGameCategory,\n  getGames,\n  recentGames,\n} from '../home/api';\nimport { GamesDatas, getGameGroupResp } from '../home/api/protocol';\nimport CardWrapper from '../home/casino/LobbySections/card/CardWrapper.vue';\nimport OriginalsHeader from './header/OriginalsHeader.vue';\n\nimport { enterGame } from '@/manager/view';\nimport { splitArrData } from '@/utils';\nimport { onMounted, ref } from 'vue';\nimport CardOriginalsEmpty from './cardOriginalsEmpty.vue';\n\nimport { lang } from '@/lang';\nimport { useRouter } from 'vue-router';\nimport dataCenter from '@/dataCenter';\nconst progress = ref(0);\nconst currentCount = ref(0);\nconst groupId = ref(0);\nconst total = ref(0);\nconst games = ref([] as GamesDatas[]);\nconst pageNumber = ref(1);\nconst isEmpty = ref(false);\nconst btnIsClick = ref(false);\nconst emptyTitle = ref('');\nconst emptyText = ref('');\nconst currentGroup = ref({} as getGameGroupResp);\nconst cardClick = (item: GamesDatas) => {\n  if (!dataCenter.layout.isMobile) {\n    enterGame(item);\n  }\n};\nconst gameCategory = ref([] as getGameGroupResp[]);\nfunction showMore() {\n  if (btnIsClick.value) return;\n  btnIsClick.value = true;\n  pageNumber.value += 1;\n  let path = router.currentRoute.value.path;\n  if (path == '/casino/favorites') {\n    gamesById('Favorites');\n  } else if (path == '/casino/recent') {\n    gamesById('Recent');\n  } else {\n    gamesData();\n  }\n}\n\nconst gamesData = () => {\n  //获取游戏\n  getGames(groupId.value, 48, pageNumber.value).then((data) => {\n    if (!data) return;\n    //判断是否为空\n    if (data.data.length > 0) {\n      isEmpty.value = false;\n    } else {\n      isEmpty.value = true;\n    }\n    games.value.push(...data.data);\n    total.value = data.total;\n    if (total.value <= 48) {\n      currentCount.value = total.value;\n    } else {\n      currentCount.value = games.value.length;\n    }\n    //获取当前进度\n    progress.value = (currentCount.value / data.total) * 100;\n    //设置进度不能越界\n    if (progress.value >= 100) {\n      progress.value = 100;\n    }\n    progress.value = 100;\n    btnIsClick.value = false;\n  });\n};\nconst gamesById = (name: string) => {\n  const arr = splitArrData(name);\n  if (arr.length < 1) {\n    isEmpty.value = true;\n    return;\n  }\n  //获取游戏\n  getGameById(arr, pageNumber.value, 48).then((data) => {\n    if (!data) return;\n    //判断是否为空\n    if (data.data.length > 0) {\n      isEmpty.value = false;\n    } else {\n      isEmpty.value = true;\n    }\n    games.value.push(...data.data);\n    total.value = data.total;\n    if (total.value <= 48) {\n      currentCount.value = total.value;\n    } else {\n      currentCount.value = games.value.length;\n    }\n    //获取当前进度\n    progress.value = (currentCount.value / data.total) * 100;\n    //设置进度不能越界\n    if (progress.value >= 100) {\n      progress.value = 100;\n    }\n    progress.value = 100;\n    btnIsClick.value = false;\n  });\n};\nfunction recentGamesFunc() {\n  //获取游戏\n  recentGames('48', pageNumber.value.toString()).then((data) => {\n    if (!data) return;\n    //判断是否为空\n    if (data.game.data.length > 0) {\n      isEmpty.value = false;\n    } else {\n      isEmpty.value = true;\n    }\n    // games.value = data.game.data;\n    games.value.push(...data.game.data);\n\n    total.value = data.game.total;\n    if (total.value <= 48) {\n      currentCount.value = total.value;\n    } else {\n      currentCount.value = games.value.length;\n    }\n    //获取当前进度\n    progress.value = (currentCount.value / data.game.total) * 100;\n    //设置进度不能越界\n    if (progress.value >= 100) {\n      progress.value = 100;\n    }\n    progress.value = 100;\n    btnIsClick.value = false;\n  });\n}\nconst router = useRouter();\nwatch(\n  () => router.currentRoute.value,\n  (currentRoute) => {\n    games.value = [];\n    if (currentRoute.path == '/casino/favorites') {\n      gamesById('Favorites');\n      emptyTitle.value = 'The list of favorite games is empty';\n      emptyText.value = 'Tag a game with \"like\" and it will be displayed here';\n      currentGroup.value = {\n        id: -1,\n        name: 'Favorites',\n        categoryId: '',\n        icon: '/svg/sprite.svg#favorites',\n        logo: '',\n        description: '',\n        extra: '',\n        total: 0,\n      };\n    } else if (currentRoute.path == '/casino/recent') {\n      emptyTitle.value = 'The list of recent games is empty';\n      emptyText.value = 'Currently, there are no recent games to display.';\n      currentGroup.value = {\n        id: -2,\n        name: 'Recent',\n        categoryId: '',\n        icon: '/svg/sprite.svg#recent',\n        logo: '',\n        description: '',\n        extra: '',\n        total: 0,\n      };\n      gamesById('Recent');\n    } else {\n      if (currentRoute.query.groupId) {\n        groupId.value = parseInt(currentRoute.query.groupId.toString());\n        getGameCategory().then((data) => {\n          if (data) {\n            gameCategory.value = data;\n            const item = data.find((item) => item.id === groupId.value);\n            if (item) {\n              currentGroup.value = item;\n            }\n            gamesData();\n          }\n        });\n      }\n    }\n  },\n);\n\nonMounted(() => {\n  games.value = [];\n  let path = router.currentRoute.value.path;\n  if (path == '/casino/favorites') {\n    gamesById('Favorites');\n    emptyTitle.value = 'The list of favorite games is empty';\n    emptyText.value = 'Tag a game with \"like\" and it will be displayed here';\n  } else if (path == '/casino/recent') {\n    emptyTitle.value = 'The list of recent games is empty';\n    emptyText.value = 'Currently, there are no recent games to display.';\n    gamesById('Recent');\n  } else {\n    if (router.currentRoute.value.query.groupId) {\n      groupId.value = parseInt(\n        router.currentRoute.value.query.groupId.toString(),\n      );\n      getGameCategory().then((data) => {\n        if (data) {\n          gameCategory.value = data;\n          const item = data.find((item) => item.id === groupId.value);\n          if (item) {\n            currentGroup.value = item;\n          }\n          gamesData();\n        }\n      });\n    }\n  }\n});\n</script>\n\n<style lang=\"less\" scoped>\n.h5 {\n  color: white;\n  font-size: 18px;\n  font-weight: 600;\n  letter-spacing: -0.02em;\n  line-height: 24px;\n  margin-bottom: 1rem;\n  margin-top: 0;\n}\n.b3 {\n  color: #55657e;\n  font-size: 12px;\n  font-weight: 500;\n  line-height: 18px;\n  margin-bottom: 1rem;\n}\n.list-item-row {\n  display: flex;\n  flex-direction: row;\n  flex-wrap: wrap;\n}\n\nul,\nli {\n  list-style: none;\n  padding: 0;\n  margin: 0;\n}\n\nli {\n  margin: 6px;\n}\n\n.show-more {\n  align-items: center;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  margin-block: 24px 36px;\n}\n\n.progress {\n  background-color: #292f42;\n  border-radius: 4px;\n  height: 4px;\n  margin-bottom: 8px;\n  margin-top: 12px;\n  position: relative;\n  width: 130px;\n}\n\n.progress-value {\n  background-color: #2283f6;\n  border-radius: 4px;\n  display: block;\n  height: 100%;\n  left: 0;\n  position: absolute;\n  top: 0;\n}\n\n.caption {\n  color: #55657e;\n  font-size: 12px;\n  margin: 0;\n}\n.show-more-button {\n  padding: 10px 20px;\n  background-color: #1c2532;\n  border-color: transparent;\n  color: #55657e;\n  border-radius: 8px;\n  font-size: 12px;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  font-weight: 700;\n}\n.show-more-button:hover {\n  background-color: #202a39;\n  color: #93acd3;\n}\n.card-wrapper {\n  width: 172px;\n  height: 227px;\n}\n.wrapper {\n  margin-bottom: 80px;\n}\n\n@media only screen and (max-width: 767px) {\n  .content {\n    margin: 6px 0;\n  }\n  .list-item-li {\n    flex: 0 0 calc(33.33% - 12px) !important; /* 子元素占据 1/3 的宽度 */\n    box-sizing: border-box; /* 让边框和内边距计算在元素的宽度内 */\n  }\n  .card-wrapper {\n    width: auto;\n    height: auto;\n  }\n}\n</style>\n"]}]}