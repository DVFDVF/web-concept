","sourcesContent":["<template>\n  <div>\n    <div class=\"chat-content\">\n      <div class=\"center\">\n        <img\n          class=\"admin\"\n          src=\"@/assets/chats/admin.jpg\"\n          v-show=\"userBrief.id == 1\"\n        />\n        <div class=\"icon\" v-show=\"userBrief.id != 1\">\n          <svg\n            width=\"100\"\n            height=\"100\"\n            viewBox=\"0 0 100 100\"\n            xmlns=\"http://www.w3.org/2000/svg\"\n            class=\"progress\"\n          >\n            <path\n              :d=\"experienceRatio\"\n              fill=\"none\"\n              stroke=\"#1BB83D\"\n              stroke-width=\"5\"\n            ></path>\n          </svg>\n          <Avatar class=\"user-icon\" :size=\"32\" :user=\"userBrief\" />\n        </div>\n        <div class=\"level\" v-show=\"userBrief.id != 1\">\n          <span>{{ userBrief.level }}</span>\n        </div>\n      </div>\n      <div\n        :class=\"['msg-content', { 'admin-content': userBrief.id == 1 }]\"\n        v-show=\"true\"\n      >\n        <div class=\"msg-content-header\">\n          <div class=\"msg-content-header-left\">\n            <div class=\"msg-header\">\n              <span class=\"msg-content-user-name\">{{ userBrief.name }}</span>\n              <div class=\"msg-content-header-right\">\n                <!-- tips暂不需要 -->\n                <!-- <button type=\"button\" class=\"msg-action\" @click=\"openTips\">\n                  <svg\n                    class=\"svg-use\"\n                    width=\"16\"\n                    height=\"16\"\n                    focusable=\"false\"\n                    aria-hidden=\"true\"\n                  >\n                    <use xlink:href=\"/svg/sprite.svg#icon-tips\"></use>\n                  </svg>\n                </button> -->\n                <button\n                  type=\"button\"\n                  class=\"msg-action\"\n                  @click=\"replyClick(msgItem)\"\n                >\n                  <svg\n                    class=\"svg-use\"\n                    width=\"16\"\n                    height=\"16\"\n                    focusable=\"false\"\n                    aria-hidden=\"true\"\n                  >\n                    <use xlink:href=\"/svg/sprite.svg#icon-chat-reply\"></use>\n                  </svg>\n                </button>\n              </div>\n            </div>\n            <!-- 回复消息 -->\n            <div class=\"msg-content-center\" v-show=\"msgItem?.replyId != 0\">\n              <div class=\"meg-line\"></div>\n              <div class=\"meg-sub-center\">\n                <span\n                  :class=\"[\n                    'user-name',\n                    { 'user-name-red': replyBrief.name == 'admin' },\n                  ]\"\n                  >{{ replyBrief.name }}</span\n                >\n                <div class=\"text-sub\">{{ replyBrief.content }}</div>\n              </div>\n            </div>\n            <!-- 暂不需要 -->\n            <!-- <div class=\"msg-img\" v-show=\"false\">\n              <img\n                src=\"https://img1.baidu.com/it/u=502963393,1491298719&fm=253&fmt=auto&app=138&f=JPEG?w=500&h=500\"\n              />\n            </div> -->\n            <p class=\"text\">{{ msgItem?.content }}</p>\n          </div>\n        </div>\n      </div>\n      <!-- 分享游戏 -->\n      <ShareGame v-show=\"false\"></ShareGame>\n      <!-- 盈利 -->\n      <ShareProfits v-show=\"false\"></ShareProfits>\n\n      <!-- 聊天喜欢，暂不需要 -->\n      <!-- <div class=\"like-block\">\n        <button class=\"like-block-btn\">\n          <svg\n            class=\"svg-use\"\n            width=\"16\"\n            height=\"16\"\n            focusable=\"false\"\n            aria-hidden=\"true\"\n          >\n            <use xlink:href=\"/svg/sprite.svg#like-heart\"></use>\n          </svg>\n        </button>\n        <span class=\"like-block-count\">0</span>\n      </div> -->\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { UserBriefRespItem } from '@/api/protocol';\nimport Avatar from '@/components/Avatar.vue';\nimport { ChatData } from '@/dataCenter/chatRoom';\nimport userManager from '@/manager/userManager';\nimport { openModal } from '@/manager/view';\nimport { getLevel, updateSVGPath } from '@/utils';\nimport { defineComponent } from 'vue';\nimport { getChat } from '../api/index';\nimport { GetChatResp } from '../api/protocol';\nimport ShareGame from './ShareGame.vue';\nimport ShareProfits from './ShareProfits.vue';\nimport TipsForPerson from './tips/TipsForPerson.vue';\n\nexport default defineComponent({\n  components: { ShareGame, ShareProfits, Avatar },\n  data() {\n    return {\n      isReply: false,\n      userBrief: {} as UserBriefRespItem,\n      // replyBrief: {} as UserBriefRespItem,\n      replyBrief: {} as GetChatResp,\n      experienceRatio: updateSVGPath(0),\n    };\n  },\n  props: {\n    msgItem: {\n      type: Object as () => ChatData,\n    },\n  },\n  methods: {\n    openTips() {\n      openModal(TipsForPerson);\n    },\n    replyClick(msgItem: ChatData | undefined) {\n      if (msgItem) {\n        this.$emit('replyClick', msgItem, this.userBrief);\n      }\n    },\n    replyData() {\n      if (this.msgItem?.replyId) {\n        if (this.msgItem.replyId != 0) {\n          getChat(this.msgItem.replyId).then((replyBrief) => {\n            if (!replyBrief) return;\n            this.replyBrief = replyBrief;\n          });\n        }\n      }\n    },\n  },\n  mounted() {\n    if (this.msgItem?.userId) {\n      userManager.getUser(this.msgItem.userId).then((userBrief) => {\n        if (userBrief) {\n          this.userBrief = userBrief;\n          this.experienceRatio = updateSVGPath(\n            getLevel(userBrief.level, userBrief.exp) * 360,\n          );\n          this.replyData();\n        }\n      });\n    }\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.chat-content {\n  display: flex;\n  flex-direction: row;\n  padding: 4px 8px;\n}\n\n.center {\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: flex-end;\n}\n\n.icon {\n  width: 40px;\n  height: 48px;\n  margin-right: 6px;\n  position: relative;\n}\n.admin {\n  width: 40px;\n  margin-right: 6px;\n  margin-bottom: 6px;\n  position: relative;\n}\n\n.icon svg {\n  width: 40px;\n  height: 48px;\n}\n\n.user-icon {\n  position: absolute;\n  left: 4px;\n  top: 8px;\n}\n.level {\n  align-items: center;\n  background-color: #2283f6;\n  border: 2px solid #111923;\n  border-radius: 50px;\n  display: flex;\n  height: 16px;\n  justify-content: center;\n  padding: 2.5px 4px;\n  margin-top: -13px;\n  z-index: 10;\n  color: white;\n}\n\n.msg-content {\n  background-color: #1c2532;\n  width: 100%;\n  border-radius: 12px;\n  padding: 8px 12px;\n\n  .msg-content-header {\n    align-items: center;\n    color: #fff;\n    display: flex;\n    width: 100%;\n    justify-content: space-between;\n    margin-bottom: 4px;\n    align-items: flex-start;\n\n    .msg-content-header-left {\n      align-items: center;\n      display: flex;\n      flex-direction: column;\n      align-items: flex-start;\n      text-align: left;\n      gap: 5px;\n      width: 100%;\n\n      .msg-content-user-name {\n        font-size: 12px;\n        font-weight: 500;\n        line-height: 20px;\n      }\n    }\n\n    .msg-content-header-right {\n      color: #55657e;\n      display: flex;\n      gap: 4px;\n      justify-content: flex-end;\n      margin-right: 2px;\n\n      .msg-action {\n        padding: 0;\n        transition: color 0.1s ease-in-out;\n      }\n\n      .msg-action:hover {\n        color: white;\n      }\n    }\n  }\n}\n\n.msg-content-center {\n  display: flex;\n  flex-direction: row;\n  justify-content: space-between;\n  position: relative;\n\n  .meg-sub-center {\n    margin-left: 10px;\n  }\n\n  .meg-line {\n    display: flex;\n    width: 3px;\n    position: absolute;\n    top: 0;\n    bottom: 0;\n    background-color: #263041;\n  }\n}\n\n.msg-content > p {\n  font-size: 13px;\n}\n\n.text {\n  color: #93acd3;\n  margin: 0;\n  word-break: break-word;\n  font-size: 13px;\n}\n\n.text-sub {\n  color: #93acd3;\n  margin: 0;\n  word-break: break-word;\n  font-size: 12px;\n}\n\n.like-block {\n  align-items: center;\n  justify-content: center;\n  display: flex;\n  flex-direction: column;\n  margin-left: 8px;\n  margin-right: 0;\n\n  .like-block-btn {\n    padding: 0;\n  }\n\n  .like-block-count {\n    color: #55657e;\n    cursor: default;\n    font-size: 9px;\n    font-weight: 600;\n    text-align: center;\n    transition: color 0.15s linear;\n  }\n}\n\n.msg-img img {\n  width: 100%;\n  border-radius: 11px;\n}\n\n.msg-header {\n  display: flex;\n  flex-direction: row;\n  width: 100%;\n  justify-content: space-between;\n}\n.admin-content {\n  border: 1px solid rgb(244, 59, 76);\n}\n.admin-content\n  .msg-content-header\n  .msg-content-header-left\n  .msg-content-user-name {\n  color: rgb(244, 59, 76);\n  font-weight: 700;\n}\n.admin-content .text {\n  color: rgb(253, 214, 258);\n}\n.user-name {\n  font-size: 12px;\n  font-weight: 500;\n  line-height: 20px;\n}\n.user-name-red {\n  color: rgb(244, 59, 76);\n}\n.user-name-red + .text-sub {\n  color: #fdd6ff;\n}\n</style>\n"]}]}