petitionResp, ranksDataItem } from '../../api/competitionProtocol';\nimport Avatar from '@/components/Avatar.vue';\nexport default {\n  data() {\n    return {\n      dataCenter,\n    };\n  },\n  components: {\n    Avatar,\n  },\n  props: {\n    ranksItem: {\n      type: Object as () => ranksDataItem,\n    },\n    ranksIndex: {\n      type: Number,\n      required: true,\n    },\n  },\n  methods: {\n    /**前三个排行图 */\n    getPlaceImage(index: number) {\n      if (index === 0) {\n        return require('@/assets/competition/top1.png');\n      } else if (index === 1) {\n        return require('@/assets/competition/top2.png');\n      } else if (index === 2) {\n        return require('@/assets/competition/top3.png');\n      }\n    },\n    //查询货币icon\n    getCuurrencyIcon(currencyid: number) {\n      if (dataCenter.webConfig.cryptoCurrencies.concat()) {\n        const foundCurrency = dataCenter.webConfig.cryptoCurrencies\n          .concat()\n          .find((currency) => currency.id === currencyid);\n        return foundCurrency?.icon;\n      }\n    },\n  },\n};\n</script>\n\n<style lang=\"less\" scoped>\n.battle-table-modals {\n  background: rgba(22, 31, 44, 0.8);\n  border: none;\n  border-radius: 8px;\n  height: 50px;\n  margin-bottom: 6px;\n  align-items: center;\n  display: flex;\n  padding: 12px 0;\n}\n.battle-table-place {\n  padding: 0 16px;\n  width: calc(19% - 17.48px);\n  color: #93acd3;\n  font-size: 14px;\n  font-weight: 600;\n  line-height: 20px;\n}\n.battle-table-player {\n  padding: 0 16px;\n  width: calc(37% - 34.04px);\n  display: flex;\n  cursor: pointer;\n  align-items: center;\n}\n.user-wrap-cut {\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n}\n.user-cut {\n  overflow-x: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n}\n.user-name {\n  font-size: 14px;\n  font-style: normal;\n  font-weight: 600;\n  letter-spacing: -0.28px;\n  line-height: 125%;\n  margin-left: 8px;\n}\n.battle-table-profit {\n  padding: 0 16px;\n  width: calc(44% - 40.48px);\n  align-items: center;\n  display: flex;\n}\n.battle-profit-img {\n  margin-right: 6px;\n}\n.balance {\n  font-weight: 600;\n  letter-spacing: -0.5px;\n  margin: auto 0;\n  overflow-x: hidden;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n}\n.battle-table-prize {\n  width: 124px;\n  padding: 0 16px;\n}\n.battle-table-body-twix-value {\n  display: none;\n}\n@media only screen and (max-width: 767px) {\n  .battle-table-profit {\n    display: none;\n  }\n  .battle-table-body-twix-value {\n    display: inline;\n    color: #93acd3;\n    font-size: 14px;\n    font-style: normal;\n    font-weight: 600;\n    letter-spacing: -0.28px;\n    line-height: 125%;\n  }\n  .battle-table-player {\n    display: flex;\n    justify-content: center;\n    align-items: center;\n  }\n  .battle-table-place {\n    width: calc(29% - 28.71px);\n  }\n  .battle-table-player {\n    width: calc(71% - 70.29px);\n    display: flex;\n    justify-content: flex-start;\n  }\n  .battle-table-prize {\n    width: 124px;\n  }\n}\n</style>\n"]}]}