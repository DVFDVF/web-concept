';\nimport { message } from 'ant-design-vue';\nimport { defineComponent } from 'vue';\n\nexport default defineComponent({\n  components: {},\n  computed: {},\n  data() {\n    return {\n      list: [\n        {\n          key: 'twitter',\n          name: 'twitter',\n          icon: '/svg/sprite.svg#icon-twitter',\n          backColor: '',\n        },\n        {\n          key: 'bitcointalk',\n          name: 'bitcointalk',\n          icon: '/svg/sprite.svg#icon-bitcointalk',\n          backColor: '',\n        },\n        {\n          key: 'discord',\n          name: 'discord',\n          icon: '/svg/sprite.svg#icon-discord',\n          backColor: '',\n        },\n        {\n          key: 'reddit',\n          name: 'reddit',\n          icon: '/svg/sprite.svg#icon-reddit',\n          backColor: '',\n        },\n        {\n          key: 'tiktok',\n          name: 'tiktok',\n          icon: '/svg/sprite.svg#icon-tiktok',\n          backColor: '',\n        },\n        {\n          key: 'youtube',\n          name: 'youtube',\n          icon: '/svg/sprite.svg#icon-youtube',\n          backColor: '',\n        },\n        {\n          key: 'debank',\n          name: 'debank',\n          icon: '/svg/sprite.svg#icon-debank',\n          backColor: '',\n        },\n      ],\n    };\n  },\n  methods: {\n    openLink() {\n      tip.info('Not yet open');\n    },\n  },\n});\n</script>\n\n<style lang=\"less\" scoped>\n.social-icons-tooltip {\n  background-color: #202a39;\n  border-radius: 100px;\n  bottom: calc(100% + 8px);\n  color: #55657e;\n  font-size: 9px;\n  font-weight: 500;\n  opacity: 0;\n  padding: 2px 5px;\n  pointer-events: none;\n  position: absolute;\n  transform: translateY(50%);\n  transition: all 0.3s;\n  white-space: nowrap;\n}\n\n.social-iconsicon {\n  align-items: center;\n  border-radius: 8px;\n  display: flex;\n  flex-direction: column;\n  height: 32px;\n  justify-content: center;\n  position: relative;\n  text-decoration: none;\n  transition: all 0.3s;\n  width: 32px;\n  margin-right: 5px;\n  cursor: pointer;\n  margin-bottom: 20px;\n  color: white;\n}\n\n.social-iconsicon--show-tip:hover .social-icons-tooltip {\n  opacity: 1;\n  transform: translateY(0);\n}\n\n.social-iconsicon--scale:hover {\n  transform: scale(1.15);\n}\n\n.back-social-0 {\n  background: linear-gradient(135deg, #3a3a3a 0%, #000 100%);\n}\n\n.back-social-1 {\n  background: linear-gradient(135deg, #ffaf37 0%, #f07300 100%);\n}\n\n.back-social-2 {\n  background: linear-gradient(135deg, #5662f6 0%, #3d2a8a 100%);\n}\n\n.back-social-3 {\n  background: linear-gradient(135deg, #ff8a00 0%, #ff4500 100%);\n}\n\n.back-social-4 {\n  background: linear-gradient(135deg, #00c2ff 0%, #1875ff 31.77%, #ff002e 100%);\n}\n\n.back-social-5 {\n  background: linear-gradient(135deg, #ed1d49 0%, #95041e 100%);\n}\n\n.back-social-6 {\n  background: linear-gradient(135deg, #ff7d5a 0%, #ff4c1c 100%);\n}\n</style>\n"]}]}