,EAAE,CAAC,CAAC,CAAC;EACjB;EACA,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE;IACf,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;EACtC;AACF","file":"/Users/nuomi/Desktop/项目/fury-client/fury-client/src/views/home/casino/RotationChart/RotationChatRoll.vue","sourceRoot":"","sourcesContent":["<template>\n  <section\n    class=\"page-section\"\n    aria-label=\"Main banners\"\n    :style=\"{ '--fll-width': +ProgressBar + '%' }\"\n  >\n    <div class=\"embla\">\n      <div\n        class=\"embla-viewport\"\n        v-on:mousedown=\"startDrag\"\n        v-on:mousemove=\"onDrag\"\n        v-on:mouseup=\"stopDrag\"\n        v-on:touchstart=\"touchStartDrag\"\n        v-on:touchmove=\"touchOnDrag\"\n        v-on:touchend=\"touchStopDrag\"\n      >\n        <div\n          class=\"embla-container\"\n          ref=\"emblaContent\"\n          :style=\"{ transform: 'translate3d(' + transX + 'px, 0px, 0px)' }\"\n        >\n          <div class=\"embla-slide\" v-for=\"item in advertiseData\" :key=\"item.id\">\n            <a\n              class=\"embla-slide-inner\"\n              draggable=\"false\"\n              @click=\"clickCmd(item.cmd)\"\n            >\n              <div class=\"wrapper\">\n                <Image\n                  :src=\"item.image\"\n                  class=\"banner-picture\"\n                  loading=\"lazy\"\n                  draggable=\"false\"\n                />\n              </div>\n            </a>\n          </div>\n        </div>\n      </div>\n      <div class=\"dots-wrapper\">\n        <ul class=\"dots\">\n          <li class=\"dots-control dots-control-prev\" @click=\"leftSwitch\">\n            <span class=\"icon\">\n              <svg\n                xmlns=\"http://www.w3.org/2000/svg\"\n                fill=\"none\"\n                viewBox=\"0 0 20 20\"\n              >\n                <path\n                  fill=\"currentColor\"\n                  fill-rule=\"evenodd\"\n                  d=\"M12.59 14.597a1.358 1.358 0 0 0 0-1.944L9.38 9.5l3.21-3.153a1.358 1.358 0 0 0 0-1.944 1.418 1.418 0 0 0-1.98 0l-4.2 4.125a1.358 1.358 0 0 0 0 1.944l4.2 4.125a1.418 1.418 0 0 0 1.98 0Z\"\n                  clip-rule=\"evenodd\"\n                ></path>\n              </svg>\n            </span>\n          </li>\n          <li\n            v-for=\"(item, index) in dots\"\n            :key=\"index\"\n            :class=\"dotStyle(index + 1)\"\n          >\n            <div class=\"start-point\"></div>\n            <div class=\"fill\"></div>\n          </li>\n          <li class=\"dots-control\" @click=\"rightSwitch\">\n            <span class=\"icon\">\n              <svg\n                xmlns=\"http://www.w3.org/2000/svg\"\n                fill=\"none\"\n                viewBox=\"0 0 20 20\"\n              >\n                <path\n                  fill=\"currentColor\"\n                  fill-rule=\"evenodd\"\n                  d=\"M7.41 14.597a1.358 1.358 0 0 1 0-1.944L10.62 9.5 7.41 6.347a1.358 1.358 0 0 1 0-1.944 1.418 1.418 0 0 1 1.98 0l4.2 4.125a1.358 1.358 0 0 1 0 1.944l-4.2 4.125a1.418 1.418 0 0 1-1.98 0Z\"\n                  clip-rule=\"evenodd\"\n                ></path>\n              </svg>\n            </span>\n          </li>\n        </ul>\n      </div>\n    </div>\n  </section>\n</template>\n<script lang=\"ts\">\nimport dataCenter from '@/dataCenter';\nimport { execute } from '@/manager/cmd';\nimport { openModal } from '@/manager/view';\nimport { MIN_MOVE_DISTANCE } from '@/utils/define';\nimport { defineComponent } from 'vue';\nimport { getAdvertiseGames } from '../../api';\nimport { GetAdvertiseResp } from '../../api/protocol';\nimport Login from '../../login/Login.vue';\n\ninterface IData {\n  dotCount: number;\n}\n\nexport default defineComponent({\n  data() {\n    return {\n      isSmallScreen: false,\n      advertiseData: [] as GetAdvertiseResp[],\n      draggings: false,\n      isRoll: false,\n      startX: 0,\n      lastX: 0,\n      transX: -0,\n      dotCount: 0,\n      ProgressBar: 10,\n      ProgressTag: 1,\n      emblaContentRect: new DOMRect(),\n      timer: 0,\n      moveDistance: 0, // 移动距离，如果超过MIN_MOVE_DISTANCE,则认为是移动\n    };\n  },\n  computed: {\n    dots() {\n      return Array.from({ length: (this.$data as IData).dotCount });\n    },\n  },\n  methods: {\n    clickCmd(cmdString: string) {\n      if (this.moveDistance > MIN_MOVE_DISTANCE) {\n        // 说明是移动的\n        return;\n      }\n      if (cmdString == 'CHARGE') {\n        if (!dataCenter.user.data) {\n          openModal(Login);\n          return;\n        }\n      }\n      execute(cmdString);\n    },\n    startDrag(e: MouseEvent) {\n      document.addEventListener('mousemove', this.onDrag);\n      document.addEventListener('mouseup', this.stopDrag);\n      const container = this.$refs.emblaContent as HTMLElement;\n      this.lastX = e.clientX;\n      if (container) {\n        this.emblaContentRect = container.getBoundingClientRect();\n      }\n      this.moveDistance = 0;\n      this.draggings = true;\n      this.startX = this.transX;\n    },\n\n    onDrag(e: MouseEvent) {\n      if (this.draggings) {\n        this.isRoll = true;\n        const percentage = e.clientX - this.lastX + this.startX;\n        // 累积移动距离，超过阈值就不触发点击事件\n        this.moveDistance += Math.abs(percentage);\n\n        if (percentage > this.emblaContentRect.width * 0.4) {\n          this.transX = this.emblaContentRect.width * 0.4;\n        } else if (\n          percentage <\n          -((this.emblaContentRect.width - 32) / 3) *\n            this.advertiseData.length +\n            (this.advertiseData.length - 1) * 16 +\n            this.emblaContentRect.width * 0.6\n        ) {\n          this.transX = -(\n            ((this.emblaContentRect.width - 32) / 3) *\n              this.advertiseData.length +\n            (this.advertiseData.length - 1) * 16 -\n            this.emblaContentRect.width * 0.6\n          );\n        } else {\n          this.transX = percentage;\n        }\n      }\n    },\n\n    stopDrag(e: MouseEvent) {\n      document.removeEventListener('mousemove', this.onDrag);\n      document.removeEventListener('mouseup', this.stopDrag);\n      this.isRoll = false;\n      const percentage = e.clientX - this.lastX + this.startX;\n      if (percentage > 0) {\n        this.transX = 0;\n        //赋值给序号\n        this.ProgressTag = 1;\n        this.ProgressBar = 10;\n      } else if (\n        percentage <\n        -((this.emblaContentRect.width - 32) / 3) * this.advertiseData.length +\n          (this.advertiseData.length - 2) +\n          this.emblaContentRect.width\n      ) {\n        this.transX = -(\n          ((this.emblaContentRect.width - 32) / 3) * this.advertiseData.length +\n          (this.advertiseData.length - 1) * 16 -\n          this.emblaContentRect.width\n        );\n        //赋值给序号\n        this.ProgressTag = this.advertiseData.length - 2;\n        this.ProgressBar = 10;\n      } else {\n        //计算当前序列\n        const result = Math.floor(\n          -(percentage / ((this.emblaContentRect.width - 32) / 3 + 16) - 0.4) +\n            1,\n        );\n\n        //赋值给序号\n        this.ProgressTag = result;\n        this.ProgressBar = 10;\n        //滑动到目标位置\n        this.transX = -(\n          ((this.emblaContentRect.width - 32) / 3) * (this.ProgressTag - 1) +\n          16 * (this.ProgressTag - 1)\n        );\n      }\n\n      this.draggings = false;\n    },\n    //左滑\n    leftSwitch() {\n      if (this.isSmallScreen) {\n        this.leftPhoneModeSwitch();\n        return;\n      }\n      const container = this.$refs.emblaContent as HTMLElement;\n      if (container) {\n        this.emblaContentRect = container.getBoundingClientRect();\n      }\n      if (this.ProgressTag == 1) {\n        this.ProgressTag = this.advertiseData.length - 2;\n      } else {\n        this.ProgressTag -= 1;\n      }\n      this.ProgressBar = 10;\n      if (this.transX >= 0) {\n        this.transX = -(\n          ((this.emblaContentRect.width - 32) / 3) * this.advertiseData.length +\n          (this.advertiseData.length - 1) * 16 -\n          this.emblaContentRect.width\n        );\n      } else {\n        this.transX = -(\n          ((this.emblaContentRect.width - 32) / 3) * (this.ProgressTag - 1) +\n          16 * (this.ProgressTag - 1)\n        );\n      }\n    },\n    //右滑\n    rightSwitch() {\n      if (this.isSmallScreen) {\n        this.rightPhoneModeSwitch();\n        return;\n      }\n      const container = this.$refs.emblaContent as HTMLElement;\n      if (container) {\n        this.emblaContentRect = container.getBoundingClientRect();\n      }\n      if (this.ProgressTag == this.advertiseData.length - 2) {\n        this.ProgressTag = 0;\n      }\n      {\n        this.ProgressTag += 1;\n      }\n      this.ProgressBar = 10;\n      if (\n        this.transX <=\n        -(\n          ((this.emblaContentRect.width - 32) / 3) * this.advertiseData.length +\n          (this.advertiseData.length - 1) * 16 -\n          this.emblaContentRect.width\n        )\n      ) {\n        this.transX = 0;\n      } else {\n        this.transX = -(\n          ((this.emblaContentRect.width - 32) / 3) * (this.ProgressTag - 1) +\n          16 * (this.ProgressTag - 1)\n        );\n      }\n    },\n    //手机模式左滑\n    leftPhoneModeSwitch() {\n      const container = this.$refs.emblaContent as HTMLElement;\n      if (container) {\n        this.emblaContentRect = container.getBoundingClientRect();\n      }\n      if (this.ProgressTag == 1) {\n        this.ProgressTag = this.advertiseData.length;\n      } else {\n        this.ProgressTag -= 1;\n      }\n      this.ProgressBar = 10;\n      if (this.transX >= 0) {\n        this.transX =\n          -this.emblaContentRect.width * (this.advertiseData.length - 1) -\n          this.ProgressTag * 16;\n      } else {\n        this.transX =\n          -this.emblaContentRect.width * (this.ProgressTag - 1) -\n          (this.ProgressTag - 1) * 16;\n      }\n    },\n    //手机模式右滑\n    rightPhoneModeSwitch() {\n      const container = this.$refs.emblaContent as HTMLElement;\n      if (container) {\n        this.emblaContentRect = container.getBoundingClientRect();\n      }\n      if (this.ProgressTag == this.advertiseData.length) {\n        this.ProgressTag = 0;\n      }\n      {\n        this.ProgressTag += 1;\n      }\n      this.ProgressBar = 10;\n      if (\n        this.transX <=\n        -this.emblaContentRect.width * this.advertiseData.length -\n          this.ProgressTag * 16\n      ) {\n        this.transX = 0;\n      } else {\n        this.transX =\n          -this.emblaContentRect.width * (this.ProgressTag - 1) -\n          (this.ProgressTag - 1) * 16;\n      }\n    },\n    //手机触摸事件\n    touchStartDrag(e: TouchEvent) {\n      const container = this.$refs.emblaContent as HTMLElement;\n      this.lastX = e.touches[0].clientX;\n      if (container) {\n        this.emblaContentRect = container.getBoundingClientRect();\n      }\n      this.moveDistance = 0;\n      this.draggings = true;\n      this.startX = this.transX;\n    },\n    touchOnDrag(e: TouchEvent) {\n      if (this.draggings) {\n        const percentage = e.touches[0].clientX - this.lastX + this.startX;\n        // 累积移动距离，超过阈值就不触发点击事件\n        this.moveDistance += Math.abs(percentage);\n\n        if (percentage > this.emblaContentRect.width * 0.4) {\n          this.transX = this.emblaContentRect.width * 0.4;\n        } else if (\n          percentage <\n          -this.emblaContentRect.width * (this.advertiseData.length - 1) -\n            this.ProgressTag * 16 -\n            this.emblaContentRect.width * 0.6\n        ) {\n          this.transX =\n            -this.emblaContentRect.width * (this.advertiseData.length - 1) -\n            this.ProgressTag * 16 -\n            this.emblaContentRect.width * 0.6;\n        } else {\n          this.transX = percentag